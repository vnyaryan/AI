{
    "command_group": "az self-help diagnostic",
    "description": "Diagnostic command will help you create diagnostics for azure resources.",
    "status": "Preview",
    "commands": [
        {
            "name": "az self-help diagnostic create",
            "summary": "Creates a diagnostics that will help you troubleshoot an issue with your azure resource.",
            "syntax": "az self-help diagnostic create --diagnostic-name\n                               --scope\n                               [--global-parameters]\n                               [--insights]\n                               [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]",
            "required_parameters": [
                "--diagnostic-name",
                "--scope"
            ],
            "optional_parameters": [
                "--global-parameters",
                "--insights",
                "--no-wait"
            ],
            "examples": [
                "az self-help diagnostic create --diagnostic-name diagnostic-name  --insights [{solutionId:Demo2InsightV2}] --scope 'subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/myresourceGroup/providers/Microsoft.KeyVault/vaults/test-keyvault-non-read'"
            ]
        },
        {
            "name": "az self-help diagnostic show",
            "summary": "Get the diagnostics using the 'diagnosticsResourceName' you chose while creating the diagnostic.",
            "syntax": "az self-help diagnostic show --diagnostic-name\n                             --scope",
            "required_parameters": [
                "--diagnostic-name",
                "--scope"
            ],
            "optional_parameters": [],
            "examples": [
                "az self-help diagnostic show --diagnostic-name diagnostic-name --scope 'subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/myresourceGroup/providers/Microsoft.KeyVault/vaults/test-keyvault-non-read'"
            ]
        },
        {
            "name": "az self-help diagnostic wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az self-help diagnostic wait --diagnostic-name\n                             --scope\n                             [--created]\n                             [--custom]\n                             [--deleted]\n                             [--exists]\n                             [--interval]\n                             [--timeout]\n                             [--updated]",
            "required_parameters": [
                "--diagnostic-name",
                "--scope"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}