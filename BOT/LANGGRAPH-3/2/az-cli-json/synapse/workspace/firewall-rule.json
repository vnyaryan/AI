{
    "command_group": "az synapse workspace firewall-rule",
    "description": "Manage a workspace's firewall rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az synapse workspace firewall-rule create",
            "summary": "Create a firewall rule.",
            "syntax": "az synapse workspace firewall-rule create --end-ip-address\n                                          --name\n                                          --resource-group\n                                          --start-ip-address\n                                          --workspace-name\n                                          [--no-wait]",
            "required_parameters": [
                "--end-ip-address",
                "--name -n",
                "--resource-group -g",
                "--start-ip-address",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse workspace firewall-rule create --name allowAll --workspace-name testsynapseworkspace \\\n--resource-group rg --start-ip-address 0.0.0.0 --end-ip-address 255.255.255.255"
            ]
        },
        {
            "name": "az synapse workspace firewall-rule delete",
            "summary": "Delete a firewall rule.",
            "syntax": "az synapse workspace firewall-rule delete [--ids]\n                                          [--name]\n                                          [--no-wait]\n                                          [--resource-group]\n                                          [--subscription]\n                                          [--workspace-name]\n                                          [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
            ],
            "examples": [
                "az synapse workspace firewall-rule delete --name rule1 --workspace-name testsynapseworkspace --resource-group rg"
            ]
        },
        {
            "name": "az synapse workspace firewall-rule list",
            "summary": "List all firewall rules.",
            "syntax": "az synapse workspace firewall-rule list --resource-group\n                                        --workspace-name",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse workspace firewall-rule list --workspace-name testsynapseworkspace --resource-group rg"
            ]
        },
        {
            "name": "az synapse workspace firewall-rule show",
            "summary": "Get a firewall rule.",
            "syntax": "az synapse workspace firewall-rule show [--ids]\n                                        [--name]\n                                        [--resource-group]\n                                        [--subscription]\n                                        [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az synapse workspace firewall-rule show --name rule1 --workspace-name testsynapseworkspace --resource-group rg"
            ]
        },
        {
            "name": "az synapse workspace firewall-rule update",
            "summary": "Update a firewall rule.",
            "syntax": "az synapse workspace firewall-rule update [--end-ip-address]\n                                          [--ids]\n                                          [--name]\n                                          [--no-wait]\n                                          [--resource-group]\n                                          [--start-ip-address]\n                                          [--subscription]\n                                          [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--end-ip-address",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--start-ip-address",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az synapse workspace firewall-rule update --name allowAll --workspace-name testsynapseworkspace \\\n--resource-group rg --start-ip-address 172.0.0.0"
            ]
        },
        {
            "name": "az synapse workspace firewall-rule wait",
            "summary": "Place the CLI in a waiting state until a condition of a firewall rule is met.",
            "syntax": "az synapse workspace firewall-rule wait --rule-name\n                                        [--created]\n                                        [--custom]\n                                        [--deleted]\n                                        [--exists]\n                                        [--ids]\n                                        [--interval]\n                                        [--resource-group]\n                                        [--subscription]\n                                        [--timeout]\n                                        [--updated]\n                                        [--workspace-name]",
            "required_parameters": [
                "--rule-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated",
                "--workspace-name"
            ],
            "examples": []
        }
    ]
}