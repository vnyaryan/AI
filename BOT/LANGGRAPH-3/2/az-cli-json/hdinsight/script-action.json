{
    "command_group": "az hdinsight script-action",
    "description": "Manage HDInsight script actions.",
    "status": "GA",
    "commands": [
        {
            "name": "az hdinsight script-action delete",
            "summary": "Deletes a specified persisted script action of the cluster.",
            "syntax": "az hdinsight script-action delete --cluster-name\n                                  --name\n                                  --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az hdinsight script-action execute",
            "summary": "Execute script actions on the specified HDInsight cluster.",
            "syntax": "az hdinsight script-action execute --cluster-name\n                                   --name\n                                   --resource-group\n                                   --roles\n                                   --script-uri\n                                   [--persist-on-success]\n                                   [--script-parameters]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g",
                "--roles",
                "--script-uri"
            ],
            "optional_parameters": [
                "--persist-on-success",
                "--script-parameters"
            ],
            "examples": [
                "az hdinsight script-action execute -g MyResourceGroup -n MyScriptActionName \\\n--cluster-name MyCluster \\\n--script-uri https://hdiconfigactions.blob.core.windows.net/linuxgiraphconfigactionv01/giraph-installer-v01.sh \\\n--roles headnode workernode \\\n--persist-on-success"
            ]
        },
        {
            "name": "az hdinsight script-action list",
            "summary": "Lists all the persisted script actions for the specified cluster.",
            "syntax": "az hdinsight script-action list --cluster-name\n                                --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az hdinsight script-action list-execution-history",
            "summary": "Lists all scripts' execution history for the specified cluster.",
            "syntax": "az hdinsight script-action list-execution-history --cluster-name\n                                                  --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az hdinsight script-action promote",
            "summary": "Promotes the specified ad-hoc script execution to a persisted script.",
            "syntax": "az hdinsight script-action promote --cluster-name\n                                   --execution-id\n                                   --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--execution-id",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az hdinsight script-action show-execution-details",
            "summary": "Gets the script execution detail for the given script execution ID.",
            "syntax": "az hdinsight script-action show-execution-details --cluster-name\n                                                  --execution-id\n                                                  --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--execution-id",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        }
    ]
}