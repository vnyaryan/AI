{
    "command_group": "az aks mesh upgrade",
    "description": "Commands to manage the upgrades for Azure Service Mesh.",
    "status": "GA",
    "commands": [
        {
            "name": "az aks mesh upgrade complete",
            "summary": "Complete Azure Service Mesh upgrade.",
            "syntax": "az aks mesh upgrade complete --name\n                             --resource-group\n                             [--no-wait]\n                             [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az aks mesh upgrade complete --resource-group MyResourceGroup --name MyManagedCluster"
            ]
        },
