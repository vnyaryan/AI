{
    "command_group": "az aks pod-identity",
    "description": "Commands to manage pod identities in managed Kubernetes cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az aks pod-identity add",
            "summary": "Add a pod identity to a managed Kubernetes cluster.",
            "syntax": "az aks pod-identity add --cluster-name\n                        --identity-resource-id\n                        --namespace\n                        --resource-group\n                        [--aks-custom-headers]\n                        [--binding-selector]\n                        [--name]",
            "required_parameters": [
                "--cluster-name",
                "--identity-resource-id",
                "--namespace",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--aks-custom-headers",
                "--binding-selector",
                "--name -n"
            ],
            "examples": [
                "az aks pod-identity add --cluster-name MyManagedCluster --resource-group MyResourceGroup --namespace my-namespace --name my-identity --identity-resource-id <my-identity-resource-id>"
            ]
        },
        {
            "name": "az aks pod-identity delete",
            "summary": "Remove a pod identity from a managed Kubernetes cluster.",
            "syntax": "az aks pod-identity delete --cluster-name\n                           --name\n                           --namespace\n                           --resource-group\n                           [--aks-custom-headers]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--namespace",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--aks-custom-headers"
            ],
            "examples": []
        },
        {
            "name": "az aks pod-identity list",
            "summary": "List pod identities in a managed Kubernetes cluster.",
            "syntax": "az aks pod-identity list --cluster-name\n                         --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        }
    ]
}