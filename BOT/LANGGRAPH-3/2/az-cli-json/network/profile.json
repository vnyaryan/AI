{
    "command_group": "az network profile",
    "description": "Manage network profiles.",
    "status": "GA",
    "commands": [
        {
            "name": "az network profile delete",
            "summary": "Delete a network profile.",
            "syntax": "az network profile delete [--ids]\n                          [--name]\n                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                          [--resource-group]\n                          [--subscription]\n                          [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az network profile delete --name MyNetworkProfile --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az network profile list",
            "summary": "List network profiles.",
            "syntax": "az network profile list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az network profile list --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az network profile show",
            "summary": "Get the details of a network profile.",
            "syntax": "az network profile show [--expand]\n                        [--ids]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--expand",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network profile show --name MyNetworkProfile --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az network profile wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az network profile wait [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--expand]\n                        [--ids]\n                        [--interval]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--ids",
                "--interval",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}