{
    "command_group": "az lock",
    "description": "Manage Azure locks.",
    "status": "GA",
    "commands": [
        {
            "name": "az lock create",
            "summary": "Create a lock.",
            "syntax": "az lock create --lock-type {CanNotDelete, ReadOnly}\n               --name\n               [--namespace]\n               [--notes]\n               [--parent]\n               [--resource]\n               [--resource-group]\n               [--resource-type]",
            "required_parameters": [
                "--lock-type -t",
                "--name -n"
            ],
            "optional_parameters": [
                "--namespace",
                "--notes",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az lock create --name lockName --lock-type ReadOnly",
                "az lock create --name lockName --resource-group group --lock-type ReadOnly",
                "az lock create --name lockName --resource-group group --lock-type ReadOnly --resource-type \\\n    Microsoft.Network/virtualNetworks --resource myVnet",
                "az lock create --name lockName --resource-group group --lock-type ReadOnly --resource-type \\\n    Microsoft.Network/subnets --parent virtualNetworks/myVnet --resource mySubnet"
            ]
        },
        {
            "name": "az lock delete",
            "summary": "Delete a lock.",
            "syntax": "az lock delete [--ids]\n               [--name]\n               [--namespace]\n               [--parent]\n               [--resource]\n               [--resource-group]\n               [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az lock delete --name lockName",
                "az lock delete --name lockName --resource-group group",
                "az lock delete --name lockName --resource-group group --resource resourceName --resource-type resourceType"
            ]
        },
        {
            "name": "az lock list",
            "summary": "List lock information.",
            "syntax": "az lock list [--filter-string]\n             [--namespace]\n             [--parent]\n             [--resource]\n             [--resource-group]\n             [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter-string",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az lock list --resource myvnet --resource-type Microsoft.Network/virtualNetworks -g group",
                "az lock list"
            ]
        },
        {
            "name": "az lock show",
            "summary": "Show the properties of a lock.",
            "syntax": "az lock show [--ids]\n             [--name]\n             [--namespace]\n             [--parent]\n             [--resource]\n             [--resource-group]\n             [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az lock show -n lockname",
                "az lock show --name lockname --resource-group MyResourceGroup --resource-name MyResource --resource-type Microsoft.Network/virtualNetworks"
            ]
        },
        {
            "name": "az lock update",
            "summary": "Update a lock.",
            "syntax": "az lock update [--ids]\n               [--lock-type {CanNotDelete, ReadOnly}]\n               [--name]\n               [--namespace]\n               [--notes]\n               [--parent]\n               [--resource]\n               [--resource-group]\n               [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--lock-type -t",
                "--name -n",
                "--namespace",
                "--notes",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az lock update --name lockName --resource-group group --notes newNotesHere --lock-type CanNotDelete"
            ]
        }
    ]
}