{
    "command_group": "az mesh service-replica",
    "description": "Manage Service Fabric Mesh service replicas.",
    "status": "Preview",
    "commands": [
        {
            "name": "az mesh service-replica list",
            "summary": "List the details of service replicas.",
            "syntax": "az mesh service-replica list --app-name\n                             --resource-group\n                             --service-name\n                             [--max-items]\n                             [--next-token]",
            "required_parameters": [
                "--app-name --application-name",
                "--resource-group -g",
                "--service-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": []
        },
        {
            "name": "az mesh service-replica show",
            "summary": "Gets the given replica of the service of an application.",
            "syntax": "az mesh service-replica show [--app-name]\n                             [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--service-name]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--app-name --application-name",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--service-name",
                "--subscription"
            ],
            "examples": []
        }
    ]
}