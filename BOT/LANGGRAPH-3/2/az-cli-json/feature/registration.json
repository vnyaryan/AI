{
    "command_group": "az feature registration",
    "description": "Manage resource provider feature registrations.",
    "status": "GA",
    "commands": [
        {
            "name": "az feature registration create",
            "summary": "Create a feature registration.",
            "syntax": "az feature registration create --name\n                               --namespace",
            "required_parameters": [
                "--name -n",
                "--namespace"
            ],
            "optional_parameters": [],
            "examples": [
                "az feature registration create --namespace Microsoft.Compute --name GalleryPreview"
            ]
        },
        {
            "name": "az feature registration delete",
            "summary": "Delete a feature registration.",
            "syntax": "az feature registration delete --name\n                               --namespace\n                               [--yes]",
            "required_parameters": [
                "--name -n",
                "--namespace"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az feature registration delete --namespace Microsoft.Compute --name GalleryPreview"
            ]
        },
        {
            "name": "az feature registration list",
            "summary": "List feature registrations.",
            "syntax": "az feature registration list [--namespace]",
            "required_parameters": [],
            "optional_parameters": [
                "--namespace"
            ],
            "examples": [
                "az feature registration list"
            ]
        },
        {
            "name": "az feature registration show",
            "summary": "Returns a feature registration.",
            "syntax": "az feature registration show --name\n                             --provider-namespace",
            "required_parameters": [
                "--name -n",
                "--provider-namespace"
            ],
            "optional_parameters": [],
            "examples": []
        }
    ]
}