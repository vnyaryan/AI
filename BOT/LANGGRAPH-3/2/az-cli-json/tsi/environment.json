{
    "command_group": "az tsi environment",
    "description": "Manage environment with Azure Time Series Insights.",
    "status": "GA",
    "commands": [
        {
            "name": "az tsi environment delete",
            "summary": "Delete the environment with the specified name in the specified subscription and resource group.",
            "syntax": "az tsi environment delete [--environment-name]\n                          [--ids]\n                          [--resource-group]\n                          [--subscription]\n                          [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--environment-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az tsi environment delete --name \"env1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az tsi environment list",
            "summary": "List all the available environments within a subscription, irrespective of the resource groups.",
            "syntax": "az tsi environment list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az tsi environment list --resource-group \"rg1\"",
                "az tsi environment list"
            ]
        },
        {
            "name": "az tsi environment show",
            "summary": "Get the environment with the specified name in the specified subscription and resource group.",
            "syntax": "az tsi environment show [--environment-name]\n                        [--expand]\n                        [--ids]\n                        [--resource-group]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--environment-name --name -n",
                "--expand",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az tsi environment show --name \"env1\" --resource-group \"rg1\""
            ]
        }
    ]
}