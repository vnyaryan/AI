{
    "command_group": "az azurestackhci networkinterface",
    "description": "Manage networkinterface with azurestackhci.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az azurestackhci networkinterface create",
            "summary": "Create a network interface.",
            "syntax": "az azurestackhci networkinterface create --name\n                                         --resource-group\n                                         [--dns-servers]\n                                         [--extended-location]\n                                         [--gateway]\n                                         [--ip-address]\n                                         [--ip-allocation-method]\n                                         [--ip-configurations]\n                                         [--location]\n                                         [--mac-address]\n                                         [--prefix-length]\n                                         [--subnet-id]\n                                         [--tags]",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--dns-servers",
                "--extended-location",
                "--gateway",
                "--ip-address",
                "--ip-allocation-method",
                "--ip-configurations",
                "--location -l",
                "--mac-address",
                "--prefix-length",
                "--subnet-id",
                "--tags"
            ],
            "examples": [
                "az azurestackhci networkinterface create --resource-group \"test-rg\" --extended-location name=\"/subscriptions/a95612cb-f1fa-4daa-a4fd-272844fa512c/resourceGroups/dogfoodarc/providers/Microsoft.ExtendedLocation/customLocations/dogfood-location\" type=\"CustomLocation\" --location \"West US2\" --subnet-id \"test-vnet\" --gateway \"sample_gateway\" --prefix-length 16 --ip-address \"10.0.0.0\" --ip-allocation-method \"Static\" --name \"test-nic\""
            ]
        },
        {
            "name": "az azurestackhci networkinterface delete",
            "summary": "Delete a network interface.",
            "syntax": "az azurestackhci networkinterface delete [--ids]\n                                         [--name]\n                                         [--resource-group]\n                                         [--subscription]\n                                         [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az azurestackhci networkinterface delete --name \"test-nic\" --resource-group \"test-rg\""
            ]
        },
        {
            "name": "az azurestackhci networkinterface list",
            "summary": "List all network interfaces.",
            "syntax": "az azurestackhci networkinterface list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az azurestackhci networkinterface list --resource-group \"test-rg\"",
                "az azurestackhci networkinterface list"
            ]
        },
        {
            "name": "az azurestackhci networkinterface show",
            "summary": "Get the details of a network interface.",
            "syntax": "az azurestackhci networkinterface show [--ids]\n                                       [--name]\n                                       [--resource-group]\n                                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az azurestackhci networkinterface show --name \"test-nic\" --resource-group \"test-rg\""
            ]
        },
        {
            "name": "az azurestackhci networkinterface update",
            "summary": "Update a network interface.",
            "syntax": "az azurestackhci networkinterface update [--ids]\n                                         [--name]\n                                         [--resource-group]\n                                         [--subscription]\n                                         [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az azurestackhci networkinterface update --tags additionalProperties=\"sample\" --name \"test-nic\" --resource-group \"test-rg\""
            ]
        }
    ]
}