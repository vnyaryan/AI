{
    "command_group": "az azurestackhci virtualmachine vnic",
    "description": "Manage vNIC of a virtual machine with azurestackhci.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az azurestackhci virtualmachine vnic add",
            "summary": "Add existing vNICs to a VM.",
            "syntax": "az azurestackhci virtualmachine vnic add --vnic-names\n                                         [--ids]\n                                         [--name]\n                                         [--resource-group]\n                                         [--subscription]",
            "required_parameters": [
                "--vnic-names"
            ],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az azurestackhci virtualmachine vnic add --resource-group \"test-rg\" --name \"test-vm\" --vnic-names \"test-vnic\""
            ]
        },
        {
            "name": "az azurestackhci virtualmachine vnic remove",
            "summary": "Remove vNICs from a VM.",
            "syntax": "az azurestackhci virtualmachine vnic remove --vnic-names\n                                            [--ids]\n                                            [--name]\n                                            [--resource-group]\n                                            [--subscription]\n                                            [--yes]",
            "required_parameters": [
                "--vnic-names"
            ],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az azurestackhci virtualmachine vnic remove --resource-group \"test-rg\" --name \"test-vm\" --vnic-names \"test-vnic\""
            ]
        }
    ]
}