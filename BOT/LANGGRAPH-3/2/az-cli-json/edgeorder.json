{
    "command_group": "az edgeorder",
    "description": "Manage Edge Order.",
    "status": "GA",
    "commands": [
        {
            "name": "az edgeorder list-config",
            "summary": "This method provides the list of configurations for the given product family, product line and product under subscription.",
            "syntax": "az edgeorder list-config --configuration-filters\n                         [--location-placement-id]\n                         [--quota-id]\n                         [--registered-features]\n                         [--skip-token]",
            "required_parameters": [
                "--configuration-filters"
            ],
            "optional_parameters": [
                "--location-placement-id",
                "--quota-id",
                "--registered-features",
                "--skip-token"
            ],
            "examples": [
                "az edgeorder list-config --configuration-filters \"[{\\\"filterableProperty\\\":[{\\\"type\\\":\\\"ShipToCountries\\\",\\\"supportedValues\\\":[\\\"US\\\"]}],\\\"hierarchyInformation\\\":{\\\"productFamilyName\\\":\\\"AzureStackEdge\\\",\\\"productLineName\\\":\\\"AzureStackEdge\\\",\\\"productName\\\":\\\"AzureStackEdgeGPU\\\"}}]\""
            ]
        },
        {
            "name": "az edgeorder list-family",
            "summary": "This method provides the list of product families for the given subscription.",
            "syntax": "az edgeorder list-family --filterable-properties\n                         [--expand]\n                         [--location-placement-id]\n                         [--quota-id]\n                         [--registered-features]\n                         [--skip-token]",
            "required_parameters": [
                "--filterable-properties"
            ],
            "optional_parameters": [
                "--expand",
                "--location-placement-id",
                "--quota-id",
                "--registered-features",
                "--skip-token"
            ],
            "examples": [
                "az edgeorder list-family --filterable-properties azurestackedge=\"{\\\"type\\\":\\\"ShipToCountries\\\",               \\\"supportedValues\\\":[\\\"US\\\"]}\""
            ]
        },
        {
            "name": "az edgeorder list-metadata",
            "summary": "This method provides the list of product families metadata for the given subscription.",
            "syntax": "az edgeorder list-metadata [--skip-token]",
            "required_parameters": [],
            "optional_parameters": [
                "--skip-token"
            ],
            "examples": [
                "az edgeorder list-metadata"
            ]
        }
    ]
}