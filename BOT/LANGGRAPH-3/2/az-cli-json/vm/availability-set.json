{
    "command_group": "az vm availability-set",
    "description": "Group resources into availability sets.",
    "status": "GA",
    "commands": [
        {
            "name": "az vm availability-set convert",
            "summary": "Convert an Azure Availability Set to contain VMs with managed disks.",
            "syntax": "az vm availability-set convert [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm availability-set convert -g MyResourceGroup -n MyAvSet",
                "az vm availability-set convert --ids $(az vm availability-set list -g MyResourceGroup --query \"[].id\" -o tsv)"
            ]
        },
        {
            "name": "az vm availability-set create",
            "summary": "Create an Azure Availability Set.",
            "syntax": "az vm availability-set create --name\n                              --resource-group\n                              [--location]\n                              [--no-wait]\n                              [--platform-fault-domain-count]\n                              [--platform-update-domain-count]\n                              [--ppg]\n                              [--tags]\n                              [--unmanaged]\n                              [--validate]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--location -l",
                "--no-wait",
                "--platform-fault-domain-count",
                "--platform-update-domain-count",
                "--ppg",
                "--tags",
                "--unmanaged",
                "--validate"
            ],
            "examples": [
                "az vm availability-set create -n MyAvSet -g MyResourceGroup --platform-fault-domain-count 2 --platform-update-domain-count 2"
            ]
        },
        {
            "name": "az vm availability-set delete",
            "summary": "Delete an availability set.",
            "syntax": "az vm availability-set delete [--availability-set-name]\n                              [--ids]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--availability-set-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm availability-set delete -n MyAvSet -g MyResourceGroup"
            ]
        },
        {
            "name": "az vm availability-set list",
            "summary": "List availability sets.",
            "syntax": "az vm availability-set list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az vm availability-set list -g MyResourceGroup"
            ]
        },
        {
            "name": "az vm availability-set list-sizes",
            "summary": "List all available virtual machine sizes that can be used to create a new virtual machine in an existing availability set.",
            "syntax": "az vm availability-set list-sizes [--availability-set-name]\n                                  [--ids]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--availability-set-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm availability-set list-sizes -n MyAvSet -g MyResourceGroup"
            ]
        },
        {
            "name": "az vm availability-set show",
            "summary": "Get information about an availability set.",
            "syntax": "az vm availability-set show [--availability-set-name]\n                            [--ids]\n                            [--resource-group]\n                            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--availability-set-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm availability-set show -n MyAvSet -g MyResourceGroup"
            ]
        },
        {
            "name": "az vm availability-set update",
            "summary": "Update an Azure Availability Set.",
            "syntax": "az vm availability-set update [--add]\n                              [--force-string]\n                              [--ids]\n                              [--name]\n                              [--ppg]\n                              [--remove]\n                              [--resource-group]\n                              [--set]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--ppg",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription"
            ],
            "examples": [
                "az vm availability-set update -n MyAvSet -g MyResourceGroup",
                "az vm availability-set update -n MyAvSet -g MyResourceGroup --set tags.foo=value",
                "az vm availability-set update -n MyAvSet -g MyResourceGroup --remove tags.foo"
            ]
        }
    ]
}