{
    "command_group": "az scvmm vm",
    "description": "Manage Arc for SCVMM Virtual Machine resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az scvmm vm create",
            "summary": "Create VM resource.",
            "syntax": "az scvmm vm create --custom-location\n                   --name\n                   --resource-group\n                   [--admin-password]\n                   [--availability-sets]\n                   [--cloud]\n                   [--cpu-count]\n                   [--disk]\n                   [--dynamic-memory-enabled {false, true}]\n                   [--dynamic-memory-max]\n                   [--dynamic-memory-min]\n                   [--inventory-item]\n                   [--location]\n                   [--memory-size]\n                   [--nic]\n                   [--no-wait]\n                   [--tags]\n                   [--vm-template]\n                   [--vmmserver]",
            "required_parameters": [
                "--custom-location",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--admin-password",
                "--availability-sets -a",
                "--cloud -c",
                "--cpu-count",
                "--disk",
                "--dynamic-memory-enabled",
                "--dynamic-memory-max",
                "--dynamic-memory-min",
                "--inventory-item -i",
                "--location -l",
                "--memory-size",
                "--nic",
                "--no-wait",
                "--tags",
                "--vm-template -t",
                "--vmmserver -v"
            ],
            "examples": [
                "az scvmm vm create --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --inventory-item 01234567-0123-0123-0123-0123456789ab --name contoso-vm",
                "az scvmm vm create --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --vm-template contoso-vmtemplate --cloud contoso-cloud --name contoso-vm",
                "az scvmm vm create --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --vm-template contoso-vmtemplate --cloud contoso-cloud --name contoso-vm --cpu-count 2 --memory-size 2048 --dynamic-memory-enabled true --disk name=disk_1 disk-size=2 bus=0 --nic name=nic_1 network=contoso-vnet"
            ]
        },
        {
            "name": "az scvmm vm create-checkpoint",
            "summary": "Create a VM checkpoint.",
            "syntax": "az scvmm vm create-checkpoint --checkpoint-description\n                              --checkpoint-name\n                              [--ids]\n                              [--name]\n                              [--no-wait]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [
                "--checkpoint-description",
                "--checkpoint-name"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm create-checkpoint --subscription contoso-sub --resource-group contoso-rg --name contoso-vm --checkpoint-name contoso-chkpt-name --checkpoint-description contoso-chkpt-description"
            ]
        },
        {
            "name": "az scvmm vm delete",
            "summary": "Delete VM resource.",
            "syntax": "az scvmm vm delete [--delete-from-host]\n                   [--delete-machine]\n                   [--force]\n                   [--ids]\n                   [--name]\n                   [--no-wait]\n                   [--resource-group]\n                   [--subscription]\n                   [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--delete-from-host",
                "--delete-machine",
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az scvmm vm delete --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/virtualMachines/contoso-vm",
                "az scvmm vm delete --subscription contoso-sub --resource-group contoso-rg --name contoso-avset",
                "az scvmm vm delete --subscription contoso-sub --resource-group contoso-rg --name contoso-avset --retain",
                "az scvmm vm delete --subscription contoso-sub --resource-group contoso-rg --name contoso-avset --force",
                "az scvmm vm delete --subscription contoso-sub --resource-group contoso-rg --name contoso-avset --deleteFromHost"
            ]
        },
        {
            "name": "az scvmm vm delete-checkpoint",
            "summary": "Delete the specified VM checkpoint.",
            "syntax": "az scvmm vm delete-checkpoint --checkpoint-id\n                              [--ids]\n                              [--name]\n                              [--no-wait]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [
                "--checkpoint-id"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm delete-checkpoint --subscription contoso-sub --resource-group contoso-rg --name contoso-vm --checkpoint-id checkpoint-guid"
            ]
        },
        {
            "name": "az scvmm vm restart",
            "summary": "Restart a VM.",
            "syntax": "az scvmm vm restart [--ids]\n                    [--name]\n                    [--no-wait]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm restart --subscription contoso-sub --resource-group contoso-rg --name contoso-vm"
            ]
        },
        {
            "name": "az scvmm vm restore-checkpoint",
            "summary": "Restore VM checkpoint.",
            "syntax": "az scvmm vm restore-checkpoint --checkpoint-id\n                               [--ids]\n                               [--name]\n                               [--no-wait]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [
                "--checkpoint-id"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm restore-checkpoint --subscription contoso-sub --resource-group contoso-rg --name contoso-vm --checkpoint-id checkpoint-guid"
            ]
        },
        {
            "name": "az scvmm vm show",
            "summary": "Get details of an VM.",
            "syntax": "az scvmm vm show [--ids]\n                 [--name]\n                 [--resource-group]\n                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm show --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/virtualMachines/contoso-vm",
                "az scvmm vm show --subscription contoso-sub --resource-group contoso-rg --name contoso-vm"
            ]
        },
        {
            "name": "az scvmm vm start",
            "summary": "Start a VM.",
            "syntax": "az scvmm vm start [--ids]\n                  [--name]\n                  [--no-wait]\n                  [--resource-group]\n                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm start --subscription contoso-sub --resource-group contoso-rg --name contoso-vm"
            ]
        },
        {
            "name": "az scvmm vm stop",
            "summary": "Stop a VM.",
            "syntax": "az scvmm vm stop [--ids]\n                 [--name]\n                 [--no-wait]\n                 [--resource-group]\n                 [--skip-shutdown {false, true}]\n                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--skip-shutdown",
                "--subscription"
            ],
            "examples": [
                "az scvmm vm stop --subscription contoso-sub --resource-group contoso-rg --name contoso-vm",
                "az scvmm vm stop --subscription contoso-sub --resource-group contoso-rg --name contoso-vm --skip-shutdown"
            ]
        },
        {
            "name": "az scvmm vm update",
            "summary": "Update a VM. Management of VM disks and NICs are not using this subcommand. There are separate subcommands for the same.",
            "syntax": "az scvmm vm update [--availability-sets]\n                   [--cpu-count]\n                   [--dynamic-memory-enabled {false, true}]\n                   [--dynamic-memory-max]\n                   [--dynamic-memory-min]\n                   [--ids]\n                   [--memory-size]\n                   [--name]\n                   [--no-wait]\n                   [--resource-group]\n                   [--subscription]\n                   [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--availability-sets -a",
                "--cpu-count",
                "--dynamic-memory-enabled",
                "--dynamic-memory-max",
                "--dynamic-memory-min",
                "--ids",
                "--memory-size",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az scvmm vm update --subscription contoso-sub --resource-group contoso-rg --name contoso-vm --cpu-count 2 --memory-size 4096 --tags department=Sales"
            ]
        },
        {
            "name": "az scvmm vm wait",
            "summary": "Place the CLI in a waiting state until a condition of the vm is met.",
            "syntax": "az scvmm vm wait --resource-group\n                 --virtual-machine-name\n                 [--created]\n                 [--custom]\n                 [--deleted]\n                 [--exists]\n                 [--interval]\n                 [--timeout]\n                 [--updated]",
            "required_parameters": [
                "--resource-group -g",
                "--virtual-machine-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}