{
    "command_group": "az cloud",
    "description": "Manage registered Azure clouds.",
    "status": "GA",
    "commands": [
        {
            "name": "az cloud list",
            "summary": "List registered clouds.",
            "syntax": "az cloud list",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az cloud list-profiles",
            "summary": "List the supported profiles for a cloud.",
            "syntax": "az cloud list-profiles [--name]\n                       [--show-all]",
            "required_parameters": [],
            "optional_parameters": [
                "--name -n",
                "--show-all"
            ],
            "examples": []
        },
        {
            "name": "az cloud register",
            "summary": "Register a cloud.",
            "syntax": "az cloud register --name\n                  [--cloud-config]\n                  [--endpoint-active-directory]\n                  [--endpoint-active-directory-data-lake-resource-id]\n                  [--endpoint-active-directory-graph-resource-id]\n                  [--endpoint-active-directory-resource-id]\n                  [--endpoint-gallery]\n                  [--endpoint-management]\n                  [--endpoint-resource-manager]\n                  [--endpoint-sql-management]\n                  [--endpoint-vm-image-alias-doc]\n                  [--profile {2017-03-09-profile, 2018-03-01-hybrid, 2019-03-01-hybrid, 2020-09-01-hybrid, latest}]\n                  [--suffix-acr-login-server-endpoint]\n                  [--suffix-azure-datalake-analytics-catalog-and-job-endpoint]\n                  [--suffix-azure-datalake-store-file-system-endpoint]\n                  [--suffix-keyvault-dns]\n                  [--suffix-sql-server-hostname]\n                  [--suffix-storage-endpoint]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--cloud-config",
                "--endpoint-active-directory",
                "--endpoint-active-directory-data-lake-resource-id",
                "--endpoint-active-directory-graph-resource-id",
                "--endpoint-active-directory-resource-id",
                "--endpoint-gallery",
                "--endpoint-management",
                "--endpoint-resource-manager",
                "--endpoint-sql-management",
                "--endpoint-vm-image-alias-doc",
                "--profile",
                "--suffix-acr-login-server-endpoint",
                "--suffix-azure-datalake-analytics-catalog-and-job-endpoint",
                "--suffix-azure-datalake-store-file-system-endpoint",
                "--suffix-keyvault-dns",
                "--suffix-sql-server-hostname",
                "--suffix-storage-endpoint"
            ],
            "examples": [
                "az cloud register -n MyCloud --cloud-config @\"cloud.json\"\n(\"cloud.json\" supports all the endpoint and suffix options in camel case or the JSON output format from `az cloud show`. See the example content below.)\n{\n  \"endpointActiveDirectory\": \"https://login.microsoftonline.us\",\n  \"suffixAcrLoginServerEndpoint\": \".azurecr.us\"\n}\nor\n{\n  \"endpoints\":{\n    \"activeDirectory\": \"https://login.microsoftonline.us\"\n  },\n  \"suffixes\":{\n    \"acrLoginServerEndpoint\": \".azurecr.us\"\n  }\n}"
            ]
        },
        {
            "name": "az cloud set",
            "summary": "Set the active cloud.",
            "syntax": "az cloud set --name\n             [--profile {2017-03-09-profile, 2018-03-01-hybrid, 2019-03-01-hybrid, 2020-09-01-hybrid, latest}]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--profile"
            ],
            "examples": [
                "az cloud set --name MyRegisteredCloud"
            ]
        },
        {
            "name": "az cloud show",
            "summary": "Get the details of a registered cloud.",
            "syntax": "az cloud show [--name]",
            "required_parameters": [],
            "optional_parameters": [
                "--name -n"
            ],
            "examples": []
        },
        {
            "name": "az cloud unregister",
            "summary": "Unregister a cloud.",
            "syntax": "az cloud unregister --name",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az cloud unregister --name MyRegisteredCloud"
            ]
        },
        {
            "name": "az cloud update",
            "summary": "Update the configuration of a cloud.",
            "syntax": "az cloud update [--cloud-config]\n                [--endpoint-active-directory]\n                [--endpoint-active-directory-data-lake-resource-id]\n                [--endpoint-active-directory-graph-resource-id]\n                [--endpoint-active-directory-resource-id]\n                [--endpoint-gallery]\n                [--endpoint-management]\n                [--endpoint-resource-manager]\n                [--endpoint-sql-management]\n                [--endpoint-vm-image-alias-doc]\n                [--name]\n                [--profile {2017-03-09-profile, 2018-03-01-hybrid, 2019-03-01-hybrid, 2020-09-01-hybrid, latest}]\n                [--suffix-acr-login-server-endpoint]\n                [--suffix-azure-datalake-analytics-catalog-and-job-endpoint]\n                [--suffix-azure-datalake-store-file-system-endpoint]\n                [--suffix-keyvault-dns]\n                [--suffix-sql-server-hostname]\n                [--suffix-storage-endpoint]",
            "required_parameters": [],
            "optional_parameters": [
                "--cloud-config",
                "--endpoint-active-directory",
                "--endpoint-active-directory-data-lake-resource-id",
                "--endpoint-active-directory-graph-resource-id",
                "--endpoint-active-directory-resource-id",
                "--endpoint-gallery",
                "--endpoint-management",
                "--endpoint-resource-manager",
                "--endpoint-sql-management",
                "--endpoint-vm-image-alias-doc",
                "--name -n",
                "--profile",
                "--suffix-acr-login-server-endpoint",
                "--suffix-azure-datalake-analytics-catalog-and-job-endpoint",
                "--suffix-azure-datalake-store-file-system-endpoint",
                "--suffix-keyvault-dns",
                "--suffix-sql-server-hostname",
                "--suffix-storage-endpoint"
            ],
            "examples": [
                "az cloud update --profile latest"
            ]
        }
    ]
}