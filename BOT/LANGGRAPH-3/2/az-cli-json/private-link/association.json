{
    "command_group": "az private-link association",
    "description": "Private-link association management on a resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az private-link association create",
            "summary": "Create a PrivateLinkAssociation.",
            "syntax": "az private-link association create --management-group-id\n                                   --name\n                                   --privatelink\n                                   --public-network-access {disabled, enabled}",
            "required_parameters": [
                "--management-group-id -m",
                "--name -n",
                "--privatelink -p",
                "--public-network-access -a"
            ],
            "optional_parameters": [],
            "examples": [
                "az private-link association create --management-group-id TestMG --name testPLA --privatelink testPL --public-network-access enabled"
            ]
        },
        {
            "name": "az private-link association delete",
            "summary": "Delete a PrivateLinkAssociation.",
            "syntax": "az private-link association delete --management-group-id\n                                   --name\n                                   [--yes]",
            "required_parameters": [
                "--management-group-id -m",
                "--name -n"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az private-link association delete --management-group-id TestMG --name testPLA"
            ]
        },
        {
            "name": "az private-link association list",
            "summary": "Get a private link association for a management group scope.",
            "syntax": "az private-link association list --management-group-id",
            "required_parameters": [
                "--management-group-id -m"
            ],
            "optional_parameters": [],
            "examples": [
                "az private-link association list --management-group-id TestMG"
            ]
        },
        {
            "name": "az private-link association show",
            "summary": "Get a private link association.",
            "syntax": "az private-link association show --management-group-id\n                                 --name",
            "required_parameters": [
                "--management-group-id -m",
                "--name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az private-link association show --management-group-id TestMG --name testPLA"
            ]
        }
    ]
}