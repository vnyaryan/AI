{
    "command_group": "az offazure hyperv cluster",
    "description": "Manage Hyper-V cluster.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az offazure hyperv cluster list",
            "summary": "Get all clusters on the on-premise site.",
            "syntax": "az offazure hyperv cluster list --resource-group\n                                --site-name",
            "required_parameters": [
                "--resource-group -g",
                "--site-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az offazure hyperv cluster list --resource-group MyResourceGroup --site-name MySiteName"
            ]
        },
        {
            "name": "az offazure hyperv cluster show",
            "summary": "Get the details of a Hyper-V cluster.",
            "syntax": "az offazure hyperv cluster show [--cluster-name]\n                                [--ids]\n                                [--resource-group]\n                                [--site-name]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name --name -n",
                "--ids",
                "--resource-group -g",
                "--site-name",
                "--subscription"
            ],
            "examples": [
                "az offazure hyperv cluster show --cluster-name MyClusterName --resource-group                  MyResourceGroup --site-name MySiteName"
            ]
        }
    ]
}