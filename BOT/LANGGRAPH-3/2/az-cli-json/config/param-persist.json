{
    "command_group": "az config param-persist",
    "description": "Manage parameter persistence.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az config param-persist delete",
            "summary": "Delete parameter persistence data.",
            "syntax": "az config param-persist delete [--all]\n                               [--purge]\n                               [--recursive]\n                               [--yes]\n                               [<NAME>]",
            "required_parameters": [],
            "optional_parameters": [
                "--all",
                "--purge",
                "--recursive",
                "--yes -y",
                "<NAME>"
            ],
            "examples": [
                "az config param-persist delete resource_group_name",
                "az config param-persist delete --all",
                "az config param-persist delete --all --purge",
                "az config param-persist delete --all --purge --recursive"
            ]
        },
        {
            "name": "az config param-persist off",
            "summary": "Turn off parameter persistence.",
            "syntax": "az config param-persist off",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az config param-persist on",
            "summary": "Turn on parameter persistence.",
            "syntax": "az config param-persist on",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az config param-persist show",
            "summary": "Show parameter persistence data.",
            "syntax": "az config param-persist show [<NAME>]",
            "required_parameters": [],
            "optional_parameters": [
                "<NAME>"
            ],
            "examples": [
                "az config param-persist show",
                "az config param-persist show resource_group_name"
            ]
        }
    ]
}