{
    "command_group": "az arcappliance createconfig",
    "description": "Command group for creating configuration files for Arc resource bridge.",
    "status": "GA",
    "commands": [
        {
            "name": "az arcappliance createconfig hci",
            "summary": "Command to create configuration files for Arc Resource Bridge on HCI.",
            "syntax": "az arcappliance createconfig hci --location\n                                 --name\n                                 --resource-group\n                                 [--authenticationport]\n                                 [--certificateFilePath]\n                                 [--cloudagent]\n                                 [--controlPlaneEndpoint]\n                                 [--dnsservers]\n                                 [--gateway]\n                                 [--http]\n                                 [--https]\n                                 [--ipaddressprefix]\n                                 [--k8snodeippoolend]\n                                 [--k8snodeippoolstart]\n                                 [--loginconfigfile]\n                                 [--noproxy]\n                                 [--out-dir]\n                                 [--overwrite-existing]\n                                 [--port]\n                                 [--storagecontainer]\n                                 [--vlanid]\n                                 [--vswitchname]\n                                 [--working-dir]",
            "required_parameters": [
                "--location -l",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--authenticationport",
                "--certificateFilePath",
                "--cloudagent",
                "--controlPlaneEndpoint",
                "--dnsservers",
                "--gateway",
                "--http",
                "--https",
                "--ipaddressprefix",
                "--k8snodeippoolend",
                "--k8snodeippoolstart",
                "--loginconfigfile",
                "--noproxy",
                "--out-dir",
                "--overwrite-existing",
                "--port",
                "--storagecontainer",
                "--vlanid",
                "--vswitchname",
                "--working-dir"
            ],
            "examples": [
                "az arcappliance createconfig hci --resource-group [REQUIRED] --name [REQUIRED] --location [REQUIRED] --out-dir [OPTIONAL] --cloudagent [OPTIONAL] --loginconfigfile [OPTIONAL]"
            ]
        },
