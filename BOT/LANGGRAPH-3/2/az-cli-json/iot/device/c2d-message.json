{
    "command_group": "az iot device c2d-message",
    "description": "Cloud-to-device messaging commands.",
    "status": "GA",
    "commands": [
        {
            "name": "az iot device c2d-message abandon",
            "summary": "Abandon a cloud-to-device message.",
            "syntax": "az iot device c2d-message abandon --device-id\n                                  [--etag]\n                                  [--hub-name]\n                                  [--login]\n                                  [--resource-group]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--etag -e",
                "--hub-name -n",
                "--login -l",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az iot device c2d-message complete",
            "summary": "Complete a cloud-to-device message.",
            "syntax": "az iot device c2d-message complete --device-id\n                                   [--etag]\n                                   [--hub-name]\n                                   [--login]\n                                   [--resource-group]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--etag -e",
                "--hub-name -n",
                "--login -l",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az iot device c2d-message purge",
            "summary": "Purge cloud-to-device message queue for a target device.",
            "syntax": "az iot device c2d-message purge --device-id\n                                [--hub-name]\n                                [--login]\n                                [--resource-group]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--hub-name -n",
                "--login -l",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az iot device c2d-message receive",
            "summary": "Receive a cloud-to-device message.",
            "syntax": "az iot device c2d-message receive --device-id\n                                  [--abandon {false, true}]\n                                  [--complete {false, true}]\n                                  [--hub-name]\n                                  [--lock-timeout]\n                                  [--login]\n                                  [--reject {false, true}]\n                                  [--resource-group]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--abandon",
                "--complete",
                "--hub-name -n",
                "--lock-timeout --lt",
                "--login -l",
                "--reject",
                "--resource-group -g"
            ],
            "examples": [
                "az iot device c2d-message receive -d {device_id} -n {hub_name} -g {resource_group}",
                "az iot device c2d-message receive -d {device_id} -n {hub_name} -g {resource_group} --lt {30}",
                "az iot device c2d-message receive -d {device_id} -n {hub_name} -g {resource_group} --complete",
                "az iot device c2d-message receive -d {device_id} -n {hub_name} -g {resource_group} --reject"
            ]
        },
        {
            "name": "az iot device c2d-message reject",
            "summary": "Reject or deadletter a cloud-to-device message.",
            "syntax": "az iot device c2d-message reject --device-id\n                                 [--etag]\n                                 [--hub-name]\n                                 [--login]\n                                 [--resource-group]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--etag -e",
                "--hub-name -n",
                "--login -l",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az iot device c2d-message send",
            "summary": "Send a cloud-to-device message.",
            "syntax": "az iot device c2d-message send --device-id\n                               [--ack {full, negative, positive}]\n                               [--auth-type {key, login}]\n                               [--ce]\n                               [--cid]\n                               [--content-type]\n                               [--da]\n                               [--data-file-path]\n                               [--expiry]\n                               [--hub-name]\n                               [--login]\n                               [--message-id]\n                               [--properties]\n                               [--repair {false, true}]\n                               [--resource-group]\n                               [--uid]\n                               [--wait {false, true}]\n                               [--yes {false, true}]",
            "required_parameters": [
                "--device-id -d"
            ],
            "optional_parameters": [
                "--ack",
                "--auth-type",
                "--ce --content-encoding",
                "--cid --correlation-id",
                "--content-type --ct",
                "--da --data",
                "--data-file-path --dfp",
                "--expiry --expiry-time-utc",
                "--hub-name -n",
                "--login -l",
                "--message-id --mid",
                "--properties --props -p",
                "--repair -r",
                "--resource-group -g",
                "--uid --user-id",
                "--wait -w",
                "--yes -y"
            ],
            "examples": [
                "az iot device c2d-message send -d {device_id} -n {iothub_name}",
                "az iot device c2d-message send -d {device_id} -n {iothub_name} --data 'Hello World' --props 'key0=value0;key1=value1'",
                "az iot device c2d-message send -d {device_id} -n {iothub_name} --ack full --wait",
                "az iot device c2d-message send -d {device_id} -n {iothub_name} --data-file-path {file_path} --content-type 'application/octet-stream'",
                "az iot device c2d-message send -d {device_id} -n {iothub_name} --data-file-path {file_path} --content-type 'application/json'"
            ]
        }
    ]
}