{
    "command_group": "az blueprint artifact policy",
    "description": "Commands to manage blueprint policy assignment artifact.",
    "status": "GA",
    "commands": [
        {
            "name": "az blueprint artifact policy create",
            "summary": "Create blueprint policy artifact.",
            "syntax": "az blueprint artifact policy create --artifact-name\n                                    --blueprint-name\n                                    --policy-definition-id\n                                    [--depends-on]\n                                    [--description]\n                                    [--display-name]\n                                    [--management-group]\n                                    [--parameters]\n                                    [--resource-group-art]\n                                    [--subscription]",
            "required_parameters": [
                "--artifact-name",
                "--blueprint-name",
                "--policy-definition-id"
            ],
            "optional_parameters": [
                "--depends-on",
                "--description",
                "--display-name",
                "--management-group -m",
                "--parameters -p",
                "--resource-group-art",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint artifact policy create \\\n--blueprint-name MyBlueprint --artifact-name MyPolicy --policy-definition-id \\\n\"/providers/Microsoft.Authorization/policyDefinitions/00000000-0000-0000-0000-000000000000\" \\\n--parameters path/to/file --display-name \"Policy to do sth\""
            ]
        },
        {
            "name": "az blueprint artifact policy update",
            "summary": "Update blueprint policy artifact.",
            "syntax": "az blueprint artifact policy update --artifact-name\n                                    --blueprint-name\n                                    [--depends-on]\n                                    [--description]\n                                    [--display-name]\n                                    [--management-group]\n                                    [--parameters]\n                                    [--resource-group-art]\n                                    [--subscription]",
            "required_parameters": [
                "--artifact-name",
                "--blueprint-name"
            ],
            "optional_parameters": [
                "--depends-on",
                "--description",
                "--display-name",
                "--management-group -m",
                "--parameters -p",
                "--resource-group-art",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint artifact policy update \\\n--blueprint-name MyBlueprint --artifact-name MyPolicy --display-name \"My Policy\""
            ]
        }
    ]
}