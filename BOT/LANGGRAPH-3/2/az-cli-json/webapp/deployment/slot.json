{
    "command_group": "az webapp deployment slot",
    "description": "Manage web app deployment slots.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp deployment slot auto-swap",
            "summary": "Configure deployment slot auto swap.",
            "syntax": "az webapp deployment slot auto-swap --slot\n                                    [--auto-swap-slot]\n                                    [--disable]\n                                    [--ids]\n                                    [--name]\n                                    [--resource-group]\n                                    [--subscription]",
            "required_parameters": [
                "--slot -s"
            ],
            "optional_parameters": [
                "--auto-swap-slot",
                "--disable",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp deployment slot auto-swap --name MyWebapp --resource-group MyResourceGroup --slot staging"
            ]
        },
        {
            "name": "az webapp deployment slot create",
            "summary": "Create a deployment slot.",
            "syntax": "az webapp deployment slot create --name\n                                 --resource-group\n                                 --slot\n                                 [--configuration-source]\n                                 [--container-image-name]\n                                 [--container-registry-password]\n                                 [--container-registry-url]\n                                 [--container-registry-user]\n                                 [--deployment-container-image-name]\n                                 [--docker-registry-server-password]\n                                 [--docker-registry-server-user]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--slot -s"
            ],
            "optional_parameters": [
                "--configuration-source",
                "--container-image-name -c",
                "--container-registry-password -w",
                "--container-registry-url -r",
                "--container-registry-user -u",
                "--deployment-container-image-name -i",
                "--docker-registry-server-password",
                "--docker-registry-server-user"
            ],
            "examples": [
                "az webapp deployment slot create --name MyWebapp --resource-group MyResourceGroup --slot staging"
            ]
        },
        {
            "name": "az webapp deployment slot delete",
            "summary": "Delete a deployment slot.",
            "syntax": "az webapp deployment slot delete --slot\n                                 [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [
                "--slot -s"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp deployment slot delete --name MyWebapp --resource-group MyResourceGroup --slot staging"
            ]
        },
        {
            "name": "az webapp deployment slot list",
            "summary": "List all deployment slots.",
            "syntax": "az webapp deployment slot list [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp deployment slot list --name MyWebapp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp deployment slot swap",
            "summary": "Swap deployment slots for a web app.",
            "syntax": "az webapp deployment slot swap --slot\n                               [--action {preview, reset, swap}]\n                               [--ids]\n                               [--name]\n                               [--preserve-vnet {false, true}]\n                               [--resource-group]\n                               [--subscription]\n                               [--target-slot]",
            "required_parameters": [
                "--slot -s"
            ],
            "optional_parameters": [
                "--action",
                "--ids",
                "--name -n",
                "--preserve-vnet",
                "--resource-group -g",
                "--subscription",
                "--target-slot"
            ],
            "examples": [
                "az webapp deployment slot swap  -g MyResourceGroup -n MyUniqueApp --slot staging \\\n    --target-slot production"
            ]
        }
    ]
}