{
    "command_group": "az webapp auth openid-connect",
    "description": "Manage webapp authentication and authorization of the custom OpenID Connect identity providers.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp auth openid-connect add",
            "summary": "Configure a new custom OpenID Connect identity provider.",
            "syntax": "az webapp auth openid-connect add --provider-name\n                                  [--client-id]\n                                  [--client-secret]\n                                  [--client-secret-setting-name]\n                                  [--ids]\n                                  [--name]\n                                  [--openid-configuration]\n                                  [--resource-group]\n                                  [--scopes]\n                                  [--slot]\n                                  [--subscription]\n                                  [--yes]",
            "required_parameters": [
                "--provider-name"
            ],
            "optional_parameters": [
                "--client-id",
                "--client-secret",
                "--client-secret-setting-name --secret-setting",
                "--ids",
                "--name -n",
                "--openid-configuration",
                "--resource-group -g",
                "--scopes",
                "--slot -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth openid-connect add -g myResourceGroup --name MyWebApp \\\n  --provider-name myOpenIdConnectProvider --client-id my-client-id \\\n  --client-secret-setting-name MY_SECRET_APP_SETTING \\\n  --openid-configuration https://myopenidprovider.net/.well-known/openid-configuration"
            ]
        },
        {
            "name": "az webapp auth openid-connect remove",
            "summary": "Removes an existing custom OpenID Connect identity provider.",
            "syntax": "az webapp auth openid-connect remove --provider-name\n                                     [--ids]\n                                     [--name]\n                                     [--resource-group]\n                                     [--slot]\n                                     [--subscription]",
            "required_parameters": [
                "--provider-name"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth openid-connect remove --name MyWebApp --resource-group MyResourceGroup \\\n  --provider-name myOpenIdConnectProvider"
            ]
        },
        {
            "name": "az webapp auth openid-connect show",
            "summary": "Show the authentication settings for the custom OpenID Connect identity provider.",
            "syntax": "az webapp auth openid-connect show --provider-name\n                                   [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--slot]\n                                   [--subscription]",
            "required_parameters": [
                "--provider-name"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth openid-connect show --name MyWebApp --resource-group MyResourceGroup \\ --provider-name myOpenIdConnectProvider"
            ]
        },
        {
            "name": "az webapp auth openid-connect update",
            "summary": "Update the client id and client secret setting name for an existing custom OpenID Connect identity provider.",
            "syntax": "az webapp auth openid-connect update --provider-name\n                                     [--client-id]\n                                     [--client-secret]\n                                     [--client-secret-setting-name]\n                                     [--ids]\n                                     [--name]\n                                     [--openid-configuration]\n                                     [--resource-group]\n                                     [--scopes]\n                                     [--slot]\n                                     [--subscription]\n                                     [--yes]",
            "required_parameters": [
                "--provider-name"
            ],
            "optional_parameters": [
                "--client-id",
                "--client-secret",
                "--client-secret-setting-name --secret-setting",
                "--ids",
                "--name -n",
                "--openid-configuration",
                "--resource-group -g",
                "--scopes",
                "--slot -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth openid-connect update -g myResourceGroup --name MyWebApp \\\n  --provider-name myOpenIdConnectProvider --client-id my-client-id \\\n  --client-secret-setting-name MY_SECRET_APP_SETTING"
            ]
        }
    ]
}