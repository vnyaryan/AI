{
    "command_group": "az spring job",
    "description": "(Enterprise Tier Only) Commands to manage jobs of Azure Spring Apps service.",
    "status": "Preview",
    "commands": [
        {
            "name": "az spring job create",
            "summary": "Create a new job in Azure Spring Apps service.",
            "syntax": "az spring job create --name\n                     --resource-group\n                     --service\n                     [--args]\n                     [--bind-config-server]\n                     [--bind-service-registry]\n                     [--cpu]\n                     [--envs]\n                     [--memory]\n                     [--parallelism]\n                     [--retry-limit]\n                     [--secret-envs]\n                     [--timeout]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--args",
                "--bind-config-server --bind-cs",
                "--bind-service-registry --bind-sr",
                "--cpu",
                "--envs",
                "--memory",
                "--parallelism",
                "--retry-limit",
                "--secret-envs",
                "--timeout"
            ],
            "examples": [
                "az spring job create -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring job delete",
            "summary": "Delete a job in the Azure Spring Apps.",
            "syntax": "az spring job delete --name\n                     --resource-group\n                     --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az spring job delete -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring job deploy",
            "summary": "Deploy artifact to a job and update related configurations.",
            "syntax": "az spring job deploy --name\n                     --resource-group\n                     --service\n                     [--args]\n                     [--artifact-path]\n                     [--build-cpu]\n                     [--build-env]\n                     [--build-memory]\n                     [--builder]\n                     [--cpu]\n                     [--disable-validation {false, true}]\n                     [--envs]\n                     [--memory]\n                     [--no-wait]\n                     [--parallelism]\n                     [--retry-limit]\n                     [--secret-envs]\n                     [--source-path]\n                     [--timeout]\n                     [--version]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--args",
                "--artifact-path",
                "--build-cpu",
                "--build-env",
                "--build-memory",
                "--builder",
                "--cpu",
                "--disable-validation",
                "--envs",
                "--memory",
                "--no-wait",
                "--parallelism",
                "--retry-limit",
                "--secret-envs",
                "--source-path",
                "--timeout",
                "--version"
            ],
            "examples": [
                "az spring job deploy -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup --artifact-path app.jar --env foo=bar",
                "az spring job deploy -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup --artifact-path app.jar --build-env BP_JVM_VERSION=11.*"
            ]
        },
        {
            "name": "az spring job list",
            "summary": "List all jobs in the Azure Spring Apps.",
            "syntax": "az spring job list --resource-group\n                   --service",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az spring job list -s MyAzureSpringAppsInstance -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring job logs",
            "summary": "Show logs for job execution instances. Logs will be streamed when setting '-f/--follow'.",
            "syntax": "az spring job logs --execution\n                   --name\n                   --resource-group\n                   --service\n                   [--all-instances]\n                   [--follow]\n                   [--instance]\n                   [--limit]\n                   [--lines]\n                   [--max-log-requests]\n                   [--since]",
            "required_parameters": [
                "--execution",
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--all-instances",
                "--follow  -f",
                "--instance -i",
                "--limit",
                "--lines",
                "--max-log-requests",
                "--since"
            ],
            "examples": [
                "az spring job logs --name job-name --execution job-execution-nam --all-instances -s MyService -g MyResourceGroup",
                "az spring job logs --name job-name --execution job-execution-nam --instance job-execution-instance -s MyService -g MyResourceGroup",
                "az spring job logs --name job-name --execution job-execution-nam --all-instances --follow -s MyService -g MyResourceGroup",
                "az spring job logs --name job-name --execution job-execution-nam --instance MyJobExecutionInstance --follow -s MyService -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring job show",
            "summary": "Show the details of a job in the Azure Spring Apps.",
            "syntax": "az spring job show --name\n                   --resource-group\n                   --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az spring job start",
            "summary": "Start an execution of the job.",
            "syntax": "az spring job start --name\n                    --resource-group\n                    --service\n                    [--args]\n                    [--cpu]\n                    [--envs]\n                    [--memory]\n                    [--secret-envs]\n                    [--wait-until-finished {false, true}]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--args",
                "--cpu",
                "--envs",
                "--memory",
                "--secret-envs",
                "--wait-until-finished"
            ],
            "examples": [
                "az spring job start -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring job update",
            "summary": "Update configurations of a job.",
            "syntax": "az spring job update --name\n                     --resource-group\n                     --service\n                     [--args]\n                     [--cpu]\n                     [--envs]\n                     [--memory]\n                     [--parallelism]\n                     [--retry-limit]\n                     [--secret-envs]\n                     [--timeout]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--args",
                "--cpu",
                "--envs",
                "--memory",
                "--parallelism",
                "--retry-limit",
                "--secret-envs",
                "--timeout"
            ],
            "examples": [
                "az spring job update -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup --envs foo=bar",
                "az spring job update -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup --envs",
                "az spring job update -n job-name -s MyAzureSpringAppsInstance -g MyResourceGroup --secret-envs"
            ]
        }
    ]
}