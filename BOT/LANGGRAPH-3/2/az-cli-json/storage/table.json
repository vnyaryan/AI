{
    "command_group": "az storage table",
    "description": "Manage NoSQL key-value storage.",
    "status": "GA",
    "commands": [
        {
            "name": "az storage table create",
            "summary": "Create a new table in the storage account.",
            "syntax": "az storage table create --name\n                        [--account-key]\n                        [--account-name]\n                        [--auth-mode {key, login}]\n                        [--connection-string]\n                        [--fail-on-exist]\n                        [--sas-token]\n                        [--table-endpoint]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--fail-on-exist",
                "--sas-token",
                "--table-endpoint"
            ],
            "examples": []
        },
        {
            "name": "az storage table delete",
            "summary": "Delete the specified table and any data it contains.",
            "syntax": "az storage table delete --name\n                        [--account-key]\n                        [--account-name]\n                        [--auth-mode {key, login}]\n                        [--connection-string]\n                        [--fail-not-exist]\n                        [--sas-token]\n                        [--table-endpoint]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--fail-not-exist",
                "--sas-token",
                "--table-endpoint"
            ],
            "examples": []
        },
        {
