{
    "command_group": "az storage account network-rule",
    "description": "Manage network rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az storage account network-rule add",
            "summary": "Add a network rule.",
            "syntax": "az storage account network-rule add --account-name\n                                    [--action]\n                                    [--ip-address]\n                                    [--resource-group]\n                                    [--resource-id]\n                                    [--subnet]\n                                    [--tenant-id]\n                                    [--vnet-name]",
            "required_parameters": [
                "--account-name -n"
            ],
            "optional_parameters": [
                "--action",
                "--ip-address",
                "--resource-group -g",
                "--resource-id",
                "--subnet",
                "--tenant-id",
                "--vnet-name"
            ],
            "examples": [
                "az storage account network-rule add -g myRg --account-name mystorageaccount --ip-address 23.45.1.0/24",
                "az storage account network-rule add -g myRg --account-name mystorageaccount --vnet-name myvnet --subnet mysubnet",
                "az storage account network-rule add -g myRg --account-name mystorageaccount  --subnet $subnetId"
            ]
        },
        {
            "name": "az storage account network-rule list",
            "summary": "List network rules.",
            "syntax": "az storage account network-rule list --account-name\n                                     [--resource-group]",
            "required_parameters": [
                "--account-name -n"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storage account network-rule list --account-name MyAccount --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az storage account network-rule remove",
            "summary": "Remove a network rule.",
            "syntax": "az storage account network-rule remove --account-name\n                                       [--ip-address]\n                                       [--resource-group]\n                                       [--resource-id]\n                                       [--subnet]\n                                       [--tenant-id]\n                                       [--vnet-name]",
            "required_parameters": [
                "--account-name -n"
            ],
            "optional_parameters": [
                "--ip-address",
                "--resource-group -g",
                "--resource-id",
                "--subnet",
                "--tenant-id",
                "--vnet-name"
            ],
            "examples": [
                "az storage account network-rule remove --account-name MyAccount --resource-group MyResourceGroup --subnet MySubnetID",
                "az storage account network-rule remove --account-name MyAccount --ip-address 23.45.1.0/24 --resource-group MyResourceGroup"
            ]
        }
    ]
}