{
    "command_group": "az storage logging",
    "description": "Manage storage service logging information.",
    "status": "GA",
    "commands": [
        {
            "name": "az storage logging off",
            "summary": "Turn off logging for a storage account.",
            "syntax": "az storage logging off [--account-key]\n                       [--account-name]\n                       [--connection-string]\n                       [--sas-token]\n                       [--services]\n                       [--timeout]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--connection-string",
                "--sas-token",
                "--services",
                "--timeout"
            ],
            "examples": [
                "az storage logging off --account-name MyAccount"
            ]
        },
        {
            "name": "az storage logging show",
            "summary": "Show logging settings for a storage account.",
            "syntax": "az storage logging show [--account-key]\n                        [--account-name]\n                        [--connection-string]\n                        [--sas-token]\n                        [--services]\n                        [--timeout]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--connection-string",
                "--sas-token",
                "--services",
                "--timeout"
            ],
            "examples": [
                "az storage logging show --account-name MyAccount --services qt"
            ]
        },
        {
            "name": "az storage logging update",
            "summary": "Update logging settings for a storage account.",
            "syntax": "az storage logging update --log\n                          --retention\n                          --services\n                          [--account-key]\n                          [--account-name]\n                          [--connection-string]\n                          [--sas-token]\n                          [--timeout]\n                          [--version]",
            "required_parameters": [
                "--log",
                "--retention",
                "--services"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--connection-string",
                "--sas-token",
                "--timeout",
                "--version"
            ],
            "examples": []
        }
    ]
}