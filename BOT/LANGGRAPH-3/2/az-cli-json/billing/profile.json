{
    "command_group": "az billing profile",
    "description": "Manage profile.",
    "status": "Preview",
    "commands": [
        {
            "name": "az billing profile create",
            "summary": "Creates or updates a billing profile. The operation is supported for billing accounts with agreement type Microsoft Customer Agreement or Microsoft Partner Agreement.",
            "syntax": "az billing profile create --account-name\n                          --name\n                          [--bill-to]\n                          [--display-name]\n                          [--enabled-azure-plans]\n                          [--invoice-email-opt-in {false, true}]\n                          [--invoice-sections-value]\n                          [--no-wait]\n                          [--po-number]",
            "required_parameters": [
                "--account-name",
                "--name -n"
            ],
            "optional_parameters": [
                "--bill-to",
                "--display-name",
                "--enabled-azure-plans",
                "--invoice-email-opt-in",
                "--invoice-sections-value",
                "--no-wait",
                "--po-number"
            ],
            "examples": [
                "az billing profile create --account-name \"{billingAccountName}\" --name \"{billingProfileName}\" --bill-to address-line1=\"Test Address 1\" city=\"Redmond\" country=\"US\" first-name=\"Test\" last-name=\"User\" postal-code=\"12345\" region=\"WA\" --display-name \"Finance\" --enabled-azure-plans sku-id=\"0001\" --enabled-azure-plans sku-id=\"0002\" --invoice-email-opt-in true --po-number \"ABC12345\""
            ]
        },
        {
            "name": "az billing profile list",
            "summary": "List the billing profiles that a user has access to. The operation is supported for billing accounts with agreement type Microsoft Customer Agreement or Microsoft Partner Agreement.",
            "syntax": "az billing profile list --account-name\n                        [--expand]",
            "required_parameters": [
                "--account-name"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": [
                "az billing profile list --account-name \"{billingAccountName}\"",
                "az billing profile list --expand \"invoiceSections\" --account-name \"{billingAccountName}\""
            ]
        },
        {
            "name": "az billing profile show",
            "summary": "Get a billing profile by its ID. The operation is supported for billing accounts with agreement type Microsoft Customer Agreement or Microsoft Partner Agreement.",
            "syntax": "az billing profile show --account-name\n                        --name\n                        [--expand]",
            "required_parameters": [
                "--account-name",
                "--name -n"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": [
                "az billing profile show --account-name \"{billingAccountName}\" --name \"{billingProfileName}\"",
                "az billing profile show --expand \"invoiceSections\" --account-name \"{billingAccountName}\" --name \"{billingProfileName}\""
            ]
        },
        {
            "name": "az billing profile update",
            "summary": "Creates or updates a billing profile. The operation is supported for billing accounts with agreement type Microsoft Customer Agreement or Microsoft Partner Agreement.",
            "syntax": "az billing profile update --account-name\n                          --name\n                          [--bill-to]\n                          [--display-name]\n                          [--enabled-azure-plans]\n                          [--invoice-email-opt-in {false, true}]\n                          [--invoice-sections-value]\n                          [--no-wait]\n                          [--po-number]",
            "required_parameters": [
                "--account-name",
                "--name -n"
            ],
            "optional_parameters": [
                "--bill-to",
                "--display-name",
                "--enabled-azure-plans",
                "--invoice-email-opt-in",
                "--invoice-sections-value",
                "--no-wait",
                "--po-number"
            ],
            "examples": []
        },
        {
            "name": "az billing profile wait",
            "summary": "Place the CLI in a waiting state until a condition of the billing profile is met.",
            "syntax": "az billing profile wait --account-name\n                        --name\n                        [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--expand]\n                        [--interval]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [
                "--account-name",
                "--name -n"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az billing profile wait --expand \"invoiceSections\" --account-name \"{billingAccountName}\" --name \"{billingProfileName}\" --created",
                "az billing profile wait --expand \"invoiceSections\" --account-name \"{billingAccountName}\" --name \"{billingProfileName}\" --updated"
            ]
        }
    ]
}