{
    "command_group": "az guestconfig guest-configuration-hcrp-assignment-report",
    "description": "Guestconfig guest-configuration-hcrp-assignment-report.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az guestconfig guest-configuration-hcrp-assignment-report list",
            "summary": "List all reports for the guest configuration assignment, latest report first.",
            "syntax": "az guestconfig guest-configuration-hcrp-assignment-report list --guest-configuration-assignment-name\n                                                               --machine-name\n                                                               --resource-group",
            "required_parameters": [
                "--guest-configuration-assignment-name",
                "--machine-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az guestconfig guest-configuration-hcrp-assignment-report list --guest-configuration-assignment-name \"AuditSecureProtocol\" --machine-name \"myMachineName\" --resource-group \"myResourceGroupName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-hcrp-assignment-report show",
            "summary": "Get a report for the guest configuration assignment, by reportId.",
            "syntax": "az guestconfig guest-configuration-hcrp-assignment-report show [--guest-configuration-assignment-name]\n                                                               [--ids]\n                                                               [--machine-name]\n                                                               [--report-id]\n                                                               [--resource-group]\n                                                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--guest-configuration-assignment-name",
                "--ids",
                "--machine-name",
                "--report-id",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az guestconfig guest-configuration-hcrp-assignment-report show --guest-configuration-assignment-name \"AuditSecureProtocol\" --machine-name \"myMachineName\" --report-id \"7367cbb8-ae99-47d0-a33b-a283564d2cb1\" --resource-group \"myResourceGroupName\""
            ]
        }
    ]
}