{
    "command_group": "az sphere device wifi",
    "description": "Manage Wi-Fi configurations for the attached device.",
    "status": "GA",
    "commands": [
        {
            "name": "az sphere device wifi add",
            "summary": "Add a Wi-Fi network on the attached device.",
            "syntax": "az sphere device wifi add --ssid\n                          [--client-cert-id]\n                          [--client-id]\n                          [--config-name]\n                          [--device]\n                          [--psk]\n                          [--root-ca-cert-id]\n                          [--targeted-scan]",
            "required_parameters": [
                "--ssid -s"
            ],
            "optional_parameters": [
                "--client-cert-id",
                "--client-id",
                "--config-name",
                "--device -d",
                "--psk -p",
                "--root-ca-cert-id",
                "--targeted-scan"
            ],
            "examples": [
                "az sphere device wifi add --ssid",
                "az sphere device wifi add --ssid NETWORK1 --psk EXAMPLEPSK",
                "az sphere device wifi add --ssid myEapTlsSsid --client-cert-id myClientCert --client-id user@domain.com --root-ca-cert-id myRootCA --config-name Network1"
            ]
        },
        {
            "name": "az sphere device wifi disable",
            "summary": "Disable a Wi-Fi connection on the attached device.",
            "syntax": "az sphere device wifi disable --id\n                              [--device]",
            "required_parameters": [
                "--id -i"
            ],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi disable --id 1"
            ]
        },
        {
            "name": "az sphere device wifi enable",
            "summary": "Enable a Wi-Fi network on the attached device.",
            "syntax": "az sphere device wifi enable --id\n                             [--device]",
            "required_parameters": [
                "--id -i"
            ],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi enable --id 1"
            ]
        },
        {
            "name": "az sphere device wifi forget",
            "summary": "Forget a Wi-Fi network on the attached device.",
            "syntax": "az sphere device wifi forget --id\n                             [--device]",
            "required_parameters": [
                "--id -i"
            ],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi forget --id 1"
            ]
        },
        {
            "name": "az sphere device wifi list",
            "summary": "List the current Wi-Fi configurations for the attached device.",
            "syntax": "az sphere device wifi list [--device]",
            "required_parameters": [],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi list"
            ]
        },
        {
            "name": "az sphere device wifi reload-config",
            "summary": "Reload the Wi-Fi network configuration on the attached device. Use this command after you add or remove a certificate (azsphere device certificate) to ensure that EAP-TLS networks use the most recent contents of the certificate store.",
            "syntax": "az sphere device wifi reload-config [--device]",
            "required_parameters": [],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi reload-config"
            ]
        },
        {
            "name": "az sphere device wifi scan",
            "summary": "Scan for available Wi-Fi networks visible to the attached device.",
            "syntax": "az sphere device wifi scan [--device]",
            "required_parameters": [],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi scan"
            ]
        },
        {
            "name": "az sphere device wifi show",
            "summary": "Show details of a Wi-Fi network on the attached device.",
            "syntax": "az sphere device wifi show --id\n                           [--device]",
            "required_parameters": [
                "--id -i"
            ],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi show"
            ]
        },
        {
            "name": "az sphere device wifi show-status",
            "summary": "Show the status of the wireless interface on the attached device.",
            "syntax": "az sphere device wifi show-status [--device]",
            "required_parameters": [],
            "optional_parameters": [
                "--device -d"
            ],
            "examples": [
                "az sphere device wifi show-status"
            ]
        }
    ]
}