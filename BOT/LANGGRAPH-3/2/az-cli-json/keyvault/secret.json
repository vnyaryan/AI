{
    "command_group": "az keyvault secret",
    "description": "Manage secrets.",
    "status": "GA",
    "commands": [
        {
            "name": "az keyvault secret backup",
            "summary": "Backs up the specified secret.",
            "syntax": "az keyvault secret backup --file\n                          [--id]\n                          [--name]\n                          [--vault-name]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret delete",
            "summary": "Delete all versions of a secret.",
            "syntax": "az keyvault secret delete [--id]\n                          [--name]\n                          [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret download",
            "summary": "Download a secret from a KeyVault.",
            "syntax": "az keyvault secret download --file\n                            [--encoding {ascii, base64, hex, utf-16be, utf-16le, utf-8}]\n                            [--id]\n                            [--name]\n                            [--vault-name]\n                            [--version]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--encoding -e",
                "--id",
                "--name -n",
                "--vault-name",
                "--version -v"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret list",
            "summary": "List secrets in a specified key vault.",
            "syntax": "az keyvault secret list [--id]\n                        [--include-managed {false, true}]\n                        [--maxresults]\n                        [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--include-managed",
                "--maxresults",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret list-deleted",
            "summary": "Lists deleted secrets for the specified vault.",
            "syntax": "az keyvault secret list-deleted [--id]\n                                [--maxresults]\n                                [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--maxresults",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret list-versions",
            "summary": "List all versions of the specified secret.",
            "syntax": "az keyvault secret list-versions [--id]\n                                 [--maxresults]\n                                 [--name]\n                                 [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--maxresults",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret purge",
            "summary": "Permanently deletes the specified secret.",
            "syntax": "az keyvault secret purge [--id]\n                         [--name]\n                         [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret recover",
            "summary": "Recovers the deleted secret to the latest version.",
            "syntax": "az keyvault secret recover [--id]\n                           [--name]\n                           [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret restore",
            "summary": "Restores a backed up secret to a vault.",
            "syntax": "az keyvault secret restore --file\n                           --vault-name",
            "required_parameters": [
                "--file -f",
                "--vault-name"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az keyvault secret set",
            "summary": "Create a secret (if one doesn't exist) or update a secret in a KeyVault.",
            "syntax": "az keyvault secret set --name\n                       --vault-name\n                       [--content-type]\n                       [--disabled {false, true}]\n                       [--encoding {ascii, base64, hex, utf-16be, utf-16le, utf-8}]\n                       [--expires]\n                       [--file]\n                       [--not-before]\n                       [--tags]\n                       [--value]",
            "required_parameters": [
                "--name -n",
                "--vault-name"
            ],
            "optional_parameters": [
                "--content-type --description",
                "--disabled",
                "--encoding -e",
                "--expires",
                "--file -f",
                "--not-before",
                "--tags",
                "--value"
            ],
            "examples": [
                "az keyvault secret set --name MySecretName --vault-name MyKeyVault --value MyVault",
                "az keyvault secret set --name MySecretName --vault-name MyKeyVault --file /path/to/file --encoding MyEncoding"
            ]
        },
        {
            "name": "az keyvault secret set-attributes",
            "summary": "Updates the attributes associated with a specified secret in a given key vault.",
            "syntax": "az keyvault secret set-attributes [--content-type]\n                                  [--enabled {false, true}]\n                                  [--expires]\n                                  [--id]\n                                  [--name]\n                                  [--not-before]\n                                  [--tags]\n                                  [--vault-name]\n                                  [--version]",
            "required_parameters": [],
            "optional_parameters": [
                "--content-type",
                "--enabled",
                "--expires",
                "--id",
                "--name -n",
                "--not-before",
                "--tags",
                "--vault-name",
                "--version -v"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret show",
            "summary": "Get a specified secret from a given key vault.",
            "syntax": "az keyvault secret show [--id]\n                        [--name]\n                        [--vault-name]\n                        [--version]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name",
                "--version -v"
            ],
            "examples": []
        },
        {
            "name": "az keyvault secret show-deleted",
            "summary": "Gets the specified deleted secret.",
            "syntax": "az keyvault secret show-deleted [--id]\n                                [--name]\n                                [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--vault-name"
            ],
            "examples": []
        }
    ]
}