{
    "command_group": "az site-recovery event",
    "description": "Manage the events of the vault.",
    "status": "GA",
    "commands": [
        {
            "name": "az site-recovery event list",
            "summary": "List the list of Azure Site Recovery events for the vault.",
            "syntax": "az site-recovery event list --resource-group\n                            --vault-name\n                            [--filter]\n                            [--max-items]\n                            [--next-token]",
            "required_parameters": [
                "--resource-group -g",
                "--vault-name"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az site-recovery event list -g rg --vault-name vault_name"
            ]
        },
        {
            "name": "az site-recovery event show",
            "summary": "Get operation to get the details of an Azure Site recovery event.",
            "syntax": "az site-recovery event show [--event-name]\n                            [--ids]\n                            [--resource-group]\n                            [--subscription]\n                            [--vault-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--event-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--vault-name"
            ],
            "examples": [
                "az site-recovery event show -g rg --vault-name vault_name -n event_name"
            ]
        }
    ]
}