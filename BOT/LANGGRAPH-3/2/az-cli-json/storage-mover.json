                "--name --storage-mover-name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az storage-mover update -g {rg} -n {mover_name} --tags {{key2:value2}} --description ExampleDesc2"
            ]
        },
        {
            "name": "az storage-mover wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az storage-mover wait [--created]\n                      [--custom]\n                      [--deleted]\n                      [--exists]\n                      [--ids]\n                      [--interval]\n                      [--name]\n                      [--resource-group]\n                      [--subscription]\n                      [--timeout]\n                      [--updated]",
            "required_parameters": [],
