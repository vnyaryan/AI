            "required_parameters": [
                "--account-name -a",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az cosmosdb table restore",
            "summary": "Restore a deleted table within the same account.",
            "syntax": "az cosmosdb table restore --account-name\n                          --resource-group\n                          --table-name\n                          [--disable-ttl]\n                          [--restore-timestamp]",
            "required_parameters": [
                "--account-name -a",
                "--resource-group -g",
                "--table-name -n"
            ],
            "optional_parameters": [
                "--disable-ttl",
                "--restore-timestamp -t"
            ],
            "examples": [
                "az cosmosdb table restore --resource-group resource_group --account-name database_account_name --table-name name_of_table_needs_to_be_restored --restore-timestamp 2020-07-13T16:03:41+0000"
            ]
        },
        {
            "name": "az cosmosdb table restore",
            "summary": "Restore a deleted table within the same account.",
            "syntax": "az cosmosdb table restore --account-name\n                          --resource-group\n                          --table-name\n                          [--disable-ttl {false, true}]\n                          [--restore-timestamp]",
            "required_parameters": [
                "--account-name -a",
                "--resource-group -g",
                "--table-name -n"
            ],
            "optional_parameters": [
                "--disable-ttl",
                "--restore-timestamp -t"
            ],
            "examples": [
                "az cosmosdb table restore --resource-group resource_group --account-name database_account_name --table-name name_of_table_needs_to_be_restored --restore-timestamp 2020-07-13T16:03:41+0000"
            ]
        },
        {
