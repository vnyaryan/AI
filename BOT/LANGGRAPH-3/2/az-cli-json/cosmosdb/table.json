{
    "command_group": "az cosmosdb table",
    "description": "Manage Table resources of Azure Cosmos DB account.",
    "status": "GA",
    "commands": [
        {
            "name": "az cosmosdb table create",
            "summary": "Create an Table under an Azure Cosmos DB account.",
            "syntax": "az cosmosdb table create --account-name\n                         --name\n                         --resource-group\n                         [--max-throughput]\n                         [--throughput]",
            "required_parameters": [
                "--account-name -a",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-throughput",
                "--throughput"
            ],
            "examples": []
        },
        {
            "name": "az cosmosdb table delete",
            "summary": "Delete the Table under an Azure Cosmos DB account.",
            "syntax": "az cosmosdb table delete --account-name\n                         --name\n                         --resource-group\n                         [--yes]",
            "required_parameters": [
                "--account-name -a",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az cosmosdb table exists",
            "summary": "Checks if an Azure Cosmos DB table exists.",
            "syntax": "az cosmosdb table exists --account-name\n                         --name\n                         --resource-group",
            "required_parameters": [
                "--account-name -a",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
