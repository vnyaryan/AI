{
    "command_group": "az hdinsight-on-aks cluster instance-view",
    "description": "Gets the status of a cluster instances.",
    "status": "Preview",
    "commands": [
        {
            "name": "az hdinsight-on-aks cluster instance-view list",
            "summary": "List the lists of instance views.",
            "syntax": "az hdinsight-on-aks cluster instance-view list --cluster-name\n                                               --cluster-pool-name\n                                               --resource-group\n                                               [--max-items]\n                                               [--next-token]",
            "required_parameters": [
                "--cluster-name",
                "--cluster-pool-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az hdinsight-on-aks cluster instance-view list --cluster-pool-name {poolName} -g {rg} --cluster-name {clusterName}"
            ]
        },
        {
            "name": "az hdinsight-on-aks cluster instance-view show",
            "summary": "Get the status of a cluster instance.",
            "syntax": "az hdinsight-on-aks cluster instance-view show [--cluster-name]\n                                               [--cluster-pool-name]\n                                               [--ids]\n                                               [--resource-group]\n                                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--cluster-pool-name",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az hdinsight-on-aks cluster instance-view show --cluster-pool-name {poolName} -g {rg} --cluster-name {clusterName}"
            ]
        }
    ]
}