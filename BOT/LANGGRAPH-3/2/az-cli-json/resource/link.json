{
    "command_group": "az resource link",
    "description": "Manage links between resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az resource link create",
            "summary": "Create a new link between resources.",
            "syntax": "az resource link create --link\n                        --target\n                        [--notes]",
            "required_parameters": [
                "--link",
                "--target"
            ],
            "optional_parameters": [
                "--notes"
            ],
            "examples": [
                "az resource link create --link {SourceID} --target {ResourceID} --notes \"SourceID depends on ResourceID\""
            ]
        },
        {
            "name": "az resource link delete",
            "summary": "Delete a link between resources.",
            "syntax": "az resource link delete --link",
            "required_parameters": [
                "--link"
            ],
            "optional_parameters": [],
            "examples": [
                "az resource link delete --link {LinkID}"
            ]
        },
        {
            "name": "az resource link list",
            "summary": "List resource links.",
            "syntax": "az resource link list [--filter]\n                      [--scope]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter",
                "--scope"
            ],
            "examples": [
                "az resource link list --filter {filter-string}",
                "az resource link list --scope /subscriptions/{SubID}/resourceGroups/{ResourceGroup}"
            ]
        },
        {
            "name": "az resource link show",
            "summary": "Gets a resource link with the specified ID.",
            "syntax": "az resource link show --link",
            "required_parameters": [
                "--link"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az resource link update",
            "summary": "Update link between resources.",
            "syntax": "az resource link update --link\n                        [--notes]\n                        [--target]",
            "required_parameters": [
                "--link"
            ],
            "optional_parameters": [
                "--notes",
                "--target"
            ],
            "examples": [
                "az resource link update --link {LinkID} --notes \"some notes to explain this link\""
            ]
        }
    ]
}