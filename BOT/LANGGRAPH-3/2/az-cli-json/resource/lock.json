{
    "command_group": "az resource lock",
    "description": "Manage Azure resource level locks.",
    "status": "GA",
    "commands": [
        {
            "name": "az resource lock create",
            "summary": "Create a resource-level lock.",
            "syntax": "az resource lock create --lock-type {CanNotDelete, ReadOnly}\n                        --name\n                        [--namespace]\n                        [--notes]\n                        [--parent]\n                        [--resource]\n                        [--resource-group]\n                        [--resource-type]",
            "required_parameters": [
                "--lock-type -t",
                "--name -n"
            ],
            "optional_parameters": [
                "--namespace",
                "--notes",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource lock create --lock-type ReadOnly -n lockName -g MyResourceGroup --resource myvnet --resource-type Microsoft.Network/virtualNetworks",
                "az resource lock create --lock-type ReadOnly -n lockName --resource /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Network/virtualNetworks/{VNETName}"
            ]
        },
        {
            "name": "az resource lock delete",
            "summary": "Delete a resource-level lock.",
            "syntax": "az resource lock delete [--ids]\n                        [--name]\n                        [--namespace]\n                        [--parent]\n                        [--resource]\n                        [--resource-group]\n                        [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource lock delete --name lockName -g MyResourceGroup --resource myvnet --resource-type Microsoft.Network/virtualNetworks",
                "az resource lock delete -n lockName --resource /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Network/virtualNetworks/{VMName}",
                "az resource lock delete --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp}"
            ]
        },
        {
            "name": "az resource lock list",
            "summary": "List lock information in the resource-level.",
            "syntax": "az resource lock list [--filter-string]\n                      [--namespace]\n                      [--parent]\n                      [--resource]\n                      [--resource-group]\n                      [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter-string",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource lock list -g MyResourceGroup --resource myvnet --resource-type Microsoft.Network/virtualNetworks"
            ]
        },
        {
            "name": "az resource lock show",
            "summary": "Show the details of a resource-level lock.",
            "syntax": "az resource lock show [--ids]\n                      [--name]\n                      [--namespace]\n                      [--parent]\n                      [--resource]\n                      [--resource-group]\n                      [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource lock show -n lockname -g MyResourceGroup --resource myvnet --resource-type Microsoft.Network/virtualNetworks"
            ]
        },
        {
            "name": "az resource lock update",
            "summary": "Update a resource-level lock.",
            "syntax": "az resource lock update [--ids]\n                        [--lock-type {CanNotDelete, ReadOnly}]\n                        [--name]\n                        [--namespace]\n                        [--notes]\n                        [--parent]\n                        [--resource]\n                        [--resource-group]\n                        [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--lock-type -t",
                "--name -n",
                "--namespace",
                "--notes",
                "--parent",
                "--resource --resource-name",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource lock update --name lockName -g MyResourceGroup --resource myvnet --resource-type Microsoft.Network/virtualNetworks --notes newNotesHere --lock-type CanNotDelete",
                "az resource lock update --lock-type CanNotDelete --name lockName --namespace Microsoft.Network --resource-group MyResourceGroup --resource-name myvnet --resource-type Microsoft.Network/virtualNetworks"
            ]
        }
    ]
}