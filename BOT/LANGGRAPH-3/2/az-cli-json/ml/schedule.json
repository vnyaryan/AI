{
    "command_group": "az ml schedule",
    "description": "Manage Azure ML schedule resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az ml schedule create",
            "summary": "Create a schedule.",
            "syntax": "az ml schedule create --file\n                      --resource-group\n                      [--name]\n                      [--no-wait]\n                      [--set]\n                      [--workspace-name]",
            "required_parameters": [
                "--file -f",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--name -n",
                "--no-wait",
                "--set",
                "--workspace-name -w"
            ],
            "examples": [
                "az ml schedule create --file schedule.yml --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml schedule delete",
            "summary": "Delete a schedule. The previous triggered jobs will NOT be deleted.",
            "syntax": "az ml schedule delete --name\n                      --resource-group\n                      --workspace-name\n                      [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml schedule disable",
            "summary": "Disable a schedule so that it will stop triggering jobs.",
            "syntax": "az ml schedule disable --name\n                       --resource-group\n                       --workspace-name\n                       [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml schedule enable",
            "summary": "Enable a schedule so that it will continue triggering jobs.",
            "syntax": "az ml schedule enable --name\n                      --resource-group\n                      --workspace-name\n                      [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml schedule list",
            "summary": "List the schedules in a workspace.",
            "syntax": "az ml schedule list --resource-group\n                    [--disabled-only]\n                    [--include-disabled]\n                    [--max-results]\n                    [--workspace-name]",
            "required_parameters": [
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--disabled-only",
                "--include-disabled",
                "--max-results -r",
                "--workspace-name -w"
            ],
            "examples": [
                "az ml schedule list --query \"[].{Name:name}\"  --output table --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml schedule show",
            "summary": "Show details of a schedule.",
            "syntax": "az ml schedule show --name\n                    --resource-group\n                    [--workspace-name]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--workspace-name -w"
            ],
            "examples": [
                "az ml schedule show --name my-schedule --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml schedule trigger",
            "summary": "Trigger a schedule once.",
            "syntax": "az ml schedule trigger --name\n                       --resource-group\n                       --workspace-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml schedule trigger --name my-schedule --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml schedule update",
            "summary": "Update a schedule.",
            "syntax": "az ml schedule update --name\n                      --resource-group\n                      --workspace-name\n                      [--add]\n                      [--force-string]\n                      [--no-wait]\n                      [--remove]\n                      [--set]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--no-wait",
                "--remove",
                "--set"
            ],
            "examples": [
                "az ml schedule update --name my-schedule --set tags.key1=value1 tags.key2=value2 --resource-group my-resource-group --workspace-name my-workspace"
            ]
        }
    ]
}