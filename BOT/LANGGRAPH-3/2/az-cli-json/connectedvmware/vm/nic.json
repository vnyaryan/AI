{
    "command_group": "az connectedvmware vm nic",
    "description": "Vm nic.",
    "status": "GA",
    "commands": [
        {
            "name": "az connectedvmware vm nic add",
            "summary": "Add virtual nic to a virtual machine.",
            "syntax": "az connectedvmware vm nic add --name\n                              --network\n                              --resource-group\n                              --vm-name\n                              [--nic-type]\n                              [--no-wait]\n                              [--power-on-boot]",
            "required_parameters": [
                "--name -n",
                "--network",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [
                "--nic-type",
                "--no-wait",
                "--power-on-boot"
            ],
            "examples": [
                "az connectedvmware vm nic add --name \"Name of the NIC\" --network \"Network Name or Id\"                --resource-group \"resource group name\" --vm-name \"Name of the virtual machine\""
            ]
        },
        {
            "name": "az connectedvmware vm nic delete",
            "summary": "Delete virtual nic to a virtual machine.",
            "syntax": "az connectedvmware vm nic delete --nics\n                                 --resource-group\n                                 --vm-name\n                                 [--no-wait]\n                                 [--yes]",
            "required_parameters": [
                "--nics",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az connectedvmware vm nic delete --nics \"Names of the NICs\" --resource-group                \"resource group name\" --vm-name \"Name of the virtual machine\""
            ]
        },
        {
            "name": "az connectedvmware vm nic list",
            "summary": "Retrieve a list of vm nic from given resource group name and vm name.",
            "syntax": "az connectedvmware vm nic list --resource-group\n                               --vm-name",
            "required_parameters": [
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az connectedvmware vm nic list --resource-group \"resource group name\" --vm-name                \"Name of the virtual machine\""
            ]
        },
        {
            "name": "az connectedvmware vm nic show",
            "summary": "Get details of a vm nic by it's name, resource-group and vm name.",
            "syntax": "az connectedvmware vm nic show --name\n                               --resource-group\n                               --vm-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az connectedvmware vm nic show --name \"Name of the NIC\" --resource-group                \"resource group name\" --vm-name \"Name of the virtual machine\""
            ]
        },
        {
            "name": "az connectedvmware vm nic update",
            "summary": "Update virtual network interface of a virtual machine.",
            "syntax": "az connectedvmware vm nic update --resource-group\n                                 --vm-name\n                                 [--device-key]\n                                 [--name]\n                                 [--network]\n                                 [--no-wait]\n                                 [--power-on-boot]",
            "required_parameters": [
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [
                "--device-key",
                "--name -n",
                "--network",
                "--no-wait",
                "--power-on-boot"
            ],
            "examples": []
        }
    ]
}