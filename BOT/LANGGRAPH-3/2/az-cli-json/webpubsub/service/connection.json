{
    "command_group": "az webpubsub service connection",
    "description": "Commands to manage Webpubsub service connections.",
    "status": "GA",
    "commands": [
        {
            "name": "az webpubsub service connection close",
            "summary": "Close a specific client connection. Error throws if operation fails.",
            "syntax": "az webpubsub service connection close --connection-id\n                                      --hub-name\n                                      [--ids]\n                                      [--name]\n                                      [--resource-group]\n                                      [--subscription]",
            "required_parameters": [
                "--connection-id",
                "--hub-name"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az webpubsub service connection exist",
            "summary": "Check whether client connection exists.",
            "syntax": "az webpubsub service connection exist --connection-id\n                                      --hub-name\n                                      [--ids]\n                                      [--name]\n                                      [--resource-group]\n                                      [--subscription]",
            "required_parameters": [
                "--connection-id",
                "--hub-name"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az webpubsub service connection send",
            "summary": "Send a message to connection. Error throws if operation fails.",
            "syntax": "az webpubsub service connection send --connection-id\n                                     --hub-name\n                                     --payload\n                                     [--ids]\n                                     [--name]\n                                     [--resource-group]\n                                     [--subscription]",
            "required_parameters": [
                "--connection-id",
                "--hub-name",
                "--payload"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        }
    ]
}