{
    "command_group": "az functionapp runtime config",
    "description": "Manage a function app's runtime configuration.",
    "status": "GA",
    "commands": [
        {
            "name": "az functionapp runtime config set",
            "summary": "Update an existing function app's runtime configuration.",
            "syntax": "az functionapp runtime config set --runtime-version\n                                  [--ids]\n                                  [--name]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [
                "--runtime-version"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az functionapp runtime config set --name MyFunctionApp --resource-group MyResourceGroup --runtime-version 3.11"
            ]
        },
        {
            "name": "az functionapp runtime config show",
            "summary": "Get the details of a function app's runtime configuration.",
            "syntax": "az functionapp runtime config show [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az functionapp runtime config show --name MyFunctionApp --resource-group MyResourceGroup"
            ]
        }
    ]
}