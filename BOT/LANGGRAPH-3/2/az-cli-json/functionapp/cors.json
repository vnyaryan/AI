{
    "command_group": "az functionapp cors",
    "description": "Manage Cross-Origin Resource Sharing (CORS).",
    "status": "GA",
    "commands": [
        {
            "name": "az functionapp cors add",
            "summary": "Add allowed origins.",
            "syntax": "az functionapp cors add --allowed-origins\n                        [--ids]\n                        [--name]\n                        [--resource-group]\n                        [--slot]\n                        [--subscription]",
            "required_parameters": [
                "--allowed-origins -a"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az functionapp cors add -g {myRG} -n {myAppName} --allowed-origins https://myapps.com"
            ]
        },
        {
            "name": "az functionapp cors credentials",
            "summary": "Enable or disable access-control-allow-credentials.",
            "syntax": "az functionapp cors credentials --enable {false, true}\n                                [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--slot]\n                                [--subscription]",
            "required_parameters": [
                "--enable"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az functionapp cors credentials --name MyFunctionApp --resource-group MyResourceGroup --enable true",
                "az functionapp cors credentials --name MyFunctionApp --resource-group MyResourceGroup --enable false"
            ]
        },
        {
            "name": "az functionapp cors remove",
            "summary": "Remove allowed origins.",
            "syntax": "az functionapp cors remove --allowed-origins\n                           [--ids]\n                           [--name]\n                           [--resource-group]\n                           [--slot]\n                           [--subscription]",
            "required_parameters": [
                "--allowed-origins -a"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az functionapp cors remove -g {myRG} -n {myAppName} --allowed-origins https://myapps.com",
                "az functionapp cors remove -g {myRG} -n {myAppName} --allowed-origins"
            ]
        },
        {
            "name": "az functionapp cors show",
            "summary": "Show allowed origins.",
            "syntax": "az functionapp cors show [--ids]\n                         [--name]\n                         [--resource-group]\n                         [--slot]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az functionapp cors show --name MyFunctionApp --resource-group MyResourceGroup"
            ]
        }
    ]
}