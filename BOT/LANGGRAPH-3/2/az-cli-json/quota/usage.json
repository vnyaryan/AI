{
    "command_group": "az quota usage",
    "description": "Manage the current usage of a resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az quota usage list",
            "summary": "List current usage for all resources for the scope specified.",
            "syntax": "az quota usage list --scope\n                    [--max-items]\n                    [--next-token]",
            "required_parameters": [
                "--scope"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az quota usage list --scope /subscriptions/0b1f6471-1bf0-4dda-aec3-cb9272f09590/providers/Microsoft.Network/locations/eastus"
            ]
        },
        {
            "name": "az quota usage show",
            "summary": "Show the current usage of a resource.",
            "syntax": "az quota usage show --resource-name\n                    --scope",
            "required_parameters": [
                "--resource-name",
                "--scope"
            ],
            "optional_parameters": [],
            "examples": [
                "az quota usage show  --scope /subscriptions/0b1f6471-1bf0-4dda-aec3-cb9272f09590/providers/Microsoft.Network/locations/eastus --resource-name MinPublicIpInterNetworkPrefixLength"
            ]
        }
    ]
}