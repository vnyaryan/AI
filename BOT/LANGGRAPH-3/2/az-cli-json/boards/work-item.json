{
    "command_group": "az boards work-item",
    "description": "Manage work items.",
    "status": "GA",
    "commands": [
        {
            "name": "az boards work-item create",
            "summary": "Create a work item.",
            "syntax": "az boards work-item create --title\n                           --type\n                           [--area]\n                           [--assigned-to]\n                           [--description]\n                           [--detect {false, true}]\n                           [--discussion]\n                           [--fields]\n                           [--iteration]\n                           [--open]\n                           [--org]\n                           [--project]\n                           [--reason]",
            "required_parameters": [
                "--title",
                "--type"
            ],
            "optional_parameters": [
                "--area",
                "--assigned-to",
                "--description -d",
                "--detect",
                "--discussion",
                "--fields -f",
                "--iteration",
                "--open",
                "--org --organization",
                "--project -p",
                "--reason"
            ],
            "examples": []
        },
        {
            "name": "az boards work-item delete",
            "summary": "Delete a work item.",
            "syntax": "az boards work-item delete --id\n                           [--destroy]\n                           [--detect {false, true}]\n                           [--org]\n                           [--project]\n                           [--yes]",
            "required_parameters": [
                "--id"
            ],
            "optional_parameters": [
                "--destroy",
                "--detect",
                "--org --organization",
                "--project -p",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az boards work-item show",
            "summary": "Show details for a work item.",
            "syntax": "az boards work-item show --id\n                         [--as-of]\n                         [--detect {false, true}]\n                         [--expand {all, fields, links, none, relations}]\n                         [--fields]\n                         [--open]\n                         [--org]",
            "required_parameters": [
                "--id"
            ],
            "optional_parameters": [
                "--as-of",
                "--detect",
                "--expand",
                "--fields -f",
                "--open",
                "--org --organization"
            ],
            "examples": []
        },
        {
            "name": "az boards work-item update",
            "summary": "Update work items.",
            "syntax": "az boards work-item update --id\n                           [--area]\n                           [--assigned-to]\n                           [--description]\n                           [--detect {false, true}]\n                           [--discussion]\n                           [--fields]\n                           [--iteration]\n                           [--open]\n                           [--org]\n                           [--reason]\n                           [--state]\n                           [--title]",
            "required_parameters": [
                "--id"
            ],
            "optional_parameters": [
                "--area",
                "--assigned-to",
                "--description -d",
                "--detect",
                "--discussion",
                "--fields -f",
                "--iteration",
                "--open",
                "--org --organization",
                "--reason",
                "--state",
                "--title"
            ],
            "examples": []
        }
    ]
}