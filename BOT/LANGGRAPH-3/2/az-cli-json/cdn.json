{
    "command_group": "az cdn",
    "description": "Manage Azure Content Delivery Networks (CDNs).",
    "status": "GA",
    "commands": [
        {
            "name": "az cdn name-exists",
            "summary": "Check the availability of a resource name. This is needed for resources where name is globally unique, such as a CDN endpoint.",
            "syntax": "az cdn name-exists --name\n                   [--type {Microsoft.Cdn/Profiles/AfdEndpoints, Microsoft.Cdn/Profiles/Endpoints}]",
            "required_parameters": [
                "--name"
            ],
            "optional_parameters": [
                "--type"
            ],
            "examples": [
                "az cdn name-exists --name contoso"
            ]
        },
        {
            "name": "az cdn usage",
            "summary": "Check the quota and actual usage of the CDN profiles under the given subscription.",
            "syntax": "az cdn usage [--max-items]\n             [--next-token]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": []
        }
    ]
}