{
    "command_group": "az network",
    "description": "Manage Azure Network resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az network list-service-aliases",
            "summary": "List available service aliases in the region which can be used for Service Endpoint Policies.",
            "syntax": "az network list-service-aliases [--location]\n                                [--max-items]\n                                [--next-token]\n                                [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--location -l",
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az network list-service-aliases --location westus2"
            ]
        },
        {
            "name": "az network list-service-tags",
            "summary": "List all service tags which are below to different resources.",
            "syntax": "az network list-service-tags --location",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [],
            "examples": [
                "az network list-service-tags --location westus2"
            ]
        },
        {
            "name": "az network list-usages",
            "summary": "List the number of network resources in a region that are used against a subscription quota.",
            "syntax": "az network list-usages --location\n                       [--max-items]\n                       [--next-token]",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az network list-usages --location eastus -o table"
            ]
        }
    ]
}