{
    "command_group": "az datafactory trigger-run",
    "description": "Manage trigger run with datafactory.",
    "status": "GA",
    "commands": [
        {
            "name": "az datafactory trigger-run cancel",
            "summary": "Cancel a single trigger instance by runId.",
            "syntax": "az datafactory trigger-run cancel [--factory-name]\n                                  [--ids]\n                                  [--resource-group]\n                                  [--run-id]\n                                  [--subscription]\n                                  [--trigger-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--factory-name",
                "--ids",
                "--resource-group -g",
                "--run-id",
                "--subscription",
                "--trigger-name"
            ],
            "examples": [
                "az datafactory trigger-run cancel --factory-name \"exampleFactoryName\" --resource-group \"exampleResourceGroup\" --run-id \"2f7fdb90-5df1-4b8e-ac2f-064cfa58202b\" --trigger-name \"exampleTrigger\""
            ]
        },
        {
            "name": "az datafactory trigger-run query-by-factory",
            "summary": "Query trigger runs.",
            "syntax": "az datafactory trigger-run query-by-factory --last-updated-after\n                                            --last-updated-before\n                                            [--continuation-token]\n                                            [--factory-name]\n                                            [--filters]\n                                            [--ids]\n                                            [--order-by]\n                                            [--resource-group]\n                                            [--subscription]",
            "required_parameters": [
                "--last-updated-after",
                "--last-updated-before"
            ],
            "optional_parameters": [
                "--continuation-token",
                "--factory-name",
                "--filters",
                "--ids",
                "--order-by",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az datafactory trigger-run query-by-factory --factory-name \"exampleFactoryName\" --filters operand=\"TriggerName\" operator=\"Equals\" values=\"exampleTrigger\" --last-updated-after \"2018-06-16T00:36:44.3345758Z\" --last-updated-before \"2018-06-16T00:49:48.3686473Z\" --resource-group \"exampleResourceGroup\""
            ]
        },
        {
            "name": "az datafactory trigger-run rerun",
            "summary": "Rerun single trigger instance by runId.",
            "syntax": "az datafactory trigger-run rerun [--factory-name]\n                                 [--ids]\n                                 [--resource-group]\n                                 [--run-id]\n                                 [--subscription]\n                                 [--trigger-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--factory-name",
                "--ids",
                "--resource-group -g",
                "--run-id",
                "--subscription",
                "--trigger-name"
            ],
            "examples": [
                "az datafactory trigger-run rerun --factory-name \"exampleFactoryName\" --resource-group \"exampleResourceGroup\" --run-id \"2f7fdb90-5df1-4b8e-ac2f-064cfa58202b\" --trigger-name \"exampleTrigger\""
            ]
        }
    ]
}