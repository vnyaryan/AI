{
    "command_group": "az datafactory trigger",
    "description": "Manage trigger with datafactory.",
    "status": "GA",
    "commands": [
        {
            "name": "az datafactory trigger create",
            "summary": "Create a trigger.",
            "syntax": "az datafactory trigger create --factory-name\n                              --name\n                              --properties\n                              --resource-group\n                              [--if-match]",
            "required_parameters": [
                "--factory-name",
                "--name --trigger-name -n",
                "--properties",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--if-match"
            ],
            "examples": [
                "az datafactory trigger create --factory-name \"exampleFactoryName\" --resource-group \"exampleResourceGroup\" --properties \"{\\\"type\\\":\\\"ScheduleTrigger\\\",\\\"pipelines\\\":[{\\\"parameters\\\":{\\\"OutputBlobNameList\\\":[\\\"exampleoutput.csv\\\"]},\\\"pipelineReference\\\":{\\\"type\\\":\\\"PipelineReference\\\",\\\"referenceName\\\":\\\"examplePipeline\\\"}}],\\\"typeProperties\\\":{\\\"recurrence\\\":{\\\"endTime\\\":\\\"2018-06-16T00:55:13.8441801Z\\\",\\\"frequency\\\":\\\"Minute\\\",\\\"interval\\\":4,\\\"startTime\\\":\\\"2018-06-16T00:39:13.8441801Z\\\",\\\"timeZone\\\":\\\"UTC\\\"}}}\" --name \"exampleTrigger\""
            ]
        },
        {
            "name": "az datafactory trigger delete",
            "summary": "Deletes a trigger.",
            "syntax": "az datafactory trigger delete [--factory-name]\n                              [--ids]\n                              [--name]\n                              [--resource-group]\n                              [--subscription]\n                              [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--factory-name",
                "--ids",
                "--name --trigger-name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az datafactory trigger delete --factory-name \"exampleFactoryName\" --resource-group \"exampleResourceGroup\" --name \"exampleTrigger\""
            ]
        },
        {
            "name": "az datafactory trigger get-event-subscription-status",
            "summary": "Get a trigger's event subscription status.",
