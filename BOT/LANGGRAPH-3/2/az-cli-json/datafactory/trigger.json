            "required_parameters": [],
            "optional_parameters": [
                "--factory-name",
                "--ids",
                "--name --trigger-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az datafactory trigger get-event-subscription-status --factory-name \"exampleFactoryName\" --resource-group \"exampleResourceGroup\" --name \"exampleTrigger\""
            ]
