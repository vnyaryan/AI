{
    "command_group": "az containerapp auth twitter",
    "description": "Manage containerapp authentication and authorization of the Twitter identity provider.",
    "status": "GA",
    "commands": [
        {
            "name": "az containerapp auth twitter show",
            "summary": "Show the authentication settings for the Twitter identity provider.",
            "syntax": "az containerapp auth twitter show [--ids]\n                                  [--name]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az containerapp auth twitter show --name my-containerapp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az containerapp auth twitter update",
            "summary": "Update the consumer key and consumer secret for the Twitter identity provider.",
            "syntax": "az containerapp auth twitter update [--consumer-key]\n                                    [--consumer-secret]\n                                    [--consumer-secret-name]\n                                    [--ids]\n                                    [--name]\n                                    [--resource-group]\n                                    [--subscription]\n                                    [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--consumer-key",
                "--consumer-secret",
                "--consumer-secret-name --secret-name",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az containerapp auth twitter update  -g myResourceGroup --name my-containerapp \\\n  --consumer-key my-client-id --consumer-secret very_secret_password"
            ]
        }
    ]
}