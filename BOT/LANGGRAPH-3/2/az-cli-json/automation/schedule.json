{
    "command_group": "az automation schedule",
    "description": "Automation Schedule.",
    "status": "GA",
    "commands": [
        {
            "name": "az automation schedule create",
            "summary": "Create automation schedule.",
            "syntax": "az automation schedule create --automation-account-name\n                              --frequency\n                              --interval\n                              --name\n                              --resource-group\n                              [--description]\n                              [--expiry-time]\n                              [--start-time]\n                              [--time-zone]",
            "required_parameters": [
                "--automation-account-name",
                "--frequency",
                "--interval",
                "--name --schedule-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--description",
                "--expiry-time",
                "--start-time",
                "--time-zone"
            ],
            "examples": [
                "az automation schedule create --automation-account-name \"myAutomationAccount\"  --resource-group \"rg\"                -n mySchedule --frequency Hour --interval 1 --start-time 2022-08-10 18:00:00 --time-zone UTC+08:00"
            ]
        },
        {
            "name": "az automation schedule delete",
            "summary": "Delete an automation schedule.",
            "syntax": "az automation schedule delete --automation-account-name\n                              --name\n                              --resource-group\n                              [--yes]",
            "required_parameters": [
                "--automation-account-name",
                "--name --schedule-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az automation schedule delete --name \"myAutomationSchedule\" --resource-group \"rg\"                --automation-account-name \"myAutomationAccount\""
            ]
        },
        {
            "name": "az automation schedule list",
            "summary": "Retrieve a list of schedules.",
            "syntax": "az automation schedule list --automation-account-name\n                            --resource-group",
            "required_parameters": [
                "--automation-account-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az automation schedule list --automation-account-name \"myAutomationAccount\" --resource-group \"rg\""
            ]
        },
        {
            "name": "az automation schedule show",
            "summary": "Retrieve the schedule identified by schedule name.",
            "syntax": "az automation schedule show --automation-account-name\n                            --name\n                            --resource-group",
            "required_parameters": [
                "--automation-account-name",
                "--name --schedule-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az automation schedule show --automation-account-name \"myAutomationAccount\" --resource-group \"rg\"                --name \"mySchedule\""
            ]
        },
        {
            "name": "az automation schedule update",
            "summary": "Update an automation schedule.",
            "syntax": "az automation schedule update --automation-account-name\n                              --name\n                              --resource-group\n                              [--description]\n                              [--is-enabled {false, true}]",
            "required_parameters": [
                "--automation-account-name",
                "--name --schedule-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--description",
                "--is-enabled"
            ],
            "examples": [
                "az automation schedule update --automation-account-name \"myAutomationAccount\" --description test                -n mySchedule --resource-group \"rg\" --is-enabled false"
            ]
        }
    ]
}