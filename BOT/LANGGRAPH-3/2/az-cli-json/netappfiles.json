{
    "command_group": "az netappfiles",
    "description": "Manage Azure NetApp Files (ANF) Resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az netappfiles check-file-path-availability",
            "summary": "Check if a file path is available.",
            "syntax": "az netappfiles check-file-path-availability --name\n                                            --subnet-id\n                                            [--availability-zone]\n                                            [--ids]\n                                            [--location]\n                                            [--subscription]",
            "required_parameters": [
                "--name",
                "--subnet-id"
            ],
            "optional_parameters": [
                "--availability-zone",
                "--ids",
                "--location -l",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az netappfiles check-name-availability",
            "summary": "Check if a resource name is available.",
            "syntax": "az netappfiles check-name-availability --name\n                                       --resource-group\n                                       --type {Microsoft.NetApp/netAppAccounts, Microsoft.NetApp/netAppAccounts/capacityPools, Microsoft.NetApp/netAppAccounts/capacityPools/volumes, Microsoft.NetApp/netAppAccounts/capacityPools/volumes/snapshots}\n                                       [--ids]\n                                       [--location]\n                                       [--subscription]",
            "required_parameters": [
                "--name",
                "--resource-group -g",
                "--type"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az netappfiles check-quota-availability",
            "summary": "Check if a quota is available.",
            "syntax": "az netappfiles check-quota-availability --name\n                                        --resource-group\n                                        --type {Microsoft.NetApp/netAppAccounts, Microsoft.NetApp/netAppAccounts/capacityPools, Microsoft.NetApp/netAppAccounts/capacityPools/volumes, Microsoft.NetApp/netAppAccounts/capacityPools/volumes/snapshots}\n                                        [--ids]\n                                        [--location]\n                                        [--subscription]",
            "required_parameters": [
                "--name",
                "--resource-group -g",
                "--type"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az netappfiles query-network-sibling-set",
            "summary": "Describe a network sibling set.",
            "syntax": "az netappfiles query-network-sibling-set --network-sibling-set-id\n                                         --subnet-id\n                                         [--ids]\n                                         [--location]\n                                         [--subscription]",
            "required_parameters": [
                "--network-sibling-set-id",
                "--subnet-id"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--subscription"
            ],
            "examples": [
                "az netappfiles query-network-sibling-set -l westus2 --network-sibling-set-id {SIBLIING_SET_ID} --subnet-id {SUBNET_ID}"
            ]
        },
        {
            "name": "az netappfiles update-network-sibling-set",
            "summary": "Update the network features of a network sibling set.",
            "syntax": "az netappfiles update-network-sibling-set --network-sibling-set-id\n                                          --network-sibling-set-state-id\n                                          --subnet-id\n                                          [--ids]\n                                          [--location]\n                                          [--network-features {Basic, Standard}]\n                                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                          [--subscription]",
            "required_parameters": [
                "--network-sibling-set-id",
                "--network-sibling-set-state-id --state-id",
                "--subnet-id"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--network-features",
                "--no-wait",
                "--subscription"
            ],
            "examples": [
                "az -l westus2 --network-sibling-set-id {SIBLIING_SET_ID} --subnet-id {SUBNET_ID} --network-sibling-set-state-id {SIBLING_SET_STATE_ID} --network-features Standard"
            ]
        }
    ]
}