{
    "command_group": "az term",
    "description": "Manage marketplace agreement with marketplaceordering.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az term accept",
            "summary": "Accept marketplace terms.",
            "syntax": "az term accept --plan\n               --product\n               --publisher",
            "required_parameters": [
                "--plan",
                "--product",
                "--publisher"
            ],
            "optional_parameters": [],
            "examples": [
                "az term accept --product \"windows-data-science-vm\" --plan \"windows2016\" --publisher \"microsoft-ads\""
            ]
        },
        {
            "name": "az term show",
            "summary": "Get marketplace terms.",
            "syntax": "az term show --plan\n             --product\n             --publisher",
            "required_parameters": [
                "--plan",
                "--product",
                "--publisher"
            ],
            "optional_parameters": [],
            "examples": [
                "az term show --product \"windows-data-science-vm\" --plan \"windows2016\" --publisher \"microsoft-ads\""
            ]
        }
    ]
}