{
    "command_group": "az mysql db",
    "description": "Manage MySQL databases on a server.",
    "status": "GA",
    "commands": [
        {
            "name": "az mysql db create",
            "summary": "Create a MySQL database.",
            "syntax": "az mysql db create --name\n                   --resource-group\n                   --server-name\n                   [--charset]\n                   [--collation]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--charset",
                "--collation"
            ],
            "examples": [
                "az mysql db create -g testgroup -s testsvr -n testdb",
                "az mysql db create -g testgroup -s testsvr -n testdb --charset {valid_charset} --collation {valid_collation}"
            ]
        },
        {
            "name": "az mysql db delete",
            "summary": "Delete a database.",
            "syntax": "az mysql db delete --name\n                   [--ids]\n                   [--resource-group]\n                   [--server-name]\n                   [--subscription]\n                   [--yes]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az mysql db delete -g testgroup -s testsvr -n testdb"
            ]
        },
        {
            "name": "az mysql db list",
            "summary": "List the databases for a server.",
            "syntax": "az mysql db list --resource-group\n                 --server-name",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az mysql db list -g testgroup -s testsvr"
            ]
        },
        {
            "name": "az mysql db show",
            "summary": "Show the details of a database.",
            "syntax": "az mysql db show --name\n                 [--ids]\n                 [--resource-group]\n                 [--server-name]\n                 [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az mysql db show -g testgroup -s testsvr -n testdb"
            ]
        }
    ]
}