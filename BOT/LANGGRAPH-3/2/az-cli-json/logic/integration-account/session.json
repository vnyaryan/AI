{
    "command_group": "az logic integration-account session",
    "description": "Manage integration account session.",
    "status": "GA",
    "commands": [
        {
            "name": "az logic integration-account session create",
            "summary": "Create an integration account session.",
            "syntax": "az logic integration-account session create --integration-account-name\n                                            --name\n                                            --resource-group\n                                            [--content]\n                                            [--location]\n                                            [--tags]",
            "required_parameters": [
                "--integration-account-name",
                "--name --session-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--content",
                "--location -l",
                "--tags"
            ],
            "examples": [
                "az logic integration-account session create -g rg -n session --integration-account-name name --content \"{'controlNumber':1234}\""
            ]
        },
        {
            "name": "az logic integration-account session delete",
            "summary": "Delete an integration account session.",
            "syntax": "az logic integration-account session delete [--ids]\n                                            [--integration-account-name]\n                                            [--name]\n                                            [--resource-group]\n                                            [--subscription]\n                                            [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--integration-account-name",
                "--name --session-name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az logic integration-account session delete -g rg -n session --integration-account-name name"
            ]
        },
        {
            "name": "az logic integration-account session list",
            "summary": "List a list of integration account sessions.",
            "syntax": "az logic integration-account session list --integration-account-name\n                                          --resource-group\n                                          [--filter]\n                                          [--max-items]\n                                          [--next-token]\n                                          [--top]",
            "required_parameters": [
                "--integration-account-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token",
                "--top"
            ],
            "examples": [
                "az logic integration-account session list -g rg --integration-account-name name"
            ]
        },
        {
            "name": "az logic integration-account session show",
            "summary": "Show an integration account session.",
            "syntax": "az logic integration-account session show [--ids]\n                                          [--integration-account-name]\n                                          [--name]\n                                          [--resource-group]\n                                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--integration-account-name",
                "--name --session-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az logic integration-account session show -g rg -n session --integration-account-name name"
            ]
        },
        {
            "name": "az logic integration-account session update",
            "summary": "Update an integration account session.",
            "syntax": "az logic integration-account session update [--add]\n                                            [--content]\n                                            [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                            [--ids]\n                                            [--integration-account-name]\n                                            [--location]\n                                            [--name]\n                                            [--remove]\n                                            [--resource-group]\n                                            [--set]\n                                            [--subscription]\n                                            [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--content",
                "--force-string",
                "--ids",
                "--integration-account-name",
                "--location -l",
                "--name --session-name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az logic integration-account session update -g rg -n session --integration-account-name name --content \"{'controlNumber':12345}\""
            ]
        }
    ]
}