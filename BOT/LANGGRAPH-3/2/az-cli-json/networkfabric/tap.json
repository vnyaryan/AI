{
    "command_group": "az networkfabric tap",
    "description": "Manage Network Tap Resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az networkfabric tap create",
            "summary": "Create a Network Tap resource.",
            "syntax": "az networkfabric tap create --destinations\n                            --network-packet-broker-id\n                            --resource-group\n                            --resource-name\n                            [--annotation]\n                            [--location]\n                            [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                            [--polling-type {Pull, Push}]\n                            [--tags]",
            "required_parameters": [
                "--destinations",
                "--network-packet-broker-id --npb-id",
                "--resource-group -g",
                "--resource-name"
            ],
            "optional_parameters": [
                "--annotation",
                "--location -l",
                "--no-wait",
                "--polling-type",
                "--tags"
            ],
            "examples": [
                "az networkfabric tap create --resource-group \"example-rg\" --location \"westus3\" --resource-name \"example-networktap\" --network-packet-broker-id \"/subscriptions/xxxxx-xxxx-xxxx-xxxx-xxxxx/resourcegroups/example-rg/providers/Microsoft.ManagedNetworkFabric/networkPacketBrokers/example-networkPacketBroker\" --polling-type \"Pull\" --destinations \"[{name:'example-destinaionName',destinationType:IsolationDomain,destinationId:'/subscriptions/xxxxx-xxxx-xxxx-xxxx-xxxxx/resourcegroups/example-rg/providers/Microsoft.ManagedNetworkFabric/l3IsloationDomains/example-l3Domain/internalNetworks/example-internalNetwork',isolationDomainProperties:{encapsulation:None,neighborGroupIds:['/subscriptions/xxxxx-xxxx-xxxx-xxxx-xxxxx/resourcegroups/example-rg/providers/Microsoft.ManagedNetworkFabric/neighborGroups/example-neighborGroup']},destinationTapRuleId:'/subscriptions/xxxxx-xxxx-xxxx-xxxx-xxxxx/resourcegroups/example-rg/providers/Microsoft.ManagedNetworkFabric/networkTapRules/example-destinationTapRule'}]\"",
                "az networkfabric tap create --destinations \"??\""
            ]
        },
        {
            "name": "az networkfabric tap delete",
            "summary": "Delete the Network Tap resource.",
            "syntax": "az networkfabric tap delete [--ids]\n                            [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                            [--resource-group]\n                            [--resource-name]\n                            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--resource-name",
                "--subscription"
            ],
            "examples": [
                "az networkfabric tap delete --resource-group \"example-rg\" --resource-name \"example-networktap\""
            ]
        },
        {
            "name": "az networkfabric tap list",
            "summary": "List all Network Taps in the provided resource group or subscription.",
            "syntax": "az networkfabric tap list [--max-items]\n                          [--next-token]\n                          [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az networkfabric tap list --resource-group \"example-rg\"",
                "az networkfabric tap list --subscription \"<subscriptionId>\""
            ]
        },
        {
            "name": "az networkfabric tap resync",
            "summary": "Implements the operation to the underlying resources.",
            "syntax": "az networkfabric tap resync [--ids]\n                            [--network-tap-name]\n                            [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                            [--resource-group]\n                            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--network-tap-name",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az networkfabric tap resync --resource-group \"example-rg\" --network-tap-name \"example-networktap\""
            ]
        },
        {
            "name": "az networkfabric tap show",
