{
    "command_group": "az healthcareapis workspace",
    "description": "Manage workspace with healthcareapis.",
    "status": "GA",
    "commands": [
        {
            "name": "az healthcareapis workspace create",
            "summary": "Create a workspace resource with the specified parameters.",
            "syntax": "az healthcareapis workspace create --name\n                                   --resource-group\n                                   [--etag]\n                                   [--location]\n                                   [--no-wait]\n                                   [--public-network-access {Disabled, Enabled}]\n                                   [--tags]",
            "required_parameters": [
                "--name --workspace-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--etag",
                "--location -l",
                "--no-wait",
                "--public-network-access",
                "--tags"
            ],
            "examples": [
                "az healthcareapis workspace create --resource-group \"testRG\" --location \"westus\" --name \"workspace1\""
            ]
        },
        {
            "name": "az healthcareapis workspace delete",
            "summary": "Deletes a specified workspace.",
            "syntax": "az healthcareapis workspace delete [--ids]\n                                   [--name]\n                                   [--no-wait]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --workspace-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az healthcareapis workspace delete --resource-group \"testRG\" --name \"workspace1\""
            ]
        },
        {
            "name": "az healthcareapis workspace list",
            "summary": "Lists all the available workspaces under the specified resource group. And Lists all the available workspaces under the specified subscription.",
            "syntax": "az healthcareapis workspace list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az healthcareapis workspace list --resource-group \"testRG\"",
                "az healthcareapis workspace list"
            ]
        },
        {
            "name": "az healthcareapis workspace show",
            "summary": "Gets the properties of the specified workspace.",
            "syntax": "az healthcareapis workspace show [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --workspace-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az healthcareapis workspace show --resource-group \"testRG\" --name \"workspace1\""
            ]
        },
        {
            "name": "az healthcareapis workspace update",
            "summary": "Patch workspace details.",
            "syntax": "az healthcareapis workspace update [--ids]\n                                   [--name]\n                                   [--no-wait]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --workspace-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az healthcareapis workspace update --resource-group \"testRG\" --name \"workspace1\" --tags tagKey=\"tagValue\""
            ]
        },
        {
            "name": "az healthcareapis workspace wait",
            "summary": "Place the CLI in a waiting state until a condition of the healthcareapis workspace is met.",
            "syntax": "az healthcareapis workspace wait [--created]\n                                 [--custom]\n                                 [--deleted]\n                                 [--exists]\n                                 [--ids]\n                                 [--interval]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]\n                                 [--timeout]\n                                 [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name --workspace-name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az healthcareapis workspace wait --resource-group \"testRG\" --name \"workspace1\" --created",
                "az healthcareapis workspace wait --resource-group \"testRG\" --name \"workspace1\" --updated",
                "az healthcareapis workspace wait --resource-group \"testRG\" --name \"workspace1\" --deleted"
            ]
        }
    ]
}