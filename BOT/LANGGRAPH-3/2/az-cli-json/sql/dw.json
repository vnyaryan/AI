{
    "command_group": "az sql dw",
    "description": "Manage data warehouses.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql dw create",
            "summary": "Create a data warehouse.",
            "syntax": "az sql dw create --name\n                 --resource-group\n                 --server\n                 [--availability-zone]\n                 [--backup-storage-redundancy]\n                 [--collation]\n                 [--max-size]\n                 [--no-wait]\n                 [--service-level-objective]\n                 [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server -s"
            ],
            "optional_parameters": [
                "--availability-zone",
                "--backup-storage-redundancy --bsr",
                "--collation",
                "--max-size",
                "--no-wait",
                "--service-level-objective --service-objective",
                "--tags"
            ],
            "examples": [
                "az sql dw create --name MyDataWarehouse --resource-group MyResourceGroup --server myserver --service-objective S0"
            ]
        },
        {
            "name": "az sql dw delete",
            "summary": "Delete a data warehouse.",
            "syntax": "az sql dw delete [--ids]\n                 [--name]\n                 [--no-wait]\n                 [--resource-group]\n                 [--server]\n                 [--subscription]\n                 [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--server -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az sql dw delete --name MyDataWarehouse --resource-group MyResourceGroup --server myserver"
            ]
        },
        {
            "name": "az sql dw list",
            "summary": "List data warehouses for a server.",
            "syntax": "az sql dw list [--ids]\n               [--resource-group]\n               [--server]\n               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql dw list --resource-group MyResourceGroup --server myserver"
            ]
        },
        {
            "name": "az sql dw pause",
            "summary": "Pauses a datawarehouse.",
            "syntax": "az sql dw pause [--ids]\n                [--name]\n                [--resource-group]\n                [--server]\n                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az sql dw resume",
            "summary": "Resumes a datawarehouse.",
            "syntax": "az sql dw resume [--ids]\n                 [--name]\n                 [--resource-group]\n                 [--server]\n                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az sql dw show",
            "summary": "Get the details for a data warehouse.",
            "syntax": "az sql dw show [--filter]\n               [--ids]\n               [--name]\n               [--resource-group]\n               [--server]\n               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql dw show --name MyDataWarehouse --resource-group MyResourceGroup --server myserver"
            ]
        },
        {
            "name": "az sql dw update",
            "summary": "Update a data warehouse.",
            "syntax": "az sql dw update [--add]\n                 [--filter]\n                 [--force-string]\n                 [--ids]\n                 [--max-size]\n                 [--name]\n                 [--no-wait]\n                 [--remove]\n                 [--resource-group]\n                 [--server]\n                 [--service-objective]\n                 [--set]\n                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--filter",
                "--force-string",
                "--ids",
                "--max-size",
                "--name -n",
                "--no-wait",
                "--remove",
                "--resource-group -g",
                "--server -s",
                "--service-objective",
                "--set",
                "--subscription"
            ],
            "examples": [
                "az sql dw update --name MyDataWarehouse --resource-group MyResourceGroup --server myserver --service-objective S1"
            ]
        }
    ]
}