{
    "command_group": "az sql midb ltr-backup",
    "description": "Manage SQL Managed Instance database long term retention backups.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql midb ltr-backup delete",
            "summary": "Delete a long term retention backup.",
            "syntax": "az sql midb ltr-backup delete [--backup-id]\n                              [--database]\n                              [--location]\n                              [--managed-instance]\n                              [--name]\n                              [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--backup-id --id",
                "--database -d",
                "--location -l",
                "--managed-instance --mi",
                "--name -n",
                "--yes -y"
            ],
            "examples": [
                "az sql midb ltr-backup delete -l southeastasia --mi myinstance -d mymanageddb --name \"3214b3fb-fba9-43e7-96a3-09e35ffcb336;132292152080000000\"",
                "az sql midb ltr-backup delete --backup-id '/subscriptions/6caa113c-794c-42f8-ab9d-878d8aa104dc/resourceGroups/mygroup/providers/Microsoft.Sql/locations/southeastasia/longTermRetentionManagedInstances/myinstance/longTermRetentionDatabases/mymanageddb/longTermRetentionManagedInstanceBackups/3214b3fb-fba9-43e7-96a3-09e35ffcb336;132292152080000000'"
            ]
        },
        {
            "name": "az sql midb ltr-backup list",
            "summary": "List the long term retention backups for a location, instance or database.",
            "syntax": "az sql midb ltr-backup list --location\n                            [--database]\n                            [--database-state]\n                            [--latest]\n                            [--managed-instance]\n                            [--resource-group]",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [
                "--database -d",
                "--database-state --state",
                "--latest --only-latest-per-database",
                "--managed-instance --mi",
                "--resource-group -g"
            ],
            "examples": [
                "az sql midb ltr-backup list -l southeastasia --mi myinstance -d mymanageddb",
                "az sql midb ltr-backup list -l southeastasia --mi myinstance --database-state Live --only-latest-per-database",
                "az sql midb ltr-backup list -l southeastasia -g mygroup --mi myinstance",
                "az sql midb ltr-backup list -l southeastasia --database-state Live --only-latest-per-database",
                "az sql midb ltr-backup list -l southeastasia -g mygroup"
            ]
        },
        {
            "name": "az sql midb ltr-backup restore",
            "summary": "Restore a long term retention backup to a new database.",
            "syntax": "az sql midb ltr-backup restore --backup-id\n                               --dest-database\n                               --dest-mi\n                               --dest-resource-group\n                               [--no-wait]",
            "required_parameters": [
                "--backup-id --id",
                "--dest-database",
                "--dest-mi",
                "--dest-resource-group"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az sql midb ltr-backup restore \\\n    --dest-database targetmidb --dest-mi myinstance --dest-resource-group mygroup \\\n    --backup-id \"/subscriptions/6caa113c-794c-42f8-ab9d-878d8aa104dc/resourceGroups/mygroup/providers/Microsoft.Sql/locations/southeastasia/longTermRetentionManagedInstances/myinstance/longTermRetentionDatabases/sourcemidb/longTermRetentionManagedInstanceBackups/3214b3fb-fba9-43e7-96a3-09e35ffcb336;132292152080000000\""
            ]
        },
        {
            "name": "az sql midb ltr-backup show",
            "summary": "Get a long term retention backup for a managed database.",
            "syntax": "az sql midb ltr-backup show [--backup-id]\n                            [--database]\n                            [--location]\n                            [--managed-instance]\n                            [--name]",
            "required_parameters": [],
            "optional_parameters": [
                "--backup-id --id",
                "--database -d",
                "--location -l",
                "--managed-instance --mi",
                "--name -n"
            ],
            "examples": [
                "az sql midb ltr-backup show --backup-id '/subscriptions/6caa113c-794c-42f8-ab9d-878d8aa104dc/resourceGroups/mygroup/providers/Microsoft.Sql/locations/southeastasia/longTermRetentionManagedInstances/myinstance/longTermRetentionDatabases/mymanageddb/longTermRetentionManagedInstanceBackups/3214b3fb-fba9-43e7-96a3-09e35ffcb336;132292152080000000'"
            ]
        },
        {
            "name": "az sql midb ltr-backup wait",
            "summary": "Place the CLI in a waiting state until a condition of the managed database is met.",
            "syntax": "az sql midb ltr-backup wait --database\n                            --managed-instance\n                            --resource-group\n                            [--created]\n                            [--custom]\n                            [--deleted]\n                            [--exists]\n                            [--interval]\n                            [--timeout]\n                            [--updated]",
            "required_parameters": [
                "--database -d",
                "--managed-instance --mi",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}