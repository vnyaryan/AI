{
    "command_group": "az sql mi op",
    "description": "Manage operations on a managed instance.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql mi op cancel",
            "summary": "Cancels the asynchronous operation on the managed instance.",
            "syntax": "az sql mi op cancel --name\n                    [--ids]\n                    [--managed-instance]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--managed-instance --mi",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az sql mi op cancel -g mygroup --mi myManagedInstance -n d2896mydb-2ba8-4c84-bac1-387c430cce40"
            ]
        },
        {
            "name": "az sql mi op list",
            "summary": "Gets a list of operations performed on the managed instance.",
            "syntax": "az sql mi op list [--ids]\n                  [--managed-instance]\n                  [--resource-group]\n                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--managed-instance --mi",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az sql mi op show",
            "summary": "Gets a management operation on a managed instance.",
            "syntax": "az sql mi op show --name\n                  [--ids]\n                  [--managed-instance]\n                  [--resource-group]\n                  [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--managed-instance --mi",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        }
    ]
}