{
    "command_group": "az postgres server firewall-rule",
    "description": "Manage firewall rules for a server.",
    "status": "GA",
    "commands": [
        {
            "name": "az postgres server firewall-rule create",
            "summary": "Create a new firewall rule for a server.",
            "syntax": "az postgres server firewall-rule create --end-ip-address\n                                        --name\n                                        --resource-group\n                                        --server-name\n                                        --start-ip-address",
            "required_parameters": [
                "--end-ip-address",
                "--name -n",
                "--resource-group -g",
                "--server-name -s",
                "--start-ip-address"
            ],
            "optional_parameters": [],
            "examples": [
                "az postgres server firewall-rule create -g testgroup -s testsvr -n allowip --start-ip-address 107.46.14.221 --end-ip-address 107.46.14.221",
                "az postgres server firewall-rule create -g testgroup -s testsvr -n allowiprange --start-ip-address 107.46.14.0 --end-ip-address 107.46.14.221"
            ]
        },
        {
            "name": "az postgres server firewall-rule delete",
            "summary": "Delete a firewall rule.",
            "syntax": "az postgres server firewall-rule delete [--ids]\n                                        [--name]\n                                        [--resource-group]\n                                        [--server-name]\n                                        [--subscription]\n                                        [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server-name -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az postgres server firewall-rule delete --name MyFirewallRule --resource-group MyResourceGroup --server-name MyServer"
            ]
        },
        {
            "name": "az postgres server firewall-rule list",
            "summary": "List all firewall rules for a server.",
            "syntax": "az postgres server firewall-rule list [--ids]\n                                      [--resource-group]\n                                      [--server-name]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az postgres server firewall-rule list --resource-group MyResourceGroup --server-name MyServer"
            ]
        },
        {
            "name": "az postgres server firewall-rule show",
            "summary": "Get the details of a firewall rule.",
            "syntax": "az postgres server firewall-rule show [--ids]\n                                      [--name]\n                                      [--resource-group]\n                                      [--server-name]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az postgres server firewall-rule show --name MyFirewallRule --resource-group MyResourceGroup --server-name MyServer"
            ]
        },
        {
            "name": "az postgres server firewall-rule update",
            "summary": "Update a firewall rule.",
            "syntax": "az postgres server firewall-rule update [--add]\n                                        [--end-ip-address]\n                                        [--force-string]\n                                        [--ids]\n                                        [--name]\n                                        [--remove]\n                                        [--resource-group]\n                                        [--server-name]\n                                        [--set]\n                                        [--start-ip-address]\n                                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--end-ip-address",
                "--force-string",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--server-name -s",
                "--set",
                "--start-ip-address",
                "--subscription"
            ],
            "examples": [
                "az postgres server firewall-rule update -g testgroup -s testsvr -n allowiprange --start-ip-address 107.46.14.1",
                "az postgres server firewall-rule update -g testgroup -s testsvr -n allowiprange --start-ip-address 107.46.14.2 --end-ip-address 107.46.14.218"
            ]
        }
    ]
}