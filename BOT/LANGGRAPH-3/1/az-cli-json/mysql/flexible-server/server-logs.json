{
    "command_group": "az mysql flexible-server server-logs",
    "description": "Manage server logs.",
    "status": "GA",
    "commands": [
        {
            "name": "az mysql flexible-server server-logs download",
            "summary": "Download log files.",
            "syntax": "az mysql flexible-server server-logs download --name\n                                              --resource-group\n                                              --server-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az mysql flexible-server server-logs download -g testgroup -s testsvr -n f1.log f2.log"
            ]
        },
        {
            "name": "az mysql flexible-server server-logs list",
            "summary": "List log files for a server.",
            "syntax": "az mysql flexible-server server-logs list --resource-group\n                                          --server-name\n                                          [--file-last-written]\n                                          [--filename-contains]\n                                          [--max-file-size]",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--file-last-written",
                "--filename-contains",
                "--max-file-size"
            ],
            "examples": [
                "az mysql flexible-server server-logs list -g testgroup -s testsvr",
                "az mysql flexible-server server-logs list -g testgroup -s testsvr --file-last-written 10",
                "az mysql flexible-server server-logs list -g testgroup -s testsvr --max-file-size 30"
            ]
        }
    ]
}