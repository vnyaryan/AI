{
    "command_group": "az sf managed-application-type",
    "description": "Manage application types on an Azure Service Fabric cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az sf managed-application-type create",
            "summary": "Create a new managed application type on an Azure Service Fabric managed cluster.",
            "syntax": "az sf managed-application-type create --application-type-name\n                                      --cluster-name\n                                      --resource-group\n                                      [--tags]",
            "required_parameters": [
                "--application-type-name --name",
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--tags"
            ],
            "examples": [
                "az sf managed-application-type create -g testRG -c testCluster --application-type-name testAppType"
            ]
        },
        {
            "name": "az sf managed-application-type delete",
            "summary": "Delete a managed application type.",
            "syntax": "az sf managed-application-type delete --application-type-name\n                                      --cluster-name\n                                      --resource-group",
            "required_parameters": [
                "--application-type-name --name",
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sf managed-application-type delete -g testRG -c testCluster --application-type-name CalcServiceApp"
            ]
        },
        {
            "name": "az sf managed-application-type list",
            "summary": "List managed application types of a given managed cluster.",
            "syntax": "az sf managed-application-type list --cluster-name\n                                    --resource-group",
            "required_parameters": [
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sf managed-application-type list -g testRG -c testCluster"
            ]
        },
        {
            "name": "az sf managed-application-type show",
            "summary": "Show the properties of a managed application type on an Azure Service Fabric managed cluster.",
            "syntax": "az sf managed-application-type show --application-type-name\n                                    --cluster-name\n                                    --resource-group",
            "required_parameters": [
                "--application-type-name --name",
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sf managed-application-type show -g testRG -c testCluster --application-type-name CalcServiceApp"
            ]
        },
        {
            "name": "az sf managed-application-type update",
            "summary": "Update an managed application type.",
            "syntax": "az sf managed-application-type update --application-type-name\n                                      --cluster-name\n                                      --resource-group\n                                      [--tags]",
            "required_parameters": [
                "--application-type-name --name",
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--tags"
            ],
            "examples": [
                "az sf managed-application-type update -g testRG -c testCluster --application-type-name CalcServiceApp --tags new=tags are=nice"
            ]
        }
    ]
}