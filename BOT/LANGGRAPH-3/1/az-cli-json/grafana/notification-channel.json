{
    "command_group": "az grafana notification-channel",
    "description": "Commands to manage notification channels of an instance.",
    "status": "GA",
    "commands": [
        {
            "name": "az grafana notification-channel create",
            "summary": "Create a notification channel.",
            "syntax": "az grafana notification-channel create --definition\n                                       --name\n                                       [--api-key]\n                                       [--resource-group]",
            "required_parameters": [
                "--definition",
                "--name -n"
            ],
            "optional_parameters": [
                "--api-key --token -t",
                "--resource-group -g"
            ],
            "examples": [
                "az grafana notification-channel create -n MyGrafana --definition '{\n  \"name\": \"Teams\",\n  \"settings\": {\n    \"uploadImage\": true,\n    \"url\": \"https://webhook.office.com/IncomingWebhook/\"\n   },\n  \"type\": \"teams\"\n}'"
            ]
        },
        {
            "name": "az grafana notification-channel delete",
            "summary": "Delete a notification channel.",
            "syntax": "az grafana notification-channel delete --name\n                                       --notification-channel\n                                       [--api-key]\n                                       [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--notification-channel"
            ],
            "optional_parameters": [
                "--api-key --token -t",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az grafana notification-channel list",
            "summary": "List all notification channels of an instance.",
