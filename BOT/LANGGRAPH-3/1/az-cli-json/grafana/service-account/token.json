{
    "command_group": "az grafana service-account token",
    "description": "Commands to manage service account tokens.",
    "status": "GA",
    "commands": [
        {
            "name": "az grafana service-account token create",
            "summary": "Create a new service account token.",
            "syntax": "az grafana service-account token create --name\n                                        --service-account\n                                        --token\n                                        [--resource-group]\n                                        [--time-to-live]",
            "required_parameters": [
                "--name -n",
                "--service-account",
                "--token"
            ],
            "optional_parameters": [
                "--resource-group -g",
                "--time-to-live"
            ],
            "examples": [
                "az grafana service-account token create -g myResourceGroup -n myGrafana --service-account myAccount --token myToken --time-to-live 1d"
            ]
        },
        {
            "name": "az grafana service-account token delete",
            "summary": "Delete a service account token.",
            "syntax": "az grafana service-account token delete --name\n                                        --service-account\n                                        --token\n                                        [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--service-account",
                "--token"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az grafana service-account token list",
            "summary": "List existing service account tokens.",
            "syntax": "az grafana service-account token list --name\n                                      --service-account\n                                      [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--service-account"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": []
        }
    ]
}