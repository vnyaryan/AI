{
    "command_group": "az webapp auth github",
    "description": "Manage webapp authentication and authorization of the GitHub identity provider.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp auth github show",
            "summary": "Show the authentication settings for the GitHub identity provider.",
            "syntax": "az webapp auth github show [--ids]\n                           [--name]\n                           [--resource-group]\n                           [--slot]\n                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth github show --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp auth github update",
            "summary": "Update the client id and client secret for the GitHub identity provider.",
            "syntax": "az webapp auth github update [--client-id]\n                             [--client-secret]\n                             [--client-secret-setting-name]\n                             [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--scopes]\n                             [--slot]\n                             [--subscription]\n                             [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--client-id",
                "--client-secret",
                "--client-secret-setting-name --secret-setting",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--scopes",
                "--slot -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth github update  -g myResourceGroup --name MyWebApp \\\n  --client-id my-client-id --client-secret very_secret_password"
            ]
        }
    ]
}