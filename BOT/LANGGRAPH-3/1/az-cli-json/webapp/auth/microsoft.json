{
    "command_group": "az webapp auth microsoft",
    "description": "Manage webapp authentication and authorization of the Microsoft identity provider.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp auth microsoft show",
            "summary": "Show the authentication settings for the Azure Active Directory identity provider.",
            "syntax": "az webapp auth microsoft show [--ids]\n                              [--name]\n                              [--resource-group]\n                              [--slot]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth microsoft show --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp auth microsoft update",
            "summary": "Update the client id and client secret for the Azure Active Directory identity provider.",
            "syntax": "az webapp auth microsoft update [--allowed-audiences]\n                                [--certificate-issuer]\n                                [--client-id]\n                                [--client-secret]\n                                [--client-secret-certificate-san]\n                                [--client-secret-certificate-thumbprint]\n                                [--client-secret-setting-name]\n                                [--ids]\n                                [--issuer]\n                                [--name]\n                                [--resource-group]\n                                [--slot]\n                                [--subscription]\n                                [--tenant-id]\n                                [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--allowed-audiences --allowed-token-audiences",
                "--certificate-issuer --client-secret-certificate-issuer",
                "--client-id",
                "--client-secret",
                "--client-secret-certificate-san --san",
                "--client-secret-certificate-thumbprint --thumbprint",
                "--client-secret-setting-name --secret-setting",
                "--ids",
                "--issuer",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription",
                "--tenant-id",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth microsoft update  -g myResourceGroup --name MyWebApp \\\n  --client-id my-client-id --client-secret very_secret_password \\\n  --issuer https://sts.windows.net/54826b22-38d6-4fb2-bad9-b7983a3e9c5a/"
            ]
        }
    ]
}