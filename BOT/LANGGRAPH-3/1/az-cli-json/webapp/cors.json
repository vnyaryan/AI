{
    "command_group": "az webapp cors",
    "description": "Manage Cross-Origin Resource Sharing (CORS).",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp cors add",
            "summary": "Add allowed origins.",
            "syntax": "az webapp cors add --allowed-origins\n                   [--ids]\n                   [--name]\n                   [--resource-group]\n                   [--slot]\n                   [--subscription]",
            "required_parameters": [
                "--allowed-origins -a"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp cors add -g {myRG} -n {myAppName} --allowed-origins https://myapps.com",
                "az webapp cors add --allowed-origins https://myapps.com --name MyWebApp --resource-group MyResourceGroup --subscription MySubscription"
            ]
        },
        {
            "name": "az webapp cors remove",
            "summary": "Remove allowed origins.",
            "syntax": "az webapp cors remove --allowed-origins\n                      [--ids]\n                      [--name]\n                      [--resource-group]\n                      [--slot]\n                      [--subscription]",
            "required_parameters": [
                "--allowed-origins -a"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp cors remove -g {myRG} -n {myAppName} --allowed-origins https://myapps.com",
                "az webapp cors remove -g {myRG} -n {myAppName} --allowed-origins"
            ]
        },
        {
            "name": "az webapp cors show",
            "summary": "Show allowed origins.",
            "syntax": "az webapp cors show [--ids]\n                    [--name]\n                    [--resource-group]\n                    [--slot]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp cors show --name MyWebApp --resource-group MyResourceGroup"
            ]
        }
    ]
}