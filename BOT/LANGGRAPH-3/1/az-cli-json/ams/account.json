{
    "command_group": "az ams account",
    "description": "Manage Azure Media Services accounts.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams account check-name",
            "summary": "Checks whether the Media Service resource name is available.",
            "syntax": "az ams account check-name --name\n                          [--location]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--location -l"
            ],
            "examples": []
        },
        {
            "name": "az ams account create",
            "summary": "Create an Azure Media Services account.",
            "syntax": "az ams account create --name\n                      --resource-group\n                      --storage-account\n                      [--default-action]\n                      [--disable-public-network]\n                      [--ip-allow-list]\n                      [--location]\n                      [--mi-system-assigned {false, true}]\n                      [--mi-user-assigned]\n                      [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--storage-account"
            ],
            "optional_parameters": [
                "--default-action",
                "--disable-public-network",
                "--ip-allow-list",
                "--location -l",
                "--mi-system-assigned",
                "--mi-user-assigned",
                "--tags"
            ],
            "examples": []
        },
        {
            "name": "az ams account delete",
            "summary": "Delete an Azure Media Services account.",
            "syntax": "az ams account delete [--ids]\n                      [--name]\n                      [--resource-group]\n                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams account list",
            "summary": "List Azure Media Services accounts for the entire subscription.",
            "syntax": "az ams account list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az ams account show",
            "summary": "Show the details of an Azure Media Services account.",
            "syntax": "az ams account show [--ids]\n                    [--name]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams account update",
            "summary": "Update the details of an Azure Media Services account.",
            "syntax": "az ams account update [--add]\n                      [--force-string]\n                      [--ids]\n                      [--name]\n                      [--remove]\n                      [--resource-group]\n                      [--set]\n                      [--subscription]\n                      [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": []
        }
    ]
}