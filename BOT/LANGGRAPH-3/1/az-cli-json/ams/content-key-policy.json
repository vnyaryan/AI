{
    "command_group": "az ams content-key-policy",
    "description": "Manage content key policies for an Azure Media Services account.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams content-key-policy create",
            "summary": "Create a new content key policy.",
            "syntax": "az ams content-key-policy create --account-name\n                                 --name\n                                 --policy-option-name\n                                 --resource-group\n                                 [--alt-rsa-token-keys]\n                                 [--alt-symmetric-token-keys]\n                                 [--alt-x509-token-keys]\n                                 [--ask]\n                                 [--audience]\n                                 [--clear-key-configuration]\n                                 [--description]\n                                 [--fair-play-pfx]\n                                 [--fair-play-pfx-password]\n                                 [--fp-playback-duration-seconds]\n                                 [--fp-storage-duration-seconds]\n                                 [--issuer]\n                                 [--open-id-connect-discovery-document]\n                                 [--open-restriction]\n                                 [--play-ready-template]\n                                 [--rental-and-lease-key-type]\n                                 [--rental-duration]\n                                 [--token-claims]\n                                 [--token-key]\n                                 [--token-key-type]\n                                 [--token-type]\n                                 [--widevine-template]",
            "required_parameters": [
                "--account-name -a",
                "--name -n",
                "--policy-option-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--alt-rsa-token-keys",
                "--alt-symmetric-token-keys",
                "--alt-x509-token-keys",
                "--ask",
                "--audience",
                "--clear-key-configuration",
                "--description",
                "--fair-play-pfx",
                "--fair-play-pfx-password",
                "--fp-playback-duration-seconds",
                "--fp-storage-duration-seconds",
                "--issuer",
                "--open-id-connect-discovery-document",
                "--open-restriction",
                "--play-ready-template",
                "--rental-and-lease-key-type",
                "--rental-duration",
                "--token-claims",
                "--token-key",
                "--token-key-type",
                "--token-type",
                "--widevine-template"
            ],
            "examples": [
                "az ams content-key-policy create -a amsAccount -g resourceGroup -n contentKeyPolicyName --policy-option-name policyOptionName --open-restriction --ask \"ask-32-chars-hex-string\" --fair-play-pfx pfxPath --fair-play-pfx-password \"pfxPassword\" --rental-and-lease-key-type PersistentUnlimited --rental-duration 5000"
            ]
        },
        {
            "name": "az ams content-key-policy delete",
            "summary": "Delete a content key policy.",
            "syntax": "az ams content-key-policy delete [--account-name]\n                                 [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams content-key-policy list",
            "summary": "List all the content key policies within an Azure Media Services account.",
            "syntax": "az ams content-key-policy list --account-name\n                               --resource-group\n                               [--filter]\n                               [--orderby]\n                               [--top]",
            "required_parameters": [
                "--account-name -a",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--filter",
                "--orderby",
                "--top"
            ],
            "examples": [
                "az ams content-key-policy list -a amsAccount -g resourceGroup --filter \"properties/lastModified gt 2022-08-16 or properties/created lt 2022-08-17\""
            ]
        },
        {
            "name": "az ams content-key-policy show",
            "summary": "Show an existing content key policy.",
            "syntax": "az ams content-key-policy show [--account-name]\n                               [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]\n                               [--with-secrets]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--with-secrets"
            ],
            "examples": []
        },
        {
            "name": "az ams content-key-policy update",
            "summary": "Update an existing content key policy.",
            "syntax": "az ams content-key-policy update [--account-name]\n                                 [--add]\n                                 [--description]\n                                 [--force-string]\n                                 [--ids]\n                                 [--name]\n                                 [--remove]\n                                 [--resource-group]\n                                 [--set]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--add",
                "--description",
                "--force-string",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription"
            ],
            "examples": [
                "az ams content-key-policy update -n contentKeyPolicyName -a amsAccount --description newDescription --set options[0].restriction.issuer=newIssuer --set options[0].restriction.audience=newAudience"
            ]
        }
    ]
}