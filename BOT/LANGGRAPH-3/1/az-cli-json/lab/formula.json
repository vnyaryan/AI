{
    "command_group": "az lab formula",
    "description": "Manage formulas for an azure devtest lab.",
    "status": "Preview",
    "commands": [
        {
            "name": "az lab formula delete",
            "summary": "Delete formula.",
            "syntax": "az lab formula delete --lab-name\n                      --name\n                      --resource-group\n                      [--yes]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az lab formula export-artifacts",
            "summary": "Export artifacts from a formula.",
            "syntax": "az lab formula export-artifacts --lab-name\n                                --name\n                                --resource-group\n                                [--expand]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": []
        },
        {
            "name": "az lab formula list",
            "summary": "List formulas in a given lab.",
            "syntax": "az lab formula list --lab-name\n                    --resource-group\n                    [--expand]\n                    [--filter]\n                    [--max-items]\n                    [--next-token]\n                    [--orderby]\n                    [--top]",
            "required_parameters": [
                "--lab-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand",
                "--filter",
                "--max-items",
                "--next-token",
                "--orderby",
                "--top"
            ],
            "examples": []
        },
        {
            "name": "az lab formula show",
            "summary": "Show formula.",
            "syntax": "az lab formula show --lab-name\n                    --name\n                    --resource-group\n                    [--expand]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": []
        }
    ]
}