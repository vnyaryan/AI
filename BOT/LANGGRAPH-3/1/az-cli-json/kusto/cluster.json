{
    "command_group": "az kusto cluster",
    "description": "Manage cluster with kusto.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az kusto cluster add-language-extension",
            "summary": "Add a list of language extensions that can run within KQL queries.",
            "syntax": "az kusto cluster add-language-extension [--cluster-name]\n                                        [--ids]\n                                        [--no-wait]\n                                        [--resource-group]\n                                        [--subscription]\n                                        [--value]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name --name -n",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--value"
            ],
            "examples": [
                "az kusto cluster add-language-extension --name \"kustoclusterrptest4\" --value language-extension-name=\"PYTHON\" --value language-extension-name=\"R\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster create",
            "summary": "Create a Kusto cluster.",
            "syntax": "az kusto cluster create --cluster-name\n                        --resource-group\n                        --sku\n                        [--accepted-audiences]\n                        [--allowed-fqdn-list]\n                        [--allowed-ip-range-list]\n                        [--enable-auto-stop {false, true}]\n                        [--enable-disk-encryption {false, true}]\n                        [--enable-double-encryption {false, true}]\n                        [--enable-purge {false, true}]\n                        [--enable-streaming-ingest {false, true}]\n                        [--engine-type {V2, V3}]\n                        [--if-match]\n                        [--if-none-match]\n                        [--key-vault-properties]\n                        [--location]\n                        [--no-wait]\n                        [--optimized-autoscale]\n                        [--outbound-net-access {Disabled, Enabled}]\n                        [--public-ip-type {DualStack, IPv4}]\n                        [--public-network-access {Disabled, Enabled}]\n                        [--tags]\n                        [--trusted-external-tenants]\n                        [--type {None, SystemAssigned, SystemAssigned, UserAssigned, UserAssigned}]\n                        [--user-assigned]\n                        [--vcluster-graduation]\n                        [--virtual-network-configuration]\n                        [--zones]",
            "required_parameters": [
                "--cluster-name --name -n",
                "--resource-group -g",
                "--sku"
            ],
            "optional_parameters": [
                "--accepted-audiences",
                "--allowed-fqdn-list",
                "--allowed-ip-range-list",
                "--enable-auto-stop",
                "--enable-disk-encryption",
                "--enable-double-encryption",
                "--enable-purge",
                "--enable-streaming-ingest",
                "--engine-type",
                "--if-match",
                "--if-none-match",
                "--key-vault-properties",
                "--location -l",
                "--no-wait",
                "--optimized-autoscale",
                "--outbound-net-access --restrict-outbound-network-access",
                "--public-ip-type",
                "--public-network-access",
                "--tags",
                "--trusted-external-tenants",
                "--type",
                "--user-assigned --user-assigned-identities",
                "--vcluster-graduation --virtual-cluster-graduation-properties",
                "--virtual-network-configuration",
                "--zones"
            ],
            "examples": [
                "az kusto cluster create --name \"kustoclusterrptest4\" --type \"SystemAssigned\" --location \"westus\" --enable-double-encryption false --enable-purge true --enable-streaming-ingest true --sku name=\"Standard_L8s\" capacity=2 tier=\"Standard\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster delete",
            "summary": "Deletes a Kusto cluster.",
            "syntax": "az kusto cluster delete [--cluster-name]\n                        [--ids]\n                        [--no-wait]\n                        [--resource-group]\n                        [--subscription]\n                        [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name --name -n",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az kusto cluster delete --name \"kustoclusterrptest4\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster detach-follower-database",
            "summary": "Detaches all followers of a database owned by this cluster.",
            "syntax": "az kusto cluster detach-follower-database --attached-database-configuration-name\n                                          --cluster-resource-id\n                                          [--cluster-name]\n                                          [--ids]\n                                          [--no-wait]\n                                          [--resource-group]\n                                          [--subscription]",
            "required_parameters": [
                "--attached-database-configuration-name",
                "--cluster-resource-id"
            ],
            "optional_parameters": [
                "--cluster-name --name -n",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az kusto cluster detach-follower-database --name \"kustoclusterrptest4\" --attached-database-configuration-name \"myAttachedDatabaseConfiguration\" --cluster-resource-id \"/subscriptions/12345678-1234-1234-1234-123456789098/resourceGroups/kustorptest/providers/Microsoft.Kusto/clusters/leader4\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster diagnose-virtual-network",
            "summary": "Diagnoses network connectivity status for external resources on which the service is dependent on.",
            "syntax": "az kusto cluster diagnose-virtual-network [--cluster-name]\n                                          [--ids]\n                                          [--no-wait]\n                                          [--resource-group]\n                                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name --name -n",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az kusto cluster diagnose-virtual-network --name \"kustoclusterrptest4\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster list",
