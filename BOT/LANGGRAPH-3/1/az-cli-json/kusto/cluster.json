            "syntax": "az kusto cluster list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az kusto cluster list --resource-group \"kustorptest\"",
                "az kusto cluster list"
            ]
        },
        {
            "name": "az kusto cluster list-follower-database",
