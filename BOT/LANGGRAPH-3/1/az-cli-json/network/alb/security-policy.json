{
    "command_group": "az network alb security-policy",
    "description": "Manage and Configure Security Policies for Application Gateway for Containers resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az network alb security-policy list",
            "summary": "List SecurityPolicy resources by TrafficController.",
            "syntax": "az network alb security-policy list --alb-name\n                                    --resource-group\n                                    [--max-items]\n                                    [--next-token]",
            "required_parameters": [
                "--alb-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az network alb security-policy list -g test-rg --alb-name test-alb"
            ]
        },
        {
            "name": "az network alb security-policy show",
            "summary": "Get a SecurityPolicy.",
            "syntax": "az network alb security-policy show [--alb-name]\n                                    [--ids]\n                                    [--name]\n                                    [--resource-group]\n                                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--alb-name",
                "--ids",
                "--name --security-policy-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network alb security-policy show -g test-rg --alb-name test-tc -n test-sp"
            ]
        }
    ]
}