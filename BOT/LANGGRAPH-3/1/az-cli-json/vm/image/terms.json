{
    "command_group": "az vm image terms",
    "description": "Manage Azure Marketplace image terms.",
    "status": "GA",
    "commands": [
        {
            "name": "az vm image terms accept",
            "summary": "Accept Azure Marketplace image terms so that the image can be used to create VMs.",
            "syntax": "az vm image terms accept [--offer]\n                         [--plan]\n                         [--publisher]\n                         [--urn]",
            "required_parameters": [],
            "optional_parameters": [
                "--offer -f",
                "--plan",
                "--publisher -p",
                "--urn"
            ],
            "examples": [
                "az vm image terms accept --urn publisher:offer:sku:version"
            ]
        },
        {
            "name": "az vm image terms cancel",
            "summary": "Cancel Azure Marketplace image terms.",
            "syntax": "az vm image terms cancel [--offer]\n                         [--plan]\n                         [--publisher]\n                         [--urn]",
            "required_parameters": [],
            "optional_parameters": [
                "--offer -f",
                "--plan",
                "--publisher -p",
                "--urn"
            ],
            "examples": [
                "az vm image terms cancel --urn publisher:offer:sku:version",
                "az vm image terms cancel --subscription MySubscription --urn publisher:offer:sku:version"
            ]
        },
        {
            "name": "az vm image terms show",
            "summary": "Get the details of Azure Marketplace image terms.",
            "syntax": "az vm image terms show [--offer]\n                       [--plan]\n                       [--publisher]\n                       [--urn]",
            "required_parameters": [],
            "optional_parameters": [
                "--offer -f",
                "--plan",
                "--publisher -p",
                "--urn"
            ],
            "examples": [
                "az vm image terms show --urn publisher:offer:sku:version"
            ]
        }
    ]
}