{
    "command_group": "az vm image",
    "description": "Information on available virtual machine images.",
    "status": "GA",
    "commands": [
        {
            "name": "az vm image accept-terms",
            "summary": "Accept Azure Marketplace term so that the image can be used to create VMs.",
            "syntax": "az vm image accept-terms [--offer]\n                         [--plan]\n                         [--publisher]\n                         [--urn]",
            "required_parameters": [],
            "optional_parameters": [
                "--offer -f",
                "--plan",
                "--publisher -p",
                "--urn"
            ],
            "examples": [
                "az vm image accept-terms --urn publisher:offer:sku:version"
            ]
        },
        {
            "name": "az vm image list",
            "summary": "List the VM/VMSS images available in the Azure Marketplace.",
            "syntax": "az vm image list [--all]\n                 [--architecture {Arm64, x64}]\n                 [--edge-zone]\n                 [--location]\n                 [--offer]\n                 [--publisher]\n                 [--sku]",
            "required_parameters": [],
            "optional_parameters": [
                "--all",
                "--architecture",
                "--edge-zone",
                "--location -l",
                "--offer -f",
                "--publisher -p",
                "--sku -s"
            ],
            "examples": [
                "az vm image list --all",
                "az vm image list -f CentOS",
                "az vm image list -f CentOS --all"
            ]
        },
        {
            "name": "az vm image list-offers",
            "summary": "List the VM image offers available in the Azure Marketplace.",
            "syntax": "az vm image list-offers --location\n                        --publisher\n                        [--edge-zone]",
            "required_parameters": [
                "--location -l",
                "--publisher -p"
            ],
            "optional_parameters": [
                "--edge-zone"
            ],
            "examples": [
                "az vm image list-offers -l westus -p MicrosoftWindowsServer",
                "az vm image list-offers -l westus -p OpenLogic"
            ]
        },
        {
            "name": "az vm image list-publishers",
            "summary": "List the VM image publishers available in the Azure Marketplace.",
            "syntax": "az vm image list-publishers --location\n                            [--edge-zone]",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [
                "--edge-zone"
            ],
            "examples": [
                "az vm image list-publishers -l westus",
                "az vm image list-publishers -l westus --query \"[?starts_with(name, 'Open')]\""
            ]
        },
        {
            "name": "az vm image list-skus",
            "summary": "List the VM image SKUs available in the Azure Marketplace.",
            "syntax": "az vm image list-skus --location\n                      --offer\n                      --publisher\n                      [--edge-zone]",
            "required_parameters": [
                "--location -l",
                "--offer -f",
                "--publisher -p"
            ],
            "optional_parameters": [
                "--edge-zone"
            ],
            "examples": [
                "az vm image list-skus -l westus -f CentOS -p OpenLogic"
            ]
        },
        {
            "name": "az vm image show",
            "summary": "Get the details for a VM image available in the Azure Marketplace.",
            "syntax": "az vm image show [--edge-zone]\n                 [--location]\n                 [--offer]\n                 [--publisher]\n                 [--sku]\n                 [--urn]\n                 [--version]",
            "required_parameters": [],
            "optional_parameters": [
                "--edge-zone",
                "--location -l",
                "--offer -f",
                "--publisher -p",
                "--sku -s",
                "--urn",
                "--version"
            ],
            "examples": [
                "latest=$(az vm image list -p OpenLogic -s 7.3 --all --query \\\n    \"[?offer=='CentOS'].version\" -o tsv | sort -u | tail -n 1)\naz vm image show -l westus -f CentOS -p OpenLogic --sku 7.3 --version ${latest}",
                "az vm image show --location westus --urn publisher:offer:sku:version"
            ]
        }
    ]
}