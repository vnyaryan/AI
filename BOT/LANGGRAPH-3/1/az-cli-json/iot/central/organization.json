{
    "command_group": "az iot central organization",
    "description": "Manage and configure organizations for an IoT Central application.",
    "status": "GA",
    "commands": [
        {
            "name": "az iot central organization create",
            "summary": "Create an organization in the application.",
            "syntax": "az iot central organization create --app-id\n                                   --org-id\n                                   [--api-version {2022-07-31}]\n                                   [--central-api-uri]\n                                   [--org-name]\n                                   [--parent-id]\n                                   [--token]",
            "required_parameters": [
                "--app-id -n",
                "--org-id"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--org-name",
                "--parent-id",
                "--token"
            ],
            "examples": [
                "az iot central organization create --app-id {appid} --org-id {organizationId}",
                "az iot central organization create --app-id {appid} --org-id {organizationId} --parent-id {parentId}"
            ]
        },
        {
            "name": "az iot central organization delete",
            "summary": "Delete an organization by ID.",
            "syntax": "az iot central organization delete --app-id\n                                   --org-id\n                                   [--api-version {2022-07-31}]\n                                   [--central-api-uri]\n                                   [--token]",
            "required_parameters": [
                "--app-id -n",
                "--org-id"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central organization delete --app-id {appid} --org-id {organizationId}"
            ]
        },
        {
            "name": "az iot central organization list",
            "summary": "Get the list of organizations for an IoT Central application.",
            "syntax": "az iot central organization list --app-id\n                                 [--api-version {2022-07-31}]\n                                 [--central-api-uri]\n                                 [--token]",
            "required_parameters": [
                "--app-id -n"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central organization list --app-id {appid}"
            ]
        },
        {
            "name": "az iot central organization show",
            "summary": "Get the details of a organization by ID.",
            "syntax": "az iot central organization show --app-id\n                                 --org-id\n                                 [--api-version {2022-07-31}]\n                                 [--central-api-uri]\n                                 [--token]",
            "required_parameters": [
                "--app-id -n",
                "--org-id"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central organization show --app-id {appid} --org-id {organizationId}"
            ]
        },
        {
            "name": "az iot central organization update",
            "summary": "Update an organization in the application.",
            "syntax": "az iot central organization update --app-id\n                                   --org-id\n                                   [--api-version {2022-07-31}]\n                                   [--central-api-uri]\n                                   [--org-name]\n                                   [--parent-id]\n                                   [--token]",
            "required_parameters": [
                "--app-id -n",
                "--org-id"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--org-name",
                "--parent-id",
                "--token"
            ],
            "examples": [
                "az iot central organization update --app-id {appid} --org-id {organizationId} --parent-id {parentId}",
                "az iot central organization update --app-id {appid} --org-id {organizationId} --org-name {organizationName}"
            ]
        }
    ]
}