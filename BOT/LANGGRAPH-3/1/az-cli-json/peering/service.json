{
    "command_group": "az peering service",
    "description": "Peering service.",
    "status": "GA",
    "commands": [
        {
            "name": "az peering service create",
            "summary": "Creates a new peering service or updates an existing peering with the specified name under the given subscription and resource group.",
            "syntax": "az peering service create --location\n                          --peering-service-name\n                          --resource-group\n                          [--peering-service-location]\n                          [--peering-service-provider]\n                          [--sku]\n                          [--tags]",
            "required_parameters": [
                "--location -l",
                "--peering-service-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--peering-service-location",
                "--peering-service-provider",
                "--sku",
                "--tags"
            ],
            "examples": [
                "az peering service create --location \"eastus\" --peering-service-location \"state1\" --peering-service-provider \"serviceProvider1\" --peering-service-name \"peeringServiceName\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az peering service delete",
            "summary": "Deletes an existing peering service with the specified name under the given subscription and resource group.",
            "syntax": "az peering service delete --peering-service-name\n                          --resource-group",
            "required_parameters": [
                "--peering-service-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az peering service delete --peering-service-name \"peeringServiceName\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az peering service list",
            "summary": "Lists all of the peerings under the given subscription.",
            "syntax": "az peering service list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az peering service list --resource-group \"rgName\""
            ]
        },
        {
            "name": "az peering service show",
            "summary": "Gets an existing peering service with the specified name under the given subscription and resource group.",
            "syntax": "az peering service show --peering-service-name\n                        --resource-group",
            "required_parameters": [
                "--peering-service-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az peering service show --peering-service-name \"peeringServiceName\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az peering service update",
            "summary": "Updates tags for a peering service with the specified name under the given subscription and resource group.",
            "syntax": "az peering service update --peering-service-name\n                          --resource-group\n                          [--tags]",
            "required_parameters": [
                "--peering-service-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--tags"
            ],
            "examples": [
                "az peering service update --peering-service-name \"peeringServiceName\" --resource-group \"rgName\" --tags tags={\"tag0\":\"value0\",\"tag1\":\"value1\"}"
            ]
        }
    ]
}