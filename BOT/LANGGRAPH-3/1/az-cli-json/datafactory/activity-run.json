{
    "command_group": "az datafactory activity-run",
    "description": "Manage activity run with datafactory.",
    "status": "GA",
    "commands": [
        {
            "name": "az datafactory activity-run query-by-pipeline-run",
            "summary": "Query activity runs based on input filter conditions.",
            "syntax": "az datafactory activity-run query-by-pipeline-run --last-updated-after\n                                                  --last-updated-before\n                                                  [--continuation-token]\n                                                  [--factory-name]\n                                                  [--filters]\n                                                  [--ids]\n                                                  [--order-by]\n                                                  [--resource-group]\n                                                  [--run-id]\n                                                  [--subscription]",
            "required_parameters": [
                "--last-updated-after",
                "--last-updated-before"
            ],
            "optional_parameters": [
                "--continuation-token",
                "--factory-name",
                "--filters",
                "--ids",
                "--order-by",
                "--resource-group -g",
                "--run-id",
                "--subscription"
            ],
            "examples": [
                "az datafactory activity-run query-by-pipeline-run --factory-name \"exampleFactoryName\" --last-updated-after \"2018-06-16T00:36:44.3345758Z\" --last-updated-before \"2018-06-16T00:49:48.3686473Z\" --resource-group \"exampleResourceGroup\" --run-id \"2f7fdb90-5df1-4b8e-ac2f-064cfa58202b\""
            ]
        }
    ]
}