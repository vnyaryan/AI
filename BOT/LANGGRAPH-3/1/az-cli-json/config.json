{
    "command_group": "az config",
    "description": "Manage Azure CLI configuration.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az config get",
            "summary": "Get a configuration.",
            "syntax": "az config get [--local]\n              [<KEY>]",
            "required_parameters": [],
            "optional_parameters": [
                "--local",
                "<KEY>"
            ],
            "examples": [
                "az config get",
                "az config get core",
                "az config get core.no_color"
            ]
        },
        {
            "name": "az config set",
            "summary": "Set a configuration.",
            "syntax": "az config set [--local]\n              [<KEY_VALUE>]",
            "required_parameters": [],
            "optional_parameters": [
                "--local",
                "<KEY_VALUE>"
            ],
            "examples": [
                "az config set core.no_color=true",
                "az config set core.only_show_errors=true",
                "az config set core.collect_telemetry=true",
                "az config set logging.enable_log_file=true\naz config set logging.log_dir=~/az-logs",
                "az config set defaults.location=westus2 defaults.group=MyResourceGroup",
                "az config set defaults.group=myRG --local"
            ]
        },
        {
            "name": "az config unset",
            "summary": "Unset a configuration.",
            "syntax": "az config unset [--local]\n                [<KEY>]",
            "required_parameters": [],
            "optional_parameters": [
                "--local",
                "<KEY>"
            ],
            "examples": [
                "az config unset core.no_color"
            ]
        }
    ]
}