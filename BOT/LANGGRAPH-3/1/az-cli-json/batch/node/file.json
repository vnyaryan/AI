{
    "command_group": "az batch node file",
    "description": "Manage Batch compute node files.",
    "status": "GA",
    "commands": [
        {
            "name": "az batch node file delete",
            "summary": "Deletes the specified file from the Compute Node.",
            "syntax": "az batch node file delete --file-path\n                          --node-id\n                          --pool-id\n                          [--account-endpoint]\n                          [--account-key]\n                          [--account-name]\n                          [--recursive {false, true}]\n                          [--yes]",
            "required_parameters": [
                "--file-path",
                "--node-id",
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--recursive",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az batch node file download",
            "summary": "Download the content of the a node file.",
            "syntax": "az batch node file download --destination\n                            --file-path\n                            --node-id\n                            --pool-id\n                            [--account-endpoint]\n                            [--account-key]\n                            [--account-name]\n                            [--end-range]\n                            [--if-modified-since]\n                            [--if-unmodified-since]\n                            [--start-range]",
            "required_parameters": [
                "--destination",
                "--file-path",
                "--node-id",
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--end-range",
                "--if-modified-since",
                "--if-unmodified-since",
                "--start-range"
            ],
            "examples": []
        },
        {
            "name": "az batch node file list",
            "summary": "Lists all of the files in Task directories on the specified Compute Node.",
            "syntax": "az batch node file list --node-id\n                        --pool-id\n                        [--account-endpoint]\n                        [--account-key]\n                        [--account-name]\n                        [--filter]\n                        [--recursive {false, true}]",
            "required_parameters": [
                "--node-id",
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--filter",
                "--recursive"
            ],
            "examples": []
        },
        {
            "name": "az batch node file show",
            "summary": "Gets the properties of the specified Compute Node file.",
            "syntax": "az batch node file show --file-path\n                        --node-id\n                        --pool-id\n                        [--account-endpoint]\n                        [--account-key]\n                        [--account-name]\n                        [--if-modified-since]\n                        [--if-unmodified-since]",
            "required_parameters": [
                "--file-path",
                "--node-id",
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--if-modified-since",
                "--if-unmodified-since"
            ],
            "examples": []
        }
    ]
}