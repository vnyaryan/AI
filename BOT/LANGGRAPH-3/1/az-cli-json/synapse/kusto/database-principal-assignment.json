            "required_parameters": [],
            "optional_parameters": [
                "--database-name",
                "--ids",
                "--kusto-pool-name",
                "--no-wait",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
