{
    "command_group": "az spatial-anchors-account",
    "description": "Manage spatial anchor account with mixed reality.",
    "status": "GA",
    "commands": [
        {
            "name": "az spatial-anchors-account create",
            "summary": "Create a Spatial Anchors Account.",
            "syntax": "az spatial-anchors-account create --name\n                                  --resource-group\n                                  [--kind]\n                                  [--location]\n                                  [--sku]\n                                  [--storage-account-name]\n                                  [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--kind",
                "--location -l",
                "--sku",
                "--storage-account-name",
                "--tags"
            ],
            "examples": [
                "az spatial-anchors-account create -n \"MyAccount\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az spatial-anchors-account delete",
            "summary": "Delete a Spatial Anchors Account.",
            "syntax": "az spatial-anchors-account delete [--ids]\n                                  [--name]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az spatial-anchors-account delete -n \"MyAccount\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az spatial-anchors-account list",
            "summary": "List Spatial Anchors Accounts by Subscription.",
            "syntax": "az spatial-anchors-account list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az spatial-anchors-account list --resource-group \"MyResourceGroup\"",
                "az spatial-anchors-account list"
            ]
        },
        {
            "name": "az spatial-anchors-account show",
            "summary": "Get a Spatial Anchors Account.",
            "syntax": "az spatial-anchors-account show [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az spatial-anchors-account show -n \"MyAccount\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az spatial-anchors-account update",
            "summary": "Update a Spatial Anchors Account.",
            "syntax": "az spatial-anchors-account update [--ids]\n                                  [--kind]\n                                  [--name]\n                                  [--resource-group]\n                                  [--sku]\n                                  [--storage-account-name]\n                                  [--subscription]\n                                  [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--kind",
                "--name -n",
                "--resource-group -g",
                "--sku",
                "--storage-account-name",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az spatial-anchors-account update -n \"MyAccount\" --resource-group \"MyResourceGroup\" --location \"eastus2euap\" --tags hero=\"romeo\" heroine=\"juliet\""
            ]
        }
    ]
}