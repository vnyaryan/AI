{
    "command_group": "az mobile-network",
    "description": "Manage mobile network.",
    "status": "GA",
    "commands": [
        {
            "name": "az mobile-network create",
            "summary": "Create a mobile network.",
            "syntax": "az mobile-network create --identifier\n                         --mobile-network-name\n                         --resource-group\n                         [--location]\n                         [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                         [--tags]",
            "required_parameters": [
                "--identifier",
                "--mobile-network-name --name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--location -l",
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az mobile-network create -n mobile-network-name -g rg --identifier \"{mcc:001,mnc:01}\""
            ]
        },
        {
            "name": "az mobile-network delete",
            "summary": "Delete the specified mobile network.",
            "syntax": "az mobile-network delete [--ids]\n                         [--mobile-network-name]\n                         [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                         [--resource-group]\n                         [--subscription]\n                         [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--mobile-network-name --name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az mobile-network delete -n mobile-network-name -g rg -y"
            ]
        },
        {
            "name": "az mobile-network list",
            "summary": "List all the mobile networks in a subscription.",
            "syntax": "az mobile-network list [--max-items]\n                       [--next-token]\n                       [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az mobile-network list -g rg"
            ]
        },
        {
            "name": "az mobile-network show",
            "summary": "Get information about the specified mobile network.",
            "syntax": "az mobile-network show [--ids]\n                       [--mobile-network-name]\n                       [--resource-group]\n                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--mobile-network-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az mobile-network show -n mobile-network-name -g rg"
            ]
        },
        {
            "name": "az mobile-network update",
            "summary": "Update mobile network tags.",
            "syntax": "az mobile-network update [--ids]\n                         [--mobile-network-name]\n                         [--resource-group]\n                         [--subscription]\n                         [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--mobile-network-name --name -n",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az mobile-network update -n mobile-network-name -g rg --tags \"{tag:test,tag2:test2}\""
            ]
        },
        {
            "name": "az mobile-network wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az mobile-network wait [--created]\n                       [--custom]\n                       [--deleted]\n                       [--exists]\n                       [--ids]\n                       [--interval]\n                       [--mobile-network-name]\n                       [--resource-group]\n                       [--subscription]\n                       [--timeout]\n                       [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--mobile-network-name --name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}