{
    "command_group": "az netappfiles quota-limit",
    "description": "Get quota limits.",
    "status": "GA",
    "commands": [
        {
            "name": "az netappfiles quota-limit list",
            "summary": "List the default and current limits for quotas.",
            "syntax": "az netappfiles quota-limit list --location",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [],
            "examples": [
                "az netappfiles quota-limit list -l eastus"
            ]
        },
        {
            "name": "az netappfiles quota-limit show",
            "summary": "Get the default and current subscription quota limit.",
            "syntax": "az netappfiles quota-limit show [--ids]\n                                [--location]\n                                [--name]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--name --quota-limit-name -n",
                "--subscription"
            ],
            "examples": [
                "az netappfiles quota-limit show -l eastus -n totalTiBsPerSubscription"
            ]
        }
    ]
}