{
    "command_group": "az k8s-runtime service",
    "description": "Manage Kubernetes runtime services of Arc Kubernetes clusters.",
    "status": "Preview",
    "commands": [
        {
            "name": "az k8s-runtime service create",
            "summary": "Create a Service.",
            "syntax": "az k8s-runtime service create --resource-uri\n                              --service-name",
            "required_parameters": [
                "--resource-uri",
                "--service-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az k8s-runtime service create --resource-uri subscriptions/00000000-1111-2222-3333-444444444444/resourceGroups/example/providers/Microsoft.Kubernetes/connectedClusters/cluster1 --service-name storageclass"
            ]
        },
        {
            "name": "az k8s-runtime service delete",
            "summary": "Delete a Service.",
            "syntax": "az k8s-runtime service delete --resource-uri\n                              --service-name\n                              [--yes]",
            "required_parameters": [
                "--resource-uri",
                "--service-name"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az k8s-runtime service delete --resource-uri subscriptions/00000000-1111-2222-3333-444444444444/resourceGroups/example/providers/Microsoft.Kubernetes/connectedClusters/cluster1 --service-name storageclass"
            ]
        },
        {
            "name": "az k8s-runtime service list",
            "summary": "List Services by parent.",
            "syntax": "az k8s-runtime service list --resource-uri\n                            [--max-items]\n                            [--next-token]",
            "required_parameters": [
                "--resource-uri"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az k8s-runtime service list --resource-uri subscriptions/00000000-1111-2222-3333-444444444444/resourceGroups/example/providers/Microsoft.Kubernetes/connectedClusters/cluster1"
            ]
        },
        {
            "name": "az k8s-runtime service show",
            "summary": "Get a Service.",
            "syntax": "az k8s-runtime service show --resource-uri\n                            --service-name",
            "required_parameters": [
                "--resource-uri",
                "--service-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az k8s-runtime service show --resource-uri subscriptions/00000000-1111-2222-3333-444444444444/resourceGroups/example/providers/Microsoft.Kubernetes/connectedClusters/cluster1 --service-name storageclass"
            ]
        }
    ]
}