{
    "command_group": "az scvmm vmmserver",
    "description": "Manage Arc for SCVMM VMMServer resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az scvmm vmmserver connect",
            "summary": "Create a vmmserver resource.",
            "syntax": "az scvmm vmmserver connect --custom-location\n                           --location\n                           [--fqdn]\n                           [--ids]\n                           [--name]\n                           [--no-wait]\n                           [--password]\n                           [--port]\n                           [--resource-group]\n                           [--subscription]\n                           [--tags]\n                           [--username]",
            "required_parameters": [
                "--custom-location",
                "--location -l"
            ],
            "optional_parameters": [
                "--fqdn",
                "--ids",
                "--name -n",
                "--no-wait",
                "--password",
                "--port",
                "--resource-group -g",
                "--subscription",
                "--tags",
                "--username"
            ],
            "examples": [
                "az scvmm vmmserver connect --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --name contoso-vmmserver --fqdn vmm.contoso.com --port 8100 --username contoso-user --password contoso-password"
            ]
        },
        {
            "name": "az scvmm vmmserver delete",
            "summary": "Delete a vmmserver resource.",
            "syntax": "az scvmm vmmserver delete [--force]\n                          [--ids]\n                          [--name]\n                          [--no-wait]\n                          [--resource-group]\n                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vmmserver delete --name contoso-vmmserver --resource-group contoso-rg --subscription contoso-sub",
                "az scvmm vmmserver delete --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/vmmServers/contoso-vmmserver"
            ]
        },
        {
            "name": "az scvmm vmmserver list",
            "summary": "Retrieve a list of vmmservers.",
            "syntax": "az scvmm vmmserver list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az scvmm vmmserver list --subscription contoso-sub --resource-group contoso-rg",
                "az scvmm vmmserver list --subscription contoso-sub"
            ]
        },
        {
            "name": "az scvmm vmmserver show",
            "summary": "Get details of a vmmserver.",
            "syntax": "az scvmm vmmserver show [--ids]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm vmmserver show --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/vmmServers/contoso-vmmserver",
                "az scvmm vmmserver show --subscription contoso-sub --resource-group contoso-rg --name contoso-vmmserver"
            ]
        },
        {
            "name": "az scvmm vmmserver update",
            "summary": "Update vmmserver resource.",
            "syntax": "az scvmm vmmserver update [--ids]\n                          [--name]\n                          [--no-wait]\n                          [--resource-group]\n                          [--subscription]\n                          [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az scvmm vmmserver update --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/vmmservers/contoso-vmmserver --tags department=Sales",
                "az scvmm vmmserver update --subscription contoso-sub --resource-group contoso-rg --name contoso-vmmserver --tags department=Sales"
            ]
        },
        {
            "name": "az scvmm vmmserver wait",
            "summary": "Place the CLI in a waiting state until a condition of the vmmserver is met.",
            "syntax": "az scvmm vmmserver wait --resource-group\n                        --vmm-server-name\n                        [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--interval]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [
                "--resource-group -g",
                "--vmm-server-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}