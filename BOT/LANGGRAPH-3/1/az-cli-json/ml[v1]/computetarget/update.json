{
    "command_group": "az ml computetarget update",
    "description": "Update a compute target (aks or amlcompute).",
    "status": "GA",
    "commands": [
        {
            "name": "az ml computetarget update aks",
            "summary": "Update an AKS compute target.",
            "syntax": "az ml computetarget update aks --name\n                               [--load-balancer-subnet]\n                               [--load-balancer-type]\n                               [--no-wait]\n                               [--path]\n                               [--resource-group]\n                               [--ssl-cert-file]\n                               [--ssl-cname]\n                               [--ssl-disable]\n                               [--ssl-key-file]\n                               [--ssl-leaf-domain-label]\n                               [--ssl-overwrite-domain]\n                               [--ssl-renew]\n                               [--subscription-id]\n                               [--workspace-name]\n                               [-v]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--load-balancer-subnet",
                "--load-balancer-type",
                "--no-wait",
                "--path",
                "--resource-group -g",
                "--ssl-cert-file",
                "--ssl-cname",
                "--ssl-disable",
                "--ssl-key-file",
                "--ssl-leaf-domain-label",
                "--ssl-overwrite-domain",
                "--ssl-renew",
                "--subscription-id",
                "--workspace-name -w",
                "-v"
            ],
            "examples": []
        },
        {
            "name": "az ml computetarget update amlcompute",
            "summary": "Update an AzureML compute target.",
            "syntax": "az ml computetarget update amlcompute --name\n                                      [--idle-seconds-before-scaledown]\n                                      [--max-nodes]\n                                      [--min-nodes]\n                                      [--path]\n                                      [--resource-group]\n                                      [--subscription-id]\n                                      [--workspace-name]\n                                      [-v]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--idle-seconds-before-scaledown",
                "--max-nodes",
                "--min-nodes",
                "--path",
                "--resource-group -g",
                "--subscription-id",
                "--workspace-name -w",
                "-v"
            ],
            "examples": []
        }
    ]
}