{
    "command_group": "az providerhub manifest",
    "description": "Manage manifest with providerhub.",
    "status": "GA",
    "commands": [
        {
            "name": "az providerhub manifest checkin",
            "summary": "Checkin the manifest.",
            "syntax": "az providerhub manifest checkin --environment\n                                [--arm-manifest-location]\n                                [--ids]\n                                [--provider-namespace]\n                                [--subscription]",
            "required_parameters": [
                "--environment"
            ],
            "optional_parameters": [
                "--arm-manifest-location --baseline-arm-manifest-location --location",
                "--ids",
                "--provider-namespace",
                "--subscription"
            ],
            "examples": [
                "az az providerhub manifest checkin --provider-namespace \"{providerNamespace}\" --environment \"Canary\""
            ]
        },
        {
            "name": "az providerhub manifest generate",
            "summary": "Generates the manifest for the given provider.",
            "syntax": "az providerhub manifest generate [--ids]\n                                 [--provider-namespace]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--provider-namespace",
                "--subscription"
            ],
            "examples": [
                "az az providerhub manifest generate --provider-namespace \"{providerNamespace}\""
            ]
        }
    ]
}