{
    "command_group": "az devcenter admin image",
    "description": "Manage images.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin image list",
            "summary": "List images for a dev center or gallery.",
            "syntax": "az devcenter admin image list --dev-center\n                              --resource-group\n                              [--gallery-name]\n                              [--max-items]\n                              [--next-token]",
            "required_parameters": [
                "--dev-center --dev-center-name -d",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--gallery-name",
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin image list --dev-center-name \"Contoso\" --resource-group \"rg1\"",
                "az devcenter admin image list --dev-center-name \"Contoso\" --gallery-name \"DevGallery\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin image show",
            "summary": "Get a gallery image.",
            "syntax": "az devcenter admin image show [--dev-center]\n                              [--gallery-name]\n                              [--ids]\n                              [--image-name]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--dev-center --dev-center-name -d",
                "--gallery-name",
                "--ids",
                "--image-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin image show --dev-center-name \"Contoso\" --gallery-name \"DefaultDevGallery\" --name \"ContosoBaseImage\" --resource-group \"rg1\""
            ]
        }
    ]
}