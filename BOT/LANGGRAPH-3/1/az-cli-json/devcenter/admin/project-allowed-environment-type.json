{
    "command_group": "az devcenter admin project-allowed-environment-type",
    "description": "Manage project allowed environment types.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin project-allowed-environment-type list",
            "summary": "List allowed environment types for a project.",
            "syntax": "az devcenter admin project-allowed-environment-type list --project\n                                                         --resource-group\n                                                         [--max-items]\n                                                         [--next-token]",
            "required_parameters": [
                "--project --project-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin project-allowed-environment-type list --project-name \"Contoso\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-allowed-environment-type show",
            "summary": "Get an allowed environment type.",
            "syntax": "az devcenter admin project-allowed-environment-type show [--environment-type-name]\n                                                         [--ids]\n                                                         [--project]\n                                                         [--resource-group]\n                                                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--environment-type-name --name -n",
                "--ids",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin project-allowed-environment-type show --environment-type-name \"DevTest\" --project-name \"Contoso\" --resource-group \"rg1\""
            ]
        }
    ]
}