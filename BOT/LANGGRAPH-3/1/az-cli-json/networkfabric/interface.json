{
    "command_group": "az networkfabric interface",
    "description": "Manage Network Interface Resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az networkfabric interface delete",
            "summary": "Delete the Network Interface resource.",
            "syntax": "az networkfabric interface delete [--device]\n                                  [--ids]\n                                  [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                  [--resource-group]\n                                  [--resource-name]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--device --network-device-name",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--resource-name",
                "--subscription"
            ],
            "examples": [
                "az networkfabric interface delete -g \"example-rg\" --network-device-name \"example-device\" --resource-name \"example-interface\""
            ]
        },
        {
            "name": "az networkfabric interface list",
            "summary": "List all Network Interfaces in the provided resource group.",
            "syntax": "az networkfabric interface list --device\n                                --resource-group\n                                [--max-items]\n                                [--next-token]",
            "required_parameters": [
                "--device --network-device-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az networkfabric interface list -g \"example-rg\" --network-device-name \"example-device\""
            ]
        },
        {
            "name": "az networkfabric interface show",
            "summary": "Show details of the provided Network Interface resource.",
            "syntax": "az networkfabric interface show [--device]\n                                [--ids]\n                                [--resource-group]\n                                [--resource-name]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--device --network-device-name",
                "--ids",
                "--resource-group -g",
                "--resource-name",
                "--subscription"
            ],
            "examples": [
                "az networkfabric interface show -g \"example-rg\" --network-device-name \"example-device\" --resource-name \"example-interface\""
            ]
        },
        {
            "name": "az networkfabric interface update-admin-state",
            "summary": "Update the admin state of the Network Interface.",
            "syntax": "az networkfabric interface update-admin-state [--device]\n                                              [--ids]\n                                              [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                              [--resource-group]\n                                              [--resource-ids]\n                                              [--resource-name]\n                                              [--state {Disable, Enable}]\n                                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--device --network-device-name",
                "--ids",
                "--no-wait",
                "--resource-group -g",
                "--resource-ids",
                "--resource-name",
                "--state",
                "--subscription"
            ],
            "examples": [
                "az networkfabric interface update-admin-state -g \"example-rg\" --network-device-name \"example-device\" --resource-name \"example-interface\" --state \"Enable\""
            ]
        },
        {
            "name": "az networkfabric interface wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az networkfabric interface wait [--created]\n                                [--custom]\n                                [--deleted]\n                                [--device]\n                                [--exists]\n                                [--ids]\n                                [--interval]\n                                [--resource-group]\n                                [--resource-name]\n                                [--subscription]\n                                [--timeout]\n                                [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--device --network-device-name",
                "--exists",
                "--ids",
                "--interval",
                "--resource-group -g",
                "--resource-name",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}