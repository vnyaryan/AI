{
    "command_group": "az automation hrwg hrw",
    "description": "Manage Automation Hybrid Runbook Worker.",
    "status": "GA",
    "commands": [
        {
            "name": "az automation hrwg hrw create",
            "summary": "Create a hybrid runbook worker.",
            "syntax": "az automation hrwg hrw create --automation-account-name\n                              --hybrid-runbook-worker-group-name\n                              --hybrid-runbook-worker-id\n                              --resource-group\n                              [--vm-resource-id]",
            "required_parameters": [
                "--automation-account-name",
                "--hybrid-runbook-worker-group-name",
                "--hybrid-runbook-worker-id --name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--vm-resource-id"
            ],
            "examples": [
                "az automation hrwg hrw create --automation-account-name accountName --resource-group groupName --hybrid-runbook-worker-group-name hybridRunbookWorkerGroupName --hybrid-runbook-worker-id hybridRunbookWorkerId --vm-resource-id vmResourceId"
            ]
        },
        {
            "name": "az automation hrwg hrw delete",
            "summary": "Delete a hybrid runbook worker.",
            "syntax": "az automation hrwg hrw delete [--automation-account-name]\n                              [--hybrid-runbook-worker-group-name]\n                              [--hybrid-runbook-worker-id]\n                              [--ids]\n                              [--resource-group]\n                              [--subscription]\n                              [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--hybrid-runbook-worker-group-name",
                "--hybrid-runbook-worker-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az automation hrwg hrw delete --automation-account-name accountName --resource-group groupName --hybrid-runbook-worker-group-name hybridRunbookWorkerGroupName --hybrid-runbook-worker-id hybridRunbookWorkerId"
            ]
        },
        {
            "name": "az automation hrwg hrw list",
            "summary": "List a list of hybrid runbook workers.",
            "syntax": "az automation hrwg hrw list --automation-account-name\n                            --hybrid-runbook-worker-group-name\n                            --resource-group\n                            [--filter]",
            "required_parameters": [
                "--automation-account-name",
                "--hybrid-runbook-worker-group-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--filter"
            ],
            "examples": [
                "az automation hrwg hrw list --automation-account-name accountName --resource-group groupName --hybrid-runbook-worker-group-name hybridRunbookWorkerGroupName"
            ]
        },
        {
            "name": "az automation hrwg hrw move",
            "summary": "Move a hybrid worker to a different group.",
            "syntax": "az automation hrwg hrw move [--automation-account-name]\n                            [--hybrid-runbook-worker-group-name]\n                            [--hybrid-runbook-worker-id]\n                            [--ids]\n                            [--resource-group]\n                            [--subscription]\n                            [--target-hybrid-runbook-worker-group-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--hybrid-runbook-worker-group-name",
                "--hybrid-runbook-worker-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--target-hybrid-runbook-worker-group-name"
            ],
            "examples": []
        },
        {
            "name": "az automation hrwg hrw show",
            "summary": "Get a hybrid runbook worker.",
            "syntax": "az automation hrwg hrw show [--automation-account-name]\n                            [--hybrid-runbook-worker-group-name]\n                            [--hybrid-runbook-worker-id]\n                            [--ids]\n                            [--resource-group]\n                            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--hybrid-runbook-worker-group-name",
                "--hybrid-runbook-worker-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az automation hrwg hrw show --automation-account-name accountName --resource-group groupName --hybrid-runbook-worker-group-name hybridRunbookWorkerGroupName --hybrid-runbook-worker-id hybridRunbookWorkerId"
            ]
        }
    ]
}