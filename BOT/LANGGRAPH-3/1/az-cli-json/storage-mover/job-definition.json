{
    "command_group": "az storage-mover job-definition",
    "description": "Manage Job Definition resource, which contains configuration for a single unit of managed data transfer.",
    "status": "GA",
    "commands": [
        {
            "name": "az storage-mover job-definition create",
            "summary": "Creates a Job Definition resource, which contains configuration for a single unit of managed data transfer.",
            "syntax": "az storage-mover job-definition create --copy-mode {Additive, Mirror}\n                                       --job-definition-name\n                                       --project-name\n                                       --resource-group\n                                       --source-name\n                                       --storage-mover-name\n                                       --target-name\n                                       [--agent-name]\n                                       [--description]\n                                       [--source-subpath]\n                                       [--target-subpath]",
            "required_parameters": [
                "--copy-mode",
                "--job-definition-name --name -n",
                "--project-name",
                "--resource-group -g",
                "--source-name",
                "--storage-mover-name",
                "--target-name"
            ],
            "optional_parameters": [
                "--agent-name",
                "--description",
                "--source-subpath",
                "--target-subpath"
            ],
            "examples": [
                "az storage-mover job-definition create -g {rg} -n {job_definition} --project-name {project_name} --storage-mover-name {mover_name} --copy-mode Additive --source-name {source_endpoint} --target-name {target_endpoint} --agent-name {agent_name} --description JobDefinitionDescription --source-subpath path1 --target-subpath path2"
            ]
        },
        {
            "name": "az storage-mover job-definition delete",
            "summary": "Deletes a Job Definition resource.",
            "syntax": "az storage-mover job-definition delete [--ids]\n                                       [--job-definition-name]\n                                       [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                       [--project-name]\n                                       [--resource-group]\n                                       [--storage-mover-name]\n                                       [--subscription]\n                                       [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--job-definition-name --name -n",
                "--no-wait",
                "--project-name",
                "--resource-group -g",
                "--storage-mover-name",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az storage-mover job-definition delete -g {rg} -n {job_definition} --project-name {project_name} --storage-mover-name {mover_name}"
            ]
        },
        {
            "name": "az storage-mover job-definition list",
            "summary": "Lists all Job Definitions in a Project.",
            "syntax": "az storage-mover job-definition list --project-name\n                                     --resource-group\n                                     --storage-mover-name\n                                     [--max-items]\n                                     [--next-token]",
            "required_parameters": [
                "--project-name",
                "--resource-group -g",
                "--storage-mover-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az storage-mover job-definition list -g {rg} --project-name {project_name} --storage-mover-name {mover_name}"
            ]
        },
        {
