{
    "command_group": "az connectedvmware datastore",
    "description": "Datastore resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az connectedvmware datastore create",
            "summary": "Create a datastore resource.",
            "syntax": "az connectedvmware datastore create --custom-location\n                                    --inventory-item\n                                    --location\n                                    --name\n                                    --resource-group\n                                    --vcenter\n                                    [--no-wait]\n                                    [--tags]",
            "required_parameters": [
                "--custom-location -c",
                "--inventory-item -i",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--vcenter -v"
            ],
            "optional_parameters": [
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az connectedvmware datastore create --custom-location \"custom location name\"                --location \"location name\" --inventory-item \"name or id the inventory item\"                --name \"datastore name\" --resource-group \"resource group name\"                --vcenter \"name or id of the vcenter\""
            ]
        },
        {
            "name": "az connectedvmware datastore delete",
            "summary": "Delete datastore resource.",
            "syntax": "az connectedvmware datastore delete [--force]\n                                    [--ids]\n                                    [--name]\n                                    [--no-wait]\n                                    [--resource-group]\n                                    [--subscription]\n                                    [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az connectedvmware datastore delete --ids \"resource id\" --name \"datastore name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        },
        {
            "name": "az connectedvmware datastore list",
            "summary": "Retrieve a list of datastore of given resource group.",
            "syntax": "az connectedvmware datastore list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az connectedvmware datastore list --resource-group \"resource group name\""
            ]
        },
        {
            "name": "az connectedvmware datastore show",
            "summary": "Get details of a datastore by id, resource-group, datastore name, or subscription.",
            "syntax": "az connectedvmware datastore show [--ids]\n                                  [--name]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az connectedvmware datastore show --ids \"resource id\" --name \"datastore name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        }
    ]
}