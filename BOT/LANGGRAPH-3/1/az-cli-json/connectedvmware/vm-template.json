{
    "command_group": "az connectedvmware vm-template",
    "description": "Vm template resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az connectedvmware vm-template create",
            "summary": "Create vm template resource.",
            "syntax": "az connectedvmware vm-template create --custom-location\n                                      --inventory-item\n                                      --location\n                                      --name\n                                      --resource-group\n                                      --vcenter\n                                      [--no-wait]\n                                      [--tags]",
            "required_parameters": [
                "--custom-location -c",
                "--inventory-item -i",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--vcenter -v"
            ],
            "optional_parameters": [
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az connectedvmware vm-template create --custom-location \"custom location name\"                --location \"location name\" --inventory-item \"name or id the inventory item\"                --name \"vm template name\" --resource-group \"resource group name\"                --vcenter \"name or id of the vcenter\""
            ]
        },
        {
            "name": "az connectedvmware vm-template delete",
            "summary": "Delete vm template resource.",
            "syntax": "az connectedvmware vm-template delete [--force]\n                                      [--ids]\n                                      [--name]\n                                      [--no-wait]\n                                      [--resource-group]\n                                      [--subscription]\n                                      [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az connectedvmware vm-template delete --ids \"resource id\" --name \"vm template name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        },
        {
            "name": "az connectedvmware vm-template list",
            "summary": "Retrieve a list of vm template of given resource group.",
            "syntax": "az connectedvmware vm-template list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az connectedvmware vm-template list --resource-group \"resource group name\""
            ]
        },
        {
            "name": "az connectedvmware vm-template show",
            "summary": "Get details of a vm template by id, resource-group, reource pool name or subscription.",
            "syntax": "az connectedvmware vm-template show [--ids]\n                                    [--name]\n                                    [--resource-group]\n                                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az connectedvmware vm-template show --ids \"resource id\" --name \"vm template name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        }
    ]
}