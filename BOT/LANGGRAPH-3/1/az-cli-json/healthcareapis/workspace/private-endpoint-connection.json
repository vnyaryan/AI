{
    "command_group": "az healthcareapis workspace private-endpoint-connection",
    "description": "Manage workspace private endpoint connection with healthcareapis.",
    "status": "GA",
    "commands": [
        {
            "name": "az healthcareapis workspace private-endpoint-connection create",
            "summary": "Update the state of the specified private endpoint connection associated with the workspace.",
            "syntax": "az healthcareapis workspace private-endpoint-connection create --name\n                                                               --resource-group\n                                                               --workspace-name\n                                                               [--no-wait]\n                                                               [--private-link-service-connection-state]",
            "required_parameters": [
                "--name --private-endpoint-connection-name -n",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--private-link-service-connection-state -s"
            ],
            "examples": [
                "az healthcareapis workspace private-endpoint-connection create --private-endpoint-connection-name \"myConnection\" --private-link-service-connection-state description=\"Auto-Approved\" status=\"Approved\" --resource-group \"testRG\" --workspace-name \"workspace1\""
            ]
        },
        {
            "name": "az healthcareapis workspace private-endpoint-connection delete",
            "summary": "Deletes a private endpoint connection.",
            "syntax": "az healthcareapis workspace private-endpoint-connection delete [--ids]\n                                                               [--name]\n                                                               [--no-wait]\n                                                               [--resource-group]\n                                                               [--subscription]\n                                                               [--workspace-name]\n                                                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --private-endpoint-connection-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
            ],
            "examples": [
                "az healthcareapis workspace private-endpoint-connection delete --private-endpoint-connection-name \"myConnection\" --resource-group \"testRG\" --workspace-name \"workspace1\""
            ]
        },
        {
            "name": "az healthcareapis workspace private-endpoint-connection list",
            "summary": "Lists all private endpoint connections for a workspace.",
            "syntax": "az healthcareapis workspace private-endpoint-connection list --resource-group\n                                                             --workspace-name",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az healthcareapis workspace private-endpoint-connection list --resource-group \"testRG\" --workspace-name \"workspace1\""
            ]
        },
