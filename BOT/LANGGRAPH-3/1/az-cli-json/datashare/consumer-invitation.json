{
    "command_group": "az datashare consumer-invitation",
    "description": "Manage consumer invitation with datashare.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az datashare consumer-invitation list-invitation",
            "summary": "Lists invitations.",
            "syntax": "az datashare consumer-invitation list-invitation [--skip-token]",
            "required_parameters": [],
            "optional_parameters": [
                "--skip-token"
            ],
            "examples": [
                "az datashare consumer-invitation list-invitation"
            ]
        },
        {
            "name": "az datashare consumer-invitation reject-invitation",
            "summary": "Reject an invitation.",
            "syntax": "az datashare consumer-invitation reject-invitation --invitation-id\n                                                   --location",
            "required_parameters": [
                "--invitation-id",
                "--location -l"
            ],
            "optional_parameters": [],
            "examples": [
                "az datashare consumer-invitation reject-invitation --invitation-id \"dfbbc788-19eb-4607-a5a1-c74181bfff03\" --location \"East US 2\""
            ]
        },
        {
            "name": "az datashare consumer-invitation show",
            "summary": "Get an invitation.",
            "syntax": "az datashare consumer-invitation show --invitation-id\n                                      --location",
            "required_parameters": [
                "--invitation-id",
                "--location -l"
            ],
            "optional_parameters": [],
            "examples": [
                "az datashare consumer-invitation show --invitation-id \"dfbbc788-19eb-4607-a5a1-c74181bfff03\" --location \"East US 2\""
            ]
        }
    ]
}