{
    "command_group": "az keyvault region",
    "description": "Manage MHSM multi-regions.",
    "status": "GA",
    "commands": [
        {
            "name": "az keyvault region add",
            "summary": "Add regions for the managed HSM Pool.",
            "syntax": "az keyvault region add --hsm-name\n                       --region\n                       [--no-wait]\n                       [--resource-group]",
            "required_parameters": [
                "--hsm-name",
                "--region --region-name -r"
            ],
            "optional_parameters": [
                "--no-wait",
                "--resource-group -g"
            ],
            "examples": [
                "az keyvault region add --region-name westus2 --hsm-name myhsm --resource-group myrg"
            ]
        },
        {
            "name": "az keyvault region list",
            "summary": "Get regions information associated with the managed HSM Pool.",
            "syntax": "az keyvault region list --hsm-name\n                        [--resource-group]",
            "required_parameters": [
                "--hsm-name"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az keyvault region remove",
            "summary": "Remove regions for the managed HSM Pool.",
            "syntax": "az keyvault region remove --hsm-name\n                          --region\n                          [--no-wait]\n                          [--resource-group]",
            "required_parameters": [
                "--hsm-name",
                "--region --region-name -r"
            ],
            "optional_parameters": [
                "--no-wait",
                "--resource-group -g"
            ],
            "examples": [
                "az keyvault region remove --region-name westus2 --hsm-name myhsm --resource-group myrg"
            ]
        },
        {
            "name": "az keyvault region wait",
            "summary": "Place the CLI in a waiting state until a condition of the HSM is met.",
            "syntax": "az keyvault region wait --hsm-name\n                        [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--interval]\n                        [--resource-group]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [
                "--hsm-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--resource-group -g",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az keyvault region wait --hsm-name myhsm --updated"
            ]
        }
    ]
}