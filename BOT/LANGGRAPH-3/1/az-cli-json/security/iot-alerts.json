{
    "command_group": "az security iot-alerts",
    "description": "View IoT Security aggregated alerts.",
    "status": "GA",
    "commands": [
        {
            "name": "az security iot-alerts delete",
            "summary": "Dismiss an aggregated IoT Security Alert.",
            "syntax": "az security iot-alerts delete --name\n                              --resource-group\n                              --solution-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--solution-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az security iot-alerts delete --solution-name 'IoT-Hub1' --resource-group 'rg1' --name 'IoT_CryptoMiner/2020-06-24'"
            ]
        },
        {
            "name": "az security iot-alerts list",
            "summary": "List all yours IoT Security solution aggregated alerts.",
            "syntax": "az security iot-alerts list --resource-group\n                            --solution-name",
            "required_parameters": [
                "--resource-group -g",
                "--solution-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az security iot-alerts list --solution-name 'IoT-Hub1' --resource-group 'rg1'"
            ]
        },
        {
            "name": "az security iot-alerts show",
            "summary": "Shows a single aggregated alert of yours IoT Security solution.",
            "syntax": "az security iot-alerts show --name\n                            --resource-group\n                            --solution-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--solution-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az security iot-alerts show --solution-name 'IoT-Hub1' --resource-group 'rg1' --name 'IoT_CryptoMiner/2020-06-24'"
            ]
        }
    ]
}