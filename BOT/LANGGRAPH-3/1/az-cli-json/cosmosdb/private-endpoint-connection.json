                "--account-name -a",
                "--id",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az cosmosdb private-endpoint-connection delete --account-name MyAccount --name MyPrivateEndpoint --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az cosmosdb private-endpoint-connection reject",
            "summary": "Reject the specified private endpoint connection associated with Azure Cosmos DB.",
            "syntax": "az cosmosdb private-endpoint-connection reject [--account-name]\n                                               [--description]\n                                               [--id]\n                                               [--ids]\n                                               [--name]\n                                               [--resource-group]\n                                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--description",
                "--id",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az cosmosdb private-endpoint-connection reject --account-name MyAccount --name MyPrivateEndpoint --resource-group MyResourceGroup --description \"Rejected\""
            ]
        },
