{
    "command_group": "az cosmosdb private-endpoint-connection",
    "description": "Manage Azure Cosmos DB private endpoint connections.",
    "status": "GA",
    "commands": [
        {
            "name": "az cosmosdb private-endpoint-connection approve",
            "summary": "Approve the specified private endpoint connection associated with Azure Cosmos DB.",
            "syntax": "az cosmosdb private-endpoint-connection approve [--account-name]\n                                                [--description]\n                                                [--id]\n                                                [--ids]\n                                                [--name]\n                                                [--resource-group]\n                                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--description",
                "--id",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az cosmosdb private-endpoint-connection approve --account-name MyAccount --name MyPrivateEndpoint --resource-group MyResourceGroup --description \"Approved\""
            ]
        },
        {
            "name": "az cosmosdb private-endpoint-connection delete",
            "summary": "Delete the specified private endpoint connection associated with Azure Cosmos DB.",
            "syntax": "az cosmosdb private-endpoint-connection delete [--account-name]\n                                               [--id]\n                                               [--ids]\n                                               [--name]\n                                               [--resource-group]\n                                               [--subscription]",
