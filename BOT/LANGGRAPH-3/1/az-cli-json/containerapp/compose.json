{
    "command_group": "az containerapp compose",
    "description": "Commands to create Azure Container Apps from Compose specifications.",
    "status": "GA",
    "commands": [
        {
            "name": "az containerapp compose create",
            "summary": "Create one or more Container Apps in a new or existing Container App Environment from a Compose specification.",
            "syntax": "az containerapp compose create --environment\n                               --resource-group\n                               [--compose-file-path]\n                               [--location]\n                               [--registry-password]\n                               [--registry-server]\n                               [--registry-username]\n                               [--tags]\n                               [--transport]\n                               [--transport-mapping]",
            "required_parameters": [
                "--environment",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--compose-file-path -f",
                "--location -l",
                "--registry-password",
                "--registry-server",
                "--registry-username",
                "--tags",
                "--transport",
                "--transport-mapping"
            ],
            "examples": [
                "az containerapp compose create -g MyResourceGroup \\\n    --environment MyContainerappEnv",
                "az containerapp compose create -g MyResourceGroup \\\n    --environment MyContainerappEnv \\\n    --compose-file-path \"path/to/docker-compose.yml\""
            ]
        },
        {
            "name": "az containerapp compose create",
            "summary": "Create one or more Container Apps in a new or existing Container App Environment from a Compose specification.",
            "syntax": "az containerapp compose create --environment\n                               --resource-group\n                               [--compose-file-path]\n                               [--location]\n                               [--registry-password]\n                               [--registry-server]\n                               [--registry-username]\n                               [--tags]\n                               [--transport]\n                               [--transport-mapping]",
            "required_parameters": [
                "--environment",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--compose-file-path -f",
                "--location -l",
                "--registry-password",
                "--registry-server",
                "--registry-username",
                "--tags",
                "--transport",
                "--transport-mapping"
            ],
            "examples": [
                "az containerapp compose create -g MyResourceGroup \\\n    --environment MyContainerappEnv",
                "az containerapp compose create -g MyResourceGroup \\\n    --environment MyContainerappEnv \\\n    --compose-file-path \"path/to/docker-compose.yml\""
            ]
        }
    ]
}