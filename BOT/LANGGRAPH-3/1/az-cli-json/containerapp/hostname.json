{
    "command_group": "az containerapp hostname",
    "description": "Commands to manage hostnames of a container app.",
    "status": "GA",
    "commands": [
        {
            "name": "az containerapp hostname add",
            "summary": "Add the hostname to a container app without binding.",
            "syntax": "az containerapp hostname add --hostname\n                             [--ids]\n                             [--location]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [
                "--hostname"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az containerapp hostname add -n my-containerapp -g MyResourceGroup --hostname MyHostname"
            ]
        },
        {
            "name": "az containerapp hostname bind",
            "summary": "Add or update the hostname and binding with a certificate.",
            "syntax": "az containerapp hostname bind --hostname\n                              [--certificate]\n                              [--environment]\n                              [--ids]\n                              [--location]\n                              [--name]\n                              [--resource-group]\n                              [--subscription]\n                              [--thumbprint]\n                              [--validation-method]",
            "required_parameters": [
                "--hostname"
            ],
            "optional_parameters": [
                "--certificate -c",
                "--environment -e",
                "--ids",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--thumbprint -t",
                "--validation-method -v"
            ],
            "examples": [
                "az containerapp hostname bind -n my-containerapp -g MyResourceGroup --hostname MyHostname --certificate MyCertificateId",
                "az containerapp hostname bind -n my-containerapp -g MyResourceGroup --hostname MyHostname"
            ]
        },
        {
            "name": "az containerapp hostname bind",
            "summary": "Add or update the hostname and binding with a certificate.",
            "syntax": "az containerapp hostname bind --hostname\n                              [--certificate]\n                              [--environment]\n                              [--ids]\n                              [--location]\n                              [--name]\n                              [--resource-group]\n                              [--subscription]\n                              [--thumbprint]\n                              [--validation-method]",
            "required_parameters": [
                "--hostname"
            ],
            "optional_parameters": [
                "--certificate -c",
                "--environment -e",
                "--ids",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--thumbprint -t",
                "--validation-method -v"
            ],
            "examples": [
                "az containerapp hostname bind -n my-containerapp -g MyResourceGroup --hostname MyHostname --certificate MyCertificateId",
                "az containerapp hostname bind -n my-containerapp -g MyResourceGroup --hostname MyHostname"
            ]
        },
        {
            "name": "az containerapp hostname delete",
            "summary": "Delete hostnames from a container app.",
            "syntax": "az containerapp hostname delete --hostname\n                                [--ids]\n                                [--location]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]\n                                [--yes]",
            "required_parameters": [
                "--hostname"
            ],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az containerapp hostname delete -n my-containerapp -g MyResourceGroup --hostname MyHostname"
            ]
        },
        {
            "name": "az containerapp hostname list",
            "summary": "List the hostnames of a container app.",
            "syntax": "az containerapp hostname list --name\n                              --resource-group\n                              [--location]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--location -l"
            ],
            "examples": [
                "az containerapp hostname list -n my-containerapp -g MyResourceGroup"
            ]
        }
    ]
}