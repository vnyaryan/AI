{
    "command_group": "az spring service-registry",
    "description": "(Enterprise Tier Only) Commands to manage Service Registry in Azure Spring Apps.",
    "status": "GA",
    "commands": [
        {
            "name": "az spring service-registry bind",
            "summary": "Bind an app or a job to Service Registry.",
            "syntax": "az spring service-registry bind --resource-group\n                                --service\n                                [--app]\n                                [--job]",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--app",
                "--job"
            ],
            "examples": [
                "az spring service-registry bind --app MyApp -s MyService -g MyResourceGroup",
                "az spring service-registry bind --job job-name -s MyService -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring service-registry create",
            "summary": "Create Service Registry.",
            "syntax": "az spring service-registry create --resource-group\n                                  --service",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az spring service-registry create -s MyService -g MyResourceGroup"
            ]
        },
        {
            "name": "az spring service-registry delete",
            "summary": "Delete Service Registry.",
            "syntax": "az spring service-registry delete --resource-group\n                                  --service\n                                  [--yes]",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az spring service-registry show",
            "summary": "Show the provisioning status and runtime status of Service Registry.",
            "syntax": "az spring service-registry show --resource-group\n                                --service",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az spring service-registry unbind",
            "summary": "Unbind an app or a job from Service Registry.",
            "syntax": "az spring service-registry unbind --resource-group\n                                  --service\n                                  [--app]\n                                  [--job]",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--app",
                "--job"
            ],
            "examples": [
                "az spring service-registry unbind --app MyApp -s MyService -g MyResourceGroup",
                "az spring service-registry unbind --job job-name -s MyService -g MyResourceGroup"
            ]
        }
    ]
}