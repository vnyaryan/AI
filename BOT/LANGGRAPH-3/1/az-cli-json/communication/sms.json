{
    "command_group": "az communication sms",
    "description": "Commands to manage SMS for a Communication Service resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az communication sms send",
            "summary": "Sends an SMS from the sender phone number to the recipient(s) phone number(s).",
            "syntax": "az communication sms send --message\n                          --recipient\n                          --sender\n                          [--connection-string]",
            "required_parameters": [
                "--message",
                "--recipient",
                "--sender"
            ],
            "optional_parameters": [
                "--connection-string"
            ],
            "examples": [
                "az communication sms send --sender \"+1833xxxxxxx\" --recipient \"+1425xxxxxxx\" \"+1426xxxxxxx\" \"+1427xxxxxxx\" --message \"Hello there!!\""
            ]
        }
    ]
}