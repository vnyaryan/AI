{
    "command_group": "az mariadb server-logs",
    "description": "Manage server logs.",
    "status": "GA",
    "commands": [
        {
            "name": "az mariadb server-logs download",
            "summary": "Download log files.",
            "syntax": "az mariadb server-logs download --name\n                                [--ids]\n                                [--resource-group]\n                                [--server-name]\n                                [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az mariadb server-logs download -g testgroup -s testsvr -n f1.log f2.log"
            ]
        },
        {
            "name": "az mariadb server-logs list",
            "summary": "List log files for a server.",
            "syntax": "az mariadb server-logs list --resource-group\n                            --server-name\n                            [--file-last-written]\n                            [--filename-contains]\n                            [--max-file-size]",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--file-last-written",
                "--filename-contains",
                "--max-file-size"
            ],
            "examples": [
                "az mariadb server-logs list -g testgroup -s testsvr",
                "az mariadb server-logs list -g testgroup -s testsvr --file-last-written 10",
                "az mariadb server-logs list -g testgroup -s testsvr --max-file-size 30"
            ]
        }
    ]
}