{
    "command_group": "az customlocation",
    "description": "Commands to Create, Get, List and Delete CustomLocations.",
    "status": "GA",
    "commands": [
        {
            "name": "az customlocation create",
            "summary": "Create a Custom Location.",
            "syntax": "az customlocation create --cluster-extension-ids\n                         --host-resource-id\n                         --name\n                         --namespace\n                         --resource-group\n                         [--assign-identity]\n                         [--kubeconfig]\n                         [--location]\n                         [--tags]",
            "required_parameters": [
                "--cluster-extension-ids -c",
                "--host-resource-id",
                "--name -n",
                "--namespace",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--assign-identity",
                "--kubeconfig -k",
                "--location -l",
                "--tags"
            ],
            "examples": []
        },
        {
            "name": "az customlocation delete",
            "summary": "Delete a Customlocation.",
            "syntax": "az customlocation delete --name\n                         --resource-group\n                         [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az customlocation list",
            "summary": "Command to list CustomLocations.",
            "syntax": "az customlocation list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az customlocation list-enabled-resource-types",
            "summary": "Get details of Enabled Resource Types for a CustomLocation.",
            "syntax": "az customlocation list-enabled-resource-types --name\n                                              --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az customlocation patch",
            "summary": "Patch a Custom Location.",
            "syntax": "az customlocation patch --name\n                        --resource-group\n                        [--assign-identity]\n                        [--cluster-extension-ids]\n                        [--display-name]\n                        [--host-resource-id]\n                        [--location]\n                        [--namespace]\n                        [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--assign-identity",
                "--cluster-extension-ids -c",
                "--display-name -d",
                "--host-resource-id",
                "--location -l",
                "--namespace",
                "--tags"
            ],
            "examples": []
        },
        {
            "name": "az customlocation show",
            "summary": "Get details of a Customlocation.",
            "syntax": "az customlocation show --name\n                       --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az customlocation update",
            "summary": "Update a Custom Location.",
            "syntax": "az customlocation update --cluster-extension-ids\n                         --host-resource-id\n                         --name\n                         --namespace\n                         --resource-group\n                         [--assign-identity]\n                         [--location]\n                         [--tags]",
            "required_parameters": [
                "--cluster-extension-ids -c",
                "--host-resource-id",
                "--name -n",
                "--namespace",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--assign-identity",
                "--location -l",
                "--tags"
            ],
            "examples": []
        }
    ]
}