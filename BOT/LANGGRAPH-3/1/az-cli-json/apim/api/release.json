{
    "command_group": "az apim api release",
    "description": "Manage Azure API Management API Release.",
    "status": "GA",
    "commands": [
        {
            "name": "az apim api release create",
            "summary": "Creates a new Release for the API.",
            "syntax": "az apim api release create --api-id\n                           --api-revision\n                           --resource-group\n                           --service-name\n                           [--if-match]\n                           [--notes]\n                           [--release-id]",
            "required_parameters": [
                "--api-id",
                "--api-revision",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--if-match",
                "--notes",
                "--release-id"
            ],
            "examples": [
                "az apim api release create --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId --release-id MyReleaseId --api-revision 2 --notes MyNotes"
            ]
        },
        {
            "name": "az apim api release delete",
            "summary": "Deletes the specified release in the API.",
            "syntax": "az apim api release delete --api-id\n                           --release-id\n                           --resource-group\n                           --service-name\n                           [--if-match]",
            "required_parameters": [
                "--api-id",
                "--release-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--if-match"
            ],
            "examples": [
                "az apim api release delete --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId --release-id MyReleaseId"
            ]
        },
        {
            "name": "az apim api release list",
            "summary": "Lists all releases of an API.",
            "syntax": "az apim api release list --api-id\n                         --resource-group\n                         --service-name",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az apim api release list --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId"
            ]
        },
        {
            "name": "az apim api release show",
            "summary": "Returns the details of an API release.",
            "syntax": "az apim api release show --api-id\n                         --release-id\n                         --resource-group\n                         --service-name",
            "required_parameters": [
                "--api-id",
                "--release-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az apim api release show --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId --release-id MyReleaseId"
            ]
        },
        {
            "name": "az apim api release update",
            "summary": "Updates the details of the release of the API specified by its identifier.",
            "syntax": "az apim api release update --api-id\n                           --release-id\n                           --resource-group\n                           --service-name\n                           [--add]\n                           [--force-string]\n                           [--if-match]\n                           [--notes]\n                           [--remove]\n                           [--set]",
            "required_parameters": [
                "--api-id",
                "--release-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--if-match",
                "--notes",
                "--remove",
                "--set"
            ],
            "examples": [
                "az apim api release update --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId --release-id MyReleaseId --notes MyNewNotes"
            ]
        }
    ]
}