{
    "command_group": "az apim api schema",
    "description": "Manage Azure API Management API Schema's.",
    "status": "GA",
    "commands": [
        {
            "name": "az apim api schema create",
            "summary": "Create an API Management API Schema.",
            "syntax": "az apim api schema create --api-id\n                          --resource-group\n                          --schema-id\n                          --schema-type\n                          --service-name\n                          [--no-wait]\n                          [--resource-type]\n                          [--schema-content]\n                          [--schema-name]\n                          [--schema-path]",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--schema-id",
                "--schema-type",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--no-wait",
                "--resource-type",
                "--schema-content",
                "--schema-name",
                "--schema-path"
            ],
            "examples": [
                "az apim api schema create --service-name MyApim -g MyResourceGroup --api-id MyApi --schema-id schemaId --schema-type schemaType --schema-path schemaFilePath"
            ]
        },
        {
            "name": "az apim api schema delete",
            "summary": "Delete an API Management API Schema.",
            "syntax": "az apim api schema delete --api-id\n                          --resource-group\n                          --schema-id\n                          --service-name\n                          [--if-match]\n                          [--no-wait]\n                          [--yes]",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--schema-id",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--if-match",
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az apim api schema delete --service-name MyApim -g MyResourceGroup --api-id MyApi --schema-id schemaId"
            ]
        },
        {
            "name": "az apim api schema get-etag",
            "summary": "Get etag of an API Management API schema.",
            "syntax": "az apim api schema get-etag --api-id\n                            --resource-group\n                            --schema-id\n                            --service-name",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--schema-id",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az apim api schema get-etag --service-name MyApim -g MyResourceGroup --api-id MyApi --schema-id schemaId"
            ]
        },
        {
            "name": "az apim api schema list",
            "summary": "List API Management API schema's.",
            "syntax": "az apim api schema list --api-id\n                        --resource-group\n                        --service-name\n                        [--filter-display-name]\n                        [--skip]\n                        [--top]",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--filter-display-name",
                "--skip",
                "--top"
            ],
            "examples": [
                "az apim api schema list --service-name MyApim -g MyResourceGroup --api-id MyApi"
            ]
        },
        {
            "name": "az apim api schema show",
            "summary": "Show details of an API Management API Schema.",
            "syntax": "az apim api schema show --api-id\n                        --resource-group\n                        --schema-id\n                        --service-name",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--schema-id",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az apim api schema show --service-name MyApim -g MyResourceGroup --api-id MyApi --schema-id schemaId"
            ]
        },
        {
            "name": "az apim api schema wait",
            "summary": "Place the CLI in a waiting state until a condition of an apim api schema is met.",
            "syntax": "az apim api schema wait --api-id\n                        --name\n                        --resource-group\n                        --schema-id\n                        [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--interval]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [
                "--api-id",
                "--name -n",
                "--resource-group -g",
                "--schema-id"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az apim api schema wait --created --api-id MyApi --name MyApim --schema-id schemaId -g MyResourceGroup"
            ]
        }
    ]
}