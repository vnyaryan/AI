{
    "command_group": "az aksarc vnet",
    "description": "Manage virtual networks for provisioned clusters.",
    "status": "GA",
    "commands": [
        {
            "name": "az aksarc vnet create",
            "summary": "Creates the virtual network.",
            "syntax": "az aksarc vnet create --custom-location\n                      --name\n                      --resource-group\n                      [--location]\n                      [--moc-group]\n                      [--moc-location]\n                      [--moc-vnet-name]\n                      [--no-wait]\n                      [--tags]",
            "required_parameters": [
                "--custom-location",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--location -l",
                "--moc-group",
                "--moc-location",
                "--moc-vnet-name",
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az aksarc vnet create --name \"test-vnet-static\" --resource-group \"test-arcappliance-resgrp\" --custom-location \"testcustomlocation\" --moc-vnet-name \"test-vnet\""
            ]
        },
        {
            "name": "az aksarc vnet delete",
            "summary": "Deletes the virtual network.",
            "syntax": "az aksarc vnet delete --name\n                      --resource-group\n                      [--no-wait]\n                      [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az aksarc vnet delete --resource-group \"test-arcappliance-resgrp\" --name \"test-vnet-static\""
            ]
        },
        {
            "name": "az aksarc vnet list",
            "summary": "Lists the virtual networks by resource group or subscription.",
            "syntax": "az aksarc vnet list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az aksarc vnet list --resource-group \"test-arcappliance-resgrp\"",
                "az aksarc vnet list"
            ]
        },
        {
            "name": "az aksarc vnet show",
            "summary": "Gets the virtual network.",
            "syntax": "az aksarc vnet show --name\n                    --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az aksarc vnet show --resource-group \"test-arcappliance-resgrp\" --name \"test-vnet-static\""
            ]
        }
    ]
}