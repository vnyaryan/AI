{
    "command_group": "az blueprint",
    "description": "Commands to manage blueprint.",
    "status": "GA",
    "commands": [
        {
            "name": "az blueprint create",
            "summary": "Create a blueprint definition.",
            "syntax": "az blueprint create --name\n                    --target-scope {managementGroup, subscription}\n                    [--description]\n                    [--display-name]\n                    [--management-group]\n                    [--parameters]\n                    [--resource-groups]\n                    [--subscription]",
            "required_parameters": [
                "--name -n",
                "--target-scope"
            ],
            "optional_parameters": [
                "--description",
                "--display-name",
                "--management-group -m",
                "--parameters -p",
                "--resource-groups",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint create --subscription MySubscription --name MyBlueprint --description \"blueprint contains all artifacts\" --target-scope subscription",
                "az blueprint create --management-group MyManagementGroup --name MyBlueprint --description \"blueprint contains all artifact\" --target-scope subscription"
            ]
        },
        {
            "name": "az blueprint delete",
            "summary": "Delete a blueprint definition.",
            "syntax": "az blueprint delete --name\n                    [--management-group]\n                    [--subscription]\n                    [--yes]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--management-group -m",
                "--subscription -s",
                "--yes -y"
            ],
            "examples": [
                "az blueprint delete --management-group MyManagementGroup --name MyBlueprint",
                "az blueprint delete --subscription MySubscription --name MyBlueprint"
            ]
        },
        {
            "name": "az blueprint export",
            "summary": "Export a blueprint definition and artifacts to json file(s).",
            "syntax": "az blueprint export --name\n                    --output-path\n                    [--management-group]\n                    [--subscription]\n                    [--yes]",
            "required_parameters": [
                "--name -n",
                "--output-path"
            ],
            "optional_parameters": [
                "--management-group -m",
                "--subscription -s",
                "--yes -y"
            ],
            "examples": [
                "az blueprint export --name MyBlueprint \\\n--output-path \"path/to/blueprint/directory\""
            ]
        },
        {
            "name": "az blueprint import",
            "summary": "Import a blueprint definition and artifacts from a directoy of json files.",
            "syntax": "az blueprint import --input-path\n                    --name\n                    [--management-group]\n                    [--subscription]\n                    [--yes]",
            "required_parameters": [
                "--input-path",
                "--name -n"
            ],
            "optional_parameters": [
                "--management-group -m",
                "--subscription -s",
                "--yes -y"
            ],
            "examples": [
                "az blueprint import --name MyBlueprint \\\n--input-path \"path/to/blueprint/directory\""
            ]
        },
        {
            "name": "az blueprint list",
            "summary": "List blueprint definitions.",
            "syntax": "az blueprint list [--management-group]\n                  [--max-items]\n                  [--next-token]\n                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--management-group -m",
                "--max-items",
                "--next-token",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint list --management-group MyManagementGroup",
                "az blueprint list --subscription MySubscription"
            ]
        },
        {
            "name": "az blueprint publish",
            "summary": "Publish a new version of the blueprint definition with the latest artifacts. Published blueprint definitions are immutable.",
            "syntax": "az blueprint publish --blueprint-name\n                     --version\n                     [--change-notes]\n                     [--management-group]\n                     [--subscription]",
            "required_parameters": [
                "--blueprint-name",
                "--version"
            ],
            "optional_parameters": [
                "--change-notes",
                "--management-group -m",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint publish --management-group MyManagementGroup --blueprint-name MyBlueprint --version v2",
                "az blueprint publish --subscription MySubscription --blueprint-name MyBlueprint --version v2"
            ]
        },
        {
            "name": "az blueprint show",
            "summary": "Get a blueprint definition.",
            "syntax": "az blueprint show --name\n                  [--management-group]\n                  [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--management-group -m",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint show --management-group MyManagementGroup --name MyBlueprint",
                "az blueprint show --subscription MySubscription --name MyBlueprint"
            ]
        },
        {
            "name": "az blueprint update",
            "summary": "Update a blueprint definition.",
            "syntax": "az blueprint update --name\n                    [--add]\n                    [--description]\n                    [--display-name]\n                    [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                    [--management-group]\n                    [--parameters]\n                    [--remove]\n                    [--resource-groups]\n                    [--set]\n                    [--subscription]\n                    [--target-scope {managementGroup, subscription}]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--add",
                "--description",
                "--display-name",
                "--force-string",
                "--management-group -m",
                "--parameters -p",
                "--remove",
                "--resource-groups",
                "--set",
                "--subscription -s",
                "--target-scope"
            ],
            "examples": []
        }
    ]
}