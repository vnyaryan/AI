{
    "command_group": "az sql mi server-configuration-option",
    "description": "Server configuration option management commands.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql mi server-configuration-option list",
            "summary": "List a list of managed instance server configuration options.",
            "syntax": "az sql mi server-configuration-option list --instance-name\n                                           --resource-group",
            "required_parameters": [
                "--instance-name --managed-instance --managed-instance-name --mi",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql mi server-configuration-option list -g 'ResourceGroup_1' --mi 'ManagedInstance_1'"
            ]
        },
        {
            "name": "az sql mi server-configuration-option set",
            "summary": "Set managed instance server configuration option.",
            "syntax": "az sql mi server-configuration-option set [--ids]\n                                          [--instance-name]\n                                          [--name {allowPolybaseExport}]\n                                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                          [--resource-group]\n                                          [--server-configuration-option-value]\n                                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--instance-name --managed-instance --managed-instance-name --mi",
                "--name --server-configuration-option-name -n",
                "--no-wait",
                "--resource-group -g",
                "--server-configuration-option-value --value",
                "--subscription"
            ],
            "examples": [
                "az mi server-configuration-option set -g 'ResourceGroup_1' --mi 'ManagedInstance_1' --name 'allowPolybaseExport' --value '1'"
            ]
        },
        {
            "name": "az sql mi server-configuration-option show",
            "summary": "Get managed instance server configuration option.",
            "syntax": "az sql mi server-configuration-option show [--ids]\n                                           [--instance-name]\n                                           [--name {allowPolybaseExport}]\n                                           [--resource-group]\n                                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--instance-name --managed-instance --managed-instance-name --mi",
                "--name --server-configuration-option-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az sql mi server-configuration-option show -g 'ResourceGroup_1' --mi 'ManagedInstance_1' --name allowPolybaseExport"
            ]
        },
        {
            "name": "az sql mi server-configuration-option wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az sql mi server-configuration-option wait [--created]\n                                           [--custom]\n                                           [--deleted]\n                                           [--exists]\n                                           [--ids]\n                                           [--instance-name]\n                                           [--interval]\n                                           [--name {allowPolybaseExport}]\n                                           [--resource-group]\n                                           [--subscription]\n                                           [--timeout]\n                                           [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--instance-name --managed-instance --managed-instance-name --mi",
                "--interval",
                "--name --server-configuration-option-name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}