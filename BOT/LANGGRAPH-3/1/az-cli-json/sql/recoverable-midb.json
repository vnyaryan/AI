{
    "command_group": "az sql recoverable-midb",
    "description": "Recoverable managed databases command group.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql recoverable-midb list",
            "summary": "Get all recoverable managed databases for given instance name.",
            "syntax": "az sql recoverable-midb list --instance-name\n                             --resource-group",
            "required_parameters": [
                "--instance-name --mi",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql recoverable-midb list -g mygroup --mi myinstance"
            ]
        },
        {
            "name": "az sql recoverable-midb show",
            "summary": "Get recoverable managed database.",
            "syntax": "az sql recoverable-midb show --database-name\n                             --instance-name\n                             --resource-group",
            "required_parameters": [
                "--database-name -n",
                "--instance-name --mi",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql recoverable-midb show -g mygroup --mi myinstance -n mymanageddb"
            ]
        }
    ]
}