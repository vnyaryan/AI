{
    "command_group": "az sql server-arc backups-policy",
    "description": "Manage backups policy.",
    "status": "Preview",
    "commands": [
        {
            "name": "az sql server-arc backups-policy delete",
            "summary": "Delete your current backups policy.",
            "syntax": "az sql server-arc backups-policy delete --name\n                                        --resource-group\n                                        [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az sql server-arc backups-policy delete --name myServerName --resource-group myResourceGroup"
            ]
        },
        {
            "name": "az sql server-arc backups-policy set",
            "summary": "Set your backups policy.",
            "syntax": "az sql server-arc backups-policy set --name\n                                     --resource-group\n                                     [--default-policy]\n                                     [--diff-backup-hours]\n                                     [--full-backup-days]\n                                     [--retention-days]\n                                     [--tlog-backup-mins]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--default-policy",
                "--diff-backup-hours",
                "--full-backup-days",
                "--retention-days",
                "--tlog-backup-mins"
            ],
            "examples": [
                "az sql server-arc backups-policy set --name myServerName --resource-group myResourceGroup --retention-days 7 --full-backup-days 7 --diff-backup-hours 12 --tlog-backup-mins 5",
                "az sql server-arc backups-policy set --name myServerName --resource-group myResourceGroup --default-policy",
                "az sql server-arc backups-policy set --name myServerName --resource-group myResourceGroup --retention-days 0"
            ]
        },
        {
            "name": "az sql server-arc backups-policy show",
            "summary": "See your current backups policy.",
            "syntax": "az sql server-arc backups-policy show --name\n                                      --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql server-arc backups-policy show --name myServerName --resource-group myResourceGroup"
            ]
        }
    ]
}