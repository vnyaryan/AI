{
    "command_group": "az databoxedge",
    "description": "Manage device with databoxedge.",
    "status": "Preview",
    "commands": [
        {
            "name": "az databoxedge list-node",
            "summary": "Get all the nodes currently configured under this Data Box Edge device.",
            "syntax": "az databoxedge list-node --device-name\n                         --resource-group",
            "required_parameters": [
                "--device-name -d",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az databoxedge list-node --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\""
            ]
        },
        {
            "name": "az databoxedge list-sku",
            "summary": "List all the available Skus in the region and information related to them.",
            "syntax": "az databoxedge list-sku [--filter]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter"
            ],
            "examples": [
                "az databoxedge list-sku"
            ]
        },
        {
            "name": "az databoxedge show-job",
            "summary": "Get the details of a specified job on a Data Box Edge/Data Box Gateway device.",
            "syntax": "az databoxedge show-job [--device-name]\n                        [--ids]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--device-name -d",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az databoxedge show-job --name \"159a00c7-8543-4343-9435-263ac87df3bb\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\""
            ]
        }
    ]
}