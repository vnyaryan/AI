{
    "command_group": "az acr credential-set",
    "description": "Manage credential sets in Azure Container Registries.",
    "status": "GA",
    "commands": [
        {
            "name": "az acr credential-set create",
            "summary": "Create a credential set.",
            "syntax": "az acr credential-set create --login-server\n                             --name\n                             --password-id\n                             --registry\n                             --username-id\n                             [--resource-group]",
            "required_parameters": [
                "--login-server -l",
                "--name -n",
                "--password-id -p",
                "--registry -r",
                "--username-id -u"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az acr credential-set create -r myregistry -n MyDockerHubCredSet -l docker.io -u https://MyKeyvault.vault.azure.net/secrets/usernamesecret -p https://MyKeyvault.vault.azure.net/secrets/passwordsecret"
            ]
        },
        {
            "name": "az acr credential-set delete",
            "summary": "Delete a credential set.",
            "syntax": "az acr credential-set delete --name\n                             --registry\n                             [--resource-group]\n                             [--yes]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g",
                "--yes -y"
            ],
            "examples": [
                "az acr credential-set delete -r myregistry -n MyCredSet"
            ]
        },
        {
            "name": "az acr credential-set list",
            "summary": "List the credential sets in an Azure Container Registry.",
            "syntax": "az acr credential-set list --registry\n                           [--resource-group]",
            "required_parameters": [
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az acr credential-set list -r myregistry"
            ]
        },
        {
            "name": "az acr credential-set show",
            "summary": "Show a credential set.",
            "syntax": "az acr credential-set show --name\n                           --registry\n                           [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az acr credential-set show -r myregistry -n MyCredSet"
            ]
        },
        {
            "name": "az acr credential-set update",
            "summary": "Update the username or password Azure Key Vault secret ID on a credential set.",
            "syntax": "az acr credential-set update --name\n                             --registry\n                             [--add]\n                             [--force-string]\n                             [--password-id]\n                             [--remove]\n                             [--resource-group]\n                             [--set]\n                             [--username-id]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--password-id -p",
                "--remove",
                "--resource-group -g",
                "--set",
                "--username-id -u"
            ],
            "examples": [
                "az acr credential-set update -r myregistry -n MyDockerHubCredSet -p https://MyKeyvault.vault.azure.net/secrets/newsecretname"
            ]
        }
    ]
}