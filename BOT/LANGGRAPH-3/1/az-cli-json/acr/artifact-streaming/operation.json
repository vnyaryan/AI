{
    "command_group": "az acr artifact-streaming operation",
    "description": "Manage the streaming artifact creation operations for ACR.",
    "status": "Preview",
    "commands": [
        {
            "name": "az acr artifact-streaming operation cancel",
            "summary": "Cancel the given streaming artifact operation for ACR.",
            "syntax": "az acr artifact-streaming operation cancel --name\n                                           [--id]\n                                           [--image]\n                                           [--password]\n                                           [--repository]\n                                           [--suffix]\n                                           [--username]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--id",
                "--image -t",
                "--password -p",
                "--repository",
                "--suffix",
                "--username -u"
            ],
            "examples": [
                "az acr artifact-streaming operation cancel -n myregistry --repository MyRepo --id OperationId"
            ]
        },
        {
            "name": "az acr artifact-streaming operation show",
            "summary": "Check the operation status for artifact streaming in an ACR.",
            "syntax": "az acr artifact-streaming operation show --name\n                                         [--id]\n                                         [--image]\n                                         [--password]\n                                         [--repository]\n                                         [--suffix]\n                                         [--username]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--id",
                "--image -t",
                "--password -p",
                "--repository",
                "--suffix",
                "--username -u"
            ],
            "examples": [
                "az acr artifact-streaming operation show -n myregistry --repository MyRepo --id OperationId"
            ]
        }
    ]
}