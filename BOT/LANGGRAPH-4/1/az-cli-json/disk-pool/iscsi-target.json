{
    "command_group": "az disk-pool iscsi-target",
    "description": "Manage iSCSI target with a Disk Pool.",
    "status": "GA",
    "commands": [
        {
            "name": "az disk-pool iscsi-target create",
            "summary": "Create an iSCSI Target.",
            "syntax": "az disk-pool iscsi-target create --acl-mode {Dynamic, Static}\n                                 --disk-pool-name\n                                 --iscsi-target-name\n                                 --resource-group\n                                 [--luns]\n                                 [--managed-by]\n                                 [--managed-by-extended]\n                                 [--no-wait]\n                                 [--static-acls]\n                                 [--target-iqn]",
            "required_parameters": [
                "--acl-mode",
                "--disk-pool-name",
                "--iscsi-target-name --name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--luns",
                "--managed-by",
                "--managed-by-extended",
                "--no-wait",
                "--static-acls",
                "--target-iqn"
            ],
            "examples": [
                "az disk-pool iscsi-target create --disk-pool-name \"myDiskPool\" --acl-mode \"Dynamic\" --luns name=\"lun0\" managed-disk-azure-resource-id=\"/subscriptions/11111111-1111-1111-1111-111111111111/resourceGroups/myResourceGroup/providers/Microsoft.Compute/disks/vm-name_DataDisk_1\" --target-iqn \"iqn.2005-03.org.iscsi:server1\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az disk-pool iscsi-target delete",
            "summary": "Delete an iSCSI Target.",
            "syntax": "az disk-pool iscsi-target delete [--disk-pool-name]\n                                 [--ids]\n                                 [--iscsi-target-name]\n                                 [--no-wait]\n                                 [--resource-group]\n                                 [--subscription]\n                                 [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--disk-pool-name",
                "--ids",
                "--iscsi-target-name --name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az disk-pool iscsi-target delete --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az disk-pool iscsi-target list",
            "summary": "Get iSCSI Targets in a Disk pool.",
            "syntax": "az disk-pool iscsi-target list --disk-pool-name\n                               --resource-group",
            "required_parameters": [
                "--disk-pool-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az disk-pool iscsi-target list --disk-pool-name \"myDiskPool\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az disk-pool iscsi-target show",
            "summary": "Get an iSCSI Target.",
            "syntax": "az disk-pool iscsi-target show [--disk-pool-name]\n                               [--ids]\n                               [--iscsi-target-name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--disk-pool-name",
                "--ids",
                "--iscsi-target-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az disk-pool iscsi-target show --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az disk-pool iscsi-target update",
            "summary": "Update an iSCSI Target.",
            "syntax": "az disk-pool iscsi-target update [--disk-pool-name]\n                                 [--ids]\n                                 [--iscsi-target-name]\n                                 [--luns]\n                                 [--managed-by]\n                                 [--managed-by-extended]\n                                 [--no-wait]\n                                 [--resource-group]\n                                 [--static-acls]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--disk-pool-name",
                "--ids",
                "--iscsi-target-name --name -n",
                "--luns",
                "--managed-by",
                "--managed-by-extended",
                "--no-wait",
                "--resource-group -g",
                "--static-acls",
                "--subscription"
            ],
            "examples": [
                "az disk-pool iscsi-target update --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --luns name=\"lun0\" managed-disk-azure-resource-id=\"/subscriptions/11111111-1111-1111-1111-111111111111/resourceGroups/myResourceGroup/providers/Microsoft.Compute/disks/vm-name_DataDisk_1\""
            ]
        },
        {
            "name": "az disk-pool iscsi-target wait",
            "summary": "Place the CLI in a waiting state until a condition of the disk-pool iscsi-target is met.",
            "syntax": "az disk-pool iscsi-target wait [--created]\n                               [--custom]\n                               [--deleted]\n                               [--disk-pool-name]\n                               [--exists]\n                               [--ids]\n                               [--interval]\n                               [--iscsi-target-name]\n                               [--resource-group]\n                               [--subscription]\n                               [--timeout]\n                               [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--disk-pool-name",
                "--exists",
                "--ids",
                "--interval",
                "--iscsi-target-name --name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az disk-pool iscsi-target wait --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\" --created",
                "az disk-pool iscsi-target wait --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\" --updated",
                "az disk-pool iscsi-target wait --disk-pool-name \"myDiskPool\" --name \"myIscsiTarget\" --resource-group \"myResourceGroup\" --deleted"
            ]
        }
    ]
}