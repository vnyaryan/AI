            "name": "az managed-cassandra datacenter create",
            "summary": "Create a Datacenter in an Azure Managed Cassandra Cluster.",
            "syntax": "az managed-cassandra datacenter create --cluster-name\n                                       --data-center-location\n                                       --data-center-name\n                                       --delegated-subnet-id\n                                       --node-count\n                                       --resource-group\n                                       [--availability-zone {false, true}]\n                                       [--backup-storage-customer-key-uri]\n                                       [--base64-encoded-cassandra-yaml-fragment]\n                                       [--disk-capacity]\n                                       [--disk-sku]\n                                       [--managed-disk-customer-key-uri]\n                                       [--no-wait]\n                                       [--sku]",
            "required_parameters": [
                "--cluster-name -c",
                "--data-center-location -l",
                "--data-center-name -d",
                "--delegated-subnet-id -s",
                "--node-count -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--availability-zone -z",
                "--backup-storage-customer-key-uri -p",
                "--base64-encoded-cassandra-yaml-fragment -b",
                "--disk-capacity",
                "--disk-sku",
                "--managed-disk-customer-key-uri -k",
                "--no-wait",
                "--sku"
            ],
            "examples": [
                "az managed-cassandra datacenter create \\\n--resource-group MyResourceGroup \\\n--cluster-name MyCluster \\\n--data-center-name MyDataCenter \\\n--data-center-location westus2 \\\n--node-count 3 \\\n--delegated-subnet-id /subscriptions/94d9b402-77b4-4049-b4c1-947bc6b7729b/resourceGroups/My-vnet/providers/Microsoft.Network/virtualNetworks/test-vnet/subnets/test-subnet"
            ]
