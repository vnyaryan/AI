{
    "command_group": "az ams job",
    "description": "Manage jobs for a transform.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams job cancel",
            "summary": "Cancel a job.",
            "syntax": "az ams job cancel [--account-name]\n                  [--delete]\n                  [--ids]\n                  [--name]\n                  [--resource-group]\n                  [--subscription]\n                  [--transform-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--delete",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--transform-name -t"
            ],
            "examples": []
        },
        {
            "name": "az ams job delete",
            "summary": "Delete a job.",
            "syntax": "az ams job delete [--account-name]\n                  [--ids]\n                  [--name]\n                  [--resource-group]\n                  [--subscription]\n                  [--transform-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--transform-name -t"
            ],
            "examples": []
        },
        {
            "name": "az ams job list",
            "summary": "List all the jobs of a transform within an Azure Media Services account.",
            "syntax": "az ams job list --account-name\n                --resource-group\n                --transform-name\n                [--filter]\n                [--orderby]",
            "required_parameters": [
                "--account-name -a",
                "--resource-group -g",
                "--transform-name -t"
            ],
            "optional_parameters": [
                "--filter",
                "--orderby"
            ],
            "examples": [
                "az ams job list -a amsAccount -g resourceGroup -t transformName --query [?priority=='Normal'].{jobName:name}",
                "az ams job list -a amsAccount -g resourceGroup -t transformName --query [].{jobName:name,jobInput:input}"
            ]
        },
        {
            "name": "az ams job show",
            "summary": "Show the details of a job.",
            "syntax": "az ams job show [--account-name]\n                [--ids]\n                [--name]\n                [--resource-group]\n                [--subscription]\n                [--transform-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--transform-name -t"
            ],
            "examples": []
        },
        {
            "name": "az ams job start",
            "summary": "Start a job.",
            "syntax": "az ams job start --output-assets\n                 [--account-name]\n                 [--base-uri]\n                 [--correlation-data]\n                 [--description]\n                 [--files]\n                 [--ids]\n                 [--input-asset-name]\n                 [--label]\n                 [--name]\n                 [--priority {High, Low, Normal}]\n                 [--resource-group]\n                 [--subscription]\n                 [--transform-name]",
            "required_parameters": [
                "--output-assets"
            ],
            "optional_parameters": [
                "--account-name -a",
                "--base-uri",
                "--correlation-data",
                "--description",
                "--files",
                "--ids",
                "--input-asset-name",
                "--label",
                "--name -n",
                "--priority",
                "--resource-group -g",
                "--subscription",
                "--transform-name -t"
            ],
            "examples": []
        },
        {
            "name": "az ams job update",
            "summary": "Update an existing job.",
            "syntax": "az ams job update [--account-name]\n                  [--add]\n                  [--description]\n                  [--force-string]\n                  [--ids]\n                  [--name]\n                  [--priority {High, Low, Normal}]\n                  [--remove]\n                  [--resource-group]\n                  [--set]\n                  [--subscription]\n                  [--transform-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--add",
                "--description",
                "--force-string",
                "--ids",
                "--name -n",
                "--priority",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--transform-name -t"
            ],
            "examples": []
        }
    ]
}