{
    "command_group": "az stack-hci-vm network nsg",
    "description": "Manage Network Security Groups (NSGs).",
    "status": "GA",
    "commands": [
        {
            "name": "az stack-hci-vm network nsg create",
            "summary": "Create a network security group.",
            "syntax": "az stack-hci-vm network nsg create --custom-location\n                                   --name\n                                   --resource-group\n                                   [--location]\n                                   [--polling-interval]\n                                   [--tags]",
            "required_parameters": [
                "--custom-location",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--location -l",
                "--polling-interval",
                "--tags"
            ],
            "examples": [
                "az stack-hci-vm network nsg create -g MyResourceGroup -n MyNsg --tags foo=bar --custom-location $customLocationId"
            ]
        },
        {
            "name": "az stack-hci-vm network nsg delete",
            "summary": "Delete a network security group.",
            "syntax": "az stack-hci-vm network nsg delete [--ids]\n                                   [--name]\n                                   [--polling-interval]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--polling-interval",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az stack-hci-vm network nsg delete -g MyResourceGroup -n MyNsg"
            ]
        },
        {
            "name": "az stack-hci-vm network nsg list",
            "summary": "List network security groups.",
            "syntax": "az stack-hci-vm network nsg list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az stack-hci-vm network nsg list -g MyResourceGroup"
            ]
        },
        {
            "name": "az stack-hci-vm network nsg show",
            "summary": "Get information about a network security group.",
            "syntax": "az stack-hci-vm network nsg show [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az stack-hci-vm network nsg show -g MyResourceGroup -n MyNsg"
            ]
        },
        {
            "name": "az stack-hci-vm network nsg update",
            "summary": "Update a network security group.",
            "syntax": "az stack-hci-vm network nsg update --name\n                                   --resource-group\n                                   [--add]\n                                   [--force-string]\n                                   [--remove]\n                                   [--set]\n                                   [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--remove",
                "--set",
                "--tags"
            ],
            "examples": [
                "az stack-hci-vm network nsg update -g MyResourceGroup -n MyNsg --tags foo=bar"
            ]
        }
    ]
}