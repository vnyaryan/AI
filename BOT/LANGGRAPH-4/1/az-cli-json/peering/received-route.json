{
    "command_group": "az peering received-route",
    "description": "Peering received-route.",
    "status": "GA",
    "commands": [
        {
            "name": "az peering received-route list",
            "summary": "Lists the prefixes received over the specified peering under the given subscription and resource group.",
            "syntax": "az peering received-route list --peering-name\n                               --resource-group\n                               [--as-path]\n                               [--origin-as-validation-state]\n                               [--prefix]\n                               [--rpki-validation-state]\n                               [--skip-token]",
            "required_parameters": [
                "--peering-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--as-path",
                "--origin-as-validation-state",
                "--prefix",
                "--rpki-validation-state",
                "--skip-token"
            ],
            "examples": [
                "az peering received-route list --as-path \"123 456\" --origin-as-validation-state \"Valid\" --peering-name \"peeringName\" --prefix \"1.1.1.0/24\" --resource-group \"rgName\" --rpki-validation-state \"Valid\""
            ]
        }
    ]
}