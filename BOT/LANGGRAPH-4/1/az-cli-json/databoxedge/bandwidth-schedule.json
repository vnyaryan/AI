{
    "command_group": "az databoxedge bandwidth-schedule",
    "description": "Manage bandwidth schedule with databoxedge.",
    "status": "Preview",
    "commands": [
        {
            "name": "az databoxedge bandwidth-schedule create",
            "summary": "Create a bandwidth schedule.",
            "syntax": "az databoxedge bandwidth-schedule create --days\n                                         --device-name\n                                         --name\n                                         --rate-in-mbps\n                                         --resource-group\n                                         --start\n                                         --stop\n                                         [--no-wait]",
            "required_parameters": [
                "--days",
                "--device-name -d",
                "--name -n",
                "--rate-in-mbps",
                "--resource-group -g",
                "--start",
                "--stop"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az databoxedge bandwidth-schedule create --name \"bandwidth-1\" --device-name \"testedgedevice\" --days \"Sunday\" \"Monday\" --rate-in-mbps 100 --start \"0:0:0\" --stop \"13:59:0\" --resource-group \"GroupForEdgeAutomation\""
            ]
        },
        {
            "name": "az databoxedge bandwidth-schedule delete",
            "summary": "Delete the specified bandwidth schedule.",
            "syntax": "az databoxedge bandwidth-schedule delete [--device-name]\n                                         [--ids]\n                                         [--name]\n                                         [--no-wait]\n                                         [--resource-group]\n                                         [--subscription]\n                                         [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--device-name -d",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az databoxedge bandwidth-schedule delete --name \"bandwidth-1\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\""
            ]
        },
        {
            "name": "az databoxedge bandwidth-schedule list",
            "summary": "Get all the bandwidth schedules for a Data Box Edge/Data Box Gateway device.",
            "syntax": "az databoxedge bandwidth-schedule list --device-name\n                                       --resource-group",
            "required_parameters": [
                "--device-name -d",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az databoxedge bandwidth-schedule list --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\""
            ]
        },
        {
            "name": "az databoxedge bandwidth-schedule show",
            "summary": "Get the properties of the specified bandwidth schedule.",
            "syntax": "az databoxedge bandwidth-schedule show [--device-name]\n                                       [--ids]\n                                       [--name]\n                                       [--resource-group]\n                                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--device-name -d",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az databoxedge bandwidth-schedule show --name \"bandwidth-1\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\""
            ]
        },
        {
            "name": "az databoxedge bandwidth-schedule update",
            "summary": "Update a bandwidth schedule.",
            "syntax": "az databoxedge bandwidth-schedule update --days\n                                         --rate-in-mbps\n                                         --start\n                                         --stop\n                                         [--add]\n                                         [--device-name]\n                                         [--force-string]\n                                         [--ids]\n                                         [--name]\n                                         [--no-wait]\n                                         [--remove]\n                                         [--resource-group]\n                                         [--set]\n                                         [--subscription]",
            "required_parameters": [
                "--days",
                "--rate-in-mbps",
                "--start",
                "--stop"
            ],
            "optional_parameters": [
                "--add",
                "--device-name -d",
                "--force-string",
                "--ids",
                "--name -n",
                "--no-wait",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription"
            ],
            "examples": [
                "az databoxedge bandwidth-schedule update --name \"bandwidth-1\" --device-name \"testedgedevice\"         --rate-in-mbps 150 --resource-group \"GroupForEdgeAutomation\" --start \"0:0:0\" --stop 12:00:00 --days Sunday"
            ]
        },
        {
            "name": "az databoxedge bandwidth-schedule wait",
            "summary": "Place the CLI in a waiting state until a condition of the databoxedge bandwidth-schedule is met.",
            "syntax": "az databoxedge bandwidth-schedule wait [--created]\n                                       [--custom]\n                                       [--deleted]\n                                       [--device-name]\n                                       [--exists]\n                                       [--ids]\n                                       [--interval]\n                                       [--name]\n                                       [--resource-group]\n                                       [--subscription]\n                                       [--timeout]\n                                       [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--device-name -d",
                "--exists",
                "--ids",
                "--interval",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az databoxedge bandwidth-schedule wait --name \"bandwidth-1\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\" --created",
                "az databoxedge bandwidth-schedule wait --name \"bandwidth-1\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\" --updated",
                "az databoxedge bandwidth-schedule wait --name \"bandwidth-1\" --device-name \"testedgedevice\" --resource-group \"GroupForEdgeAutomation\" --deleted"
            ]
        }
    ]
}