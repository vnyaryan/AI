{
    "command_group": "az cosmosdb network-rule",
    "description": "Manage Azure Cosmos DB network rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az cosmosdb network-rule add",
            "summary": "Adds a virtual network rule to an existing Cosmos DB database account.",
            "syntax": "az cosmosdb network-rule add --subnet\n                             [--ids]\n                             [--ignore-missing-endpoint {false, true}]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]\n                             [--virtual-network]",
            "required_parameters": [
                "--subnet"
            ],
            "optional_parameters": [
                "--ids",
                "--ignore-missing-endpoint --ignore-missing-vnet-service-endpoint",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--virtual-network --vnet-name"
            ],
            "examples": []
        },
        {
            "name": "az cosmosdb network-rule list",
            "summary": "Lists the virtual network accounts associated with a Cosmos DB account.",
            "syntax": "az cosmosdb network-rule list --name\n                              --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az cosmosdb network-rule remove",
            "summary": "Remove a virtual network rule from an existing Cosmos DB database account.",
            "syntax": "az cosmosdb network-rule remove --subnet\n                                [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]\n                                [--virtual-network]",
            "required_parameters": [
                "--subnet"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--virtual-network --vnet-name"
            ],
            "examples": []
        }
    ]
}