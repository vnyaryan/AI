{
    "command_group": "az synapse pipeline-run",
    "description": "Manage Synapse's pipeline run.",
    "status": "GA",
    "commands": [
        {
            "name": "az synapse pipeline-run cancel",
            "summary": "Cancel a pipeline run by its run ID.",
            "syntax": "az synapse pipeline-run cancel --run-id\n                               --workspace-name\n                               [--is-recursive {false, true}]\n                               [--yes]",
            "required_parameters": [
                "--run-id",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--is-recursive",
                "--yes -y"
            ],
            "examples": [
                "az synapse pipeline-run cancel --workspace-name testsynapseworkspace \\\n  --run-id \"16ac5348-ff82-4f95-a80d-638c1d47b721\""
            ]
        },
        {
            "name": "az synapse pipeline-run query-by-workspace",
            "summary": "Query pipeline runs in the workspace based on input filter conditions.",
            "syntax": "az synapse pipeline-run query-by-workspace --last-updated-after\n                                           --last-updated-before\n                                           --workspace-name\n                                           [--continuation-token]\n                                           [--filters]\n                                           [--order-by]",
            "required_parameters": [
                "--last-updated-after",
                "--last-updated-before",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--continuation-token",
                "--filters",
                "--order-by"
            ],
            "examples": [
                "az synapse pipeline-run query-by-workspace --workspace-name testsynapseworkspace --filters \\\n  operand=\"PipelineName\" operator=\"Equals\" values=\"testpipeline\" --last-updated-after \"2020-09-03T00:36:44.3345758Z\" \\\n  --last-updated-before \"2020-09-03T00:49:48.3686473Z\""
            ]
        },
        {
            "name": "az synapse pipeline-run show",
            "summary": "Get a pipeline run by its run ID.",
            "syntax": "az synapse pipeline-run show --run-id\n                             --workspace-name",
            "required_parameters": [
                "--run-id",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse pipeline-run show --workspace-name testsynapseworkspace \\\n  --run-id \"2f7fdb90-5df1-4b8e-ac2f-064cfa58202b\""
            ]
        }
    ]
}