{
    "command_group": "az synapse kusto pool-principal-assignment",
    "description": "Manage kusto pool principal assignment with synapse.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az synapse kusto pool-principal-assignment create",
            "summary": "Create a Kusto pool principalAssignment.",
            "syntax": "az synapse kusto pool-principal-assignment create --kusto-pool-name\n                                                  --principal-assignment-name\n                                                  --resource-group\n                                                  --workspace-name\n                                                  [--no-wait]\n                                                  [--principal-id]\n                                                  [--principal-type {App, Group, User}]\n                                                  [--role {AllDatabasesAdmin, AllDatabasesViewer}]\n                                                  [--tenant-id]",
            "required_parameters": [
                "--kusto-pool-name",
                "--principal-assignment-name",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--principal-id",
                "--principal-type",
                "--role",
                "--tenant-id"
            ],
            "examples": [
                "az synapse kusto pool-principal-assignment create --kusto-pool-name \"kustoclusterrptest4\" --principal-id \"87654321-1234-1234-1234-123456789123\" --principal-type \"App\" --role \"AllDatabasesAdmin\" --tenant-id \"12345678-1234-1234-1234-123456789123\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --workspace-name \"synapseWorkspaceName\""
            ]
        },
        {
            "name": "az synapse kusto pool-principal-assignment delete",
            "summary": "Deletes a Kusto pool principalAssignment.",
            "syntax": "az synapse kusto pool-principal-assignment delete [--ids]\n                                                  [--kusto-pool-name]\n                                                  [--no-wait]\n                                                  [--principal-assignment-name]\n                                                  [--resource-group]\n                                                  [--subscription]\n                                                  [--workspace-name]\n                                                  [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--kusto-pool-name",
                "--no-wait",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
            ],
            "examples": [
                "az synapse kusto pool-principal-assignment delete --kusto-pool-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --workspace-name \"synapseWorkspaceName\""
            ]
        },
        {
            "name": "az synapse kusto pool-principal-assignment list",
            "summary": "Lists all Kusto pool principalAssignments.",
            "syntax": "az synapse kusto pool-principal-assignment list --kusto-pool-name\n                                                --resource-group\n                                                --workspace-name",
            "required_parameters": [
                "--kusto-pool-name",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse kusto pool-principal-assignment list --kusto-pool-name \"kustoclusterrptest4\" --resource-group \"kustorptest\" --workspace-name \"synapseWorkspaceName\""
            ]
        },
        {
            "name": "az synapse kusto pool-principal-assignment show",
            "summary": "Gets a Kusto pool principalAssignment.",
            "syntax": "az synapse kusto pool-principal-assignment show [--ids]\n                                                [--kusto-pool-name]\n                                                [--principal-assignment-name]\n                                                [--resource-group]\n                                                [--subscription]\n                                                [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--kusto-pool-name",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az synapse kusto pool-principal-assignment show --kusto-pool-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --workspace-name \"synapseWorkspaceName\""
            ]
        },
        {
            "name": "az synapse kusto pool-principal-assignment update",
            "summary": "Update a Kusto pool principalAssignment.",
            "syntax": "az synapse kusto pool-principal-assignment update [--add]\n                                                  [--force-string]\n                                                  [--ids]\n                                                  [--kusto-pool-name]\n                                                  [--no-wait]\n                                                  [--principal-assignment-name]\n                                                  [--principal-id]\n                                                  [--principal-type {App, Group, User}]\n                                                  [--remove]\n                                                  [--resource-group]\n                                                  [--role {AllDatabasesAdmin, AllDatabasesViewer}]\n                                                  [--set]\n                                                  [--subscription]\n                                                  [--tenant-id]\n                                                  [--workspace-name]",
            "required_parameters": [],
