{
    "command_group": "az kusto cluster-principal-assignment",
    "description": "Manage cluster principal assignment with kusto.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az kusto cluster-principal-assignment create",
            "summary": "Create a Kusto cluster principalAssignment.",
            "syntax": "az kusto cluster-principal-assignment create --cluster-name\n                                             --principal-assignment-name\n                                             --resource-group\n                                             [--no-wait]\n                                             [--principal-id]\n                                             [--principal-type {App, Group, User}]\n                                             [--role {AllDatabasesAdmin, AllDatabasesViewer}]\n                                             [--tenant-id]",
            "required_parameters": [
                "--cluster-name",
                "--principal-assignment-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--principal-id",
                "--principal-type",
                "--role",
                "--tenant-id"
            ],
            "examples": [
                "az kusto cluster-principal-assignment create --cluster-name \"kustoclusterrptest4\" --principal-id \"87654321-1234-1234-1234-123456789123\" --principal-type \"App\" --role \"AllDatabasesAdmin\" --tenant-id \"12345678-1234-1234-1234-123456789123\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster-principal-assignment delete",
            "summary": "Deletes a Kusto cluster principalAssignment.",
            "syntax": "az kusto cluster-principal-assignment delete [--cluster-name]\n                                             [--ids]\n                                             [--no-wait]\n                                             [--principal-assignment-name]\n                                             [--resource-group]\n                                             [--subscription]\n                                             [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--ids",
                "--no-wait",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az kusto cluster-principal-assignment delete --cluster-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster-principal-assignment list",
            "summary": "Lists all Kusto cluster principalAssignments.",
            "syntax": "az kusto cluster-principal-assignment list --cluster-name\n                                           --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az kusto cluster-principal-assignment list --cluster-name \"kustoclusterrptest4\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster-principal-assignment show",
            "summary": "Gets a Kusto cluster principalAssignment.",
            "syntax": "az kusto cluster-principal-assignment show [--cluster-name]\n                                           [--ids]\n                                           [--principal-assignment-name]\n                                           [--resource-group]\n                                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--ids",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az kusto cluster-principal-assignment show --cluster-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto cluster-principal-assignment update",
            "summary": "Update a Kusto cluster principalAssignment.",
            "syntax": "az kusto cluster-principal-assignment update [--add]\n                                             [--cluster-name]\n                                             [--force-string]\n                                             [--ids]\n                                             [--no-wait]\n                                             [--principal-assignment-name]\n                                             [--principal-id]\n                                             [--principal-type {App, Group, User}]\n                                             [--remove]\n                                             [--resource-group]\n                                             [--role {AllDatabasesAdmin, AllDatabasesViewer}]\n                                             [--set]\n                                             [--subscription]\n                                             [--tenant-id]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--cluster-name",
                "--force-string",
                "--ids",
                "--no-wait",
                "--principal-assignment-name",
                "--principal-id",
                "--principal-type",
                "--remove",
                "--resource-group -g",
                "--role",
                "--set",
                "--subscription",
                "--tenant-id"
            ],
            "examples": []
        },
        {
            "name": "az kusto cluster-principal-assignment wait",
            "summary": "Place the CLI in a waiting state until a condition of the kusto cluster-principal-assignment is met.",
            "syntax": "az kusto cluster-principal-assignment wait [--cluster-name]\n                                           [--created]\n                                           [--custom]\n                                           [--deleted]\n                                           [--exists]\n                                           [--ids]\n                                           [--interval]\n                                           [--principal-assignment-name]\n                                           [--resource-group]\n                                           [--subscription]\n                                           [--timeout]\n                                           [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az kusto cluster-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --created",
                "az kusto cluster-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --updated",
                "az kusto cluster-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --deleted"
            ]
        }
    ]
}