{
    "command_group": "az kusto database-principal-assignment",
    "description": "Manage database principal assignment with kusto.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az kusto database-principal-assignment create",
            "summary": "Creates a Kusto cluster database principalAssignment.",
            "syntax": "az kusto database-principal-assignment create --cluster-name\n                                              --database-name\n                                              --principal-assignment-name\n                                              --resource-group\n                                              [--no-wait]\n                                              [--principal-id]\n                                              [--principal-type {App, Group, User}]\n                                              [--role {Admin, Ingestor, Monitor, UnrestrictedViewer, User, Viewer}]\n                                              [--tenant-id]",
            "required_parameters": [
                "--cluster-name",
                "--database-name",
                "--principal-assignment-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--principal-id",
                "--principal-type",
                "--role",
                "--tenant-id"
            ],
            "examples": [
                "az kusto database-principal-assignment create --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-id \"87654321-1234-1234-1234-123456789123\" --principal-type \"App\" --role \"Admin\" --tenant-id \"12345678-1234-1234-1234-123456789123\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto database-principal-assignment delete",
            "summary": "Deletes a Kusto principalAssignment.",
            "syntax": "az kusto database-principal-assignment delete [--cluster-name]\n                                              [--database-name]\n                                              [--ids]\n                                              [--no-wait]\n                                              [--principal-assignment-name]\n                                              [--resource-group]\n                                              [--subscription]\n                                              [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--database-name",
                "--ids",
                "--no-wait",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az kusto database-principal-assignment delete --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto database-principal-assignment list",
            "summary": "Lists all Kusto cluster database principalAssignments.",
            "syntax": "az kusto database-principal-assignment list --cluster-name\n                                            --database-name\n                                            --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--database-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az kusto database-principal-assignment list --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto database-principal-assignment show",
            "summary": "Gets a Kusto cluster database principalAssignment.",
            "syntax": "az kusto database-principal-assignment show [--cluster-name]\n                                            [--database-name]\n                                            [--ids]\n                                            [--principal-assignment-name]\n                                            [--resource-group]\n                                            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--database-name",
                "--ids",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az kusto database-principal-assignment show --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\""
            ]
        },
        {
            "name": "az kusto database-principal-assignment update",
            "summary": "Update a Kusto cluster database principalAssignment.",
            "syntax": "az kusto database-principal-assignment update [--add]\n                                              [--cluster-name]\n                                              [--database-name]\n                                              [--force-string]\n                                              [--ids]\n                                              [--no-wait]\n                                              [--principal-assignment-name]\n                                              [--principal-id]\n                                              [--principal-type {App, Group, User}]\n                                              [--remove]\n                                              [--resource-group]\n                                              [--role {Admin, Ingestor, Monitor, UnrestrictedViewer, User, Viewer}]\n                                              [--set]\n                                              [--subscription]\n                                              [--tenant-id]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--cluster-name",
                "--database-name",
                "--force-string",
                "--ids",
                "--no-wait",
                "--principal-assignment-name",
                "--principal-id",
                "--principal-type",
                "--remove",
                "--resource-group -g",
                "--role",
                "--set",
                "--subscription",
                "--tenant-id"
            ],
            "examples": []
        },
        {
            "name": "az kusto database-principal-assignment wait",
            "summary": "Place the CLI in a waiting state until a condition of the kusto database-principal-assignment is met.",
            "syntax": "az kusto database-principal-assignment wait [--cluster-name]\n                                            [--created]\n                                            [--custom]\n                                            [--database-name]\n                                            [--deleted]\n                                            [--exists]\n                                            [--ids]\n                                            [--interval]\n                                            [--principal-assignment-name]\n                                            [--resource-group]\n                                            [--subscription]\n                                            [--timeout]\n                                            [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--cluster-name",
                "--created",
                "--custom",
                "--database-name",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--principal-assignment-name",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az kusto database-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --created",
                "az kusto database-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --updated",
                "az kusto database-principal-assignment wait --cluster-name \"kustoclusterrptest4\" --database-name \"Kustodatabase8\" --principal-assignment-name \"kustoprincipal1\" --resource-group \"kustorptest\" --deleted"
            ]
        }
    ]
}