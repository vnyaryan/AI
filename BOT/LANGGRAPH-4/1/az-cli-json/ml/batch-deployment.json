{
    "command_group": "az ml batch-deployment",
    "description": "Manage Azure ML batch deployments.",
    "status": "GA",
    "commands": [
        {
            "name": "az ml batch-deployment create",
            "summary": "Create a deployment. If the deployment already exists, it will be over-written with the new settings.",
            "syntax": "az ml batch-deployment create --file\n                              --resource-group\n                              --workspace-name\n                              [--endpoint-name]\n                              [--name]\n                              [--no-wait]\n                              [--set]\n                              [--set-default]\n                              [--skip-script-validation]",
            "required_parameters": [
                "--file -f",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--endpoint-name -e",
                "--name -n",
                "--no-wait",
                "--set",
                "--set-default",
                "--skip-script-validation"
            ],
            "examples": [
                "az ml batch-deployment create --file deployment.yaml --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml batch-deployment delete",
            "summary": "Delete a deployment.",
            "syntax": "az ml batch-deployment delete --endpoint-name\n                              --name\n                              --resource-group\n                              --workspace-name\n                              [--no-wait]\n                              [--yes]",
            "required_parameters": [
                "--endpoint-name -e",
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az ml batch-deployment delete --name my-deployment --endpoint-name my-endpoint --yes --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml batch-deployment list",
            "summary": "List deployments.",
            "syntax": "az ml batch-deployment list --endpoint-name\n                            --resource-group\n                            --workspace-name",
            "required_parameters": [
                "--endpoint-name -e",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml batch-deployment list --endpoint-name my-endpoint --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml batch-deployment list-jobs",
            "summary": "List the batch scoring jobs for a batch deployment.",
            "syntax": "az ml batch-deployment list-jobs --endpoint-name\n                                 --name\n                                 --resource-group\n                                 --workspace-name",
            "required_parameters": [
                "--endpoint-name -e",
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml batch-deployment list-jobs --name my-batch-endpoint --endpoint-name my-endpoint --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml batch-deployment show",
            "summary": "Show a deployment.",
            "syntax": "az ml batch-deployment show --endpoint-name\n                            --name\n                            --resource-group\n                            --workspace-name",
            "required_parameters": [
                "--endpoint-name -e",
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml batch-deployment show --name my-deployment --endpoint-name my-endpoint --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml batch-deployment update",
            "summary": "Update a deployment.",
            "syntax": "az ml batch-deployment update --resource-group\n                              --workspace-name\n                              [--add]\n                              [--endpoint-name]\n                              [--file]\n                              [--force-string]\n                              [--name]\n                              [--no-wait]\n                              [--remove]\n                              [--set]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--add",
                "--endpoint-name -e",
                "--file -f",
                "--force-string",
                "--name -n",
                "--no-wait",
                "--remove",
                "--set"
            ],
            "examples": [
                "az ml batch-deployment update --file deployment.yaml --resource-group my-resource-group --workspace-name my-workspace"
            ]
        }
    ]
}