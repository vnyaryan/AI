{
    "command_group": "az ml connection",
    "description": "Manage Azure ML workspace connection and its more specific derivatives.",
    "status": "Preview",
    "commands": [
        {
            "name": "az ml connection create",
            "summary": "Create a connection.",
            "syntax": "az ml connection create --file\n                        --resource-group\n                        --workspace-name\n                        [--name]\n                        [--populate-secrets]\n                        [--set]",
            "required_parameters": [
                "--file -f",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--name -n",
                "--populate-secrets -p",
                "--set"
            ],
            "examples": [
                "az ml connection create --file connection.yml --resource-group my-resource-group --workspace-name my-workspace",
                "az ml connection create --file connection.yml --resource-group my-resource-group --workspace-name my-workspace --populate-secrets"
            ]
        },
        {
            "name": "az ml connection delete",
            "summary": "Delete a connection.",
            "syntax": "az ml connection delete --name\n                        --resource-group\n                        --workspace-name\n                        [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az ml connection list",
            "summary": "List all connections.",
            "syntax": "az ml connection list --resource-group\n                      --workspace-name\n                      [--max-results]\n                      [--populate-secrets]\n                      [--type]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--max-results -r",
                "--populate-secrets -p",
                "--type -t"
            ],
            "examples": [
                "az ml connection list --resource-group my-resource-group --workspace-name my-workspace",
                "az ml connection list --resource-group my-resource-group --workspace-name my-workspace --populate-secrets",
                "az ml connection list --query \"[].{Name:name}\"  --output table --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml connection show",
            "summary": "Show details of a connection.",
            "syntax": "az ml connection show --name\n                      --resource-group\n                      --workspace-name\n                      [--populate-secrets]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--populate-secrets -p"
            ],
            "examples": [
                "az ml connection show --resource-group my-resource-group --name my-connection-name --workspace-name my-workspace --populate-secrets"
            ]
        },
        {
            "name": "az ml connection update",
            "summary": "Update a connection.",
            "syntax": "az ml connection update --name\n                        --resource-group\n                        --workspace-name\n                        [--add]\n                        [--force-string]\n                        [--populate-secrets]\n                        [--remove]\n                        [--set]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--populate-secrets -p",
                "--remove",
                "--set"
            ],
            "examples": [
                "az ml connection update --resource-group my-resource-group --name my-connection-name --workspace-name my-workspace"
            ]
        }
    ]
}