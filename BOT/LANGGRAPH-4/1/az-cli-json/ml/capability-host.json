{
    "command_group": "az ml capability-host",
    "description": "The Capability Host enables the Hub and Project to seamlessly interact with the Azure AI Agent Service.",
    "status": "Preview",
    "commands": [
        {
            "name": "az ml capability-host create",
            "summary": "Create a capability host.",
            "syntax": "az ml capability-host create --resource-group\n                             --workspace-name\n                             [--ai-services-connections]\n                             [--capability-host-kind]\n                             [--description]\n                             [--file]\n                             [--name]\n                             [--no-wait]\n                             [--set]\n                             [--storage-connections]\n                             [--vector-store-connections]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--ai-services-connections -a",
                "--capability-host-kind",
                "--description",
                "--file -f",
                "--name -n",
                "--no-wait",
                "--set",
                "--storage-connections -s",
                "--vector-store-connections -v"
            ],
            "examples": [
                "az ml capability-host create --file caphost.yml --resource-group my-resource-group --workspace-name my-workspace",
                "az ml capability-host create --name my-host-name --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml capability-host delete",
            "summary": "Delete a capability host.",
            "syntax": "az ml capability-host delete --name\n                             --resource-group\n                             --workspace-name\n                             [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az ml capability-host delete --resource-group my-resource-group --name my-host-name --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml capability-host show",
            "summary": "Show details of a capability host.",
            "syntax": "az ml capability-host show --name\n                           --resource-group\n                           --workspace-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml capability-host show --resource-group my-resource-group --name my-host-name --workspace-name my-workspace"
            ]
        }
    ]
}