            "name": "az batch job-schedule create",
            "summary": "Add a Batch job schedule to an account.",
            "syntax": "az batch job-schedule create [--account-endpoint]\n                             [--account-key]\n                             [--account-name]\n                             [--allow-task-preemption]\n                             [--do-not-run-after]\n                             [--do-not-run-until]\n                             [--id]\n                             [--job-manager-task-application-package-references]\n                             [--job-manager-task-command-line]\n                             [--job-manager-task-environment-settings]\n                             [--job-manager-task-id]\n                             [--job-manager-task-resource-files]\n                             [--job-max-task-retry-count]\n                             [--job-max-wall-clock-time]\n                             [--job-metadata]\n                             [--json-file]\n                             [--max-parallel-tasks]\n                             [--metadata]\n                             [--on-all-tasks-complete]\n                             [--pool-id]\n                             [--priority]\n                             [--recurrence-interval]\n                             [--required-slots]\n                             [--start-window]\n                             [--uses-task-dependencies]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--allow-task-preemption",
                "--do-not-run-after",
                "--do-not-run-until",
                "--id",
                "--job-manager-task-application-package-references",
                "--job-manager-task-command-line",
                "--job-manager-task-environment-settings",
                "--job-manager-task-id",
                "--job-manager-task-resource-files",
                "--job-max-task-retry-count",
                "--job-max-wall-clock-time",
                "--job-metadata",
                "--json-file",
                "--max-parallel-tasks",
                "--metadata",
                "--on-all-tasks-complete",
                "--pool-id",
                "--priority",
                "--recurrence-interval",
                "--required-slots",
                "--start-window",
                "--uses-task-dependencies"
            ],
            "examples": [
                "az batch job-schedule create --id jobschedule1 --pool-id pool1 --recurrence-interval P1D"
            ]
        },
        {
            "name": "az batch job-schedule delete",
            "summary": "Deletes a Job Schedule from the specified Account.",
            "syntax": "az batch job-schedule delete --job-schedule-id\n                             [--account-endpoint]\n                             [--account-key]\n                             [--account-name]\n                             [--if-match]\n                             [--if-modified-since]\n                             [--if-none-match]\n                             [--if-unmodified-since]\n                             [--yes]",
            "required_parameters": [
