{
    "command_group": "az offazure hyperv site",
    "description": "Manage Hyper-V site.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az offazure hyperv site create",
            "summary": "Create a Hyper-V site.",
            "syntax": "az offazure hyperv site create --name\n                               --resource-group\n                               [--agent]\n                               [--appliance-name]\n                               [--identity]\n                               [--location]\n                               [--solution-id]\n                               [--tags]",
            "required_parameters": [
                "--name --site-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--agent",
                "--appliance-name",
                "--identity",
                "--location -l",
                "--solution-id",
                "--tags"
            ],
            "examples": [
                "az offazure hyperv site create --resource-group MyResourceGroup --site-name MySiteName                  --location centralus"
            ]
        },
        {
            "name": "az offazure hyperv site delete",
            "summary": "Delete a Hyper-V site.",
            "syntax": "az offazure hyperv site delete [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --site-name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az offazure hyperv site delete --resource-group MyResourceGroup --site-name MySiteName"
            ]
        },
        {
            "name": "az offazure hyperv site show",
            "summary": "Get the details of a site.",
            "syntax": "az offazure hyperv site show [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --site-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az offazure hyperv site show --resource-group MyResourceGroup --site-name MySiteName"
            ]
        }
    ]
}