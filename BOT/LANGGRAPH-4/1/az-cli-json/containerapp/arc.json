{
    "command_group": "az containerapp arc",
    "description": "Install prerequisites for Kubernetes cluster on Arc.",
    "status": "Preview",
    "commands": [
        {
            "name": "az containerapp arc setup-core-dns",
            "summary": "Setup CoreDNS for Kubernetes cluster on Arc.",
            "syntax": "az containerapp arc setup-core-dns --distro {AksAzureLocal}\n                                   [--kube-config]\n                                   [--kube-context]\n                                   [--skip-ssl-verification]\n                                   [--yes]",
            "required_parameters": [
                "--distro"
            ],
            "optional_parameters": [
                "--kube-config",
                "--kube-context",
                "--skip-ssl-verification",
                "--yes -y"
            ],
            "examples": [
                "az containerapp arc setup-core-dns --distro AksAzureLocal",
                "az containerapp arc setup-core-dns --distro AksAzureLocal --kube-config /path/to/kubeconfig --kube-context kubeContextName"
            ]
        }
    ]
}