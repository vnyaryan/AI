{
    "command_group": "az webapp traffic-routing",
    "description": "Manage traffic routing for web apps.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp traffic-routing clear",
            "summary": "Clear the routing rules and send all traffic to production.",
            "syntax": "az webapp traffic-routing clear [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp traffic-routing clear --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp traffic-routing set",
            "summary": "Configure routing traffic to deployment slots.",
            "syntax": "az webapp traffic-routing set --distribution\n                              [--ids]\n                              [--name]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [
                "--distribution -d"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp traffic-routing set --distribution staging=50 --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp traffic-routing show",
            "summary": "Display the current distribution of traffic across slots.",
            "syntax": "az webapp traffic-routing show [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webapp traffic-routing show --name MyWebApp --resource-group MyResourceGroup"
            ]
        }
    ]
}