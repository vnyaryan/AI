{
    "command_group": "az webapp auth facebook",
    "description": "Manage webapp authentication and authorization of the Facebook identity provider.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp auth facebook show",
            "summary": "Show the authentication settings for the Facebook identity provider.",
            "syntax": "az webapp auth facebook show [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--slot]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth facebook show --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp auth facebook update",
            "summary": "Update the app id and app secret for the Facebook identity provider.",
            "syntax": "az webapp auth facebook update [--app-id]\n                               [--app-secret]\n                               [--app-secret-setting-name]\n                               [--graph-api-version]\n                               [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--scopes]\n                               [--slot]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--app-id",
                "--app-secret",
                "--app-secret-setting-name --secret-setting",
                "--graph-api-version",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--scopes",
                "--slot -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth facebook update  -g myResourceGroup --name MyWebApp \\\n  --app-id my-client-id --app-secret very_secret_password"
            ]
        }
    ]
}