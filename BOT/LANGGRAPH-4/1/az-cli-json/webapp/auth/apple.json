{
    "command_group": "az webapp auth apple",
    "description": "Manage webapp authentication and authorization of the Apple identity provider.",
    "status": "GA",
    "commands": [
        {
            "name": "az webapp auth apple show",
            "summary": "Show the authentication settings for the Apple identity provider.",
            "syntax": "az webapp auth apple show [--ids]\n                          [--name]\n                          [--resource-group]\n                          [--slot]\n                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az webapp auth apple show --name MyWebApp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az webapp auth apple update",
            "summary": "Update the client id and client secret for the Apple identity provider.",
            "syntax": "az webapp auth apple update [--client-id]\n                            [--client-secret]\n                            [--client-secret-setting-name]\n                            [--ids]\n                            [--name]\n                            [--resource-group]\n                            [--scopes]\n                            [--slot]\n                            [--subscription]\n                            [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--client-id",
                "--client-secret",
                "--client-secret-setting-name --secret-setting",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--scopes",
                "--slot -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az webapp auth apple update  -g myResourceGroup --name MyWebApp \\\n  --client-id my-client-id --client-secret very_secret_password"
            ]
        }
    ]
}