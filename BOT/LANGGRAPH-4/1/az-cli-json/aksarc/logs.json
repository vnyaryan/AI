{
    "command_group": "az aksarc logs",
    "description": "Collect logs from provisioned clusters.",
    "status": "GA",
    "commands": [
        {
            "name": "az aksarc logs hci",
            "summary": "Collect logs from provisioned clusters in HCI environment.",
            "syntax": "az aksarc logs hci [--credentials-dir]\n                   [--ip]\n                   [--kubeconfig]\n                   [--out-dir]",
            "required_parameters": [],
            "optional_parameters": [
                "--credentials-dir --ssh-key",
                "--ip --ips",
                "--kubeconfig",
                "--out-dir"
            ],
            "examples": [
                "az aksarc logs hci --ip 192.168.200.31 192.168.200.32 --ssh-key ~/.ssh --out-dir ./logs",
                "az aksarc logs hci --kubeconfig ~/.kube/config --ssh-key ~/.ssh/id_rsa --out-dir ./logs"
            ]
        }
    ]
}