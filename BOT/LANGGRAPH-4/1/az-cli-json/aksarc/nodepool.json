{
    "command_group": "az aksarc nodepool",
    "description": "Manage nodepools in a provisioned cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az aksarc nodepool add",
            "summary": "Adds a nodepool in the provisioned cluster.",
            "syntax": "az aksarc nodepool add --cluster-name\n                       --name\n                       --resource-group\n                       [--enable-cluster-autoscaler]\n                       [--labels]\n                       [--max-count]\n                       [--max-pods]\n                       [--min-count]\n                       [--no-wait]\n                       [--node-count]\n                       [--node-taints]\n                       [--node-vm-size]\n                       [--os-sku]\n                       [--os-type]\n                       [--tags]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--enable-cluster-autoscaler -e",
                "--labels",
                "--max-count",
                "--max-pods -m",
                "--min-count",
                "--no-wait",
                "--node-count -c",
                "--node-taints",
                "--node-vm-size",
                "--os-sku",
                "--os-type",
                "--tags"
            ],
            "examples": [
                "az aksarc nodepool add --name \"samplenodepool\" --cluster-name \"samplecluster\" --resource-group \"sample-rg\""
            ]
        },
        {
            "name": "az aksarc nodepool delete",
            "summary": "Deletes the nodepool in the provisioned cluster.",
            "syntax": "az aksarc nodepool delete --cluster-name\n                          --name\n                          --resource-group\n                          [--no-wait]\n                          [--yes]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az aksarc nodepool delete --name \"samplenodepool\" --cluster-name \"samplecluster\" --resource-group \"sample-rg\""
            ]
        },
        {
            "name": "az aksarc nodepool list",
            "summary": "Lists all the nodepools in the provisioned cluster.",
            "syntax": "az aksarc nodepool list --cluster-name\n                        --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az aksarc nodepool list --cluster-name \"samplecluster\" --resource-group \"sample-rg\""
            ]
        },
        {
            "name": "az aksarc nodepool scale",
            "summary": "Scales the nodepool in the provisioned cluster .",
            "syntax": "az aksarc nodepool scale --cluster-name\n                         --name\n                         --resource-group\n                         [--no-wait]\n                         [--node-count]\n                         [--node-vm-size]\n                         [--yes]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--node-count -c",
                "--node-vm-size",
                "--yes -y"
            ],
            "examples": [
                "az aksarc nodepool scale --name \"samplenodepool\" --cluster-name \"samplecluster\" --resource-group \"sample-rg\" --node-count 3 --node-vm-size \"Standard_A2_v2\""
            ]
        },
        {
            "name": "az aksarc nodepool show",
            "summary": "Gets the nodepool in the provisioned cluster.",
            "syntax": "az aksarc nodepool show --cluster-name\n                        --name\n                        --resource-group",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az aksarc nodepool show --name \"samplenodepool\" --cluster-name \"samplecluster\" --resource-group \"sample-rg\""
            ]
        },
        {
            "name": "az aksarc nodepool update",
            "summary": "Update a nodepool in the provisioned cluster.",
            "syntax": "az aksarc nodepool update --cluster-name\n                          --name\n                          --resource-group\n                          [--disable-cluster-autoscaler]\n                          [--enable-cluster-autoscaler]\n                          [--labels]\n                          [--max-count]\n                          [--min-count]\n                          [--no-wait]\n                          [--node-taints]\n                          [--update-cluster-autoscaler]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--disable-cluster-autoscaler -d",
                "--enable-cluster-autoscaler -e",
                "--labels",
                "--max-count",
                "--min-count",
                "--no-wait",
                "--node-taints",
                "--update-cluster-autoscaler -u"
            ],
            "examples": [
                "az aksarc nodepool update --name \"samplenodepool\" --cluster-name \"samplecluster\" --resource-group \"sample-rg\" --disable-cluster-autoscaler"
            ]
        }
    ]
}