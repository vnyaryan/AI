{
    "command_group": "az arcappliance delete",
    "description": "Command group for deletion of an Arc resource bridge on-premises appliance VM and its Azure resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az arcappliance delete hci",
            "summary": "Command to delete the on-premises appliance VM on Azure Stack HCI and Arc resource bridge Azure resource.",
            "syntax": "az arcappliance delete hci --config-file\n                           [--cloudagent]\n                           [--location]\n                           [--loginconfigfile]\n                           [--name]\n                           [--resource-group]\n                           [--yes]",
            "required_parameters": [
                "--config-file"
            ],
            "optional_parameters": [
                "--cloudagent",
                "--location -l",
                "--loginconfigfile",
                "--name -n",
                "--resource-group -g",
                "--yes -y"
            ],
            "examples": [
                "az arcappliance delete hci --config-file [REQUIRED] --cloudagent [OPTIONAL] --loginconfigfile [OPTIONAL]"
            ]
        },
        {
            "name": "az arcappliance delete scvmm",
            "summary": "Command to delete the on-premises appliance VM on SCVMM and Azure resource.",
            "syntax": "az arcappliance delete scvmm --config-file\n                             [--address]\n                             [--location]\n                             [--name]\n                             [--password]\n                             [--port]\n                             [--resource-group]\n                             [--username]\n                             [--yes]",
            "required_parameters": [
                "--config-file"
            ],
            "optional_parameters": [
                "--address",
                "--location -l",
                "--name -n",
                "--password",
                "--port",
                "--resource-group -g",
                "--username",
                "--yes -y"
            ],
            "examples": [
                "az arcappliance delete scvmm --config-file [REQUIRED] --address [OPTIONAL] --port [OPTIONAL] --username [OPTIONAL] --password [OPTIONAL]"
            ]
        },
        {
            "name": "az arcappliance delete vmware",
            "summary": "Command to delete the on-premises appliance VM and Azure resource for Arc resource bridge (Arc-enabled VMware).",
            "syntax": "az arcappliance delete vmware --config-file\n                              [--address]\n                              [--location]\n                              [--name]\n                              [--password]\n                              [--resource-group]\n                              [--username]\n                              [--yes]",
            "required_parameters": [
                "--config-file"
            ],
            "optional_parameters": [
                "--address",
                "--location -l",
                "--name -n",
                "--password",
                "--resource-group -g",
                "--username",
                "--yes -y"
            ],
            "examples": [
                "az arcappliance delete vmware --config-file [REQUIRED] --address [OPTIONAL] --username [OPTIONAL] --password [OPTIONAL]"
            ]
        }
    ]
