{
    "command_group": "az scvmm cloud",
    "description": "Manage Arc for SCVMM Cloud resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az scvmm cloud create",
            "summary": "Create cloud resource.",
            "syntax": "az scvmm cloud create --custom-location\n                      --location\n                      --name\n                      --resource-group\n                      [--inventory-item]\n                      [--no-wait]\n                      [--tags]\n                      [--uuid]\n                      [--vmmserver]",
            "required_parameters": [
                "--custom-location",
                "--location -l",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--inventory-item -i",
                "--no-wait",
                "--tags",
                "--uuid",
                "--vmmserver -v"
            ],
            "examples": [
                "az scvmm cloud create --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --vmmserver contoso-vmmserver --inventory-item 01234567-0123-0123-0123-0123456789ab --name contoso-cloud"
            ]
        },
        {
            "name": "az scvmm cloud delete",
            "summary": "Delete cloud resource.",
            "syntax": "az scvmm cloud delete [--force]\n                      [--ids]\n                      [--name]\n                      [--no-wait]\n                      [--resource-group]\n                      [--subscription]\n                      [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az scvmm cloud delete --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/clouds/contoso-cloud",
                "az scvmm cloud delete --subscription contoso-sub --resource-group contoso-rg --name contoso-cloud"
            ]
        },
        {
            "name": "az scvmm cloud list",
            "summary": "Retrieve a list of clouds.",
            "syntax": "az scvmm cloud list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az scvmm cloud list --subscription contoso-sub --resource-group contoso-rg",
                "az scvmm cloud list --subscription contoso-sub"
            ]
        },
        {
            "name": "az scvmm cloud show",
            "summary": "Get details of a cloud.",
            "syntax": "az scvmm cloud show [--ids]\n                    [--name]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm cloud show --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/clouds/contoso-cloud",
                "az scvmm cloud show --subscription contoso-sub --resource-group contoso-rg --name contoso-cloud"
            ]
        },
        {
            "name": "az scvmm cloud update",
            "summary": "Update cloud resource.",
            "syntax": "az scvmm cloud update [--ids]\n                      [--name]\n                      [--no-wait]\n                      [--resource-group]\n                      [--subscription]\n                      [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az scvmm cloud update --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/clouds/contoso-cloud --tags department=Sales",
                "az scvmm cloud update --subscription contoso-sub --resource-group contoso-rg --name contoso-cloud --tags department=Sales"
            ]
        },
        {
            "name": "az scvmm cloud wait",
            "summary": "Place the CLI in a waiting state until a condition of the cloud is met.",
            "syntax": "az scvmm cloud wait --cloud-name\n                    --resource-group\n                    [--created]\n                    [--custom]\n                    [--deleted]\n                    [--exists]\n                    [--interval]\n                    [--timeout]\n                    [--updated]",
            "required_parameters": [
                "--cloud-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}