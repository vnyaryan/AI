{
    "command_group": "az ml computetarget attach",
    "description": "Attach subgroup commands.",
    "status": "GA",
    "commands": [
        {
            "name": "az ml computetarget attach aks",
            "summary": "Attach an AKS cluster to the workspace.",
            "syntax": "az ml computetarget attach aks --compute-resource-id\n                               --name\n                               [--resource-group]\n                               [--workspace-name]",
            "required_parameters": [
                "--compute-resource-id -i",
                "--name -n"
            ],
            "optional_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "examples": []
        },
        {
            "name": "az ml computetarget attach kubernetes",
            "summary": "Attach a KubernetesCompute as a compute target to the workspace.",
            "syntax": "az ml computetarget attach kubernetes --compute-resource-id\n                                      --name\n                                      [--namespace]\n                                      [--resource-group]\n                                      [--workspace-name]",
            "required_parameters": [
                "--compute-resource-id -i",
                "--name -n"
            ],
            "optional_parameters": [
                "--namespace",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "examples": []
        },
        {
            "name": "az ml computetarget attach remote",
            "summary": "Attach a remote machine without Docker as a compute target to the workspace.",
            "syntax": "az ml computetarget attach remote --address\n                                  --name\n                                  --ssh-port\n                                  --username\n                                  [--password]\n                                  [--private-key-file]\n                                  [--private-key-passphrase]\n                                  [--resource-group]\n                                  [--workspace-name]",
            "required_parameters": [
                "--address -a",
                "--name -n",
                "--ssh-port",
                "--username -u"
            ],
            "optional_parameters": [
                "--password -p",
                "--private-key-file",
                "--private-key-passphrase",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "examples": []
        }
    ]
}