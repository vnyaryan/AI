{
    "command_group": "az dt job deletion",
    "description": "Manage and configure jobs for deleting model, twin and relationships data in a digital twin instance.",
    "status": "GA",
    "commands": [
        {
            "name": "az dt job deletion create",
            "summary": "Create and execute a deletion job on a digital twin instance.",
            "syntax": "az dt job deletion create --dt-name\n                          [--job-id]\n                          [--resource-group]\n                          [--timeout]\n                          [--yes]",
            "required_parameters": [
                "--dt-name --dtn -n"
            ],
            "optional_parameters": [
                "--job-id -j",
                "--resource-group -g",
                "--timeout -t",
                "--yes -y"
            ],
            "examples": [
                "az dt job deletion create -n {instance_or_hostname} -j {job_id}",
                "az dt job deletion create -n {instance_or_hostname} -y"
            ]
        },
        {
            "name": "az dt job deletion list",
            "summary": "List all deletion jobs executed on a digital twins instance.",
            "syntax": "az dt job deletion list --dt-name\n                        [--resource-group]",
            "required_parameters": [
                "--dt-name --dtn -n"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az dt job deletion list -n {instance_or_hostname}"
            ]
        },
        {
            "name": "az dt job deletion show",
            "summary": "Show details of a deletion job executed on a digital twins instance.",
            "syntax": "az dt job deletion show --dt-name\n                        --job-id\n                        [--resource-group]",
            "required_parameters": [
                "--dt-name --dtn -n",
                "--job-id -j"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az dt job deletion show -n {instance_or_hostname} -j {job_id}"
            ]
        }
    ]
}