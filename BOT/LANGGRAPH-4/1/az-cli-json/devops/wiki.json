{
    "command_group": "az devops wiki",
    "description": "Manage wikis.",
    "status": "GA",
    "commands": [
        {
            "name": "az devops wiki create",
            "summary": "Create a wiki.",
            "syntax": "az devops wiki create [--detect {false, true}]\n                      [--mapped-path]\n                      [--name]\n                      [--org]\n                      [--project]\n                      [--repository]\n                      [--type {codewiki, projectwiki}]\n                      [--version]",
            "required_parameters": [],
            "optional_parameters": [
                "--detect",
                "--mapped-path",
                "--name",
                "--org --organization",
                "--project -p",
                "--repository -r",
                "--type --wiki-type",
                "--version -v"
            ],
            "examples": [
                "az devops wiki create --name myprojectwiki",
                "az devops wiki create --name WIKI_NAME --type codewiki --version BRANCH_NAME\n--repository REPO_NAME --mapped-path PATH_TO_PUBLISH"
            ]
        },
        {
            "name": "az devops wiki delete",
            "summary": "Delete a wiki.",
            "syntax": "az devops wiki delete --wiki\n                      [--detect {false, true}]\n                      [--org]\n                      [--project]\n                      [--yes]",
            "required_parameters": [
                "--wiki"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--project -p",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az devops wiki list",
            "summary": "List all the wikis in a project or organization.",
            "syntax": "az devops wiki list [--detect {false, true}]\n                    [--org]\n                    [--project]\n                    [--scope {organization, project}]",
            "required_parameters": [],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--project -p",
                "--scope"
            ],
            "examples": [
                "az devops wiki list",
                "az devops wiki list --scope organization"
            ]
        },
        {
            "name": "az devops wiki show",
            "summary": "Show details of a wiki.",
            "syntax": "az devops wiki show --wiki\n                    [--detect {false, true}]\n                    [--open]\n                    [--org]\n                    [--project]",
            "required_parameters": [
                "--wiki"
            ],
            "optional_parameters": [
                "--detect",
                "--open",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        }
    ]
}