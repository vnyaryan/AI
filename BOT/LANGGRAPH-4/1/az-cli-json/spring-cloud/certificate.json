{
    "command_group": "az spring-cloud certificate",
    "description": "Commands to manage certificates.",
    "status": "Deprecated",
    "commands": [
        {
            "name": "az spring-cloud certificate add",
            "summary": "Add a certificate in Azure Spring Cloud.",
            "syntax": "az spring-cloud certificate add --name\n                                --resource-group\n                                --service\n                                [--only-public-cert {false, true}]\n                                [--public-certificate-file]\n                                [--vault-certificate-name]\n                                [--vault-uri]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--only-public-cert",
                "--public-certificate-file -f",
                "--vault-certificate-name",
                "--vault-uri"
            ],
            "examples": [
                "az spring-cloud certificate add --name MyCertName --vault-uri MyKeyVaultUri --vault-certificate-name MyKeyVaultCertName --service MyCluster --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az spring-cloud certificate list",
            "summary": "List all certificates in Azure Spring Cloud.",
            "syntax": "az spring-cloud certificate list --resource-group\n                                 --service\n                                 [--certificate-type {ContentCertificate, KeyVaultCertificate}]",
            "required_parameters": [
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--certificate-type"
            ],
            "examples": [
                "az spring-cloud certificate list --service MyCluster --resource-group MyResourceGroup -o table"
            ]
        },
        {
            "name": "az spring-cloud certificate list-reference-app",
            "summary": "List all the apps reference an existing certificate in the Azure Spring Cloud.",
            "syntax": "az spring-cloud certificate list-reference-app --name\n                                               --resource-group\n                                               --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az spring-cloud certificate list-reference-app --service MyCluster --resource-group MyResourceGroup --name MyCertName"
            ]
        },
        {
            "name": "az spring-cloud certificate remove",
            "summary": "Remove a certificate in Azure Spring Cloud.",
            "syntax": "az spring-cloud certificate remove --name\n                                   --resource-group\n                                   --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az spring-cloud certificate show",
            "summary": "Show a certificate in Azure Spring Cloud.",
            "syntax": "az spring-cloud certificate show --name\n                                 --resource-group\n                                 --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": []
        }
    ]
}