{
    "command_group": "az devcenter admin project-policy",
    "description": "Manage project policies.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin project-policy create",
            "summary": "Create a project policy.",
            "syntax": "az devcenter admin project-policy create --dev-center\n                                         --name\n                                         --resource-group\n                                         --resource-policies\n                                         [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                         [--scopes]",
            "required_parameters": [
                "--dev-center --dev-center-name -d",
                "--name --project-policy-name -n",
                "--resource-group -g",
                "--resource-policies"
            ],
            "optional_parameters": [
                "--no-wait",
                "--scopes"
            ],
            "examples": [
                "az devcenter admin project-policy create --dev-center-name \"Contoso\" --project-policy-name \"DevOnlyResources\" --resource-group \"rg1\" --scopes \"[\"/subscriptions/0ac520ee-14c0-480f-b6c9-0a90c58ffff1/resourceGroups/rg1/providers/Microsoft.DevCenter/projects/DevProject\"] --resource-policies \"[{\"resources\": \"/subscriptions/0ac520ee-14c0-480f-b6c9-0a90c58ffff1/resourceGroups/rg1/providers/Microsoft.DevCenter/devcenters/Contoso/attachednetworks/network-westus3\"}]\""
            ]
        },
        {
            "name": "az devcenter admin project-policy delete",
            "summary": "Delete an project policy.",
            "syntax": "az devcenter admin project-policy delete [--dev-center]\n                                         [--ids]\n                                         [--name]\n                                         [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                         [--resource-group]\n                                         [--subscription]\n                                         [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--dev-center --dev-center-name -d",
                "--ids",
                "--name --project-policy-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az devcenter admin project-policy delete --dev-center-name \"Contoso\" --project-policy-name \"DevOnlyResources\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-policy list",
            "summary": "List all project policies in the dev center.",
            "syntax": "az devcenter admin project-policy list --dev-center\n                                       --resource-group\n                                       [--max-items]\n                                       [--next-token]",
            "required_parameters": [
                "--dev-center --dev-center-name -d",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin project-policy list --dev-center-name \"Contoso\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-policy show",
            "summary": "Get a specific project policy.",
            "syntax": "az devcenter admin project-policy show [--dev-center]\n                                       [--ids]\n                                       [--name]\n                                       [--resource-group]\n                                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--dev-center --dev-center-name -d",
                "--ids",
                "--name --project-policy-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin project-policy show --dev-center-name \"Contoso\" --project-policy-name \"DevOnlyResources\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-policy update",
            "summary": "Update an project policy.",
            "syntax": "az devcenter admin project-policy update [--add]\n                                         [--dev-center]\n                                         [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                         [--ids]\n                                         [--name]\n                                         [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                         [--remove]\n                                         [--resource-group]\n                                         [--resource-policies]\n                                         [--scopes]\n                                         [--set]\n                                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--dev-center --dev-center-name -d",
                "--force-string",
                "--ids",
