{
    "command_group": "az connectedvmware cluster",
    "description": "Cluster resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az connectedvmware cluster create",
            "summary": "Create a cluster resource.",
            "syntax": "az connectedvmware cluster create --custom-location\n                                  --inventory-item\n                                  --location\n                                  --name\n                                  --resource-group\n                                  --vcenter\n                                  [--no-wait]\n                                  [--tags]",
            "required_parameters": [
                "--custom-location -c",
                "--inventory-item -i",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--vcenter -v"
            ],
            "optional_parameters": [
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az connectedvmware cluster create --custom-location \"custom location name\"                --location \"location name\" --inventory-item \"name or id the inventory item\"                --name \"cluster name\" --resource-group \"resource group name\"                --vcenter \"name or id of the vcenter\""
            ]
        },
        {
            "name": "az connectedvmware cluster delete",
            "summary": "Delete cluster resource.",
            "syntax": "az connectedvmware cluster delete [--force]\n                                  [--ids]\n                                  [--name]\n                                  [--no-wait]\n                                  [--resource-group]\n                                  [--subscription]\n                                  [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az connectedvmware cluster delete --ids \"resource id\" --name \"cluster name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        },
        {
            "name": "az connectedvmware cluster list",
            "summary": "Retrieve a list of cluster of given resource group.",
            "syntax": "az connectedvmware cluster list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az connectedvmware cluster list --resource-group \"resource group name\""
            ]
        },
        {
            "name": "az connectedvmware cluster show",
            "summary": "Get details of a cluster by id, resource-group, cluster name, or subscription.",
            "syntax": "az connectedvmware cluster show [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az connectedvmware cluster show --ids \"resource id\" --name \"cluster name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        }
    ]
}