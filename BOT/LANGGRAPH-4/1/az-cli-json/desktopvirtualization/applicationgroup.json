{
    "command_group": "az desktopvirtualization applicationgroup",
    "description": "Manage desktop virtualization application group.",
    "status": "GA",
    "commands": [
        {
            "name": "az desktopvirtualization applicationgroup create",
            "summary": "Create an application group.",
            "syntax": "az desktopvirtualization applicationgroup create --application-group-type {Desktop, RemoteApp}\n                                                 --host-pool-arm-path\n                                                 --name\n                                                 --resource-group\n                                                 [--description]\n                                                 [--friendly-name]\n                                                 [--location]\n                                                 [--tags]",
            "required_parameters": [
                "--application-group-type",
                "--host-pool-arm-path",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--description",
                "--friendly-name",
                "--location -l",
                "--tags"
            ],
            "examples": [
                "az desktopvirtualization applicationgroup create -g rg -n app-group-name --host-pool-arm-path host-pool-id --application-group-type remoteapp"
            ]
        },
        {
            "name": "az desktopvirtualization applicationgroup delete",
            "summary": "Delete an application group.",
            "syntax": "az desktopvirtualization applicationgroup delete [--ids]\n                                                 [--name]\n                                                 [--resource-group]\n                                                 [--subscription]\n                                                 [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az desktopvirtualization applicationgroup delete -g rg -n app-group-name"
            ]
        },
        {
            "name": "az desktopvirtualization applicationgroup list",
            "summary": "List application groups.",
            "syntax": "az desktopvirtualization applicationgroup list [--filter]\n                                               [--max-items]\n                                               [--next-token]\n                                               [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az desktopvirtualization applicationgroup list -g rg"
            ]
        },
        {
            "name": "az desktopvirtualization applicationgroup show",
            "summary": "Show an application group.",
            "syntax": "az desktopvirtualization applicationgroup show [--ids]\n                                               [--name]\n                                               [--resource-group]\n                                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az desktopvirtualization applicationgroup show -g rg -n app-group-name"
            ]
        },
        {
            "name": "az desktopvirtualization applicationgroup update",
            "summary": "Update an application group.",
            "syntax": "az desktopvirtualization applicationgroup update [--add]\n                                                 [--application-group-type {Desktop, RemoteApp}]\n                                                 [--description]\n                                                 [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                                 [--friendly-name]\n                                                 [--host-pool-arm-path]\n                                                 [--ids]\n                                                 [--name]\n                                                 [--remove]\n                                                 [--resource-group]\n                                                 [--set]\n                                                 [--subscription]\n                                                 [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--application-group-type",
                "--description",
                "--force-string",
                "--friendly-name",
                "--host-pool-arm-path",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az desktopvirtualization applicationgroup update -g rg -n app-group-name --tags tag=test"
            ]
        }
    ]
}