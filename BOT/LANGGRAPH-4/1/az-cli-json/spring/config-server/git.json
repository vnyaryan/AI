{
    "command_group": "az spring config-server git",
    "description": "Commands to manage Config Server git property in Azure Spring Apps.",
    "status": "GA",
    "commands": [
        {
            "name": "az spring config-server git set",
            "summary": "Set git property of Config Server, will totally override the old one.",
            "syntax": "az spring config-server git set --resource-group\n                                --service\n                                --uri\n                                [--defer]\n                                [--host-key]\n                                [--host-key-algorithm]\n                                [--host-key-check]\n                                [--label]\n                                [--password]\n                                [--private-key]\n                                [--search-paths]\n                                [--username]",
            "required_parameters": [
                "--resource-group -g",
                "--service -s",
                "--uri"
            ],
            "optional_parameters": [
                "--defer",
                "--host-key",
                "--host-key-algorithm",
                "--host-key-check --strict-host-key-checking",
                "--label",
                "--password",
                "--private-key",
                "--search-paths",
                "--username"
            ],
            "examples": [
                "az spring config-server git set -s MyService -g MyResourceGroup --uri UrlOfGitRepository --label LabelOfGitRepository --search-paths \"/path1,/path2\""
            ]
        }
    ]
}