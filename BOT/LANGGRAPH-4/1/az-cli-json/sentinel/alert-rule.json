{
    "command_group": "az sentinel alert-rule",
    "description": "Manage alert rule with sentinel.",
    "status": "GA",
    "commands": [
        {
            "name": "az sentinel alert-rule create",
            "summary": "Create the alert rule.",
            "syntax": "az sentinel alert-rule create --name\n                              --resource-group\n                              --workspace-name\n                              [--etag]\n                              [--fusion]\n                              [--ml-behavior-analytics]\n                              [--ms-security-incident]\n                              [--nrt]\n                              [--scheduled]\n                              [--threat-intelligence]",
            "required_parameters": [
                "--name --rule-name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--etag",
                "--fusion",
                "--ml-behavior-analytics",
                "--ms-security-incident",
                "--nrt",
                "--scheduled",
                "--threat-intelligence"
            ],
            "examples": []
        },
        {
