            "syntax": "az sentinel alert-rule delete [--ids]\n                              [--name]\n                              [--resource-group]\n                              [--subscription]\n                              [--workspace-name]\n                              [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --rule-name -n",
                "--resource-group -g",
                "--subscription",
                "--workspace-name -w",
                "--yes -y"
            ],
            "examples": []
        },
        {
