                "--entity-query-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--workspace-name -w"
            ],
            "examples": []
        },
        {
            "name": "az sentinel entity-query update",
            "summary": "Update the entity query.",
            "syntax": "az sentinel entity-query update [--activity]\n                                [--add]\n                                [--entity-query-id]\n                                [--etag]\n                                [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                [--ids]\n                                [--remove]\n                                [--resource-group]\n                                [--set]\n                                [--subscription]\n                                [--workspace-name]",
