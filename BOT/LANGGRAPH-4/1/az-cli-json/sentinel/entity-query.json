{
    "command_group": "az sentinel entity-query",
    "description": "Manage entity query with sentinel.",
    "status": "GA",
    "commands": [
        {
            "name": "az sentinel entity-query create",
            "summary": "Create the entity query.",
            "syntax": "az sentinel entity-query create --entity-query-id\n                                --resource-group\n                                --workspace-name\n                                [--activity]\n                                [--etag]",
            "required_parameters": [
                "--entity-query-id --name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--activity",
                "--etag"
            ],
            "examples": []
        },
        {
            "name": "az sentinel entity-query delete",
            "summary": "Delete the entity query.",
            "syntax": "az sentinel entity-query delete [--entity-query-id]\n                                [--ids]\n                                [--resource-group]\n                                [--subscription]\n                                [--workspace-name]\n                                [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--entity-query-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--workspace-name -w",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az sentinel entity-query list",
            "summary": "Get all entity queries.",
            "syntax": "az sentinel entity-query list --resource-group\n                              --workspace-name\n                              [--kind {Activity, Expansion}]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--kind"
            ],
            "examples": []
        },
        {
            "name": "az sentinel entity-query show",
