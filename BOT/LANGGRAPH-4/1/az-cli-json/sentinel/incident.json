{
    "command_group": "az sentinel incident",
    "description": "Manage incident with sentinel.",
    "status": "GA",
    "commands": [
        {
            "name": "az sentinel incident create",
            "summary": "Create the incident.",
            "syntax": "az sentinel incident create --incident-id\n                            --resource-group\n                            --workspace-name\n                            [--classification {BenignPositive, FalsePositive, TruePositive, Undetermined}]\n                            [--classification-comment]\n                            [--classification-reason {InaccurateData, IncorrectAlertLogic, SuspiciousActivity, SuspiciousButExpected}]\n                            [--description]\n                            [--etag]\n                            [--first-activity-time-utc]\n                            [--labels]\n                            [--last-activity-time-utc]\n                            [--owner]\n                            [--provider-incident-id]\n                            [--provider-name]\n                            [--severity {High, Informational, Low, Medium}]\n                            [--status {Active, Closed, New}]\n                            [--title]",
            "required_parameters": [
                "--incident-id --name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--classification",
                "--classification-comment",
                "--classification-reason",
                "--description",
                "--etag",
                "--first-activity-time-utc",
                "--labels",
                "--last-activity-time-utc",
                "--owner",
                "--provider-incident-id",
                "--provider-name",
                "--severity",
                "--status",
                "--title"
            ],
            "examples": []
        },
        {
            "name": "az sentinel incident create-team",
            "summary": "Create a Microsoft team to investigate the incident by sharing information and insights between participants.",
            "syntax": "az sentinel incident create-team --incident-id\n                                 --resource-group\n                                 --team-name\n                                 --workspace-name\n                                 [--group-ids]\n                                 [--member-ids]\n                                 [--team-description]",
            "required_parameters": [
                "--incident-id",
                "--resource-group -g",
                "--team-name",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--group-ids",
                "--member-ids",
                "--team-description"
            ],
            "examples": []
        },
