{
    "command_group": "az data-boundary",
    "description": "Data boundary operations.",
    "status": "GA",
    "commands": [
        {
            "name": "az data-boundary create",
            "summary": "Create tenant to data boundary.",
            "syntax": "az data-boundary create --default {default}\n                        [--data-boundary {EU, Global, NotDefined}]",
            "required_parameters": [
                "--default"
            ],
            "optional_parameters": [
                "--data-boundary"
            ],
            "examples": [
                "az data-boundary create --data-boundary EU --default default"
            ]
        },
        {
            "name": "az data-boundary show",
            "summary": "Get data boundary at specified scope.",
            "syntax": "az data-boundary show --default {default}\n                      --scope",
            "required_parameters": [
                "--default",
                "--scope"
            ],
            "optional_parameters": [],
            "examples": [
                "az data-boundary show --scope subscriptions/11111111-1111-1111-1111-111111111111 --default default\naz data-boundary show --scope subscriptions/11111111-1111-1111-1111-111111111111/resourcegroups/my-resource-group --default default"
            ]
        },
        {
            "name": "az data-boundary show-tenant",
            "summary": "Get data boundary of tenant.",
            "syntax": "az data-boundary show-tenant --default {default}",
            "required_parameters": [
                "--default"
            ],
            "optional_parameters": [],
            "examples": [
                "az data-boundary show-tenant --default default"
            ]
        }
    ]
}