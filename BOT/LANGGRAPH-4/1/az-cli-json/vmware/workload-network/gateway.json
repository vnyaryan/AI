{
    "command_group": "az vmware workload-network gateway",
    "description": "Commands to manage a Gateway workload network.",
    "status": "GA",
    "commands": [
        {
            "name": "az vmware workload-network gateway list",
            "summary": "List of gateways in a private cloud workload network.",
            "syntax": "az vmware workload-network gateway list --private-cloud\n                                        --resource-group\n                                        [--max-items]\n                                        [--next-token]",
            "required_parameters": [
                "--private-cloud -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az vmware workload-network gateway list --resource-group group1 --private-cloud cloud1"
            ]
        },
        {
            "name": "az vmware workload-network gateway show",
            "summary": "Get a gateway by id in a private cloud workload network.",
            "syntax": "az vmware workload-network gateway show [--gateway]\n                                        [--ids]\n                                        [--private-cloud]\n                                        [--resource-group]\n                                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--gateway --name -n",
                "--ids",
                "--private-cloud -c",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vmware workload-network gateway show --resource-group group1 --private-cloud cloud1 --gateway gateway1"
            ]
        }
    ]
}