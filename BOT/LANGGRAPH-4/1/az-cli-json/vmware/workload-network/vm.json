{
    "command_group": "az vmware workload-network vm",
    "description": "Commands to manage a Virtual Machine workload network.",
    "status": "GA",
    "commands": [
        {
            "name": "az vmware workload-network vm list",
            "summary": "List of virtual machines in a private cloud workload network.",
            "syntax": "az vmware workload-network vm list --private-cloud\n                                   --resource-group\n                                   [--max-items]\n                                   [--next-token]",
            "required_parameters": [
                "--private-cloud -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az vmware workload-network vm list --resource-group group1 --private-cloud cloud1"
            ]
        },
        {
            "name": "az vmware workload-network vm show",
            "summary": "Get a virtual machine by id in a private cloud workload network.",
            "syntax": "az vmware workload-network vm show [--ids]\n                                   [--name]\n                                   [--private-cloud]\n                                   [--resource-group]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --virtual-machine -n",
                "--private-cloud -c",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vmware workload-network vm show --resource-group group1 --private-cloud cloud1 --virtual-machine vm1"
            ]
        }
    ]
}