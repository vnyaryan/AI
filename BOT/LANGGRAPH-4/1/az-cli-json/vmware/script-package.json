{
    "command_group": "az vmware script-package",
    "description": "Commands to list and show script packages available to run on the private cloud.",
    "status": "GA",
    "commands": [
        {
            "name": "az vmware script-package list",
            "summary": "List script packages available to run on the private cloud.",
            "syntax": "az vmware script-package list --private-cloud\n                              --resource-group\n                              [--max-items]\n                              [--next-token]",
            "required_parameters": [
                "--private-cloud -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az vmware script-package list --resource-group group1 --private-cloud cloud1"
            ]
        },
        {
            "name": "az vmware script-package show",
            "summary": "Get a script package available to run on a private cloud.",
            "syntax": "az vmware script-package show [--ids]\n                              [--name]\n                              [--private-cloud]\n                              [--resource-group]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --script-package-name -n",
                "--private-cloud -c",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vmware script-package show --resource-group group1 --private-cloud cloud1 --name package1"
            ]
        }
    ]
}