{
    "command_group": "az postgres db",
    "description": "Manage PostgreSQL databases on a server. Is scheduled to retire by March 28, 2025.",
    "status": "GA",
    "commands": [
        {
            "name": "az postgres db create",
            "summary": "Create a PostgreSQL database.",
            "syntax": "az postgres db create --name\n                      --resource-group\n                      --server-name\n                      [--charset]\n                      [--collation]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--charset",
                "--collation"
            ],
            "examples": [
                "az postgres db create -g testgroup -s testsvr -n testdb",
                "az postgres db create -g testgroup -s testsvr -n testdb --charset {valid_charset} --collation {valid_collation}"
            ]
        },
        {
            "name": "az postgres db delete",
            "summary": "Delete a database.",
            "syntax": "az postgres db delete --name\n                      [--ids]\n                      [--resource-group]\n                      [--server-name]\n                      [--subscription]\n                      [--yes]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az postgres db delete -g testgroup -s testsvr -n testdb"
            ]
        },
        {
            "name": "az postgres db list",
            "summary": "List the databases for a server.",
            "syntax": "az postgres db list --resource-group\n                    --server-name",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az postgres db list -g testgroup -s testsvr"
            ]
        },
        {
            "name": "az postgres db show",
            "summary": "Show the details of a database.",
            "syntax": "az postgres db show --name\n                    [--ids]\n                    [--resource-group]\n                    [--server-name]\n                    [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az postgres db show -g testgroup -s testsvr -n testdb"
            ]
        }
    ]
}