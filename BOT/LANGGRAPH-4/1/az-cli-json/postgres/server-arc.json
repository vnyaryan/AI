{
    "command_group": "az postgres server-arc",
    "description": "Manage Azure Arc enabled PostgreSQL servers.",
    "status": "Preview",
    "commands": [
        {
            "name": "az postgres server-arc create",
            "summary": "Create an Azure Arc enabled PostgreSQL server.",
            "syntax": "az postgres server-arc create --name\n                              [--ad-account-name]\n                              [--ad-connector-name]\n                              [--admin-login-secret]\n                              [--cert-private-key-file]\n                              [--cert-public-key-file]\n                              [--cores-limit]\n                              [--cores-request]\n                              [--dev]\n                              [--dns-name]\n                              [--extensions]\n                              [--k8s-namespace]\n                              [--keytab-secret]\n                              [--log-level]\n                              [--memory-limit]\n                              [--memory-request]\n                              [--no-external-endpoint]\n                              [--no-wait]\n                              [--path]\n                              [--port]\n                              [--retention-days]\n                              [--service-annotations]\n                              [--service-cert-secret]\n                              [--service-labels]\n                              [--service-type]\n                              [--storage-class-backups]\n                              [--storage-class-data]\n                              [--storage-class-logs]\n                              [--use-k8s]\n                              [--volume-size-backups]\n                              [--volume-size-data]\n                              [--volume-size-logs]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ad-account-name",
                "--ad-connector-name",
                "--admin-login-secret",
                "--cert-private-key-file",
                "--cert-public-key-file",
                "--cores-limit",
                "--cores-request",
                "--dev",
                "--dns-name",
                "--extensions",
                "--k8s-namespace -k",
                "--keytab-secret",
                "--log-level",
                "--memory-limit",
                "--memory-request",
                "--no-external-endpoint",
                "--no-wait",
                "--path",
                "--port",
                "--retention-days",
                "--service-annotations",
                "--service-cert-secret",
                "--service-labels",
                "--service-type",
                "--storage-class-backups",
                "--storage-class-data",
                "--storage-class-logs",
                "--use-k8s",
                "--volume-size-backups",
                "--volume-size-data",
                "--volume-size-logs"
            ],
            "examples": [
                "az postgres server-arc create -n pg1 --k8s-namespace namespace --use-k8s"
            ]
        },
        {
            "name": "az postgres server-arc delete",
            "summary": "Delete an Azure Arc enabled PostgreSQL server.",
            "syntax": "az postgres server-arc delete --name\n                              [--force]\n                              [--k8s-namespace]\n                              [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--force -f",
                "--k8s-namespace -k",
                "--use-k8s"
            ],
            "examples": [
                "az postgres server-arc delete -n pg1 --k8s-namespace namespace --use-k8s"
            ]
        },
        {
            "name": "az postgres server-arc list",
            "summary": "List Azure Arc enabled PostgreSQL server.",
            "syntax": "az postgres server-arc list [--k8s-namespace]\n                            [--use-k8s]",
            "required_parameters": [],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--use-k8s"
            ],
            "examples": [
                "az postgres server-arc list --k8s-namespace namespace --use-k8s"
            ]
        },
        {
            "name": "az postgres server-arc restore",
            "summary": "Restore an Azure Arc enabled PostgreSQL server from backup from another server.",
            "syntax": "az postgres server-arc restore --k8s-namespace\n                               --name\n                               --source-server\n                               [--dry-run]\n                               [--no-wait]\n                               [--storage-class-backups]\n                               [--storage-class-data]\n                               [--storage-class-logs]\n                               [--time]\n                               [--use-k8s]\n                               [--volume-size-backups]\n                               [--volume-size-data]\n                               [--volume-size-logs]",
            "required_parameters": [
                "--k8s-namespace -k",
                "--name -n",
                "--source-server"
            ],
            "optional_parameters": [
                "--dry-run",
                "--no-wait",
                "--storage-class-backups",
                "--storage-class-data",
                "--storage-class-logs",
                "--time -t",
                "--use-k8s",
                "--volume-size-backups",
                "--volume-size-data",
                "--volume-size-logs"
            ],
            "examples": [
                "az postgres server-arc restore -n <to-pg> --source-server <from-pg> --k8s-namespace namespace --use-k8s"
            ]
        },
        {
            "name": "az postgres server-arc show",
            "summary": "Show the details of an Azure Arc enabled PostgreSQL server.",
            "syntax": "az postgres server-arc show --name\n                            [--k8s-namespace]\n                            [--path]\n                            [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--path",
                "--use-k8s"
            ],
            "examples": [
                "az postgres server-arc show -n pg1 --k8s-namespace namespace --use-k8s"
            ]
        },
        {
            "name": "az postgres server-arc update",
            "summary": "Update the configuration of an Azure Arc enabled PostgreSQL server.",
            "syntax": "az postgres server-arc update --name\n                              [--cert-private-key-file]\n                              [--cert-public-key-file]\n                              [--cores-limit]\n                              [--cores-request]\n                              [--dev]\n                              [--extensions]\n                              [--k8s-namespace]\n                              [--keytab-secret]\n                              [--log-level]\n                              [--memory-limit]\n                              [--memory-request]\n                              [--no-external-endpoint]\n                              [--no-wait]\n                              [--path]\n                              [--port]\n                              [--retention-days]\n                              [--service-annotations]\n                              [--service-cert-secret]\n                              [--service-labels]\n                              [--service-type]\n                              [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--cert-private-key-file",
                "--cert-public-key-file",
                "--cores-limit",
                "--cores-request",
                "--dev",
                "--extensions",
                "--k8s-namespace -k",
                "--keytab-secret",
                "--log-level",
                "--memory-limit",
                "--memory-request",
                "--no-external-endpoint",
                "--no-wait",
                "--path",
                "--port",
                "--retention-days",
                "--service-annotations",
                "--service-cert-secret",
                "--service-labels",
                "--service-type",
                "--use-k8s"
            ],
            "examples": [
                "az postgres server-arc update --path ./spec.json -n pg1 --k8s-namespace namespace --use-k8s",
                "az postgres server-arc update -n pg1 --extensions 'pgaudit,pg_partman' --k8s-namespace namespace --use-k8s",
                "az postgres server-arc update -n pg1 --extensions \"''\" --k8s-namespace namespace --use-k8s"
            ]
        }
    ]
}