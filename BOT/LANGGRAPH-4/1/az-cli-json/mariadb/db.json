{
    "command_group": "az mariadb db",
    "description": "Manage MariaDB databases on a server.",
    "status": "GA",
    "commands": [
        {
            "name": "az mariadb db create",
            "summary": "Create a MariaDB database.",
            "syntax": "az mariadb db create --name\n                     --resource-group\n                     --server-name\n                     [--charset]\n                     [--collation]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--charset",
                "--collation"
            ],
            "examples": [
                "az mariadb db create -g testgroup -s testsvr -n testdb",
                "az mariadb db create -g testgroup -s testsvr -n testdb --charset {valid_charset} --collation {valid_collation}"
            ]
        },
        {
            "name": "az mariadb db delete",
            "summary": "Delete a database.",
            "syntax": "az mariadb db delete --name\n                     [--ids]\n                     [--resource-group]\n                     [--server-name]\n                     [--subscription]\n                     [--yes]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az mariadb db delete -g testgroup -s testsvr -n testdb"
            ]
        },
        {
            "name": "az mariadb db list",
            "summary": "List the databases for a server.",
            "syntax": "az mariadb db list --resource-group\n                   --server-name",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az mariadb db list -g testgroup -s testsvr"
            ]
        },
        {
            "name": "az mariadb db show",
            "summary": "Show the details of a database.",
            "syntax": "az mariadb db show --name\n                   [--ids]\n                   [--resource-group]\n                   [--server-name]\n                   [--subscription]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server-name -s",
                "--subscription"
            ],
            "examples": [
                "az mariadb db show -g testgroup -s testsvr -n testdb"
            ]
        }
    ]
}