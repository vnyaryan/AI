{
    "command_group": "az signalr custom-certificate",
    "description": "Manage custom certificate settings.",
    "status": "GA",
    "commands": [
        {
            "name": "az signalr custom-certificate create",
            "summary": "Create a custom certificate of SignalR Service.",
            "syntax": "az signalr custom-certificate create --keyvault-base-uri\n                                     --keyvault-secret-name\n                                     --name\n                                     --resource-group\n                                     --signalr-name\n                                     [--keyvault-secret-version]",
            "required_parameters": [
                "--keyvault-base-uri",
                "--keyvault-secret-name",
                "--name",
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [
                "--keyvault-secret-version"
            ],
            "examples": []
        },
        {
            "name": "az signalr custom-certificate delete",
            "summary": "Delete a custom certificate of SignalR Service.",
            "syntax": "az signalr custom-certificate delete --name\n                                     --resource-group\n                                     --signalr-name",
            "required_parameters": [
                "--name",
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az signalr custom-certificate list",
            "summary": "List custom certificate of SignalR Service.",
            "syntax": "az signalr custom-certificate list --resource-group\n                                   --signalr-name",
            "required_parameters": [
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az signalr custom-certificate show",
            "summary": "Show the detail of a custom certificate of SignalR Service.",
            "syntax": "az signalr custom-certificate show --name\n                                   --resource-group\n                                   --signalr-name",
            "required_parameters": [
                "--name",
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az signalr custom-certificate update",
            "summary": "Update a custom certificate of SignalR Service.",
            "syntax": "az signalr custom-certificate update --name\n                                     --resource-group\n                                     --signalr-name\n                                     [--add]\n                                     [--force-string]\n                                     [--keyvault-base-uri]\n                                     [--keyvault-secret-name]\n                                     [--keyvault-secret-version]\n                                     [--remove]\n                                     [--set]",
            "required_parameters": [
                "--name",
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--keyvault-base-uri",
                "--keyvault-secret-name",
                "--keyvault-secret-version",
                "--remove",
                "--set"
            ],
            "examples": []
        }
    ]
