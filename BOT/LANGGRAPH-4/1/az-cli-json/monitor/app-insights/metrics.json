{
    "command_group": "az monitor app-insights metrics",
    "description": "Retrieve metrics from an application.",
    "status": "GA",
    "commands": [
        {
            "name": "az monitor app-insights metrics get-metadata",
            "summary": "Get the metadata for metrics on a particular application.",
            "syntax": "az monitor app-insights metrics get-metadata [--app]\n                                             [--ids]\n                                             [--resource-group]\n                                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--app -a",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az monitor app-insights metrics get-metadata --app e292531c-eb03-4079-9bb0-fe6b56b99f8b"
            ]
        },
        {
            "name": "az monitor app-insights metrics show",
            "summary": "View the value of a single metric.",
            "syntax": "az monitor app-insights metrics show --metrics\n                                     [--aggregation]\n                                     [--app]\n                                     [--end-time]\n                                     [--filter]\n                                     [--ids]\n                                     [--interval]\n                                     [--offset]\n                                     [--orderby]\n                                     [--resource-group]\n                                     [--segment]\n                                     [--start-time]\n                                     [--subscription]\n                                     [--top]",
            "required_parameters": [
                "--metrics -m"
            ],
            "optional_parameters": [
                "--aggregation",
                "--app -a",
                "--end-time",
                "--filter",
                "--ids",
                "--interval",
                "--offset",
                "--orderby",
                "--resource-group -g",
                "--segment",
                "--start-time",
                "--subscription",
                "--top"
            ],
            "examples": [
                "az monitor app-insights metrics show --app e292531c-eb03-4079-9bb0-fe6b56b99f8b --metric availabilityResults/count"
            ]
        }
    ]
}