{
    "command_group": "az webpubsub service",
    "description": "Commands to manage Webpubsub service.",
    "status": "GA",
    "commands": [
        {
            "name": "az webpubsub service broadcast",
            "summary": "Broadcast messages to hub. Error throws if operation fails.",
            "syntax": "az webpubsub service broadcast --hub-name\n                               --payload\n                               [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [
                "--hub-name",
                "--payload"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az webpubsub service broadcast -n MyWebPubSub -g MyResourceGroup --hub-name MyHub --payload MyPayload"
            ]
        }
    ]
}