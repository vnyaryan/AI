{
    "command_group": "az remote-rendering-account",
    "description": "Manage remote rendering account with mixed reality.",
    "status": "GA",
    "commands": [
        {
            "name": "az remote-rendering-account create",
            "summary": "Create a Remote Rendering Account.",
            "syntax": "az remote-rendering-account create --name\n                                   --resource-group\n                                   [--kind]\n                                   [--location]\n                                   [--sku]\n                                   [--storage-account-name]\n                                   [--tags]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--kind",
                "--location -l",
                "--sku",
                "--storage-account-name",
                "--tags"
            ],
            "examples": [
                "az remote-rendering-account create -n \"MyAccount\" --location \"eastus2euap\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az remote-rendering-account delete",
            "summary": "Delete a Remote Rendering Account.",
            "syntax": "az remote-rendering-account delete [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az remote-rendering-account delete -n \"MyAccount\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az remote-rendering-account list",
            "summary": "List Remote Rendering Accounts by Subscription.",
            "syntax": "az remote-rendering-account list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az remote-rendering-account list --resource-group \"MyResourceGroup\"",
                "az remote-rendering-account list"
            ]
        },
        {
            "name": "az remote-rendering-account show",
            "summary": "Get a Remote Rendering Account.",
            "syntax": "az remote-rendering-account show [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az remote-rendering-account show -n \"MyAccount\" --resource-group \"MyResourceGroup\""
            ]
        },
        {
            "name": "az remote-rendering-account update",
            "summary": "Update a Remote Rendering Account.",
            "syntax": "az remote-rendering-account update [--identity]\n                                   [--ids]\n                                   [--kind]\n                                   [--name]\n                                   [--resource-group]\n                                   [--sku]\n                                   [--storage-account-name]\n                                   [--subscription]\n                                   [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--identity",
                "--ids",
                "--kind",
                "--name -n",
                "--resource-group -g",
                "--sku",
                "--storage-account-name",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az remote-rendering-account update -n \"MyAccount\" --tags hero=\"romeo\" heroine=\"juliet\" --resource-group \"MyResourceGroup\""
            ]
        }
    ]
}