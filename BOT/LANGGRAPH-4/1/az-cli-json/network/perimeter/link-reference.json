            "syntax": "az network perimeter link-reference delete [--ids]\n                                           [--link-reference-name]\n                                           [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                           [--perimeter-name]\n                                           [--resource-group]\n                                           [--subscription]\n                                           [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--link-reference-name --name -n",
                "--no-wait",
                "--perimeter-name",
                "--resource-group -g",
