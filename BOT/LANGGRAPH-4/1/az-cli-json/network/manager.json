{
    "command_group": "az network manager",
    "description": "Manage network-manager with network.",
    "status": "GA",
    "commands": [
        {
            "name": "az network manager create",
            "summary": "Create a Network Manager.",
            "syntax": "az network manager create --location\n                          --name\n                          --network-manager-scopes\n                          --resource-group\n                          --scope-accesses\n                          [--description]\n                          [--id]\n                          [--tags]",
            "required_parameters": [
                "--location -l",
                "--name --network-manager-name -n",
                "--network-manager-scopes",
                "--resource-group -g",
                "--scope-accesses"
            ],
            "optional_parameters": [
                "--description",
                "--id",
                "--tags"
            ],
            "examples": [
                "az network manager create --name \"TestNetworkManager\" -l eastus2euap --description \"My Test Network Manager\" --scope-accesses \"SecurityAdmin\" \"Connectivity\" --network-manager-scopes management-groups=\"/providers/Microsoft.Management/testmg\" subscriptions=\"/subscriptions/00000000-0000-0000-0000-000000000000\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az network manager delete",
            "summary": "Delete a network manager.",
            "syntax": "az network manager delete [--force {0, 1, f, false, n, no, t, true, y, yes}]\n                          [--ids]\n                          [--name]\n                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                          [--resource-group]\n                          [--subscription]\n                          [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name --network-manager-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az network manager delete --name \"testNetworkManager\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az network manager list",
            "summary": "List all network managers in a subscription.",
            "syntax": "az network manager list [--max-items]\n                        [--next-token]\n                        [--resource-group]\n                        [--skip-token]\n                        [--top]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g",
                "--skip-token",
                "--top"
            ],
            "examples": [
                "az network manager list --resource-group \"rg1\""
            ]
        },
        {
            "name": "az network manager list-active-connectivity-config",
            "summary": "Lists active connectivity configurations in a network manager.",
            "syntax": "az network manager list-active-connectivity-config [--ids]\n                                                   [--name]\n                                                   [--regions]\n                                                   [--resource-group]\n                                                   [--skip-token]\n                                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --network-manager-name -n",
                "--regions",
                "--resource-group -g",
                "--skip-token",
                "--subscription"
            ],
            "examples": [
                "az network manager list-active-connectivity-config --network-manager-name \"testNetworkManager\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az network manager list-active-security-admin-rule",
            "summary": "Lists active security admin rules in a network manager.",
            "syntax": "az network manager list-active-security-admin-rule [--ids]\n                                                   [--name]\n                                                   [--regions]\n                                                   [--resource-group]\n                                                   [--skip-token]\n                                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --network-manager-name -n",
                "--regions",
                "--resource-group -g",
                "--skip-token",
                "--subscription"
            ],
            "examples": [
                "az network manager list-active-security-admin-rule --network-manager-name \"testNetworkManager\" --resource-group \"myResourceGroup\" --region \"eastus2euap\""
            ]
        },
        {
            "name": "az network manager list-deploy-status",
            "summary": "Post List of Network Manager Deployment Status.",
            "syntax": "az network manager list-deploy-status [--deployment-types]\n                                      [--ids]\n                                      [--name]\n                                      [--regions]\n                                      [--resource-group]\n                                      [--skip-token]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--deployment-types",
                "--ids",
                "--name --network-manager-name -n",
                "--regions",
                "--resource-group -g",
                "--skip-token",
                "--subscription"
            ],
            "examples": [
                "az network manager list-deploy-status --network-manager-name \"testNetworkManager\" --deployment-types \"Connectivity\" \"SecurityAdmin\" --regions \"eastus\" \"westus\" --resource-group \"resoureGroupSample\""
            ]
        },
        {
            "name": "az network manager list-effective-connectivity-config",
            "summary": "List all effective connectivity configurations applied on a virtual network.",
            "syntax": "az network manager list-effective-connectivity-config [--ids]\n                                                      [--resource-group]\n                                                      [--skip-token]\n                                                      [--subscription]\n                                                      [--virtual-network-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--skip-token",
                "--subscription",
                "--virtual-network-name --vnet-name"
            ],
            "examples": [
                "az network manager list-effective-connectivity-config --virtual-network-name \"myVirtualNetwork\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az network manager list-effective-security-admin-rule",
            "summary": "List all effective security admin rules applied on a virtual network.",
            "syntax": "az network manager list-effective-security-admin-rule [--ids]\n                                                      [--resource-group]\n                                                      [--skip-token]\n                                                      [--subscription]\n                                                      [--virtual-network-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--skip-token",
                "--subscription",
                "--virtual-network-name --vnet-name"
            ],
            "examples": [
                "az network manager list-effective-security-admin-rule --virtual-network-name \"myVirtualNetwork\" --resource-group \"myResourceGroup\""
            ]
        },
        {
            "name": "az network manager post-commit",
            "summary": "Post a Network Manager Commit.",
            "syntax": "az network manager post-commit --commit-type {Connectivity, SecurityAdmin}\n                               --target-locations\n                               [--configuration-ids]\n                               [--ids]\n                               [--name]\n                               [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [
                "--commit-type",
                "--target-locations"
            ],
            "optional_parameters": [
                "--configuration-ids",
                "--ids",
                "--name --network-manager-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network manager post-commit --network-manager-name \"testNetworkManager\" --commit-type \"SecurityAdmin\" --configuration-ids \"/subscriptions/subscriptionC/resourceGroups/resoureGroupSample/providers/Microsoft.Network/networkManagers/testNetworkManager/securityConfigurations/SampleSecurityConfig\" --target-locations \"eastus\" --resource-group \"resoureGroupSample\""
            ]
        },
        {
            "name": "az network manager show",
            "summary": "Get the specified Network Manager.",
            "syntax": "az network manager show [--ids]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --network-manager-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network manager show --name \"testNetworkManager\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az network manager update",
            "summary": "Update a Network Manager.",
            "syntax": "az network manager update [--description]\n                          [--id]\n                          [--ids]\n                          [--location]\n                          [--name]\n                          [--network-manager-scopes]\n                          [--resource-group]\n                          [--scope-accesses]\n                          [--subscription]\n                          [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--description",
                "--id",
                "--ids",
                "--location -l",
                "--name --network-manager-name -n",
                "--network-manager-scopes",
                "--resource-group -g",
                "--scope-accesses",
                "--subscription",
                "--tags"
            ],
            "examples": []
        },
        {
            "name": "az network manager wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az network manager wait [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--ids]\n                        [--interval]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]\n                        [--timeout]\n                        [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name --network-manager-name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}