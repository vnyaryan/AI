{
    "command_group": "az network vhub bgpconnection",
    "description": "Manage virtual hub bgpconnections.",
    "status": "GA",
    "commands": [
        {
            "name": "az network vhub bgpconnection create",
            "summary": "Create a virtual hub bgpconnection.",
            "syntax": "az network vhub bgpconnection create --name\n                                     --resource-group\n                                     --vhub-name\n                                     [--no-wait]\n                                     [--peer-asn]\n                                     [--peer-ip]\n                                     [--vhub-conn]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--peer-asn",
                "--peer-ip",
                "--vhub-conn"
            ],
            "examples": [
                "az network vhub bgpconnection create -n MyConnection -g MyRG --vhub-name MyHub --peer-asn 20000  --peer-ip \"10.5.0.3\"\n  --vhub-conn <vhub_connection_resource_id>"
            ]
        },
        {
            "name": "az network vhub bgpconnection delete",
            "summary": "Delete a virtual hub bgpconnection.",
            "syntax": "az network vhub bgpconnection delete [--ids]\n                                     [--name]\n                                     [--no-wait]\n                                     [--resource-group]\n                                     [--subscription]\n                                     [--vhub-name]\n                                     [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--vhub-name",
                "--yes -y"
            ],
            "examples": [
                "az network vhub bgpconnection delete -n MyConnection --vhub-name MyHub -g MyRG"
            ]
        },
        {
            "name": "az network vhub bgpconnection list",
            "summary": "List virtual hub bgpconnections.",
            "syntax": "az network vhub bgpconnection list --resource-group\n                                   --vhub-name",
            "required_parameters": [
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az network vhub bgpconnection list --vhub-name MyHub -g MyRG"
            ]
        },
        {
            "name": "az network vhub bgpconnection show",
            "summary": "Get the details of a virtual hub bgpconnection.",
            "syntax": "az network vhub bgpconnection show [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--vhub-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--vhub-name"
            ],
            "examples": [
                "az network vhub bgpconnection show -n MyConnection --vhub-name MyHub -g MyRG"
            ]
        },
        {
            "name": "az network vhub bgpconnection update",
            "summary": "Update settings of a virtual hub bgpconnection.",
            "syntax": "az network vhub bgpconnection update [--add]\n                                     [--force-string]\n                                     [--ids]\n                                     [--name]\n                                     [--no-wait]\n                                     [--peer-asn]\n                                     [--peer-ip]\n                                     [--remove]\n                                     [--resource-group]\n                                     [--set]\n                                     [--subscription]\n                                     [--vhub-conn]\n                                     [--vhub-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--no-wait",
                "--peer-asn",
                "--peer-ip",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--vhub-conn",
                "--vhub-name"
            ],
            "examples": [
                "az network vhub bgpconnection update -n MyConnection --vhub-name MyHub -g MyRG --peer-asn 15000"
            ]
        },
        {
            "name": "az network vhub bgpconnection wait",
            "summary": "Place the CLI in a waiting state until a condition of virtual hub bgpconnection is met.",
            "syntax": "az network vhub bgpconnection wait [--created]\n                                   [--custom]\n                                   [--deleted]\n                                   [--exists]\n                                   [--ids]\n                                   [--interval]\n                                   [--name]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--timeout]\n                                   [--updated]\n                                   [--vhub-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated",
                "--vhub-name"
            ],
            "examples": []
        }
    ]
}