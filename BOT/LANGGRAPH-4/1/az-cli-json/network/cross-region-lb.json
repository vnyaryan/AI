{
    "command_group": "az network cross-region-lb",
    "description": "Manage and configure cross-region load balancers.",
    "status": "GA",
    "commands": [
        {
            "name": "az network cross-region-lb create",
            "summary": "Create a cross-region load balancer.",
            "syntax": "az network cross-region-lb create --name\n                                  --resource-group\n                                  [--address-allocation {Dynamic, Static}]\n                                  [--backend-pool-name]\n                                  [--frontend-ip-name]\n                                  [--frontend-ip-zone]\n                                  [--location]\n                                  [--no-wait]\n                                  [--public-ip-address]\n                                  [--public-ip-dns-name]\n                                  [--public-ip-zone]\n                                  [--tags]\n                                  [--validate]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--address-allocation --public-ip-address-allocation",
                "--backend-pool-name",
                "--frontend-ip-name",
                "--frontend-ip-zone",
                "--location -l",
                "--no-wait",
                "--public-ip-address",
                "--public-ip-dns-name",
                "--public-ip-zone",
                "--tags",
                "--validate"
            ],
            "examples": [
                "az network cross-region-lb create -g MyResourceGroup -n MyLb"
            ]
        },
        {
            "name": "az network cross-region-lb delete",
            "summary": "Delete the specified load balancer.",
            "syntax": "az network cross-region-lb delete [--ids]\n                                  [--name]\n                                  [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                  [--resource-group]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network cross-region-lb delete -g MyResourceGroup -n MyLb"
            ]
        },
        {
            "name": "az network cross-region-lb list",
            "summary": "List load balancers.",
            "syntax": "az network cross-region-lb list [--max-items]\n                                [--next-token]\n                                [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az network cross-region-lb list -g MyResourceGroup"
            ]
        },
        {
            "name": "az network cross-region-lb show",
            "summary": "Get the details of a load balancer.",
            "syntax": "az network cross-region-lb show [--expand]\n                                [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--expand",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az network cross-region-lb show -g MyResourceGroup -n MyLb"
            ]
        },
        {
            "name": "az network cross-region-lb update",
            "summary": "Update a load balancer.",
            "syntax": "az network cross-region-lb update [--add]\n                                  [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                  [--ids]\n                                  [--name]\n                                  [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                  [--probes]\n                                  [--remove]\n                                  [--resource-group]\n                                  [--set]\n                                  [--subscription]\n                                  [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--no-wait",
                "--probes",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az network cross-region-lb update -g MyResourceGroup -n MyLB --tags CostCenter=MyTestGroup"
            ]
        },
        {
            "name": "az network cross-region-lb wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az network cross-region-lb wait [--created]\n                                [--custom]\n                                [--deleted]\n                                [--exists]\n                                [--expand]\n                                [--ids]\n                                [--interval]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]\n                                [--timeout]\n                                [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--ids",
                "--interval",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}