{
    "command_group": "az tsi event-source",
    "description": "Manage event source with timeseriesinsights.",
    "status": "GA",
    "commands": [
        {
            "name": "az tsi event-source delete",
            "summary": "Delete the event source with the specified name in the specified subscription, resource group, and environment.",
            "syntax": "az tsi event-source delete [--environment-name]\n                           [--event-source-name]\n                           [--ids]\n                           [--resource-group]\n                           [--subscription]\n                           [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--environment-name",
                "--event-source-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az tsi event-source delete --environment-name \"env1\" --name \"es1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az tsi event-source list",
            "summary": "List all the available event sources associated with the subscription and within the specified resource group and environment.",
            "syntax": "az tsi event-source list --environment-name\n                         --resource-group",
            "required_parameters": [
                "--environment-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az tsi event-source list --environment-name \"env1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az tsi event-source show",
            "summary": "Show the event source with the specified name in the specified environment.",
            "syntax": "az tsi event-source show [--environment-name]\n                         [--event-source-name]\n                         [--ids]\n                         [--resource-group]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--environment-name",
                "--event-source-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az tsi event-source show --environment-name \"env1\" --name \"es1\" --resource-group \"rg1\""
            ]
        }
    ]
}