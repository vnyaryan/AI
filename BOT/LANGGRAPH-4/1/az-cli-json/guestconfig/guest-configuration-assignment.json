{
    "command_group": "az guestconfig guest-configuration-assignment",
    "description": "Guestconfig guest-configuration-assignment.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az guestconfig guest-configuration-assignment create",
            "summary": "Creates an association between a VM and guest configuration.",
            "syntax": "az guestconfig guest-configuration-assignment create --guest-configuration-assignment-name\n                                                     --resource-group\n                                                     --vm-name\n                                                     [--context]\n                                                     [--guest-configuration-configuration-parameter]\n                                                     [--guest-configuration-configuration-setting]\n                                                     [--guest-configuration-name]\n                                                     [--guest-configuration-version]\n                                                     [--latest-assignment-report-assignment]\n                                                     [--location]\n                                                     [--name]\n                                                     [--no-wait]",
            "required_parameters": [
                "--guest-configuration-assignment-name",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [
                "--context",
                "--guest-configuration-configuration-parameter",
                "--guest-configuration-configuration-setting",
                "--guest-configuration-name",
                "--guest-configuration-version",
                "--latest-assignment-report-assignment",
                "--location -l",
                "--name",
                "--no-wait"
            ],
            "examples": [
                "az guestconfig guest-configuration-assignment create --guest-configuration-assignment-name \"WhitelistedApplication\" --guest-configuration-name \"WhitelistedApplication\" --location \"westcentralus\" --context \"Azure policy\" --guest-configuration-name \"WhitelistedApplication\" --guest-configuration-configuration-parameter name=\"[InstalledApplication]bwhitelistedapp;Name\" value=\"NotePad,sql\" --guest-configuration-configuration-setting action-after-reboot=\"ContinueConfiguration\" configuration-mode=\"MonitorOnly\" configuration-mode-frequency-mins=15 reboot-if-needed=\"False\" --guest-configuration-version \"1.*\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-assignment delete",
            "summary": "Delete a guest configuration assignment.",
            "syntax": "az guestconfig guest-configuration-assignment delete [--ids]\n                                                     [--name]\n                                                     [--no-wait]\n                                                     [--resource-group]\n                                                     [--subscription]\n                                                     [--vm-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--vm-name"
            ],
            "examples": [
                "az guestconfig guest-configuration-assignment delete --name \"SecureProtocol\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-assignment list",
            "summary": "List all guest configuration assignments for a virtual machine.",
            "syntax": "az guestconfig guest-configuration-assignment list --resource-group\n                                                   --vm-name",
            "required_parameters": [
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az guestconfig guest-configuration-assignment list --resource-group \"myResourceGroupName\" --vm-name \"myVMName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-assignment show",
            "summary": "Get information about a guest configuration assignment.",
            "syntax": "az guestconfig guest-configuration-assignment show [--ids]\n                                                   [--name]\n                                                   [--resource-group]\n                                                   [--subscription]\n                                                   [--vm-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--vm-name"
            ],
            "examples": [
                "az guestconfig guest-configuration-assignment show --name \"SecureProtocol\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-assignment update",
            "summary": "Creates an association between a VM and guest configuration.",
            "syntax": "az guestconfig guest-configuration-assignment update [--context]\n                                                     [--guest-configuration-assignment-name]\n                                                     [--guest-configuration-configuration-parameter]\n                                                     [--guest-configuration-configuration-setting]\n                                                     [--guest-configuration-name]\n                                                     [--guest-configuration-version]\n                                                     [--ids]\n                                                     [--latest-assignment-report-assignment]\n                                                     [--location]\n                                                     [--name]\n                                                     [--no-wait]\n                                                     [--resource-group]\n                                                     [--subscription]\n                                                     [--vm-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--context",
                "--guest-configuration-assignment-name",
                "--guest-configuration-configuration-parameter",
                "--guest-configuration-configuration-setting",
                "--guest-configuration-name",
                "--guest-configuration-version",
                "--ids",
                "--latest-assignment-report-assignment",
                "--location -l",
                "--name",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--vm-name"
            ],
            "examples": [
                "az guestconfig guest-configuration-assignment update --guest-configuration-assignment-name \"WhitelistedApplication\" --guest-configuration-name \"WhitelistedApplication\" --location \"westcentralus\" --context \"Azure policy\" --guest-configuration-name \"WhitelistedApplication\" --guest-configuration-configuration-parameter name=\"[InstalledApplication]bwhitelistedapp;Name\" value=\"NotePad,sql\" --guest-configuration-configuration-setting action-after-reboot=\"ContinueConfiguration\" configuration-mode=\"MonitorOnly\" configuration-mode-frequency-mins=15 reboot-if-needed=\"False\" --guest-configuration-version \"1.*\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\""
            ]
        },
        {
            "name": "az guestconfig guest-configuration-assignment wait",
            "summary": "Place the CLI in a waiting state until a condition of the guestconfig guest-configuration-assignment is met.",
            "syntax": "az guestconfig guest-configuration-assignment wait [--created]\n                                                   [--custom]\n                                                   [--deleted]\n                                                   [--exists]\n                                                   [--ids]\n                                                   [--interval]\n                                                   [--name]\n                                                   [--resource-group]\n                                                   [--subscription]\n                                                   [--timeout]\n                                                   [--updated]\n                                                   [--vm-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated",
                "--vm-name"
            ],
            "examples": [
                "az guestconfig guest-configuration-assignment wait --name \"SecureProtocol\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\" --created",
                "az guestconfig guest-configuration-assignment wait --name \"SecureProtocol\" --resource-group \"myResourceGroupName\" --vm-name \"myVMName\" --deleted"
            ]
        }
    ]
}