{
    "command_group": "az account management-group",
    "description": "Manage Azure Management Groups.",
    "status": "GA",
    "commands": [
        {
            "name": "az account management-group check-name-availability",
            "summary": "Check if a Management Group Name is Valid.",
            "syntax": "az account management-group check-name-availability --name",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az account management-group check-name-availability --name GroupName"
            ]
        },
        {
            "name": "az account management-group create",
            "summary": "Create a new management group.",
            "syntax": "az account management-group create --name\n                                   [--display-name]\n                                   [--no-register]\n                                   [--parent]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--display-name -d",
                "--no-register",
                "--parent -p"
            ],
            "examples": [
                "az account management-group create --name GroupName",
                "az account management-group create --name GroupName --display-name DisplayName",
                "az account management-group create --name GroupName --parent ParentId/ParentName",
                "az account management-group create --name GroupName --display-name DisplayName --parent ParentId/ParentName"
            ]
        },
        {
            "name": "az account management-group delete",
            "summary": "Delete an existing management group.",
            "syntax": "az account management-group delete --name\n                                   [--no-register]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--no-register"
            ],
            "examples": [
                "az account management-group delete --name GroupName"
            ]
        },
        {
            "name": "az account management-group list",
            "summary": "List all management groups in the current tenant.",
            "syntax": "az account management-group list [--no-register]",
            "required_parameters": [],
            "optional_parameters": [
                "--no-register"
            ],
            "examples": [
                "az account management-group list"
            ]
        },
        {
            "name": "az account management-group show",
            "summary": "Get the details of the management group.",
            "syntax": "az account management-group show --name\n                                 [--expand]\n                                 [--no-register]\n                                 [--recurse]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--expand -e",
                "--no-register",
                "--recurse -r"
            ],
            "examples": [
                "az account management-group show --name GroupName",
                "az account management-group show --name GroupName -e",
                "az account management-group show --name GroupName -e -r"
            ]
        },
        {
            "name": "az account management-group update",
            "summary": "Update an existing management group.",
            "syntax": "az account management-group update --name\n                                   [--add]\n                                   [--display-name]\n                                   [--force-string]\n                                   [--parent]\n                                   [--remove]\n                                   [--set]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--add",
                "--display-name -d",
                "--force-string",
                "--parent -p",
                "--remove",
                "--set"
            ],
            "examples": [
                "az account management-group update --name GroupName --display-name DisplayName",
                "az account management-group update --name GroupName --parent ParentId/ParentName",
                "az account management-group update --name GroupName --display-name DisplayName --parent ParentId/ParentName"
            ]
        }
    ]
}