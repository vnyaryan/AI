{
    "command_group": "az ml workspace",
    "description": "Manage Azure ML workspaces.",
    "status": "GA",
    "commands": [
        {
            "name": "az ml workspace create",
            "summary": "Create a workspace.",
            "syntax": "az ml workspace create --resource-group\n                       [--allow-roleassignment-on-rg]\n                       [--application-insights]\n                       [--container-registry]\n                       [--default-resource-group]\n                       [--description]\n                       [--display-name]\n                       [--enable-data-isolation]\n                       [--file]\n                       [--hub-id]\n                       [--image-build-compute]\n                       [--key-vault]\n                       [--kind]\n                       [--location]\n                       [--managed-network]\n                       [--name]\n                       [--no-wait]\n                       [--primary-user-assigned-identity]\n                       [--provision-network-now]\n                       [--public-network-access]\n                       [--set]\n                       [--storage-account]\n                       [--system-datastores-auth-mode]\n                       [--tags]\n                       [--update-dependent-resources]",
            "required_parameters": [
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--allow-roleassignment-on-rg -r",
                "--application-insights -a",
                "--container-registry -c",
                "--default-resource-group",
                "--description",
                "--display-name",
                "--enable-data-isolation -e",
                "--file -f",
                "--hub-id",
                "--image-build-compute -i",
                "--key-vault -k",
                "--kind",
                "--location -l",
                "--managed-network -m",
                "--name -n",
                "--no-wait",
                "--primary-user-assigned-identity -p",
                "--provision-network-now",
                "--public-network-access",
                "--set",
                "--storage-account -s",
                "--system-datastores-auth-mode -d",
                "--tags",
                "--update-dependent-resources -u"
            ],
            "examples": [
                "az ml workspace create --file workspace.yml --resource-group my-resource-group"
            ]
        },
        {
            "name": "az ml workspace delete",
            "summary": "Delete a workspace.",
            "syntax": "az ml workspace delete --name\n                       --resource-group\n                       [--all-resources]\n                       [--no-wait]\n                       [--permanently-delete]\n                       [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--all-resources",
                "--no-wait",
                "--permanently-delete -p",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az ml workspace diagnose",
            "summary": "Diagnose workspace setup problems.",
            "syntax": "az ml workspace diagnose --name\n                         --resource-group\n                         [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az ml workspace diagnose --name my-workspace-name --no-wait -g my-resource-group"
            ]
        },
        {
            "name": "az ml workspace list",
            "summary": "List all the workspaces in a subscription.",
            "syntax": "az ml workspace list [--filtered-kinds]\n                     [--max-results]\n                     [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--filtered-kinds",
                "--max-results -r",
                "--resource-group -g"
            ],
            "examples": [
                "az ml workspace list --resource-group my-resource-group",
                "az ml workspace list --query \"[].{Name:name}\"  --output table --resource-group my-resource-group"
            ]
        },
        {
            "name": "az ml workspace list-keys",
            "summary": "List workspace keys for dependent resources such as Azure Storage, Azure Container Registry, and Azure Application Insights.",
            "syntax": "az ml workspace list-keys --name\n                          --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az ml workspace provision-network",
            "summary": "Provision workspace managed network.",
            "syntax": "az ml workspace provision-network --name\n                                  --resource-group\n                                  [--include-spark]\n                                  [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--include-spark",
                "--no-wait"
            ],
            "examples": [
                "az ml workspace provision-network --include-spark --name my-workspace-name --no-wait -g my-resource-group"
            ]
        },
        {
            "name": "az ml workspace show",
            "summary": "Show details for a workspace.",
            "syntax": "az ml workspace show --name\n                     --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az ml workspace sync-keys",
            "summary": "Sync workspace keys for dependent resources such as Azure Storage, Azure Container Registry, and Azure Application Insights.",
            "syntax": "az ml workspace sync-keys --name\n                          --resource-group\n                          [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml workspace update",
            "summary": "Update a workspace.",
            "syntax": "az ml workspace update --name\n                       --resource-group\n                       [--add]\n                       [--allow-roleassignment-on-rg]\n                       [--application-insights]\n                       [--container-registry]\n                       [--description]\n                       [--display-name]\n                       [--file]\n                       [--force-string]\n                       [--image-build-compute]\n                       [--managed-network]\n                       [--network-acls]\n                       [--no-wait]\n                       [--primary-user-assigned-identity]\n                       [--public-network-access]\n                       [--remove]\n                       [--set]\n                       [--system-datastores-auth-mode]\n                       [--update-dependent-resources]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--add",
                "--allow-roleassignment-on-rg -r",
                "--application-insights -a",
                "--container-registry -c",
                "--description",
                "--display-name",
                "--file -f",
                "--force-string",
                "--image-build-compute -i",
                "--managed-network -m",
                "--network-acls",
                "--no-wait",
                "--primary-user-assigned-identity -p",
                "--public-network-access",
                "--remove",
                "--set",
                "--system-datastores-auth-mode -d",
                "--update-dependent-resources -u"
            ],
            "examples": [
                "az ml workspace update --resource-group my-resource-group --name my-workspace-name --file workspace.yml"
            ]
        }
    ]
}