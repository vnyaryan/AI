{
    "command_group": "az sql stg",
    "description": "Manage Server Trust Groups.",
    "status": "Preview",
    "commands": [
        {
            "name": "az sql stg create",
            "summary": "Create a Server Trust Group.",
            "syntax": "az sql stg create --group-member\n                  --location\n                  --name\n                  --resource-group\n                  --trust-scope\n                  [--no-wait]",
            "required_parameters": [
                "--group-member -m",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--trust-scope"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az sql stg create -g resourcegroup -l location -n stg-name --trust-scope GlobalTransactions -m $mi1-id $mi2-id"
            ]
        },
        {
            "name": "az sql stg delete",
            "summary": "Delete a Server Trust Group.",
            "syntax": "az sql stg delete --location\n                  --name\n                  --resource-group\n                  [--no-wait]\n                  [--yes]",
            "required_parameters": [
                "--location -l",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az sql stg delete -g resourcegroup -l location -n stg-name"
            ]
        },
        {
            "name": "az sql stg list",
            "summary": "Retrieve a list of Server Trust Groups.",
            "syntax": "az sql stg list --resource-group\n                [--instance-name]\n                [--location]",
            "required_parameters": [
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--instance-name",
                "--location -l"
            ],
            "examples": [
                "az sql stg list -g resourcegroup --instance-name mi1-name",
                "az sql stg list -g resourcegroup -l location"
            ]
        },
        {
            "name": "az sql stg show",
            "summary": "Retrieve a Server Trust Group.",
            "syntax": "az sql stg show --location\n                --name\n                --resource-group",
            "required_parameters": [
                "--location -l",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql stg show -g resourcegroup -l location -n stg-name"
            ]
        }
    ]
}