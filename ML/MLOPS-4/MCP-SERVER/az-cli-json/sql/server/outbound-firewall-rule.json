                "--ids",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql server outbound-firewall-rule list -g mygroup -s myserver"
            ]
        },
        {
            "name": "az sql server outbound-firewall-rule show",
            "summary": "Show the details for an outbound firewall rule.",
            "syntax": "az sql server outbound-firewall-rule show --outbound-rule-fqdn\n                                          [--ids]\n                                          [--resource-group]\n                                          [--server]\n                                          [--subscription]",
