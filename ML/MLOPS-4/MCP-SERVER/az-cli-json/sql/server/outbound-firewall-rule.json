{
    "command_group": "az sql server outbound-firewall-rule",
    "description": "Manage a server's outbound firewall rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql server outbound-firewall-rule create",
            "summary": "Create a new outbound firewall rule.",
            "syntax": "az sql server outbound-firewall-rule create --outbound-rule-fqdn\n                                            --resource-group\n                                            --server",
            "required_parameters": [
                "--outbound-rule-fqdn -n",
                "--resource-group -g",
                "--server -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az sql server outbound-firewall-rule create -g mygroup -s myserver -n allowedFQDN",
                "az sql server outbound-firewall-rule create -g mygroup -s myserver --outbound-rule-fqdn allowedFQDN"
            ]
        },
        {
            "name": "az sql server outbound-firewall-rule delete",
            "summary": "Delete the outbound firewall rule.",
            "syntax": "az sql server outbound-firewall-rule delete --outbound-rule-fqdn\n                                            [--ids]\n                                            [--resource-group]\n                                            [--server]\n                                            [--subscription]",
            "required_parameters": [
                "--outbound-rule-fqdn -n"
            ],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql server outbound-firewall-rule delete -g mygroup -s myserver -n myrule",
                "az sql server outbound-firewall-rule delete -g mygroup -s myserver --outbound-rule-fqdn allowedFQDN"
            ]
        },
        {
            "name": "az sql server outbound-firewall-rule list",
