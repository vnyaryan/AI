{
    "command_group": "az sql server firewall-rule",
    "description": "Manage a server's firewall rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql server firewall-rule create",
            "summary": "Create a firewall rule.",
            "syntax": "az sql server firewall-rule create --name\n                                   --resource-group\n                                   --server\n                                   [--end-ip-address]\n                                   [--start-ip-address]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server -s"
            ],
            "optional_parameters": [
                "--end-ip-address",
                "--start-ip-address"
            ],
            "examples": [
                "az sql server firewall-rule create -g mygroup -s myserver -n myrule --start-ip-address 1.2.3.4 --end-ip-address 5.6.7.8",
                "az sql server firewall-rule create -g mygroup -s myserver -n myrule --start-ip-address 0.0.0.0 --end-ip-address 0.0.0.0"
            ]
        },
        {
            "name": "az sql server firewall-rule delete",
            "summary": "Deletes a firewall rule.",
            "syntax": "az sql server firewall-rule delete [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--server]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az sql server firewall-rule list",
            "summary": "List a server's firewall rules.",
            "syntax": "az sql server firewall-rule list [--ids]\n                                 [--resource-group]\n                                 [--server]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql server firewall-rule list -g mygroup -s myserver"
            ]
        },
        {
            "name": "az sql server firewall-rule show",
            "summary": "Shows the details for a firewall rule.",
            "syntax": "az sql server firewall-rule show [--ids]\n                                 [--name]\n                                 [--resource-group]\n                                 [--server]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--subscription"
            ],
            "examples": [
                "az sql server firewall-rule show -g mygroup -s myserver -n myrule"
            ]
        },
        {
            "name": "az sql server firewall-rule update",
            "summary": "Update a firewall rule.",
            "syntax": "az sql server firewall-rule update [--end-ip-address]\n                                   [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--server]\n                                   [--start-ip-address]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--end-ip-address",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--server -s",
                "--start-ip-address",
                "--subscription"
            ],
            "examples": [
                "az sql server firewall-rule update -g mygroup -s myserver -n myrule --start-ip-address 5.4.3.2 --end-ip-address 9.8.7.6"
            ]
        }
    ]
}