{
    "command_group": "az security assessment",
    "description": "View your security assessment results.",
    "status": "GA",
    "commands": [
        {
            "name": "az security assessment create",
            "summary": "Creates a customer managed security assessment.",
            "syntax": "az security assessment create --name\n                              --status-code\n                              [--additional-data]\n                              [--assessed-resource-id]\n                              [--status-cause]\n                              [--status-description]",
            "required_parameters": [
                "--name -n",
                "--status-code"
            ],
            "optional_parameters": [
                "--additional-data",
                "--assessed-resource-id",
                "--status-cause",
                "--status-description"
            ],
            "examples": [
                "az security assessment create -n '4fb6c0a0-1137-42c7-a1c7-4bd37c91de8d' --status-code  'unhealthy'"
            ]
        },
        {
            "name": "az security assessment delete",
            "summary": "Deletes a security assessment.",
            "syntax": "az security assessment delete --name\n                              [--assessed-resource-id]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--assessed-resource-id"
            ],
            "examples": [
                "az security assessment delete -n '4fb6c0a0-1137-42c7-a1c7-4bd37c91de8d'"
            ]
        },
        {
            "name": "az security assessment list",
            "summary": "List all security assessment results.",
            "syntax": "az security assessment list",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": [
                "az security assessment list"
            ]
        },
        {
            "name": "az security assessment show",
            "summary": "Shows a security assessment.",
            "syntax": "az security assessment show --name\n                            [--assessed-resource-id]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--assessed-resource-id"
            ],
            "examples": [
                "az security assessment show -n '4fb6c0a0-1137-42c7-a1c7-4bd37c91de8d'"
            ]
        }
    ]
}