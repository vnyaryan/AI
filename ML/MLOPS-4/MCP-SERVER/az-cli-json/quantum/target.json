{
    "command_group": "az quantum target",
    "description": "Manage targets for Azure Quantum workspaces.",
    "status": "Preview",
    "commands": [
        {
            "name": "az quantum target clear",
            "summary": "Clear the default target-id.",
            "syntax": "az quantum target clear",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": [
                "az quantum target clear"
            ]
        },
        {
            "name": "az quantum target list",
            "summary": "Get the list of providers and their targets in an Azure Quantum workspace.",
            "syntax": "az quantum target list --location\n                       --resource-group\n                       --workspace-name",
            "required_parameters": [
                "--location -l",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az quantum target list -g MyResourceGroup -w MyWorkspace -l MyLocation"
            ]
        },
        {
            "name": "az quantum target set",
            "summary": "Select the default target to use when submitting jobs to Azure Quantum.",
            "syntax": "az quantum target set --target-id",
            "required_parameters": [
                "--target-id -t"
            ],
            "optional_parameters": [],
            "examples": [
                "az quantum target set -t target-id"
            ]
        },
        {
            "name": "az quantum target show",
            "summary": "Get the Target ID of the current default target to use when submitting jobs to Azure Quantum.",
            "syntax": "az quantum target show [--target-id]",
            "required_parameters": [],
            "optional_parameters": [
                "--target-id -t"
            ],
            "examples": [
                "az quantum target show"
            ]
        }
    ]
}