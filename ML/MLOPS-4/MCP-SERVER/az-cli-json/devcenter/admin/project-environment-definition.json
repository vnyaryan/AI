{
    "command_group": "az devcenter admin project-environment-definition",
    "description": "Manage environment definitions in the project catalog.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin project-environment-definition get-error-detail",
            "summary": "Gets Environment Definition error details.",
            "syntax": "az devcenter admin project-environment-definition get-error-detail [--catalog-name]\n                                                                   [--environment-definition-name]\n                                                                   [--ids]\n                                                                   [--project]\n                                                                   [--resource-group]\n                                                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--catalog-name",
                "--environment-definition-name --name -n",
                "--ids",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin project-environment-definition get-error-detail --catalog-name \"myCatalog\" --project \"Contoso\" --environment-definition-name \"myEnvironmentDefinition\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-environment-definition list",
            "summary": "List the environment definitions in this project catalog.",
            "syntax": "az devcenter admin project-environment-definition list --catalog-name\n                                                       --project\n                                                       --resource-group\n                                                       [--max-items]\n                                                       [--next-token]",
            "required_parameters": [
                "--catalog-name",
                "--project --project-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin project-environment-definition list --catalog-name \"myCatalog\" --project \"ContosoProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-environment-definition show",
            "summary": "Get an environment definition from the catalog.",
            "syntax": "az devcenter admin project-environment-definition show [--catalog-name]\n                                                       [--environment-definition-name]\n                                                       [--ids]\n                                                       [--project]\n                                                       [--resource-group]\n                                                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--catalog-name",
                "--environment-definition-name --name -n",
                "--ids",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin environment-definition show --catalog-name \"myCatalog\" --project \"ContosoProject\" --environment-definition-name \"myEnvironmentDefinition\" --resource-group \"rg1\""
            ]
        }
    ]
}