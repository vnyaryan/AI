{
    "command_group": "az devcenter admin image-definition-build",
    "description": "Manage image definition builds.",
    "status": "Preview",
    "commands": [
        {
            "name": "az devcenter admin image-definition-build cancel",
            "summary": "Cancels the specified build for an image definition.",
            "syntax": "az devcenter admin image-definition-build cancel [--build-name]\n                                                 [--catalog-name]\n                                                 [--ids]\n                                                 [--image-definition-name]\n                                                 [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                                 [--project]\n                                                 [--resource-group]\n                                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--build-name --name -n",
                "--catalog-name",
                "--ids",
                "--image-definition-name -i",
                "--no-wait",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin image-definition-build cancel --build-name \"0a28fc61-6f87-4611-8fe2-32df44ab93b7\" --catalog-name \"CentralCatalog\" --image-definition-name \"DefaultDevImage\" --project-name \"rg1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin image-definition-build get-build-detail",
            "summary": "Gets Build details.",
            "syntax": "az devcenter admin image-definition-build get-build-detail [--build-name]\n                                                           [--catalog-name]\n                                                           [--ids]\n                                                           [--image-definition-name]\n                                                           [--project]\n                                                           [--resource-group]\n                                                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--build-name --name -n",
                "--catalog-name",
                "--ids",
                "--image-definition-name -i",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin image-definition-build get-build-detail --build-name \"0a28fc61-6f87-4611-8fe2-32df44ab93b7\" --catalog-name \"CentralCatalog\" --image-definition-name \"DefaultDevImage\" --project-name \"rg1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin image-definition-build list",
            "summary": "List builds for a specified image definition.",
            "syntax": "az devcenter admin image-definition-build list --catalog-name\n                                               --image-definition-name\n                                               --project\n                                               --resource-group\n                                               [--max-items]\n                                               [--next-token]",
            "required_parameters": [
                "--catalog-name",
                "--image-definition-name -i",
                "--project --project-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin image-definition-build list --catalog-name \"CentralCatalog\" --image-definition-name \"DefaultDevImage\" --project-name \"rg1\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin image-definition-build show",
            "summary": "Get a build for a specified image definition.",
            "syntax": "az devcenter admin image-definition-build show [--build-name]\n                                               [--catalog-name]\n                                               [--ids]\n                                               [--image-definition-name]\n                                               [--project]\n                                               [--resource-group]\n                                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--build-name --name -n",
                "--catalog-name",
                "--ids",
                "--image-definition-name -i",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin image-definition-build show --build-name \"0a28fc61-6f87-4611-8fe2-32df44ab93b7\" --catalog-name \"CentralCatalog\" --image-definition-name \"DefaultDevImage\" --project-name \"rg1\" --resource-group \"rg1\""
            ]
        }
    ]
}