            "syntax": "az devcenter admin gallery wait [--created]\n                                [--custom]\n                                [--deleted]\n                                [--dev-center]\n                                [--exists]\n                                [--gallery-name]\n                                [--ids]\n                                [--interval]\n                                [--resource-group]\n                                [--subscription]\n                                [--timeout]\n                                [--updated]",
