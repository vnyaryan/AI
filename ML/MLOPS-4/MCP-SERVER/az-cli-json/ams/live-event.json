            "syntax": "az ams live-event wait [--account-name]\n                       [--created]\n                       [--custom]\n                       [--deleted]\n                       [--exists]\n                       [--ids]\n                       [--interval]\n                       [--name]\n                       [--resource-group]\n                       [--subscription]\n                       [--timeout]\n                       [--updated]",
