{
    "command_group": "az ams live-event",
    "description": "Manage live events for an Azure Media Service account.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams live-event create",
            "summary": "Create a live event.",
            "syntax": "az ams live-event create --account-name\n                         --ips\n                         --name\n                         --resource-group\n                         --streaming-protocol {FragmentedMP4, RTMP}\n                         [--access-token]\n                         [--alternative-media-id]\n                         [--auto-start]\n                         [--client-access-policy]\n                         [--cross-domain-policy]\n                         [--description]\n                         [--encoding-type]\n                         [--hostname-prefix]\n                         [--key-frame-interval]\n                         [--key-frame-interval-duration]\n                         [--no-wait]\n                         [--preset-name]\n                         [--preview-ips]\n                         [--preview-locator]\n                         [--stream-options {Default, LowLatency, LowLatencyV2}]\n                         [--streaming-policy-name]\n                         [--stretch-mode]\n                         [--tags]\n                         [--transcription-lang]\n                         [--use-static-hostname]",
            "required_parameters": [
                "--account-name -a",
                "--ips",
                "--name -n",
                "--resource-group -g",
                "--streaming-protocol"
            ],
            "optional_parameters": [
                "--access-token",
                "--alternative-media-id",
                "--auto-start",
                "--client-access-policy",
                "--cross-domain-policy",
                "--description",
                "--encoding-type",
                "--hostname-prefix",
                "--key-frame-interval",
                "--key-frame-interval-duration",
                "--no-wait",
                "--preset-name",
                "--preview-ips",
                "--preview-locator",
                "--stream-options",
                "--streaming-policy-name",
                "--stretch-mode",
                "--tags",
                "--transcription-lang",
                "--use-static-hostname"
            ],
            "examples": []
        },
        {
            "name": "az ams live-event delete",
            "summary": "Delete a live event.",
            "syntax": "az ams live-event delete [--account-name]\n                         [--ids]\n                         [--name]\n                         [--resource-group]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams live-event list",
            "summary": "List all the live events of an Azure Media Services account.",
            "syntax": "az ams live-event list --account-name\n                       --resource-group",
            "required_parameters": [
                "--account-name -a",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az ams live-event list -a amsAccount -g resourceGroup --query [].{liveEventName:name,state:resourceState}"
            ]
        },
        {
            "name": "az ams live-event reset",
