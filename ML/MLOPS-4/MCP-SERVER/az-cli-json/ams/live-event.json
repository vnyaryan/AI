            "name": "az ams live-event update",
            "summary": "Update the details of a live event.",
            "syntax": "az ams live-event update [--account-name]\n                         [--add]\n                         [--client-access-policy]\n                         [--cross-domain-policy]\n                         [--description]\n                         [--force-string]\n                         [--ids]\n                         [--ips]\n                         [--key-frame-interval-duration]\n                         [--name]\n                         [--preview-ips]\n                         [--remove]\n                         [--resource-group]\n                         [--set]\n                         [--subscription]\n                         [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--add",
