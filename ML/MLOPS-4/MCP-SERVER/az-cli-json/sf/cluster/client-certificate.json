{
    "command_group": "az sf cluster client-certificate",
    "description": "Manage the client certificate of a cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az sf cluster client-certificate add",
            "summary": "Add a common name or certificate thumbprint to the cluster for client authentication.",
            "syntax": "az sf cluster client-certificate add --cluster-name\n                                     --resource-group\n                                     [--admin-client-thumbprints]\n                                     [--cert-common-name]\n                                     [--cert-issuer-tp]\n                                     [--client-cert-cn]\n                                     [--is-admin]\n                                     [--readonly-client-thumbprints]\n                                     [--thumbprint]",
            "required_parameters": [
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--admin-client-thumbprints --admin-client-tps",
                "--cert-common-name --certificate-common-name",
                "--cert-issuer-tp --certificate-issuer-thumbprint",
                "--client-cert-cn --client-certificate-common-names",
                "--is-admin",
                "--readonly-client-thumbprints --readonly-client-tps",
                "--thumbprint"
            ],
            "examples": [
                "az sf cluster client-certificate add -g group-name -c cluster1 --thumbprint '5F3660C715EBBDA31DB1FFDCF508302348DE8E7A'"
            ]
        },
        {
            "name": "az sf cluster client-certificate remove",
            "summary": "Remove client certificates or subject names used for authentication.",
            "syntax": "az sf cluster client-certificate remove --cluster-name\n                                        --resource-group\n                                        [--cert-common-name]\n                                        [--cert-issuer-tp]\n                                        [--client-cert-cn]\n                                        [--thumbprints]",
            "required_parameters": [
                "--cluster-name -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--cert-common-name --certificate-common-name",
                "--cert-issuer-tp --certificate-issuer-thumbprint",
                "--client-cert-cn --client-certificate-common-names",
                "--thumbprints"
            ],
            "examples": [
                "az sf cluster client-certificate remove -g group-name -c cluster1 --thumbprint '5F3660C715EBBDA31DB1FFDCF508302348DE8E7A'"
            ]
        }
    ]
}