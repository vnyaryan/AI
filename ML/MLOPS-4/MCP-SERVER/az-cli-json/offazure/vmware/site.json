{
    "command_group": "az offazure vmware site",
    "description": "Manage VMware site.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az offazure vmware site create",
            "summary": "Create a site for VMware resources.",
            "syntax": "az offazure vmware site create --name\n                               --resource-group\n                               [--agent]\n                               [--appliance-name]\n                               [--identity]\n                               [--location]\n                               [--solution-id]\n                               [--tags]",
            "required_parameters": [
                "--name --site-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--agent",
                "--appliance-name",
                "--identity",
                "--location -l",
                "--solution-id",
                "--tags"
            ],
            "examples": [
                "az offazure vmware site create -g MyResourceGroup --site-name MySiteName --location centralus"
            ]
        },
        {
            "name": "az offazure vmware site delete",
            "summary": "Delete a VMware site.",
            "syntax": "az offazure vmware site delete [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --site-name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az offazure vmware site delete --resource-group MyResourceGroup --name MySiteName"
            ]
        },
        {
            "name": "az offazure vmware site show",
            "summary": "Get the details of a VMware site.",
            "syntax": "az offazure vmware site show [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --site-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az offazure vmware site show --resource-group MyResourceGroup --name MySiteName"
            ]
        }
    ]
}