{
    "command_group": "az offazure hyperv run-as-account",
    "description": "Manage Hyper-V run-as-account.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az offazure hyperv run-as-account list",
            "summary": "List all run-as-accounts on the on-premise site.",
            "syntax": "az offazure hyperv run-as-account list --resource-group\n                                       --site-name",
            "required_parameters": [
                "--resource-group -g",
                "--site-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az offazure hyperv run-as-account list --resource-group MyResourceGroup                  --site-name MySiteName"
            ]
        },
        {
            "name": "az offazure hyperv run-as-account show",
            "summary": "Get the details of a run-as-account.",
            "syntax": "az offazure hyperv run-as-account show [--account-name]\n                                       [--ids]\n                                       [--resource-group]\n                                       [--site-name]\n                                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name --name -n",
                "--ids",
                "--resource-group -g",
                "--site-name",
                "--subscription"
            ],
            "examples": [
                "az offazure hyperv run-as-account show --account-name MyAccount --resource-group                  MyResourceGroup --site-name MySiteName"
            ]
        }
    ]
}