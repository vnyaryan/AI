{
    "command_group": "az mdp pool",
    "description": "Manage a pool resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az mdp pool create",
            "summary": "Create a pool.",
            "syntax": "az mdp pool create --name\n                   --resource-group\n                   [--agent-profile]\n                   [--devcenter-project-id]\n                   [--fabric-profile]\n                   [--identity]\n                   [--location]\n                   [--maximum-concurrency]\n                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                   [--organization-profile]\n                   [--tags]",
            "required_parameters": [
                "--name --pool-name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--agent-profile",
                "--devcenter-project-id --devcenter-project-resource-id",
                "--fabric-profile",
                "--identity",
                "--location -l",
                "--maximum-concurrency",
                "--no-wait",
                "--organization-profile",
                "--tags"
            ],
            "examples": [
                "az mdp pool create --location \"eastus\" --name \"cli-contoso-pool\" --resource-group \"rg1\"\n--identity \"type=userAssigned\" \"user-assigned-identities={'/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/example/providers/Microsoft.ManagedIdentity/userAssignedIdentities/test-msi':{}}\"\n--maximum-concurrency 3\n--agent-profile \"Stateless={}\"\n--organization-profile \"azure-dev-ops={organizations:[{url:'https://dev.azure.com/test-org',parallelism:2}],permissionProfile:{kind:'CreatorOnly'}}\"\n--devcenter-project-resource-id \"/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/example/providers/Microsoft.DevCenter/projects/contoso-proj\"\n--fabric-profile \"vmss={sku:{name:Standard_D2ads_v5},storageProfile:{osDiskStorageAccountType:Standard},images:[{resourceId:'/Subscriptions/00000000-0000-0000-0000-000000000000/Providers/Microsoft.Compute/Locations/eastus2/Publishers/canonical/ArtifactTypes/VMImage/Offers/0001-com-ubuntu-server-focal/Skus/20_04-lts-gen2/versions/latest',buffer:*}],osProfile:{secretsManagementSettings:{observedCertificates:[],keyExportable:false},logonType:Service}}\"",
                "az mdp pool create --location \"eastus\" --name \"cli-contoso-pool\" --resource-group \"rg1\"\n--maximum-concurrency 3\n--agent-profile \"{Stateless:{},resourcePredictionsProfile:{Manual:{}},resourcePredictions:{timezone:'UTC',daysData:[{},{},{},{'09:00:00':2,'11:00:00':0},{},{},{}]}}\"\n--organization-profile \"azure-dev-ops={organizations:[{url:'https://dev.azure.com/test-org',parallelism:2}],permissionProfile:{kind:'CreatorOnly'}}\"\n--devcenter-project-resource-id \"/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/example/providers/Microsoft.DevCenter/projects/contoso-proj\"\n--fabric-profile \"vmss={sku:{name:Standard_D2ads_v5},storageProfile:{osDiskStorageAccountType:Standard},images:[{resourceId:'/Subscriptions/00000000-0000-0000-0000-000000000000/Providers/Microsoft.Compute/Locations/eastus2/Publishers/canonical/ArtifactTypes/VMImage/Offers/0001-com-ubuntu-server-focal/Skus/20_04-lts-gen2/versions/latest',buffer:*}],osProfile:{secretsManagementSettings:{observedCertificates:[],keyExportable:false},logonType:Service}}\""
            ]
        },
        {
            "name": "az mdp pool delete",
            "summary": "Delete a pool.",
            "syntax": "az mdp pool delete [--ids]\n                   [--name]\n                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                   [--resource-group]\n                   [--subscription]\n                   [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --pool-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az mdp pool delete --name \"cli-contoso-pool\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az mdp pool list",
            "summary": "List all pools.",
            "syntax": "az mdp pool list [--max-items]\n                 [--next-token]\n                 [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az mdp pool list --resource-group \"rg1\"",
                "az mdp pool list"
            ]
        },
        {
            "name": "az mdp pool show",
            "summary": "Get a pool.",
            "syntax": "az mdp pool show [--ids]\n                 [--name]\n                 [--resource-group]\n                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --pool-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az mdp pool show --name \"cli-contoso-pool\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az mdp pool update",
            "summary": "Update a pool.",
            "syntax": "az mdp pool update [--add]\n                   [--agent-profile]\n                   [--devcenter-project-id]\n                   [--fabric-profile]\n                   [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                   [--identity]\n                   [--ids]\n                   [--maximum-concurrency]\n                   [--name]\n                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                   [--organization-profile]\n                   [--remove]\n                   [--resource-group]\n                   [--set]\n                   [--subscription]\n                   [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--agent-profile",
                "--devcenter-project-id --devcenter-project-resource-id",
                "--fabric-profile",
                "--force-string",
                "--identity",
                "--ids",
                "--maximum-concurrency",
                "--name --pool-name -n",
                "--no-wait",
                "--organization-profile",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az mdp pool update --name \"cli-contoso-pool\" --resource-group \"rg1\" --tags CostCode=\"12345\""
            ]
        },
        {
            "name": "az mdp pool wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az mdp pool wait [--created]\n                 [--custom]\n                 [--deleted]\n                 [--exists]\n                 [--ids]\n                 [--interval]\n                 [--name]\n                 [--resource-group]\n                 [--subscription]\n                 [--timeout]\n                 [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name --pool-name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}