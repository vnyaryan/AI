{
    "command_group": "az aksarc release",
    "description": "Manage VHD images for provisioned clusters.",
    "status": "Preview",
    "commands": [
        {
            "name": "az aksarc release download",
            "summary": "Downloads the VHD images from a release.",
            "syntax": "az aksarc release download --staging-folder\n                           [--audience]\n                           [--catalog]\n                           [--mode {full, minimum}]\n                           [--version]",
            "required_parameters": [
                "--staging-folder"
            ],
            "optional_parameters": [
                "--audience",
                "--catalog",
                "--mode",
                "--version"
            ],
            "examples": [
                "az aksarc release download --staging-folder ./aksarc-release"
            ]
        },
        {
            "name": "az aksarc release save",
            "summary": "Provisions the downloaded VHD images to the arc resource bridge.",
            "syntax": "az aksarc release save --staging-folder\n                       [--config-file]",
            "required_parameters": [
                "--staging-folder"
            ],
            "optional_parameters": [
                "--config-file"
            ],
            "examples": [
                "az aksarc release save --config-file ./work/hci-appliance.yaml --staging-folder ./aksarc-release"
            ]
        }
    ]
}