{
    "command_group": "az edgeorder order-item",
    "description": "Manage order item with edgeorder sub group order-item.",
    "status": "GA",
    "commands": [
        {
            "name": "az edgeorder order-item cancel",
            "summary": "Cancel order item.",
            "syntax": "az edgeorder order-item cancel --reason\n                               [--ids]\n                               [--name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [
                "--reason"
            ],
            "optional_parameters": [
                "--ids",
                "--name --order-item-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az edgeorder order-item cancel --reason \"Order cancelled\" --name \"TestOrderItemName1\" --resource-group \"TestRG\""
            ]
        },
        {
            "name": "az edgeorder order-item create",
            "summary": "Create an order item. Existing order item cannot be updated with this api and should instead be updated with the Update order item API.",
            "syntax": "az edgeorder order-item create --name\n                               --order-item-resource\n                               --resource-group\n                               [--no-wait]",
            "required_parameters": [
                "--name --order-item-name -n",
                "--order-item-resource",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az edgeorder order-item create --name \"TestOrderItemName01\" --order-item-resource \"{\\\"location\\\":\\\"eastus\\\",\\\"tags\\\":{\\\"carrot\\\":\\\"vegetable\\\",\\\"mango\\\":\\\"fruit\\\"},\\\"orderItemDetails\\\":{\\\"orderItemType\\\":\\\"Purchase\\\",\\\"preferences\\\":{\\\"transportPreferences\\\":{\\\"preferredShipmentType\\\":\\\"MicrosoftManaged\\\"}},\\\"productDetails\\\":{\\\"hierarchyInformation\\\":{\\\"configurationName\\\":\\\"edgep_base\\\",\\\"productFamilyName\\\":\\\"azurestackedge\\\",\\\"productLineName\\\":\\\"azurestackedge\\\",\\\"productName\\\":\\\"azurestackedgegpu\\\"}}},\\\"addressDetails\\\":{\\\"forwardAddress\\\":{\\\"contactDetails\\\":{\\\"contactName\\\":\\\"Petr Cech\\\",\\\"emailList\\\":[\\\"ssemmail@microsoft.com\\\",\\\"vishwamdir@microsoft.com\\\"],\\\"phone\\\":\\\"3213131190\\\",\\\"phoneExtension\\\":\\\"\\\"},\\\"shippingAddress\\\":{\\\"addressType\\\":\\\"None\\\",\\\"city\\\":\\\"San Francisco\\\",\\\"companyName\\\":\\\"Microsoft\\\",\\\"country\\\":\\\"US\\\",\\\"postalCode\\\":\\\"94107\\\",\\\"stateOrProvince\\\":\\\"CA\\\",\\\"streetAddress1\\\":\\\"16 TOWNSEND ST\\\",\\\"streetAddress2\\\":\\\"UNIT 1\\\"}}},\\\"orderId\\\":\\\"/subscriptions/fa68082f-8ff7-4a25-95c7-ce9da541242f/resourceGroups/TestRG/providers/Microsoft.EdgeOrder/locations/eastus/orders/TestOrderItemName01\\\"}\" --resource-group \"TestRG\""
            ]
        },
        {
            "name": "az edgeorder order-item delete",
            "summary": "Delete an order item.",
            "syntax": "az edgeorder order-item delete [--ids]\n                               [--name]\n                               [--no-wait]\n                               [--resource-group]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --order-item-name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az edgeorder order-item delete --name \"TestOrderItemName01\" --resource-group \"TestRG\""
            ]
        },
        {
            "name": "az edgeorder order-item list",
            "summary": "List order item at resource group level. And List order item at subscription level.",
            "syntax": "az edgeorder order-item list [--expand]\n                             [--filter]\n                             [--resource-group]\n                             [--skip-token]",
            "required_parameters": [],
            "optional_parameters": [
                "--expand",
                "--filter",
                "--resource-group -g",
                "--skip-token"
            ],
            "examples": [
                "az edgeorder order-item list --resource-group \"TestRG\"",
                "az edgeorder order-item list"
            ]
        },
        {
            "name": "az edgeorder order-item return",
            "summary": "Return order item.",
            "syntax": "az edgeorder order-item return --return-reason\n                               [--contact-details]\n                               [--ids]\n                               [--name]\n                               [--no-wait]\n                               [--resource-group]\n                               [--service-tag]\n                               [--shipping-address]\n                               [--shipping-box-required {false, true}]\n                               [--subscription]",
            "required_parameters": [
                "--return-reason"
            ],
            "optional_parameters": [
                "--contact-details",
                "--ids",
                "--name --order-item-name -n",
                "--no-wait",
                "--resource-group -g",
                "--service-tag",
                "--shipping-address",
                "--shipping-box-required",
                "--subscription"
            ],
            "examples": [
                "az edgeorder order-item return --name \"TestOrderName1\" --resource-group \"TestRG\" --return-reason \"Order returned\""
            ]
        },
        {
            "name": "az edgeorder order-item show",
            "summary": "Get an order item.",
            "syntax": "az edgeorder order-item show [--expand]\n                             [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--expand",
                "--ids",
                "--name --order-item-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az edgeorder order-item show --name \"TestOrderItemName01\" --resource-group \"TestRG\""
            ]
        },
        {
            "name": "az edgeorder order-item update",
            "summary": "Update the properties of an existing order item.",
            "syntax": "az edgeorder order-item update [--contact-details]\n                               [--encryption-preferences]\n                               [--ids]\n                               [--if-match]\n                               [--mgmt-preferences]\n                               [--name]\n                               [--no-wait]\n                               [--notif-email-list]\n                               [--notif-preferences]\n                               [--resource-group]\n                               [--shipping-address]\n                               [--subscription]\n                               [--tags]\n                               [--transport-preferences]",
            "required_parameters": [],
            "optional_parameters": [
                "--contact-details",
                "--encryption-preferences",
                "--ids",
                "--if-match",
                "--mgmt-preferences",
                "--name --order-item-name -n",
                "--no-wait",
                "--notif-email-list",
                "--notif-preferences",
                "--resource-group -g",
                "--shipping-address",
                "--subscription",
                "--tags",
                "--transport-preferences"
            ],
            "examples": [
                "az edgeorder order-item update --name \"TestOrderItemName01\" --contact-details contact-name=\"Updated contact name\" email-list=\"testemail@microsoft.com\" phone=\"2222200000\" --transport-preferences preferred-shipment-type=\"CustomerManaged\" --tags ant=\"insect\" pigeon=\"bird\" tiger=\"animal\" --resource-group \"TestRG\""
            ]
        },
        {
            "name": "az edgeorder order-item wait",
            "summary": "Place the CLI in a waiting state until a condition of the order-item is met.",
            "syntax": "az edgeorder order-item wait --order-item-name\n                             --resource-group\n                             [--created]\n                             [--custom]\n                             [--deleted]\n                             [--exists]\n                             [--expand]\n                             [--interval]\n                             [--timeout]\n                             [--updated]",
            "required_parameters": [
                "--order-item-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}