{
    "command_group": "az datashare data-set-mapping",
    "description": "Manage data set mapping with datashare.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az datashare data-set-mapping create",
            "summary": "Create a DataSetMapping.",
            "syntax": "az datashare data-set-mapping create --account-name\n                                     --data-set-mapping-name\n                                     --resource-group\n                                     --share-subscription-name\n                                     [--adls-gen2-file-data-set-mapping]\n                                     [--adls-gen2-file-system-data-set-mapping]\n                                     [--adls-gen2-folder-data-set-mapping]\n                                     [--blob-container-data-set-mapping]\n                                     [--blob-data-set-mapping]\n                                     [--blob-folder-data-set-mapping]\n                                     [--kusto-cluster-data-set-mapping]\n                                     [--kusto-database-data-set-mapping]\n                                     [--kusto-table-data-set-mapping]\n                                     [--sqldb-table-data-set-mapping]\n                                     [--sqldw-table-data-set-mapping]\n                                     [--synapse-workspace-sql-pool-table-data-set-mapping]",
            "required_parameters": [
                "--account-name",
                "--data-set-mapping-name --name -n",
                "--resource-group -g",
                "--share-subscription-name"
            ],
            "optional_parameters": [
                "--adls-gen2-file-data-set-mapping",
                "--adls-gen2-file-system-data-set-mapping",
                "--adls-gen2-folder-data-set-mapping",
                "--blob-container-data-set-mapping",
                "--blob-data-set-mapping",
                "--blob-folder-data-set-mapping",
                "--kusto-cluster-data-set-mapping",
                "--kusto-database-data-set-mapping",
                "--kusto-table-data-set-mapping",
                "--sqldb-table-data-set-mapping",
                "--sqldw-table-data-set-mapping",
                "--synapse-workspace-sql-pool-table-data-set-mapping"
            ],
            "examples": [
                "az datashare data-set-mapping create --account-name \"Account1\" --adls-gen2-file-data-set-mapping data-set-id=\"a08f184b-0567-4b11-ba22-a1199336d226\" file-path=\"file21\" file-system=\"fileSystem\" output-type=\"Csv\" resource-group=\"SampleResourceGroup\" storage-account-name=\"storage2\" subscription-id=\"433a8dfd-e5d5-4e77-ad86-90acdc75eb1a\" --name \"DatasetMapping1\" --resource-group \"SampleResourceGroup\" --share-subscription-name \"ShareSubscription1\""
            ]
        },
        {
            "name": "az datashare data-set-mapping delete",
            "summary": "Delete a DataSetMapping in a shareSubscription.",
            "syntax": "az datashare data-set-mapping delete [--account-name]\n                                     [--data-set-mapping-name]\n                                     [--ids]\n                                     [--resource-group]\n                                     [--share-subscription-name]\n                                     [--subscription]\n                                     [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--data-set-mapping-name --name -n",
                "--ids",
                "--resource-group -g",
                "--share-subscription-name",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az datashare data-set-mapping delete --account-name \"Account1\" --name \"DatasetMapping1\" --resource-group \"SampleResourceGroup\" --share-subscription-name \"ShareSubscription1\""
            ]
        },
        {
            "name": "az datashare data-set-mapping list",
            "summary": "List DataSetMappings in a share subscription.",
            "syntax": "az datashare data-set-mapping list --account-name\n                                   --resource-group\n                                   --share-subscription-name\n                                   [--filter]\n                                   [--orderby]\n                                   [--skip-token]",
            "required_parameters": [
                "--account-name",
                "--resource-group -g",
                "--share-subscription-name"
            ],
            "optional_parameters": [
                "--filter",
                "--orderby",
                "--skip-token"
            ],
            "examples": [
                "az datashare data-set-mapping list --account-name \"Account1\" --resource-group \"SampleResourceGroup\" --share-subscription-name \"ShareSubscription1\""
            ]
        },
        {
            "name": "az datashare data-set-mapping show",
            "summary": "Get a DataSetMapping in a shareSubscription.",
            "syntax": "az datashare data-set-mapping show [--account-name]\n                                   [--data-set-mapping-name]\n                                   [--ids]\n                                   [--resource-group]\n                                   [--share-subscription-name]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--data-set-mapping-name --name -n",
                "--ids",
                "--resource-group -g",
                "--share-subscription-name",
                "--subscription"
            ],
            "examples": [
                "az datashare data-set-mapping show --account-name \"Account1\" --name \"DatasetMapping1\" --resource-group \"SampleResourceGroup\" --share-subscription-name \"ShareSubscription1\""
            ]
        }
    ]
}