{
    "command_group": "az billing customer",
    "description": "Billing customer.",
    "status": "Preview",
    "commands": [
        {
            "name": "az billing customer list",
            "summary": "List the customers that are billed to a billing account. The operation is supported only for billing accounts with agreement type Microsoft Partner Agreement.",
            "syntax": "az billing customer list --account-name\n                         [--filter]\n                         [--profile-name]\n                         [--search]",
            "required_parameters": [
                "--account-name"
            ],
            "optional_parameters": [
                "--filter",
                "--profile-name",
                "--search"
            ],
            "examples": [
                "az billing customer list --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\""
            ]
        },
        {
            "name": "az billing customer show",
            "summary": "Get a customer by its ID. The operation is supported only for billing accounts with agreement type Microsoft Partner Agreement.",
            "syntax": "az billing customer show --account-name\n                         --customer-name\n                         [--expand]",
            "required_parameters": [
                "--account-name",
                "--customer-name --name -n"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": [
                "az billing customer show --account-name \"{billingAccountName}\" --name \"{customerName}\"",
                "az billing customer show --expand \"enabledAzurePlans,resellers\" --account-name \"{billingAccountName}\" --name \"{customerName}\""
            ]
        }
    ]
}