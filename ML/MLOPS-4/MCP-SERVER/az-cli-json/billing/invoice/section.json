{
    "command_group": "az billing invoice section",
    "description": "Manage invoice section.",
    "status": "Preview",
    "commands": [
        {
            "name": "az billing invoice section create",
            "summary": "Creates or updates an invoice section. The operation is supported only for billing accounts with agreement type Microsoft Customer Agreement.",
            "syntax": "az billing invoice section create --account-name\n                                  --invoice-section-name\n                                  --profile-name\n                                  [--display-name]\n                                  [--labels]\n                                  [--no-wait]",
            "required_parameters": [
                "--account-name",
                "--invoice-section-name --name -n",
                "--profile-name"
            ],
            "optional_parameters": [
                "--display-name",
                "--labels",
                "--no-wait"
            ],
            "examples": [
                "az billing invoice section create --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\" --name \"{invoiceSectionName}\" --display-name \"invoiceSection1\" --labels costCategory=\"Support\" pcCode=\"A123456\""
            ]
        },
        {
            "name": "az billing invoice section initiate-transfer",
            "summary": "Sends a request to a user in another billing account to transfer billing ownership of their subscriptions. The operation is supported only for billing accounts with agreement type Microsoft Customer Agreement.",
            "syntax": "az billing invoice section initiate-transfer --billing-account-name\n                                             --billing-profile-name\n                                             --invoice-section-name\n                                             [--recipient-email-id]\n                                             [--reseller-id]",
            "required_parameters": [
                "--billing-account-name",
                "--billing-profile-name",
                "--invoice-section-name"
            ],
            "optional_parameters": [
                "--recipient-email-id",
                "--reseller-id"
            ],
            "examples": []
        },
        {
            "name": "az billing invoice section list",
            "summary": "List the invoice sections that a user has access to. The operation is supported only for billing accounts with agreement type Microsoft Customer Agreement.",
            "syntax": "az billing invoice section list --account-name\n                                --profile-name",
            "required_parameters": [
                "--account-name",
                "--profile-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az billing invoice section list --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\""
            ]
        },
        {
            "name": "az billing invoice section show",
            "summary": "Get an invoice section by its ID. The operation is supported only for billing accounts with agreement type Microsoft Customer Agreement.",
            "syntax": "az billing invoice section show --account-name\n                                --invoice-section-name\n                                --profile-name",
            "required_parameters": [
                "--account-name",
                "--invoice-section-name --name -n",
                "--profile-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az billing invoice section show --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\" --name \"{invoiceSectionName}\""
            ]
        },
        {
            "name": "az billing invoice section update",
            "summary": "Creates or updates an invoice section. The operation is supported only for billing accounts with agreement type Microsoft Customer Agreement.",
            "syntax": "az billing invoice section update --account-name\n                                  --invoice-section-name\n                                  --profile-name\n                                  [--display-name]\n                                  [--labels]\n                                  [--no-wait]",
            "required_parameters": [
                "--account-name",
                "--invoice-section-name --name -n",
                "--profile-name"
            ],
            "optional_parameters": [
                "--display-name",
                "--labels",
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az billing invoice section wait",
            "summary": "Place the CLI in a waiting state until a condition of the billing invoice section is met.",
            "syntax": "az billing invoice section wait --account-name\n                                --invoice-section-name\n                                --profile-name\n                                [--created]\n                                [--custom]\n                                [--deleted]\n                                [--exists]\n                                [--interval]\n                                [--timeout]\n                                [--updated]",
            "required_parameters": [
                "--account-name",
                "--invoice-section-name --name -n",
                "--profile-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az billing invoice section wait --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\" --name \"{invoiceSectionName}\" --created",
                "az billing invoice section wait --account-name \"{billingAccountName}\" --profile-name \"{billingProfileName}\" --name \"{invoiceSectionName}\" --updated"
            ]
        }
    ]
}