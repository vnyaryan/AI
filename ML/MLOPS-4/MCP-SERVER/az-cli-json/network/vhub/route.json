{
    "command_group": "az network vhub route",
    "description": "Manage entries in the virtual hub route table.",
    "status": "Deprecated",
    "commands": [
        {
            "name": "az network vhub route add",
            "summary": "Add a route to the virtual hub route table.",
            "syntax": "az network vhub route add --address-prefixes\n                          --next-hop\n                          --resource-group\n                          --vhub-name\n                          [--no-wait]",
            "required_parameters": [
                "--address-prefixes",
                "--next-hop",
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az network vhub route list",
            "summary": "List routes in the virtual hub route table.",
            "syntax": "az network vhub route list --resource-group\n                           --vhub-name",
            "required_parameters": [
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az network vhub route remove",
            "summary": "Remove a route from the virtual hub route table.",
            "syntax": "az network vhub route remove --index\n                             --resource-group\n                             --vhub-name\n                             [--no-wait]",
            "required_parameters": [
                "--index",
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az network vhub route reset",
            "summary": "Reset virtual hub route when the route state is failed.",
            "syntax": "az network vhub route reset --resource-group\n                            --vhub-name\n                            [--no-wait]",
            "required_parameters": [
                "--resource-group -g",
                "--vhub-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        }
    ]
}