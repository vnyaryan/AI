{
    "command_group": "az network dns record-set",
    "description": "Manage DNS records and record sets.",
    "status": "GA",
    "commands": [
        {
            "name": "az network dns record-set list",
            "summary": "List all record sets within a DNS zone.",
            "syntax": "az network dns record-set list --resource-group\n                               --zone-name\n                               [--max-items]\n                               [--next-token]",
            "required_parameters": [
                "--resource-group -g",
                "--zone-name -z"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az network dns record-set list -g MyResourceGroup -z www.mysite.com --query \"[?name=='@']\""
            ]
        }
    ]
}