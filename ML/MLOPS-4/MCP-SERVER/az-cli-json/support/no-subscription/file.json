{
    "command_group": "az support no-subscription file",
    "description": "File operations for no subscription.",
    "status": "GA",
    "commands": [
        {
            "name": "az support no-subscription file list",
            "summary": "List all the Files information under a workspace.",
            "syntax": "az support no-subscription file list --file-workspace-name\n                                     [--max-items]\n                                     [--next-token]",
            "required_parameters": [
                "--file-workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az support no-subscription file list --file-workspace \"TestFileWorkspaceName\""
            ]
        },
        {
            "name": "az support no-subscription file show",
            "summary": "Get details of a specific file in a work space.",
            "syntax": "az support no-subscription file show --file-name\n                                     --file-workspace-name",
            "required_parameters": [
                "--file-name",
                "--file-workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az support no-subscription file show --file-workspace-name \"TestWorkspaceName\" --file-name \"FileName\""
            ]
        },
        {
            "name": "az support no-subscription file upload",
            "summary": "Uploads a file to a workspace.",
            "syntax": "az support no-subscription file upload --file-path\n                                       --file-workspace-name",
            "required_parameters": [
                "--file-path",
                "--file-workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az support no-subscription file upload \\\n  --file-workspace-name \"TestFileWorkspaceName\" \\\n  --file-path \"C:/Users/TestUsers/TestFolder/ExampleFile.txt\"",
                "az support no-subscription file upload \\\n  --file-workspace-name \"2300000000000358\" \\\n  --file-path \"C:/Users/TestUsers/TestFolder/ExampleFile.txt\""
            ]
        }
    ]
}