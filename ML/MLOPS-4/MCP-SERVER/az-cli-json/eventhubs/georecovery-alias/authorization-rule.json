            "syntax": "az eventhubs georecovery-alias authorization-rule show [--alias]\n                                                       [--authorization-rule-name]\n                                                       [--ids]\n                                                       [--namespace-name]\n                                                       [--resource-group]\n                                                       [--subscription]",
            "required_parameters": [],
