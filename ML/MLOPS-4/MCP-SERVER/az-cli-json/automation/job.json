{
    "command_group": "az automation job",
    "description": "Automation Job.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az automation job list",
            "summary": "Retrieve a list of jobs.",
            "syntax": "az automation job list --automation-account-name\n                       --resource-group",
            "required_parameters": [
                "--automation-account-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az automation job list --automation-account-name \"myAutomationAccount\" --resource-group \"rg\""
            ]
        },
        {
            "name": "az automation job resume",
            "summary": "Resume the job identified by jobName.",
            "syntax": "az automation job resume [--automation-account-name]\n                         [--ids]\n                         [--job-name]\n                         [--resource-group]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--ids",
                "--job-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az automation job resume --automation-account-name \"myAutomationAccount\" --name \"foo\"                --resource-group \"rg\""
            ]
        },
        {
            "name": "az automation job show",
            "summary": "Retrieve the job identified by job name.",
            "syntax": "az automation job show [--automation-account-name]\n                       [--ids]\n                       [--job-name]\n                       [--resource-group]\n                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--ids",
                "--job-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az automation job show --automation-account-name \"myAutomationAccount\" --name \"foo\" --resource-group \"rg\""
            ]
        },
        {
            "name": "az automation job stop",
            "summary": "Stop the job identified by jobName.",
            "syntax": "az automation job stop [--automation-account-name]\n                       [--ids]\n                       [--job-name]\n                       [--resource-group]\n                       [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--ids",
                "--job-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az automation job stop --automation-account-name \"myAutomationAccount\" --name \"foo\" --resource-group \"rg\""
            ]
        },
        {
            "name": "az automation job suspend",
            "summary": "Suspend the job identified by job name.",
            "syntax": "az automation job suspend [--automation-account-name]\n                          [--ids]\n                          [--job-name]\n                          [--resource-group]\n                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--automation-account-name",
                "--ids",
                "--job-name --name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az automation job suspend --automation-account-name \"myAutomationAccount\" --name \"foo\"                --resource-group \"rg\""
            ]
        }
    ]
}