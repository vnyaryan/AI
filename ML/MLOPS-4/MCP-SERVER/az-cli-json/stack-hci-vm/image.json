{
    "command_group": "az stack-hci-vm image",
    "description": "Manage images with stack-hci-vm.",
    "status": "GA",
    "commands": [
        {
            "name": "az stack-hci-vm image create",
            "summary": "Create a gallery image.",
            "syntax": "az stack-hci-vm image create --custom-location\n                             --name\n                             --os-type\n                             --resource-group\n                             [--image-path]\n                             [--imagerepo-password]\n                             [--imagerepo-username]\n                             [--location]\n                             [--offer]\n                             [--polling-interval]\n                             [--publisher]\n                             [--sku]\n                             [--source-vm]\n                             [--storage-path-id]\n                             [--tags]\n                             [--urn]\n                             [--version]",
            "required_parameters": [
                "--custom-location",
                "--name",
                "--os-type",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--image-path",
                "--imagerepo-password",
                "--imagerepo-username",
                "--location -l",
                "--offer",
                "--polling-interval",
                "--publisher",
                "--sku",
                "--source-vm",
                "--storage-path-id",
                "--tags",
                "--urn",
                "--version"
            ],
            "examples": [
                "az stack-hci-vm image create --custom-location \"/subscriptions/a95612cb-f1fa-4daa-a4fd-272844fa512c/resourceGroups/dogfoodarc/providers/Microsoft.ExtendedLocation/customLocations/dogfood-location\" --location \"West US2\" --storage-path-id \"/subscriptions/a95612cb-f1fa-4daa-a4fd-272844fa512c/resourceGroups/dogfoodarc/providers/Microsoft.AzureStackHCI/storagecontainers/test-storagepath\" --image-path \"C:\\\\test.vhdx\" --name \"test-gallery-image\" --resource-group \"test-rg\" --os-type \"Windows\""
            ]
        },
        {
            "name": "az stack-hci-vm image delete",
            "summary": "Delete an image.",
            "syntax": "az stack-hci-vm image delete [--ids]\n                             [--name]\n                             [--polling-interval]\n                             [--resource-group]\n                             [--subscription]\n                             [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--polling-interval",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az stack-hci-vm image delete --name \"test-gallery-image\" --resource-group \"test-rg\""
            ]
        },
        {
            "name": "az stack-hci-vm image list",
            "summary": "List all gallery images.",
            "syntax": "az stack-hci-vm image list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az stack-hci-vm image list --resource-group \"test-rg\"",
                "az stack-hci-vm image list"
            ]
        },
        {
            "name": "az stack-hci-vm image show",
            "summary": "Get the details of an image.",
            "syntax": "az stack-hci-vm image show [--ids]\n                           [--name]\n                           [--resource-group]\n                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az stack-hci-vm image show --name \"test-gallery-image\" --resource-group \"test-rg\""
            ]
        },
        {
            "name": "az stack-hci-vm image update",
            "summary": "Update an image.",
            "syntax": "az stack-hci-vm image update [--ids]\n                             [--name]\n                             [--polling-interval]\n                             [--resource-group]\n                             [--subscription]\n                             [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--polling-interval",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az stack-hci-vm image update --tags additionalProperties=\"sample\" --name \"test-gallery-image\" --resource-group \"test-rg\""
            ]
        }
    ]
