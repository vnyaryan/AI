            "optional_parameters": [
                "--cloudagent",
                "--command",
                "--credentials-dir",
                "--ip",
                "--kubeconfig",
                "--loginconfigfile"
            ],
            "examples": [
                "az arcappliance troubleshoot command hci --command [REQUIRED] --ip [REQUIRED] --credentials-dir [OPTIONAL]",
