{
    "command_group": "az bot facebook",
    "description": "Manage the Facebook Channel on a bot.",
    "status": "Preview",
    "commands": [
        {
            "name": "az bot facebook create",
            "summary": "Create the Facebook Channel on a bot.",
            "syntax": "az bot facebook create --appid\n                       --name\n                       --page-id\n                       --resource-group\n                       --secret\n                       --token\n                       [--add-disabled {false, true}]\n                       [--location]",
            "required_parameters": [
                "--appid",
                "--name -n",
                "--page-id",
                "--resource-group -g",
                "--secret",
                "--token"
            ],
            "optional_parameters": [
                "--add-disabled",
                "--location -l"
            ],
            "examples": [
                "az bot facebook create -n botName -g MyResourceGroup --appid myAppId \\\n--page-id myPageId --secret mySecret --token myToken"
            ]
        },
        {
            "name": "az bot facebook delete",
            "summary": "Delete the Facebook Channel on a bot.",
            "syntax": "az bot facebook delete --name\n                       --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az bot facebook show",
            "summary": "Get details of the Facebook Channel on a bot.",
            "syntax": "az bot facebook show --name\n                     --resource-group\n                     [--with-secrets {false, true}]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--with-secrets"
            ],
            "examples": []
        }
    ]
}