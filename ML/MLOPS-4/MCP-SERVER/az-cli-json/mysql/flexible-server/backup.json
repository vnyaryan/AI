{
    "command_group": "az mysql flexible-server backup",
    "description": "Manage flexible server backups.",
    "status": "GA",
    "commands": [
        {
            "name": "az mysql flexible-server backup create",
            "summary": "Create a backup for a given server with specified backup name.",
            "syntax": "az mysql flexible-server backup create --backup-name\n                                       --name\n                                       --resource-group\n                                       [--parameters]",
            "required_parameters": [
                "--backup-name -b",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--parameters"
            ],
            "examples": [
                "az mysql flexible-server backup create -g testgroup -n testsvr --backup-name testbackup"
            ]
        },
        {
            "name": "az mysql flexible-server backup list",
            "summary": "List all the backups for a given server.",
            "syntax": "az mysql flexible-server backup list --name\n                                     --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az mysql flexible-server backup list -g testgroup -n testsvr"
            ]
        },
        {
            "name": "az mysql flexible-server backup show",
            "summary": "Show the details of a specific backup for a given server.",
            "syntax": "az mysql flexible-server backup show [--backup-name]\n                                     [--ids]\n                                     [--name]\n                                     [--resource-group]\n                                     [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--backup-name -b",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az mysql flexible-server backup show -g testgroup -n testsvr --backup-name testbackup"
            ]
        }
    ]
}