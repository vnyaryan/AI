{
    "command_group": "az signalr replica",
    "description": "Manage replica settings.",
    "status": "GA",
    "commands": [
        {
            "name": "az signalr replica create",
            "summary": "Create a replica of SignalR Service.",
            "syntax": "az signalr replica create --replica-name\n                          --resource-group\n                          --signalr-name\n                          --sku\n                          [--location]\n                          [--tags]\n                          [--unit-count]",
            "required_parameters": [
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--sku"
            ],
            "optional_parameters": [
                "--location -l",
                "--tags",
                "--unit-count"
            ],
            "examples": [
                "az signalr replica create --sku Premium_P1 -l eastus --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica delete",
            "summary": "Delete a replica of SignalR Service.",
            "syntax": "az signalr replica delete [--ids]\n                          [--replica-name]\n                          [--resource-group]\n                          [--signalr-name]\n                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--subscription"
            ],
            "examples": [
                "az signalr replica delete --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica list",
            "summary": "List replicas of SignalR Service.",
            "syntax": "az signalr replica list --resource-group\n                        --signalr-name",
            "required_parameters": [
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az signalr replica list --signalr-name MySignalR -g MyResourceGroup -o table"
            ]
        },
        {
            "name": "az signalr replica restart",
            "summary": "Restart a replica of SignalR Service.",
            "syntax": "az signalr replica restart [--ids]\n                           [--replica-name]\n                           [--resource-group]\n                           [--signalr-name]\n                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--subscription"
            ],
            "examples": [
                "az signalr replica restart --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica show",
            "summary": "Show the details of a replica.",
            "syntax": "az signalr replica show [--ids]\n                        [--replica-name]\n                        [--resource-group]\n                        [--signalr-name]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--subscription"
            ],
            "examples": [
                "az signalr replica show --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica start",
            "summary": "Start a replica of SignalR Service.",
            "syntax": "az signalr replica start [--ids]\n                         [--replica-name]\n                         [--resource-group]\n                         [--signalr-name]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--subscription"
            ],
            "examples": [
                "az signalr replica start --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica stop",
            "summary": "Stop a replica of SignalR Service.",
            "syntax": "az signalr replica stop [--ids]\n                        [--replica-name]\n                        [--resource-group]\n                        [--signalr-name]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--replica-name",
                "--resource-group -g",
                "--signalr-name",
                "--subscription"
            ],
            "examples": [
                "az signalr replica stop --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup"
            ]
        },
        {
            "name": "az signalr replica update",
            "summary": "Update a replica of SignalR Service.",
            "syntax": "az signalr replica update --replica-name\n                          --resource-group\n                          --signalr-name\n                          [--add]\n                          [--force-string]\n                          [--region-endpoint-enabled {false, true}]\n                          [--remove]\n                          [--set]\n                          [--unit-count]",
            "required_parameters": [
                "--replica-name",
                "--resource-group -g",
                "--signalr-name"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--region-endpoint-enabled",
                "--remove",
                "--set",
                "--unit-count"
            ],
            "examples": [
                "az signalr replica update --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup --region-endpoint-enabled true",
                "az signalr replica update --replica-name MyReplica --signalr-name MySignalR -g MyResourceGroup --unit-count 2"
            ]
        }
    ]
}