{
    "command_group": "az fleet updatestrategy",
    "description": "Commands to manage update strategies.",
    "status": "GA",
    "commands": [
        {
            "name": "az fleet updatestrategy create",
            "summary": "Creates or updates an update strategy.",
            "syntax": "az fleet updatestrategy create --fleet-name\n                               --name\n                               --resource-group\n                               --stages\n                               [--no-wait]",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g",
                "--stages"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az fleet updatestrategy create -g MyFleetResourceGroup -f MyFleetName -n MyUpdateStrategy --stages MyUpdateStrategyFile.json"
            ]
        },
        {
            "name": "az fleet updatestrategy delete",
            "summary": "Deletes a update strategy.",
            "syntax": "az fleet updatestrategy delete --fleet-name\n                               --name\n                               --resource-group\n                               [--no-wait]\n                               [--yes]",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az fleet updatestrategy delete -g MyFleetResourceGroup -f MyFleetName -n MyUpdateStrategy"
            ]
        },
        {
            "name": "az fleet updatestrategy list",
            "summary": "Lists the fleet's update strategies.",
            "syntax": "az fleet updatestrategy list --fleet-name\n                             --resource-group",
            "required_parameters": [
                "--fleet-name -f",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az fleet updatestrategy list -g MyFleetResourceGroup -f MyFleetName"
            ]
        },
        {
            "name": "az fleet updatestrategy show",
            "summary": "Shows an update strategy.",
            "syntax": "az fleet updatestrategy show --fleet-name\n                             --name\n                             --resource-group",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az fleet updatestrategy show -g MyFleetResourceGroup -f MyFleetName -n MyUpdateStrategy"
            ]
        },
        {
            "name": "az fleet updatestrategy wait",
            "summary": "Wait for a update strategy resource to reach a desired state.",
            "syntax": "az fleet updatestrategy wait --fleet-name\n                             --resource-group\n                             --update-strategy-name\n                             [--created]\n                             [--custom]\n                             [--deleted]\n                             [--exists]\n                             [--interval]\n                             [--timeout]\n                             [--updated]",
            "required_parameters": [
                "--fleet-name -f",
                "--resource-group -g",
                "--update-strategy-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}