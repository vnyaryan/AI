{
    "command_group": "az cli-translator arm",
    "description": "Translate ARM template to CLI scripts(Currently only support Compute, Network and Storage).",
    "status": "Experimental",
    "commands": [
        {
            "name": "az cli-translator arm translate",
            "summary": "Translate ARM template to CLI scripts(Currently only support Compute, Network and Storage).",
            "syntax": "az cli-translator arm translate --parameters\n                                --resource-group\n                                --template\n                                [--target-subscription]",
            "required_parameters": [
                "--parameters",
                "--resource-group -g",
                "--template"
            ],
            "optional_parameters": [
                "--target-subscription"
            ],
            "examples": [
                "az cli-translator arm translate --target-subscription 00000000-0000-0000-0000-000000000000 --resource-group groupName --template /path/to/local/template --parameters /path/to/local/parameters"
            ]
        }
    ]
}