{
    "command_group": "az networkcloud rack",
    "description": "Manage rack.",
    "status": "GA",
    "commands": [
        {
            "name": "az networkcloud rack list",
            "summary": "List racks in the provided resource group or subscription.",
            "syntax": "az networkcloud rack list [--max-items]\n                          [--next-token]\n                          [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az networkcloud rack list",
                "az networkcloud rack list --resource-group \"resourceGroupName\""
            ]
        },
        {
            "name": "az networkcloud rack show",
            "summary": "Get properties of the provided rack.",
            "syntax": "az networkcloud rack show [--ids]\n                          [--name]\n                          [--resource-group]\n                          [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --rack-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az networkcloud rack show --name \"rackName\" --resource-group \"resourceGroupName\""
            ]
        },
        {
            "name": "az networkcloud rack update",
            "summary": "Update properties of the provided rack, or update the tags associated with the rack. Properties and tag updates can be done independently.",
            "syntax": "az networkcloud rack update [--ids]\n                            [--name]\n                            [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                            [--rack-location]\n                            [--rack-serial-number]\n                            [--resource-group]\n                            [--subscription]\n                            [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --rack-name -n",
                "--no-wait",
                "--rack-location",
                "--rack-serial-number",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az networkcloud rack update --name \"rackName\" --rack-location \"Rack 2B\" --rack-serial-number \"RACK_SERIAL_NUMBER\" --tags key1=\"myvalue1\" key2=\"myvalue2\" --resource-group \"resourceGroupName\""
            ]
        },
        {
            "name": "az networkcloud rack wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az networkcloud rack wait [--created]\n                          [--custom]\n                          [--deleted]\n                          [--exists]\n                          [--ids]\n                          [--interval]\n                          [--name]\n                          [--resource-group]\n                          [--subscription]\n                          [--timeout]\n                          [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name --rack-name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}