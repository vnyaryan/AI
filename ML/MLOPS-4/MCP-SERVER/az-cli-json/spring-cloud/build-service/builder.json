{
    "command_group": "az spring-cloud build-service builder",
    "description": "(Enterprise Tier Only) Commands to manage builder of build service.",
    "status": "Preview and Deprecated",
    "commands": [
        {
            "name": "az spring-cloud build-service builder create",
            "summary": "Create a builder.",
            "syntax": "az spring-cloud build-service builder create --name\n                                             --resource-group\n                                             --service\n                                             [--builder-file]\n                                             [--builder-json]\n                                             [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--builder-file",
                "--builder-json",
                "--no-wait"
            ],
            "examples": [
                "az spring-cloud build-service builder create --name my-builder --builder-json MyJson.json --service clitest --resource-group cli"
            ]
        },
        {
            "name": "az spring-cloud build-service builder delete",
            "summary": "Delete a builder.",
            "syntax": "az spring-cloud build-service builder delete --name\n                                             --resource-group\n                                             --service\n                                             [--no-wait]\n                                             [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az spring-cloud build-service builder delete --name my-builder --service clitest --resource-group cli"
            ]
        },
        {
            "name": "az spring-cloud build-service builder show",
            "summary": "Show a builder.",
            "syntax": "az spring-cloud build-service builder show --name\n                                           --resource-group\n                                           --service",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az spring-cloud build-service builder show --name my-builder --service clitest --resource-group cli"
            ]
        },
        {
            "name": "az spring-cloud build-service builder update",
            "summary": "Update a builder.",
            "syntax": "az spring-cloud build-service builder update --name\n                                             --resource-group\n                                             --service\n                                             [--builder-file]\n                                             [--builder-json]\n                                             [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--service -s"
            ],
            "optional_parameters": [
                "--builder-file",
                "--builder-json",
                "--no-wait"
            ],
            "examples": [
                "az spring-cloud build-service builder update --name my-builder --builder-json MyJson.json --service clitest --resource-group cli"
            ]
        }
    ]
}