{
    "command_group": "az acat report snapshot",
    "description": "Manage ACAT report snapshots.",
    "status": "GA",
    "commands": [
        {
            "name": "az acat report snapshot download",
            "summary": "Download compliance needs from snapshot, like: Compliance Report, Resource List.",
            "syntax": "az acat report snapshot download --download-type {ComplianceDetailedPdfReport, CompliancePdfReport, ComplianceReport, ResourceList}\n                                 --report-name\n                                 --snapshot-name\n                                 [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                 [--offer-guid]\n                                 [--tenant]",
            "required_parameters": [
                "--download-type",
                "--report-name",
                "--snapshot-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--offer-guid",
                "--tenant"
            ],
            "examples": []
        },
        {
            "name": "az acat report snapshot list",
            "summary": "List the AppComplianceAutomation snapshot list.",
            "syntax": "az acat report snapshot list --report-name\n                             [--filter]\n                             [--max-items]\n                             [--next-token]\n                             [--offer-guid]\n                             [--orderby]\n                             [--select]\n                             [--skip-token]\n                             [--tenant]\n                             [--top]",
            "required_parameters": [
                "--report-name"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token",
                "--offer-guid",
                "--orderby",
                "--select",
                "--skip-token",
                "--tenant",
                "--top"
            ],
            "examples": []
        }
    ]
}