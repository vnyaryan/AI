{
    "command_group": "az scvmm avset",
    "description": "Manage Arc for SCVMM Availability Set resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az scvmm avset create",
            "summary": "Create availabilty set resource.",
            "syntax": "az scvmm avset create --avset-name\n                      --custom-location\n                      --location\n                      --name\n                      --resource-group\n                      --vmmserver\n                      [--no-wait]\n                      [--tags]",
            "required_parameters": [
                "--avset-name -a",
                "--custom-location",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--vmmserver -v"
            ],
            "optional_parameters": [
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az scvmm avset create --subscription contoso-sub --resource-group contoso-rg --location eastus --custom-location contoso-cl --vmmserver contoso-vmmserver --avset-name \"name of the availability set in vmm\" --name contoso-avset"
            ]
        },
        {
            "name": "az scvmm avset delete",
            "summary": "Delete availabilty set resource.",
            "syntax": "az scvmm avset delete [--force]\n                      [--ids]\n                      [--name]\n                      [--no-wait]\n                      [--resource-group]\n                      [--subscription]\n                      [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az scvmm avset delete --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/availabiltySets/contoso-avset",
                "az scvmm avset delete --subscription contoso-sub --resource-group contoso-rg --name contoso-avset"
            ]
        },
        {
            "name": "az scvmm avset list",
            "summary": "Retrieve a list of availabilty sets.",
            "syntax": "az scvmm avset list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az scvmm avset list --subscription contoso-sub --resource-group contoso-rg",
                "az scvmm avset list --subscription contoso-sub"
            ]
        },
        {
            "name": "az scvmm avset show",
            "summary": "Get details of an availabilty set.",
            "syntax": "az scvmm avset show [--ids]\n                    [--name]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az scvmm avset show --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/availabiltySets/contoso-avset",
                "az scvmm avset show --subscription contoso-sub --resource-group contoso-rg --name contoso-avset"
            ]
        },
        {
            "name": "az scvmm avset update",
            "summary": "Update availabilty set resource.",
            "syntax": "az scvmm avset update [--ids]\n                      [--name]\n                      [--no-wait]\n                      [--resource-group]\n                      [--subscription]\n                      [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az scvmm avset update --ids /subscriptions/01234567-0123-0123-0123-0123456789ab/resourceGroups/contoso-rg/providers/Microsoft.ScVmm/availabiltySets/contoso-avset --tags department=Sales",
                "az scvmm avset update --subscription contoso-sub --resource-group contoso-rg --name contoso-avset --tags department=Sales"
            ]
        },
        {
            "name": "az scvmm avset wait",
            "summary": "Place the CLI in a waiting state until a condition of the availability set is met.",
            "syntax": "az scvmm avset wait --availability-set-name\n                    --resource-group\n                    [--created]\n                    [--custom]\n                    [--deleted]\n                    [--exists]\n                    [--interval]\n                    [--timeout]\n                    [--updated]",
            "required_parameters": [
                "--availability-set-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}