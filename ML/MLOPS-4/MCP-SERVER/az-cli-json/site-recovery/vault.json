{
    "command_group": "az site-recovery vault",
    "description": "Manage Vault.",
    "status": "GA",
    "commands": [
        {
            "name": "az site-recovery vault list-appliance",
            "summary": "List the list of Azure Site Recovery appliances for the vault.",
            "syntax": "az site-recovery vault list-appliance --resource-group\n                                      --vault-name\n                                      [--filter]\n                                      [--max-items]\n                                      [--next-token]",
            "required_parameters": [
                "--resource-group -g",
                "--vault-name"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az site-recovery vault list-appliance -g rg --vault-name vault_name"
            ]
        },
        {
            "name": "az site-recovery vault list-migration-item",
            "summary": "Get the list of ASR replication migration items in the vault.",
            "syntax": "az site-recovery vault list-migration-item --resource-group\n                                           --vault-name\n                                           [--filter]\n                                           [--max-items]\n                                           [--next-token]\n                                           [--skip-token]\n                                           [--take-token]",
            "required_parameters": [
                "--resource-group -g",
                "--vault-name"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token",
                "--skip-token",
                "--take-token"
            ],
            "examples": [
                "az site-recovery vault list-migration-item -g rg --vault-name vault_name"
            ]
        },
        {
            "name": "az site-recovery vault list-network",
            "summary": "List the networks available in a vault.",
            "syntax": "az site-recovery vault list-network --resource-group\n                                    --vault-name\n                                    [--max-items]\n                                    [--next-token]",
