            "syntax": "az connectedvmware vm delete [--delete-from-host]\n                             [--force]\n                             [--ids]\n                             [--name]\n                             [--no-wait]\n                             [--resource-group]\n                             [--retain-machine]\n                             [--subscription]\n                             [--yes]",
