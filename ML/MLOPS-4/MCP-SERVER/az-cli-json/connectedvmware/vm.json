            "syntax": "az connectedvmware vm update [--ids]\n                             [--memory-size]\n                             [--name]\n                             [--no-wait]\n                             [--num-CPUs]\n                             [--num-cores-per-socket]\n                             [--resource-group]\n                             [--subscription]\n                             [--tags]",
