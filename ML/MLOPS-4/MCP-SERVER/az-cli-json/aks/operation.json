{
    "command_group": "az aks operation",
    "description": "Commands to manage and view operations on managed Kubernetes cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az aks operation show",
            "summary": "Show the details for a specific operation on managed Kubernetes cluster.",
            "syntax": "az aks operation show --name\n                      --operation-id\n                      --resource-group\n                      [--nodepool-name]",
            "required_parameters": [
                "--name -n",
                "--operation-id",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--nodepool-name"
            ],
            "examples": []
        },
        {
            "name": "az aks operation show-latest",
            "summary": "Show the details for the latest operation on managed Kubernetes cluster.",
            "syntax": "az aks operation show-latest --name\n                             --resource-group\n                             [--nodepool-name]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--nodepool-name"
            ],
            "examples": []
        }
    ]
}