{
    "command_group": "az aks pod-identity exception",
    "description": "Commands to manage pod identity exceptions in managed Kubernetes cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az aks pod-identity exception add",
            "summary": "Add a pod identity exception to a managed Kubernetes cluster.",
            "syntax": "az aks pod-identity exception add --cluster-name\n                                  --namespace\n                                  --pod-labels\n                                  --resource-group\n                                  [--aks-custom-headers]\n                                  [--name]",
            "required_parameters": [
                "--cluster-name",
                "--namespace",
                "--pod-labels",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--aks-custom-headers",
                "--name -n"
            ],
            "examples": []
        },
        {
            "name": "az aks pod-identity exception delete",
            "summary": "Remove a pod identity exception from a managed Kubernetes cluster.",
            "syntax": "az aks pod-identity exception delete --cluster-name\n                                     --name\n                                     --namespace\n                                     --resource-group\n                                     [--aks-custom-headers]",
            "required_parameters": [
                "--cluster-name",
                "--name -n",
                "--namespace",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--aks-custom-headers"
            ],
            "examples": []
        },
        {
            "name": "az aks pod-identity exception list",
