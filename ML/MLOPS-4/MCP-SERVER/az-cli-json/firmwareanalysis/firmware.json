{
    "command_group": "az firmwareanalysis firmware",
    "description": "Commands to perform operation on a particular firmware.",
    "status": "GA",
    "commands": [
        {
            "name": "az firmwareanalysis firmware binary-hardening",
            "summary": "List binary hardening analysis results of a firmware.",
            "syntax": "az firmwareanalysis firmware binary-hardening --firmware-id\n                                              --resource-group\n                                              --workspace-name\n                                              [--max-items]\n                                              [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware binary-hardening --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware create",
            "summary": "Create a new firmware.",
            "syntax": "az firmwareanalysis firmware create --resource-group\n                                    --workspace-name\n                                    [--description]\n                                    [--file-name]\n                                    [--file-size]\n                                    [--firmware-id]\n                                    [--model]\n                                    [--status {Analyzing, Error, Extracting, Pending, Ready}]\n                                    [--status-messages]\n                                    [--vendor]\n                                    [--version]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--description",
                "--file-name",
                "--file-size",
                "--firmware-id --name -n",
                "--model",
                "--status",
                "--status-messages",
                "--vendor",
                "--version"
            ],
            "examples": [
                "az firmwareanalysis firmware create --resource-group {resourceGroupName} --workspace-name {workspaceName} --description {description} --file-name {fileName} --file-size {fileSize} --vendor {vendorName} --model {model} --version {version} --status {status} --status-messages ['hi','message']"
            ]
        },
        {
            "name": "az firmwareanalysis firmware crypto-certificate",
            "summary": "List cryptographic certificate analysis results found in a firmware.",
            "syntax": "az firmwareanalysis firmware crypto-certificate --firmware-id\n                                                --resource-group\n                                                --workspace-name\n                                                [--max-items]\n                                                [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware crypto-certificate --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware crypto-key",
            "summary": "List cryptographic key analysis results found in a firmware.",
            "syntax": "az firmwareanalysis firmware crypto-key --firmware-id\n                                        --resource-group\n                                        --workspace-name\n                                        [--max-items]\n                                        [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware crypto-key --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware cve",
            "summary": "List CVE analysis results of a firmware.",
            "syntax": "az firmwareanalysis firmware cve --firmware-id\n                                 --resource-group\n                                 --workspace-name\n                                 [--max-items]\n                                 [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az az firmwareanalysis firmware cve --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware delete",
            "summary": "Delete a firmware.",
            "syntax": "az firmwareanalysis firmware delete [--firmware-id]\n                                    [--ids]\n                                    [--resource-group]\n                                    [--subscription]\n                                    [--workspace-name]\n                                    [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--firmware-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
            ],
            "examples": [
                "az firmwareanalysis firmware delete --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware generate-filesystem-download-url",
            "summary": "Get a url for tar file download.",
            "syntax": "az firmwareanalysis firmware generate-filesystem-download-url [--firmware-id]\n                                                              [--ids]\n                                                              [--resource-group]\n                                                              [--subscription]\n                                                              [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--firmware-id",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az firmwareanalysis firmware generate-filesystem-download-url --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware list",
            "summary": "List all the firmware images inside a workspace.",
            "syntax": "az firmwareanalysis firmware list --resource-group\n                                  --workspace-name\n                                  [--max-items]\n                                  [--next-token]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware show --resource-group {ResourceGroupName} --workspace-name {workspaceName}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware password-hash",
            "summary": "List password hash analysis results of a firmware.",
            "syntax": "az firmwareanalysis firmware password-hash --firmware-id\n                                           --resource-group\n                                           --workspace-name\n                                           [--max-items]\n                                           [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware password-hash --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware sbom-component",
            "summary": "List SBOM analysis results of a firmware.",
            "syntax": "az firmwareanalysis firmware sbom-component --firmware-id\n                                            --resource-group\n                                            --workspace-name\n                                            [--max-items]\n                                            [--next-token]",
            "required_parameters": [
                "--firmware-id",
                "--resource-group -g",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az firmwareanalysis firmware sbom-component --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware show",
            "summary": "Get a firmware.",
            "syntax": "az firmwareanalysis firmware show [--firmware-id]\n                                  [--ids]\n                                  [--resource-group]\n                                  [--subscription]\n                                  [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--firmware-id --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az firmwareanalysis firmware show --resource-group {ResourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId}"
            ]
        },
        {
            "name": "az firmwareanalysis firmware summary",
            "summary": "Get an analysis result summary of a firmware by name.",
            "syntax": "az firmwareanalysis firmware summary [--firmware-id]\n                                     [--ids]\n                                     [--name {BinaryHardening, CVE, CryptoCertificate, CryptoKey, Firmware}]\n                                     [--resource-group]\n                                     [--subscription]\n                                     [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--firmware-id",
                "--ids",
                "--name --summary-name -n",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az firmwareanalysis firmware summary --resource-group {resourceGroupName} --workspace-name {workspaceName} --firmware-id {firmwareId} -n {analysisType}"
            ]
        }
    ]
}