{
    "command_group": "az iot dps",
    "description": "Manage Azure IoT Hub Device Provisioning Service.",
    "status": "GA",
    "commands": [
        {
            "name": "az iot dps create",
            "summary": "Create an Azure IoT Hub Device Provisioning Service instance.",
            "syntax": "az iot dps create --name\n                  --resource-group\n                  [--edr {false, true}]\n                  [--location]\n                  [--sku {S1}]\n                  [--tags]\n                  [--unit]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--edr --enforce-data-residency",
                "--location -l",
                "--sku",
                "--tags",
                "--unit"
            ],
            "examples": [
                "az iot dps create --name MyDps --resource-group MyResourceGroup",
                "az iot dps create --name MyDps --resource-group MyResourceGroup --location eastus",
                "az iot dps create --name MyDps --resource-group MyResourceGroup --edr"
            ]
        },
        {
            "name": "az iot dps delete",
            "summary": "Delete an Azure IoT Hub Device Provisioning Service instance.",
            "syntax": "az iot dps delete [--ids]\n                  [--name]\n                  [--resource-group]\n                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az iot dps delete --name MyDps --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az iot dps list",
            "summary": "List Azure IoT Hub Device Provisioning Service instances.",
            "syntax": "az iot dps list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az iot dps list",
                "az iot dps list --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az iot dps show",
            "summary": "Get the details of an Azure IoT Hub Device Provisioning Service instance.",
            "syntax": "az iot dps show [--ids]\n                [--name]\n                [--resource-group]\n                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az iot dps show --name MyDps --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az iot dps update",
            "summary": "Update an Azure IoT Hub Device Provisioning Service instance.",
            "syntax": "az iot dps update [--add]\n                  [--force-string]\n                  [--ids]\n                  [--name]\n                  [--remove]\n                  [--resource-group]\n                  [--set]\n                  [--subscription]\n                  [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az iot dps update --name MyDps --resource-group MyResourceGroup --set properties.allocationPolicy=\"GeoLatency\""
            ]
        }
    ]
}