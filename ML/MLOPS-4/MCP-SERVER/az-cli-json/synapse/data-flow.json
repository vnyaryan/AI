{
    "command_group": "az synapse data-flow",
    "description": "Manage Synapse's data flows.",
    "status": "GA",
    "commands": [
        {
            "name": "az synapse data-flow create",
            "summary": "Create a data flow.",
            "syntax": "az synapse data-flow create --file\n                            --name\n                            --workspace-name\n                            [--no-wait]",
            "required_parameters": [
                "--file",
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse data-flow create --workspace-name testsynapseworkspace \\\n  --name testdataflow --file @\"path/dataflow.json\""
            ]
        },
        {
            "name": "az synapse data-flow delete",
            "summary": "Delete a data flow.",
            "syntax": "az synapse data-flow delete --name\n                            --workspace-name\n                            [--no-wait]\n                            [--yes]",
            "required_parameters": [
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az synapse data-flow delete --workspace-name testsynapseworkspace \\\n  --name testdataflow"
            ]
        },
        {
            "name": "az synapse data-flow list",
            "summary": "List data flows.",
            "syntax": "az synapse data-flow list --workspace-name",
            "required_parameters": [
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse data-flow list --workspace-name testsynapseworkspace"
            ]
        },
        {
            "name": "az synapse data-flow set",
            "summary": "Set an exist data flow.",
            "syntax": "az synapse data-flow set --file\n                         --name\n                         --workspace-name\n                         [--no-wait]",
            "required_parameters": [
                "--file",
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse data-flow set --workspace-name testsynapseworkspace \\\n  --name testdataflow --file @\"path/dataflow.json\""
            ]
        },
        {
            "name": "az synapse data-flow show",
            "summary": "Get a data flow.",
            "syntax": "az synapse data-flow show --name\n                          --workspace-name",
            "required_parameters": [
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse data-flow show --workspace-name testsynapseworkspace \\\n  --name testdataflow"
            ]
        }
    ]
}