{
    "command_group": "az synapse linked-service",
    "description": "Manage Synapse's linked services.",
    "status": "GA",
    "commands": [
        {
            "name": "az synapse linked-service create",
            "summary": "Create a linked service.",
            "syntax": "az synapse linked-service create --file\n                                 --name\n                                 --workspace-name\n                                 [--no-wait]",
            "required_parameters": [
                "--file",
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse linked-service create --workspace-name testsynapseworkspace \\\n  --name testlinkedservice --file @\"path/linkedservice.json\""
            ]
        },
        {
            "name": "az synapse linked-service delete",
            "summary": "Delete a linked service.",
            "syntax": "az synapse linked-service delete --name\n                                 --workspace-name\n                                 [--no-wait]\n                                 [--yes]",
            "required_parameters": [
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az synapse linked-service delete --workspace-name testsynapseworkspace \\\n  --name testlinkedservice"
            ]
        },
        {
            "name": "az synapse linked-service list",
            "summary": "List linked services.",
            "syntax": "az synapse linked-service list --workspace-name",
            "required_parameters": [
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse linked-service list --workspace-name testsynapseworkspace"
            ]
        },
        {
            "name": "az synapse linked-service set",
            "summary": "Update an exist linked service.",
            "syntax": "az synapse linked-service set --file\n                              --name\n                              --workspace-name\n                              [--no-wait]",
            "required_parameters": [
                "--file",
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse linked-service set --workspace-name testsynapseworkspace \\\n  --name testlinkedservice --file @\"path/linkedservice.json\""
            ]
        },
        {
            "name": "az synapse linked-service show",
            "summary": "Get a linked service.",
            "syntax": "az synapse linked-service show --name\n                               --workspace-name",
            "required_parameters": [
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az synapse linked-service show --workspace-name testsynapseworkspace \\\n  --name testlinkedservice"
            ]
        },
        {
            "name": "az synapse linked-service update",
            "summary": "Update an exist linked service.",
            "syntax": "az synapse linked-service update --file\n                                 --name\n                                 --workspace-name\n                                 [--no-wait]",
            "required_parameters": [
                "--file",
                "--name -n",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az synapse linked-service update --workspace-name testsynapseworkspace \\\n  --name testlinkedservice --file @\"path/linkedservice.json\""
            ]
        }
    ]
}