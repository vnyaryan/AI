{
    "command_group": "az synapse kusto pool",
    "description": "Manage kusto pool with synapse.",
    "status": "GA",
    "commands": [
        {
            "name": "az synapse kusto pool add-language-extension",
            "summary": "Add a list of language extensions that can run within KQL queries.",
            "syntax": "az synapse kusto pool add-language-extension [--ids]\n                                             [--kusto-pool-name]\n                                             [--no-wait]\n                                             [--resource-group]\n                                             [--subscription]\n                                             [--value]\n                                             [--workspace-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--kusto-pool-name --name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--value",
                "--workspace-name"
            ],
            "examples": [
                "az synapse kusto pool add-language-extension --name \"kustoclusterrptest4\" --value language-extension-name=\"PYTHON\" --value language-extension-name=\"R\" --resource-group \"kustorptest\" --workspace-name \"kustorptest\""
            ]
        },
        {
            "name": "az synapse kusto pool create",
            "summary": "Create a Kusto pool.",
            "syntax": "az synapse kusto pool create --kusto-pool-name\n                             --resource-group\n                             --sku\n                             --workspace-name\n                             [--enable-purge {false, true}]\n                             [--enable-streaming-ingest {false, true}]\n                             [--if-match]\n                             [--if-none-match]\n                             [--location]\n                             [--no-wait]\n                             [--optimized-autoscale]\n                             [--tags]\n                             [--workspace-uid]",
            "required_parameters": [
                "--kusto-pool-name --name -n",
                "--resource-group -g",
                "--sku",
                "--workspace-name"
            ],
            "optional_parameters": [
                "--enable-purge",
                "--enable-streaming-ingest --esig",
                "--if-match",
                "--if-none-match",
                "--location -l",
                "--no-wait",
                "--optimized-autoscale",
                "--tags",
                "--workspace-uid"
            ],
            "examples": [
                "az synapse kusto pool create --name \"kustoclusterrptest4\" --location \"westus\" --enable-purge true --enable-streaming-ingest true --workspace-uid \"11111111-2222-3333-444444444444\" --sku name=\"Storage optimized\" capacity=2 size=\"Medium\" --resource-group \"kustorptest\" --workspace-name \"synapseWorkspaceName\""
            ]
        },
        {
            "name": "az synapse kusto pool delete",
            "summary": "Deletes a Kusto pool.",
            "syntax": "az synapse kusto pool delete [--ids]\n                             [--kusto-pool-name]\n                             [--no-wait]\n                             [--resource-group]\n                             [--subscription]\n                             [--workspace-name]\n                             [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--kusto-pool-name --name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--workspace-name",
                "--yes -y"
            ],
            "examples": [
                "az synapse kusto pool delete --name \"kustoclusterrptest4\" --resource-group \"kustorptest\" --workspace-name \"kustorptest\""
            ]
        },
        {
            "name": "az synapse kusto pool detach-follower-database",
            "summary": "Detaches all followers of a database owned by this Kusto Pool.",
            "syntax": "az synapse kusto pool detach-follower-database --adcn\n                                               --kusto-pool-resource-id\n                                               [--ids]\n                                               [--kusto-pool-name]\n                                               [--no-wait]\n                                               [--resource-group]\n                                               [--subscription]\n                                               [--workspace-name]",
            "required_parameters": [
                "--adcn --attached-database-configuration-name",
                "--kusto-pool-resource-id"
            ],
            "optional_parameters": [
                "--ids",
                "--kusto-pool-name --name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--workspace-name"
            ],
            "examples": [
                "az synapse kusto pool detach-follower-database --attached-database-configuration-name \"myAttachedDatabaseConfiguration\" --kusto-pool-resource-id \"/subscriptions/12345678-1234-1234-1234-123456789098/resourceGroups/kustorptest/providers/Microsoft.Synapse/workspaces/kustorptest/kustoPools/leader4\" --name \"kustoclusterrptest4\" --resource-group \"kustorptest\" --workspace-name \"kustorptest\""
            ]
        },
        {
            "name": "az synapse kusto pool list",
