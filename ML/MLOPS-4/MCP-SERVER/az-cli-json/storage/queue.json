{
    "command_group": "az storage queue",
    "description": "Manage storage queues.",
    "status": "Preview",
    "commands": [
        {
            "name": "az storage queue create",
            "summary": "Create a queue under the given account.",
            "syntax": "az storage queue create --name\n                        [--account-key]\n                        [--account-name]\n                        [--auth-mode {key, login}]\n                        [--connection-string]\n                        [--fail-on-exist]\n                        [--metadata]\n                        [--queue-endpoint]\n                        [--sas-token]\n                        [--timeout]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--fail-on-exist",
                "--metadata",
                "--queue-endpoint",
                "--sas-token",
                "--timeout"
            ],
            "examples": [
                "az storage queue create -n myqueue --metadata key1=value1 key2=value2 --account-name mystorageaccount"
            ]
        },
        {
            "name": "az storage queue delete",
            "summary": "Delete the specified queue and any messages it contains.",
            "syntax": "az storage queue delete --name\n                        [--account-key]\n                        [--account-name]\n                        [--auth-mode {key, login}]\n                        [--connection-string]\n                        [--fail-not-exist]\n                        [--queue-endpoint]\n                        [--sas-token]\n                        [--timeout]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--fail-not-exist",
                "--queue-endpoint",
                "--sas-token",
                "--timeout"
            ],
            "examples": [
                "az storage queue delete -n myqueue --fail-not-exist --account-name mystorageaccount"
            ]
        },
        {
            "name": "az storage queue exists",
            "summary": "Return a boolean indicating whether the queue exists.",
            "syntax": "az storage queue exists --name\n                        [--account-key]\n                        [--account-name]\n                        [--auth-mode {key, login}]\n                        [--connection-string]\n                        [--queue-endpoint]\n                        [--sas-token]\n                        [--timeout]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--queue-endpoint",
                "--sas-token",
                "--timeout"
            ],
            "examples": [
                "az storage queue exists -n myqueue --account-name mystorageaccount"
            ]
        },
        {
            "name": "az storage queue generate-sas",
            "summary": "Generate a shared access signature for the queue.Use the returned signature with the sas_token parameter of QueueService.",
            "syntax": "az storage queue generate-sas --name\n                              [--account-key]\n                              [--account-name]\n                              [--connection-string]\n                              [--expiry]\n                              [--https-only]\n                              [--ip]\n                              [--permissions]\n                              [--policy-name]\n                              [--queue-endpoint]\n                              [--start]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--connection-string",
                "--expiry",
                "--https-only",
                "--ip",
                "--permissions",
                "--policy-name",
                "--queue-endpoint",
                "--start"
            ],
            "examples": [
                "end=`date -u -d \"30 minutes\" '+%Y-%m-%dT%H:%MZ'`\naz storage queue generate-sas -n myqueue --account-name mystorageaccount --permissions r --expiry $end --https-only",
                "end=`date -u -d \"30 minutes\" '+%Y-%m-%dT%H:%MZ'`\naz storage queue generate-sas -n myqueue --account-name mystorageaccount --ip \"176.134.171.0-176.134.171.255\" --permissions r --expiry $end --https-only"
            ]
        },
        {
            "name": "az storage queue list",
            "summary": "List queues in a storage account.",
            "syntax": "az storage queue list [--account-key]\n                      [--account-name]\n                      [--auth-mode {key, login}]\n                      [--connection-string]\n                      [--include-metadata]\n                      [--marker]\n                      [--num-results]\n                      [--prefix]\n                      [--queue-endpoint]\n                      [--sas-token]\n                      [--show-next-marker]\n                      [--timeout]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--include-metadata",
                "--marker",
                "--num-results",
                "--prefix",
                "--queue-endpoint",
                "--sas-token",
                "--show-next-marker",
                "--timeout"
            ],
            "examples": [
                "az storage queue list --prefix myprefix --account-name mystorageaccount"
            ]
        },
        {
            "name": "az storage queue stats",
            "summary": "Retrieve statistics related to replication for the Queue service. It is only available when read-access geo-redundant replication is enabled for the storage account.",
            "syntax": "az storage queue stats [--account-key]\n                       [--account-name]\n                       [--auth-mode {key, login}]\n                       [--connection-string]\n                       [--queue-endpoint]\n                       [--sas-token]\n                       [--timeout]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-key",
                "--account-name",
                "--auth-mode",
                "--connection-string",
                "--queue-endpoint",
                "--sas-token",
                "--timeout"
            ],
            "examples": [
                "az storage queue stats --account-name mystorageaccount"
            ]
        }
    ]
}