{
    "command_group": "az fluid-relay container",
    "description": "Manage Fluid Relay Container.",
    "status": "GA",
    "commands": [
        {
            "name": "az fluid-relay container delete",
            "summary": "Delete a Fluid Relay container.",
            "syntax": "az fluid-relay container delete [--container-name]\n                                [--ids]\n                                [--resource-group]\n                                [--server-name]\n                                [--subscription]\n                                [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--container-name --name -n",
                "--ids",
                "--resource-group -g",
                "--server-name",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az fluid-relay container delete -g MyResourceGroup --server-name MyServerName -n MyContainerName"
            ]
        },
        {
            "name": "az fluid-relay container list",
            "summary": "List all Fluid Relay containers which are children of a given Fluid Relay server.",
            "syntax": "az fluid-relay container list --resource-group\n                              --server-name",
            "required_parameters": [
                "--resource-group -g",
                "--server-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az fluid-relay container list -g MyResourceGroup --server-name MyServerName"
            ]
        },
        {
            "name": "az fluid-relay container show",
            "summary": "Get a Fluid Relay container.",
            "syntax": "az fluid-relay container show [--container-name]\n                              [--ids]\n                              [--resource-group]\n                              [--server-name]\n                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--container-name --name -n",
                "--ids",
                "--resource-group -g",
                "--server-name",
                "--subscription"
            ],
            "examples": [
                "az fluid-relay container show -g MyResourceGroup --server-name MyServerName -n MyContainerName"
            ]
        }
    ]
}