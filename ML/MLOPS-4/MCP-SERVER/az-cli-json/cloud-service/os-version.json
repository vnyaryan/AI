{
    "command_group": "az cloud-service os-version",
    "description": "Manage cloud service os version.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az cloud-service os-version list",
            "summary": "List all guest operating system versions available to be specified in the XML service configuration (.cscfg) for a cloud service.",
            "syntax": "az cloud-service os-version list --location\n                                 [--max-items]\n                                 [--next-token]",
            "required_parameters": [
                "--location -l"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az cloud-service os-version list -l eastus"
            ]
        },
        {
            "name": "az cloud-service os-version show",
            "summary": "Show guest operating system version that can be specified in the XML service configuration (.cscfg) for a cloud service.",
            "syntax": "az cloud-service os-version show [--ids]\n                                 [--location]\n                                 [--name]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--location -l",
                "--name --os-version-name -n",
                "--subscription"
            ],
            "examples": [
                "az cloud-service os-version show -n os-version -l westus"
            ]
        }
    ]
}