{
    "command_group": "az quota request",
    "description": "Manage quota request details and status.",
    "status": "GA",
    "commands": [
        {
            "name": "az quota request list",
            "summary": "List the specified scope, get the current quota requests for a one year period ending at the time is made. Use the **oData** filter to select quota requests.",
            "syntax": "az quota request list --scope\n                      [--filter]\n                      [--max-items]\n                      [--next-token]\n                      [--skiptoken]\n                      [--top]",
            "required_parameters": [
                "--scope"
            ],
            "optional_parameters": [
                "--filter",
                "--max-items",
                "--next-token",
                "--skiptoken",
                "--top"
            ],
            "examples": [
                "az quota request list --scope /subscriptions/00000000-0000-0000-0000-000000000000/providers/Microsoft.Network/locations/eastus --top 3"
            ]
        },
        {
            "name": "az quota request show",
            "summary": "Show the quota request details and status by quota request ID for the resources of the resource provider at a specific location.",
            "syntax": "az quota request show --id\n                      --scope",
            "required_parameters": [
                "--id",
                "--scope"
            ],
            "optional_parameters": [],
            "examples": [
                "az quota request show --scope /subscriptions/00000000-0000-0000-0000-000000000000/providers/Microsoft.Network/locations/eastus --id resourcename/subRequestId"
            ]
        }
    ]
}