{
    "command_group": "az resource",
    "description": "Manage Azure resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az resource create",
            "summary": "Create a resource.",
            "syntax": "az resource create --properties\n                   [--api-version]\n                   [--id]\n                   [--is-full-object]\n                   [--latest-include-preview]\n                   [--location]\n                   [--name]\n                   [--namespace]\n                   [--parent]\n                   [--resource-group]\n                   [--resource-type]",
            "required_parameters": [
                "--properties -p"
            ],
            "optional_parameters": [
                "--api-version",
                "--id",
                "--is-full-object",
                "--latest-include-preview -v",
                "--location -l",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource create -g myRG -n myApiApp --resource-type Microsoft.web/sites \\\n    --is-full-object --properties \"{ \\\"kind\\\": \\\"api\\\", \\\"location\\\": \\\n        \\\"West US\\\", \\\"properties\\\": { \\\"serverFarmId\\\": \\\n            \\\"/subscriptions/{SubID}/resourcegroups/{ResourceGroup} \\\n                /providers/Microsoft.Web/serverfarms/{ServicePlan}\\\" } }\"",
                "az resource create -g myRG -n myApiApp --resource-type Microsoft.web/sites --is-full-object --properties @jsonConfigFile",
                "az resource create -g myRG -n myWeb --resource-type Microsoft.web/sites \\\n    --properties \"{ \\\"serverFarmId\\\":\\\"/subscriptions/{SubID}/resourcegroups/ \\\n        {ResourceGroup}/providers/Microsoft.Web/serverfarms/{ServicePlan}\\\" }\"",
                "az resource create -g myRG -n myApiApp --resource-type Microsoft.web/sites --is-full-object --properties @jsonConfigFile --latest-include-preview",
                "az resource create -g myRG --api-version \"2018-02-01\" \\\n    --name \"{sitename+slot}/siteextensions/Contrast.NetCore.Azure.SiteExtension\"  \\\n        --resource-type Microsoft.Web/sites/siteextensions --is-full-object \\\n            --properties \"{ \\\"id\\\": \\\"Contrast.NetCore.Azure.SiteExtension\\\", \\\n                \\\"location\\\": \\\"West US\\\", \\\"version\\\": \\\"1.9.0\\\" }\""
            ]
        },
        {
            "name": "az resource delete",
            "summary": "Delete a resource.",
            "syntax": "az resource delete [--api-version]\n                   [--ids]\n                   [--latest-include-preview]\n                   [--name]\n                   [--namespace]\n                   [--no-wait]\n                   [--parent]\n                   [--resource-group]\n                   [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--api-version",
                "--ids",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--no-wait",
                "--parent",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource delete -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\"",
                "az resource delete --ids /subscriptions/0b1f6471-1bf0-4dda-aec3-111111111111/resourceGroups/MyResourceGroup/providers/Microsoft.Web/sites/MyWebapp",
                "az resource delete --ids /subscriptions/0b1f6471-1bf0-4dda-aec3-111111111111/resourceGroups/MyResourceGroup/providers/Microsoft.Network/virtualNetworks/MyVnet/subnets/MySubnet",
                "az resource delete -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\" --latest-include-preview"
            ]
        },
        {
            "name": "az resource invoke-action",
            "summary": "Invoke an action on the resource.",
            "syntax": "az resource invoke-action --action\n                          [--api-version]\n                          [--ids]\n                          [--latest-include-preview]\n                          [--name]\n                          [--namespace]\n                          [--no-wait]\n                          [--parent]\n                          [--request-body]\n                          [--resource-group]\n                          [--resource-type]",
            "required_parameters": [
                "--action"
            ],
            "optional_parameters": [
                "--api-version",
                "--ids",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--no-wait",
                "--parent",
                "--request-body",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource invoke-action --action powerOff \\\n  --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Compute/virtualMachines/{VMName}",
                "az resource invoke-action --action capture \\\n  --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/ \\\n    Microsoft.Compute/virtualMachines/{VMName} \\\n  --request-body \"{ \\\"vhdPrefix\\\": \\\"myPrefix\\\", \\\"destinationContainerName\\\": \\\n    \\\"myContainer\\\", \\\"overwriteVhds\\\": true }\"",
                "az resource invoke-action --action capture --name MyResource --resource-group MyResourceGroup --resource-type Microsoft.web/sites"
            ]
        },
        {
            "name": "az resource list",
            "summary": "List resources.",
            "syntax": "az resource list [--location]\n                 [--name]\n                 [--namespace]\n                 [--resource-group]\n                 [--resource-type]\n                 [--tag]",
            "required_parameters": [],
            "optional_parameters": [
                "--location -l",
                "--name -n",
                "--namespace",
                "--resource-group -g",
                "--resource-type",
                "--tag"
            ],
            "examples": [
                "az resource list --location westus",
                "az resource list --name 'resourceName'",
                "az resource list --tag test",
                "az resource list --tag 'test*'",
                "az resource list --tag test=example"
            ]
        },
        {
            "name": "az resource move",
            "summary": "Move resources from one resource group to another (can be under different subscription).",
            "syntax": "az resource move --destination-group\n                 --ids\n                 [--destination-subscription-id]",
            "required_parameters": [
                "--destination-group",
                "--ids"
            ],
            "optional_parameters": [
                "--destination-subscription-id"
            ],
            "examples": [
                "az resource move --destination-group ResourceGroup --destination-subscription-id SubscriptionId --ids \"ResourceId1\" \"ResourceId2\" \"ResourceId3\""
            ]
        },
        {
            "name": "az resource patch",
            "summary": "Update a resource by PATCH request.",
            "syntax": "az resource patch --properties\n                  [--api-version]\n                  [--ids]\n                  [--is-full-object]\n                  [--latest-include-preview]\n                  [--name]\n                  [--namespace]\n                  [--parent]\n                  [--resource-group]\n                  [--resource-type]",
            "required_parameters": [
                "--properties -p"
            ],
            "optional_parameters": [
                "--api-version",
                "--ids",
                "--is-full-object",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource patch --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp} \\\n  --latest-include-preview --is-full-object --properties \"{ \\\"tags\\\": { \\\"key\\\": \\\"value\\\" } }\"",
                "az resource patch --name MyResource --resource-group MyResourceGroup --resource-type Microsoft.web/sites \\\n  --is-full-object --properties @jsonConfigFile",
                "az resource patch --name MyApiApp --resource-group MyResourceGroup --resource-type Microsoft.web/sites \\\n    --is-full-object --properties \"{ \\\"kind\\\": \\\"api\\\", \\\"properties\\\": { \\\"serverFarmId\\\": \\\n            \\\"/subscriptions/{SubID}/resourcegroups/{ResourceGroup} \\\n                /providers/Microsoft.Web/serverfarms/{ServicePlan}\\\" } }\""
            ]
        },
        {
            "name": "az resource show",
            "summary": "Get the details of a resource.",
            "syntax": "az resource show [--api-version]\n                 [--ids]\n                 [--include-response-body {false, true}]\n                 [--latest-include-preview]\n                 [--name]\n                 [--namespace]\n                 [--parent]\n                 [--resource-group]\n                 [--resource-type]",
            "required_parameters": [],
            "optional_parameters": [
                "--api-version",
                "--ids",
                "--include-response-body",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource show -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\"",
                "az resource show --ids /subscriptions/0b1f6471-1bf0-4dda-aec3-111111111111/resourceGroups/MyResourceGroup/providers/Microsoft.Web/sites/MyWebapp",
                "az resource show -g MyResourceGroup -n MySubnet --namespace Microsoft.Network --parent virtualnetworks/MyVnet --resource-type subnets",
                "az resource show --ids /subscriptions/0b1f6471-1bf0-4dda-aec3-111111111111/resourceGroups/MyResourceGroup/providers/Microsoft.Network/virtualNetworks/MyVnet/subnets/MySubnet",
                "az resource show -g MyResourceGroup --namespace Microsoft.Network --parent applicationGateways/ag1/urlPathMaps/map1 --resource-type pathRules -n rule1",
                "az resource show -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\" --latest-include-preview"
            ]
        },
        {
            "name": "az resource tag",
            "summary": "Tag a resource.",
            "syntax": "az resource tag --tags\n                [--api-version]\n                [--ids]\n                [--is-incremental]\n                [--latest-include-preview]\n                [--name]\n                [--namespace]\n                [--parent]\n                [--resource-group]\n                [--resource-type]",
            "required_parameters": [
                "--tags"
            ],
            "optional_parameters": [
                "--api-version",
                "--ids",
                "--is-incremental -i",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource-group -g",
                "--resource-type"
            ],
            "examples": [
                "az resource tag --tags vmlist=vm1 -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\"",
                "az resource tag --tags vmlist=vm1 --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp}",
                "az resource tag --tags vmlist=vm1 -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\" -i",
                "az resource tag --tags vmlist=vm1 -g MyResourceGroup -n MyVm --resource-type \"Microsoft.Compute/virtualMachines\" --latest-include-preview"
            ]
        },
        {
            "name": "az resource update",
            "summary": "Update a resource by PUT request.",
            "syntax": "az resource update [--add]\n                   [--api-version]\n                   [--force-string]\n                   [--ids]\n                   [--include-response-body {false, true}]\n                   [--latest-include-preview]\n                   [--name]\n                   [--namespace]\n                   [--parent]\n                   [--remove]\n                   [--resource-group]\n                   [--resource-type]\n                   [--set]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--api-version",
                "--force-string",
                "--ids",
                "--include-response-body",
                "--latest-include-preview -v",
                "--name -n",
                "--namespace",
                "--parent",
                "--remove",
                "--resource-group -g",
                "--resource-type",
                "--set"
            ],
            "examples": [
                "az resource update --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp} --set tags.key=value --latest-include-preview",
                "az resource update --ids $id --set properties.connectionType=Proxy",
                "az resource update --name MyResource --resource-group MyResourceGroup --resource-type subnets --set tags.key=value"
            ]
        },
        {
            "name": "az resource wait",
            "summary": "Place the CLI in a waiting state until a condition of a resources is met.",
            "syntax": "az resource wait [--api-version]\n                 [--created]\n                 [--custom]\n                 [--deleted]\n                 [--exists]\n                 [--ids]\n                 [--include-response-body {false, true}]\n                 [--interval]\n                 [--name]\n                 [--namespace]\n                 [--parent]\n                 [--resource-group]\n                 [--resource-type]\n                 [--timeout]\n                 [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--api-version",
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--include-response-body",
                "--interval",
                "--name -n",
                "--namespace",
                "--parent",
                "--resource-group -g",
                "--resource-type",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az resource wait --exists --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp}",
                "az resource wait --exists --ids /subscriptions/{SubID}/resourceGroups/{ResourceGroup}/providers/Microsoft.Web/sites/{WebApp} --include-response-body true",
                "az resource wait --exists --name MyResource --resource-group MyResourceGroup --resource-type subnets"
            ]
        }
    ]
}