{
    "command_group": "az functionapp deployment github-actions",
    "description": "Configure GitHub Actions for a functionapp.",
    "status": "GA",
    "commands": [
        {
            "name": "az functionapp deployment github-actions add",
            "summary": "Add a GitHub Actions workflow file to the specified repository. The workflow will build and deploy your app to the specified functionapp.",
            "syntax": "az functionapp deployment github-actions add --repo\n                                             [--branch]\n                                             [--build-path]\n                                             [--force]\n                                             [--ids]\n                                             [--login-with-github]\n                                             [--name]\n                                             [--resource-group]\n                                             [--runtime]\n                                             [--runtime-version]\n                                             [--slot]\n                                             [--subscription]\n                                             [--token]",
            "required_parameters": [
                "--repo"
            ],
            "optional_parameters": [
                "--branch -b",
                "--build-path",
                "--force -f",
                "--ids",
                "--login-with-github",
                "--name -n",
                "--resource-group -g",
                "--runtime -r",
                "--runtime-version -v",
                "--slot -s",
                "--subscription",
                "--token"
            ],
            "examples": [
                "az functionapp deployment github-actions add --repo \"githubUser/githubRepo\" -g MyResourceGroup -n MyFunctionapp --token MyPersonalAccessToken",
                "az functionapp deployment github-actions add --repo \"githubUser/githubRepo\" -g MyResourceGroup -n MyFunctionapp --login-with-github"
            ]
        },
        {
            "name": "az functionapp deployment github-actions remove",
            "summary": "Remove and disconnect the GitHub Actions workflow file from the specified repository.",
            "syntax": "az functionapp deployment github-actions remove --repo\n                                                [--branch]\n                                                [--ids]\n                                                [--login-with-github]\n                                                [--name]\n                                                [--resource-group]\n                                                [--slot]\n                                                [--subscription]\n                                                [--token]",
            "required_parameters": [
                "--repo"
            ],
            "optional_parameters": [
                "--branch -b",
                "--ids",
                "--login-with-github",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription",
                "--token"
            ],
            "examples": [
                "az functionapp deployment github-actions remove --repo \"githubUser/githubRepo\" -g MyResourceGroup -n MyFunctionapp --token MyPersonalAccessToken",
                "az functionapp deployment github-actions remove --repo \"githubUser/githubRepo\" -g MyResourceGroup -n MyFunctionapp --login-with-github"
            ]
        }
    ]
}