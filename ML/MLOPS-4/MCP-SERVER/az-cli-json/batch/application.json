{
    "command_group": "az batch application",
    "description": "Manage Batch applications.",
    "status": "GA",
    "commands": [
        {
            "name": "az batch application create",
            "summary": "Adds an application to the specified Batch account.",
            "syntax": "az batch application create --application-name\n                            --name\n                            --resource-group\n                            [--parameters]",
            "required_parameters": [
                "--application-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--parameters"
            ],
            "examples": []
        },
        {
            "name": "az batch application delete",
            "summary": "Deletes an application.",
            "syntax": "az batch application delete --application-name\n                            --name\n                            --resource-group\n                            [--yes]",
            "required_parameters": [
                "--application-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az batch application list",
            "summary": "Lists all of the applications in the specified account.",
            "syntax": "az batch application list --name\n                          --resource-group\n                          [--maxresults]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--maxresults"
            ],
            "examples": []
        },
        {
            "name": "az batch application set",
            "summary": "Update properties for a Batch application.",
            "syntax": "az batch application set --application-name\n                         --name\n                         --resource-group\n                         [--allow-updates]\n                         [--default-version]\n                         [--display-name]",
            "required_parameters": [
                "--application-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--allow-updates",
                "--default-version",
                "--display-name"
            ],
            "examples": []
        },
        {
            "name": "az batch application show",
            "summary": "Gets information about the specified application.",
            "syntax": "az batch application show --application-name\n                          --name\n                          --resource-group",
            "required_parameters": [
                "--application-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        }
    ]
}