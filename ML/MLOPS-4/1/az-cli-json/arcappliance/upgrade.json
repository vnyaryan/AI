{
    "command_group": "az arcappliance upgrade",
    "description": "Command group for upgrading an Appliance cluster.",
    "status": "GA",
    "commands": [
        {
            "name": "az arcappliance upgrade hci",
            "summary": "Command to upgrade an Appliance on Azure Stack HCI.",
            "syntax": "az arcappliance upgrade hci --config-file\n                            [--location]\n                            [--name]\n                            [--no-wait]\n                            [--resource-group]",
            "required_parameters": [
                "--config-file"
            ],
            "optional_parameters": [
                "--location -l",
                "--name -n",
                "--no-wait",
                "--resource-group -g"
            ],
            "examples": [
                "az arcappliance upgrade hci --config-file [REQUIRED]"
            ]
        },
        {
            "name": "az arcappliance upgrade scvmm",
            "summary": "Command to upgrade an Appliance on SCVMM.",
            "syntax": "az arcappliance upgrade scvmm --config-file\n                              [--location]\n                              [--name]\n                              [--no-wait]\n                              [--resource-group]",
