            ],
            "optional_parameters": [
                "--location -l",
                "--name -n",
                "--no-wait",
                "--resource-group -g"
            ],
            "examples": [
                "az arcappliance upgrade scvmm --config-file [REQUIRED]"
            ]
        },
        {
            "name": "az arcappliance upgrade vmware",
            "summary": "Command to upgrade an Appliance on VMware.",
            "syntax": "az arcappliance upgrade vmware --config-file\n                               [--location]\n                               [--name]\n                               [--no-wait]\n                               [--resource-group]",
            "required_parameters": [
