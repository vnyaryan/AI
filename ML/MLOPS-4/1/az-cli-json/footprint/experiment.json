{
    "command_group": "az footprint experiment",
    "description": "Commands to manage experiments under a profile.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az footprint experiment create",
            "summary": "Creates or updates a Footprint experiment with the specified properties.",
            "syntax": "az footprint experiment create --experiment-name\n                               --profile-name\n                               --resource-group\n                               [--description]",
            "required_parameters": [
                "--experiment-name --name -n",
                "--profile-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--description"
            ],
            "examples": [
                "az footprint experiment create --name \"fpExp1\" --description \"An experiment description.\" --profile-name \"fpProfile1\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az footprint experiment delete",
            "summary": "Deletes an existing Footprint experiment.",
            "syntax": "az footprint experiment delete [--experiment-name]\n                               [--ids]\n                               [--profile-name]\n                               [--resource-group]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--experiment-name --name -n",
                "--ids",
                "--profile-name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az footprint experiment delete --name \"fpExp1\" --profile-name \"fpProfile1\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az footprint experiment list",
            "summary": "Retrieves the information about all experiments under a Footprint profile.",
            "syntax": "az footprint experiment list --profile-name\n                             --resource-group",
            "required_parameters": [
                "--profile-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az footprint experiment list --profile-name \"fpProfile1\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az footprint experiment show",
            "summary": "Retrieves the information about a single Footprint experiment.",
            "syntax": "az footprint experiment show [--experiment-name]\n                             [--ids]\n                             [--profile-name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--experiment-name --name -n",
                "--ids",
                "--profile-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az footprint experiment show --name \"fpExp1\" --profile-name \"fpProfile1\" --resource-group \"rgName\""
            ]
        },
        {
            "name": "az footprint experiment update",
            "summary": "Creates or updates a Footprint experiment with the specified properties.",
            "syntax": "az footprint experiment update [--description]\n                               [--experiment-name]\n                               [--ids]\n                               [--profile-name]\n                               [--resource-group]\n                               [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--description",
                "--experiment-name --name -n",
                "--ids",
                "--profile-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        }
    ]
}