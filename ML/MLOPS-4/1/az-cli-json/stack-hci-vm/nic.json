{
    "command_group": "az stack-hci-vm nic",
    "description": "Manage vNIC of a virtual machine with stack-hci-vm.",
    "status": "GA",
    "commands": [
        {
            "name": "az stack-hci-vm nic add",
            "summary": "Add existing vNICs to a VM.",
            "syntax": "az stack-hci-vm nic add --nics\n                        --resource-group\n                        --vm-name",
            "required_parameters": [
                "--nics",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [],
            "examples": [
                "az stack-hci-vm nic add --resource-group \"test-rg\" --vm-name \"test-vm\" --nics \"test-vnic\""
            ]
        },
        {
            "name": "az stack-hci-vm nic remove",
            "summary": "Remove vNICs from a VM.",
            "syntax": "az stack-hci-vm nic remove --nics\n                           --resource-group\n                           --vm-name\n                           [--yes]",
            "required_parameters": [
                "--nics",
                "--resource-group -g",
                "--vm-name"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az stack-hci-vm nic remove --resource-group \"test-rg\" --vm-name \"test-vm\" --nics \"test-vnic\""
            ]
        }
    ]
}