{
    "command_group": "az acr cache",
    "description": "Manage cache rules in Azure Container Registries.",
    "status": "GA",
    "commands": [
        {
            "name": "az acr cache create",
            "summary": "Create a cache rule.",
            "syntax": "az acr cache create --name\n                    --registry\n                    --source-repo\n                    --target-repo\n                    [--cred-set]\n                    [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--registry -r",
                "--source-repo -s",
                "--target-repo -t"
            ],
            "optional_parameters": [
                "--cred-set -c",
                "--resource-group -g"
            ],
            "examples": [
                "az acr cache create -r myregistry -n MyRule -s docker.io/library/ubuntu -t ubuntu",
                "az acr cache create -r myregistry -n MyRule -s docker.io/library/ubuntu -t ubuntu -c MyCredSet"
            ]
        },
        {
            "name": "az acr cache delete",
            "summary": "Delete a cache rule.",
            "syntax": "az acr cache delete --name\n                    --registry\n                    [--resource-group]\n                    [--yes]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g",
                "--yes -y"
            ],
            "examples": [
                "az acr cache delete -r myregistry -n MyRule"
            ]
        },
        {
            "name": "az acr cache list",
            "summary": "List the cache rules in an Azure Container Registry.",
            "syntax": "az acr cache list --registry\n                  [--resource-group]",
            "required_parameters": [
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az acr cache list -r myregistry"
            ]
        },
        {
            "name": "az acr cache show",
            "summary": "Show a cache rule.",
            "syntax": "az acr cache show --name\n                  --registry\n                  [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az acr cache show -r myregistry -n MyRule"
            ]
        },
        {
            "name": "az acr cache update",
            "summary": "Update the credential set on a cache rule.",
            "syntax": "az acr cache update --name\n                    --registry\n                    [--add]\n                    [--cred-set]\n                    [--force-string]\n                    [--remove]\n                    [--remove-cred-set]\n                    [--resource-group]\n                    [--set]",
            "required_parameters": [
                "--name -n",
                "--registry -r"
            ],
            "optional_parameters": [
                "--add",
                "--cred-set -c",
                "--force-string",
                "--remove",
                "--remove-cred-set",
                "--resource-group -g",
                "--set"
            ],
            "examples": [
                "az acr cache update -r myregistry -n MyRule -c NewCredSet",
                "az acr cache update -r myregistry -n MyRule --remove-cred-set"
            ]
        }
    ]
}