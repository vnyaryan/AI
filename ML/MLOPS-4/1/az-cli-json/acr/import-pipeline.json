{
    "command_group": "az acr import-pipeline",
    "description": "Manage ACR import pipelines.",
    "status": "Preview",
    "commands": [
        {
            "name": "az acr import-pipeline create",
            "summary": "Create an import pipeline.",
            "syntax": "az acr import-pipeline create --name\n                              --registry\n                              --resource-group\n                              --secret-uri\n                              --storage-container-uri\n                              [--assign-identity]\n                              [--options]\n                              [--source-trigger-enabled {false, true}]",
            "required_parameters": [
                "--name -n",
                "--registry -r",
                "--resource-group -g",
                "--secret-uri -s",
                "--storage-container-uri -c"
            ],
            "optional_parameters": [
                "--assign-identity -i",
                "--options -z",
                "--source-trigger-enabled -e"
            ],
            "examples": [
                "az acr import-pipeline create --resource-group $MyRG --registry $MyReg --name $MyPipeline --secret-uri https://$MyKV.vault.azure.net/secrets/$MySecret --storage-container-uri https://$MyStorage.blob.core.windows.net/$MyContainer",
                "az acr import-pipeline create --resource-group $MyRG --registry $MyReg --name $MyPipeline --secret-uri https://$MyKV.vault.azure.net/secrets/$MySecret --storage-container-uri https://$MyStorage.blob.core.windows.net/$MyContainer --options DeleteSourceBlobOnSuccess OverwriteTags ContinueOnErrors --assign-identity /subscriptions/$MySubID/resourceGroups/$MyRG/providers/Microsoft.ManagedIdentity/userAssignedIdentities/$MyIdentity --source-trigger-enabled False"
            ]
        },
        {
            "name": "az acr import-pipeline delete",
            "summary": "Delete an import pipeline.",
            "syntax": "az acr import-pipeline delete --name\n                              --registry\n                              --resource-group",
            "required_parameters": [
                "--name -n",
                "--registry -r",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az acr import-pipeline delete --resource-group $MyRG --registry $MyReg --name $MyPipeline"
            ]
        },
        {
            "name": "az acr import-pipeline list",
            "summary": "List import pipelines on a Container Registry.",
            "syntax": "az acr import-pipeline list --registry\n                            --resource-group",
            "required_parameters": [
                "--registry -r",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az acr import-pipeline list --resource-group $MyRG --registry $MyReg"
            ]
        },
        {
            "name": "az acr import-pipeline show",
            "summary": "Show an import pipeline in detail.",
            "syntax": "az acr import-pipeline show --name\n                            --registry\n                            --resource-group",
            "required_parameters": [
                "--name -n",
                "--registry -r",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az acr import-pipeline show --resource-group $MyRG --registry $MyReg --name $MyPipeline"
            ]
        }
    ]
}