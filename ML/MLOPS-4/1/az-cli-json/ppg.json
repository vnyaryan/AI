{
    "command_group": "az ppg",
    "description": "Manage Proximity Placement Groups.",
    "status": "GA",
    "commands": [
        {
            "name": "az ppg create",
            "summary": "Create a proximity placement group.",
            "syntax": "az ppg create --name\n              --resource-group\n              [--intent-vm-sizes]\n              [--location]\n              [--tags]\n              [--type {Standard, Ultra}]\n              [--zone]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--intent-vm-sizes",
                "--location -l",
                "--tags",
                "--type -t",
                "--zone -z"
            ],
            "examples": [
                "az ppg create --name MyProximityPlacementGroup --resource-group MyResourceGroup",
                "az ppg create --name MyProximityPlacementGroup --resource-group MyResourceGroup \\\n    --intent-vm-sizes Standard_E64s_v4 Standard_M416ms_v2",
                "az ppg create --name MyProximityPlacementGroup --resource-group MyResourceGroup \\\n    --intent-vm-sizes Standard_E64s_v4 Standard_M416ms_v2 --zone 1"
            ]
        },
        {
            "name": "az ppg delete",
            "summary": "Delete a proximity placement group.",
            "syntax": "az ppg delete [--ids]\n              [--name]\n              [--resource-group]\n              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --proximity-placement-group-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ppg list",
            "summary": "List proximity placement groups.",
            "syntax": "az ppg list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az ppg list --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az ppg show",
            "summary": "Get a proximity placement group.",
            "syntax": "az ppg show [--ids]\n            [--include-colocation-status {False, True}]\n            [--name]\n            [--resource-group]\n            [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--include-colocation-status",
                "--name --proximity-placement-group-name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az ppg show --name MyProximityPlacementGroup --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az ppg update",
            "summary": "Update a proximity placement group.",
            "syntax": "az ppg update --name\n              --resource-group\n              [--add]\n              [--force-string]\n              [--include-colocation-status]\n              [--intent-vm-sizes]\n              [--remove]\n              [--set]\n              [--type {Standard, Ultra}]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--include-colocation-status",
                "--intent-vm-sizes",
                "--remove",
                "--set",
                "--type -t"
            ],
            "examples": [
                "az ppg update --name MyProximityPlacementGroup --resource-group MyResourceGroup \\\n    --intent-vm-sizes Standard_E64s_v4 Standard_M416ms_v2"
            ]
        }
    ]
}