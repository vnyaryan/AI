                "--enable-agent",
                "--enable-vm-config-agent",
                "--force-string",
                "--ids",
                "--memory-mb",
                "--name",
                "--nics",
                "--polling-interval",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription",
                "--tags",
                "--v-cpus-available",
