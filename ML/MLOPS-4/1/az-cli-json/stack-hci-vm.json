        {
            "name": "az stack-hci-vm update",
            "summary": "Update a VM.",
            "syntax": "az stack-hci-vm update [--add]\n                       [--computer-name]\n                       [--enable-agent {false, true}]\n                       [--enable-vm-config-agent {false, true}]\n                       [--force-string]\n                       [--ids]\n                       [--memory-mb]\n                       [--name]\n                       [--nics]\n                       [--polling-interval]\n                       [--remove]\n                       [--resource-group]\n                       [--set]\n                       [--subscription]\n                       [--tags]\n                       [--v-cpus-available]\n                       [--vhds]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
