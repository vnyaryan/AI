            "syntax": "az arc-multicloud solution-type list [--max-items]\n                                     [--next-token]\n                                     [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": [
                "az arc-multicloud solution-type list --resource-group multiCloudRG --subscription 2c60e476-6048-5ff0-9635-373175a378d9"
            ]
        },
        {
            "name": "az arc-multicloud solution-type show",
            "summary": "Get a SolutionTypeResource.",
            "syntax": "az arc-multicloud solution-type show [--ids]\n                                     [--name]\n                                     [--resource-group]\n                                     [--subscription]",
            "required_parameters": [],
