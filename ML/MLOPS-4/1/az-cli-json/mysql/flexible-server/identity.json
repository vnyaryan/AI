{
    "command_group": "az mysql flexible-server identity",
    "description": "Manage server user assigned identities.",
    "status": "GA",
    "commands": [
        {
            "name": "az mysql flexible-server identity assign",
            "summary": "Add user asigned managed identities to the server.",
            "syntax": "az mysql flexible-server identity assign --identity\n                                         --resource-group\n                                         --server-name\n                                         [--no-wait]",
            "required_parameters": [
                "--identity -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az mysql flexible-server identity assign -g testgroup -s testsvr --identity test-identity test-identity-2"
            ]
        },
        {
            "name": "az mysql flexible-server identity list",
            "summary": "List all user assigned managed identities from the server.",
            "syntax": "az mysql flexible-server identity list --resource-group\n                                       --server-name",
            "required_parameters": [
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az mysql flexible-server identity list -g testgroup -s testsvr"
            ]
        },
        {
            "name": "az mysql flexible-server identity remove",
            "summary": "Remove user asigned managed identites from the server.",
            "syntax": "az mysql flexible-server identity remove --identity\n                                         --resource-group\n                                         --server-name\n                                         [--no-wait]\n                                         [--yes]",
            "required_parameters": [
                "--identity -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az mysql flexible-server identity remove -g testgroup -s testsvr --identity test-identity"
            ]
        },
        {
            "name": "az mysql flexible-server identity show",
            "summary": "Get an user assigned managed identity from the server.",
            "syntax": "az mysql flexible-server identity show --identity\n                                       --resource-group\n                                       --server-name",
            "required_parameters": [
                "--identity -n",
                "--resource-group -g",
                "--server-name -s"
            ],
            "optional_parameters": [],
            "examples": [
                "az mysql flexible-server identity show -g testgroup -s testsvr --identity test-identity"
            ]
        }
    ]
}