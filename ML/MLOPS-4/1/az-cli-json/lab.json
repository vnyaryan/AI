{
    "command_group": "az lab",
    "description": "Manage azure devtest labs.",
    "status": "Preview",
    "commands": [
        {
            "name": "az lab delete",
            "summary": "Delete lab. This operation can take a while to complete.",
            "syntax": "az lab delete --name\n              --resource-group\n              [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n              [--yes]",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az lab get",
            "summary": "Get lab.",
            "syntax": "az lab get --name\n           --resource-group\n           [--expand]",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": []
        },
        {
            "name": "az lab wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az lab wait [--created]\n            [--custom]\n            [--deleted]\n            [--exists]\n            [--expand]\n            [--ids]\n            [--interval]\n            [--name]\n            [--resource-group]\n            [--subscription]\n            [--timeout]\n            [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--ids",
                "--interval",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}