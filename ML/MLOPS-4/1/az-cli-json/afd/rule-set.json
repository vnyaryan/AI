{
    "command_group": "az afd rule-set",
    "description": "Manage rule set for the specified profile.",
    "status": "GA",
    "commands": [
        {
            "name": "az afd rule-set create",
            "summary": "Create a new rule set within the specified profile.",
            "syntax": "az afd rule-set create --name\n                       --profile-name\n                       --resource-group",
            "required_parameters": [
                "--name --rule-set-name -n",
                "--profile-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az afd rule-set create -g group --rule-set-name ruleset1 --profile-name profile"
            ]
        },
        {
            "name": "az afd rule-set delete",
            "summary": "Delete an existing AzureFrontDoor rule set with the specified rule set name under the specified subscription, resource group and profile.",
            "syntax": "az afd rule-set delete [--ids]\n                       [--name]\n                       [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                       [--profile-name]\n                       [--resource-group]\n                       [--subscription]\n                       [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --rule-set-name -n",
                "--no-wait",
                "--profile-name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az afd rule-set delete -g group --rule-set-name ruleset1 --profile-name profile"
            ]
        },
        {
            "name": "az afd rule-set list",
            "summary": "List existing AzureFrontDoor rule sets within a profile.",
            "syntax": "az afd rule-set list --profile-name\n                     --resource-group\n                     [--max-items]\n                     [--next-token]",
            "required_parameters": [
                "--profile-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": []
        },
        {
            "name": "az afd rule-set show",
            "summary": "Get an existing AzureFrontDoor rule set with the specified rule set name under the specified subscription, resource group and profile.",
            "syntax": "az afd rule-set show [--ids]\n                     [--name]\n                     [--profile-name]\n                     [--resource-group]\n                     [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name --rule-set-name -n",
                "--profile-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az afd rule-set usage",
            "summary": "Checks the quota and actual usage of endpoints under the given Azure Front Door profile..",
            "syntax": "az afd rule-set usage --profile-name\n                      --resource-group\n                      --rule-set-name\n                      [--max-items]\n                      [--next-token]",
            "required_parameters": [
                "--profile-name",
                "--resource-group -g",
                "--rule-set-name"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": []
        },
        {
            "name": "az afd rule-set wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az afd rule-set wait [--created]\n                     [--custom]\n                     [--deleted]\n                     [--exists]\n                     [--ids]\n                     [--interval]\n                     [--name]\n                     [--profile-name]\n                     [--resource-group]\n                     [--subscription]\n                     [--timeout]\n                     [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--name --rule-set-name -n",
                "--profile-name",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}