{
    "command_group": "az apim api revision",
    "description": "Manage Azure API Management API Revision.",
    "status": "GA",
    "commands": [
        {
            "name": "az apim api revision create",
            "summary": "Create API revision.",
            "syntax": "az apim api revision create --api-id\n                            --api-revision\n                            --resource-group\n                            --service-name\n                            [--api-revision-description]",
            "required_parameters": [
                "--api-id",
                "--api-revision",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--api-revision-description --rev-description"
            ],
            "examples": [
                "az apim api revision create --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId --api-revision RevisionNumber --api-revision-description RevisionDescription"
            ]
        },
        {
            "name": "az apim api revision list",
            "summary": "Lists all revisions of an API.",
            "syntax": "az apim api revision list --api-id\n                          --resource-group\n                          --service-name",
            "required_parameters": [
                "--api-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": [
                "az apim api revision list --resource-group MyResourceGroup --service-name MyServiceName --api-id MyApiId"
            ]
        }
    ]
}