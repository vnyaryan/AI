{
    "command_group": "az apim nv",
    "description": "Manage Azure API Management Named Values.",
    "status": "GA",
    "commands": [
        {
            "name": "az apim nv create",
            "summary": "Create an API Management Named Value.",
            "syntax": "az apim nv create --display-name\n                  --named-value-id\n                  --resource-group\n                  --service-name\n                  [--if-match]\n                  [--no-wait]\n                  [--secret {false, true}]\n                  [--tags]\n                  [--value]",
            "required_parameters": [
                "--display-name",
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--if-match",
                "--no-wait",
                "--secret",
                "--tags",
                "--value"
            ],
            "examples": [
                "az apim nv create --service-name MyApim -g MyResourceGroup --named-value-id MyNamedValue --display-name 'My Named Value' --value 'foo'"
            ]
        },
        {
            "name": "az apim nv delete",
            "summary": "Delete an API Management Named Value.",
            "syntax": "az apim nv delete --named-value-id\n                  --resource-group\n                  --service-name\n                  [--yes]",
            "required_parameters": [
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az apim nv list",
            "summary": "List API Management Named Values.",
            "syntax": "az apim nv list --resource-group\n                --service-name",
            "required_parameters": [
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az apim nv show",
            "summary": "Show details of an API Management Named Value.",
            "syntax": "az apim nv show --named-value-id\n                --resource-group\n                --service-name",
            "required_parameters": [
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az apim nv show-secret",
            "summary": "Gets the secret of an API Management Named Value.",
            "syntax": "az apim nv show-secret --named-value-id\n                       --resource-group\n                       --service-name",
            "required_parameters": [
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az apim nv update",
            "summary": "Update an API Management Named Value.",
            "syntax": "az apim nv update --named-value-id\n                  --resource-group\n                  --service-name\n                  [--add]\n                  [--force-string]\n                  [--if-match]\n                  [--remove]\n                  [--secret {false, true}]\n                  [--set]\n                  [--tags]\n                  [--value]",
            "required_parameters": [
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--if-match",
                "--remove",
                "--secret",
                "--set",
                "--tags",
                "--value"
            ],
            "examples": [
                "az apim nv update --service-name MyApim -g MyResourceGroup --named-value-id MyNamedValue --value foo"
            ]
        },
        {
            "name": "az apim nv wait",
            "summary": "Place the CLI in a waiting state until a condition of an apim named value is met.",
            "syntax": "az apim nv wait --named-value-id\n                --resource-group\n                --service-name\n                [--created]\n                [--custom]\n                [--deleted]\n                [--exists]\n                [--interval]\n                [--timeout]\n                [--updated]",
            "required_parameters": [
                "--named-value-id",
                "--resource-group -g",
                "--service-name -n"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az apim nv wait --created --service-name MyApim -g MyResourceGroup --named-value-id MyNamedValue --resource-group MyResourceGroup"
            ]
        }
    ]
}