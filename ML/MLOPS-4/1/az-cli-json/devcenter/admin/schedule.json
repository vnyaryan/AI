{
    "command_group": "az devcenter admin schedule",
    "description": "Manage schedules.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin schedule create",
            "summary": "Create a schedule.",
            "syntax": "az devcenter admin schedule create --pool-name\n                                   --project\n                                   --resource-group\n                                   --time\n                                   --time-zone\n                                   [--location]\n                                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                   [--state {Disabled, Enabled}]\n                                   [--tags]",
            "required_parameters": [
                "--pool-name",
                "--project --project-name",
                "--resource-group -g",
                "--time",
                "--time-zone"
            ],
            "optional_parameters": [
                "--location",
                "--no-wait",
                "--state",
                "--tags"
            ],
            "examples": [
                "az devcenter admin schedule create --state \"Enabled\" --time \"17:30\" --time-zone \"America/Los_Angeles\" --pool-name \"DevPool\" --project-name \"DevProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin schedule delete",
            "summary": "Delete a schedule.",
            "syntax": "az devcenter admin schedule delete [--ids]\n                                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                   [--pool-name]\n                                   [--project]\n                                   [--resource-group]\n                                   [--subscription]\n                                   [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--no-wait",
                "--pool-name",
                "--project --project-name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az devcenter admin schedule delete --pool-name \"DevPool\" --project-name \"TestProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin schedule list",
            "summary": "List schedules for a pool.",
            "syntax": "az devcenter admin schedule list --pool-name\n                                 --project\n                                 --resource-group\n                                 [--max-items]\n                                 [--next-token]",
            "required_parameters": [
                "--pool-name",
                "--project --project-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin schedule list --pool-name \"DevPool\" --project-name \"TestProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin schedule show",
            "summary": "Get a schedule.",
            "syntax": "az devcenter admin schedule show [--ids]\n                                 [--pool-name]\n                                 [--project]\n                                 [--resource-group]\n                                 [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--pool-name",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin schedule show --pool-name \"DevPool\" --project-name \"TestProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin schedule update",
            "summary": "Update a schedule.",
            "syntax": "az devcenter admin schedule update [--add]\n                                   [--force-string {0, 1, f, false, n, no, t, true, y, yes}]\n                                   [--ids]\n                                   [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                                   [--pool-name]\n                                   [--project]\n                                   [--remove]\n                                   [--resource-group]\n                                   [--set]\n                                   [--state {Disabled, Enabled}]\n                                   [--subscription]\n                                   [--tags]\n                                   [--time]\n                                   [--time-zone]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--no-wait",
                "--pool-name",
                "--project --project-name",
                "--remove",
                "--resource-group -g",
                "--set",
                "--state",
                "--subscription",
                "--tags",
                "--time",
                "--time-zone"
            ],
            "examples": [
                "az devcenter admin schedule update --time \"18:00\" --pool-name \"DevPool\" --project-name \"TestProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin schedule wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az devcenter admin schedule wait [--created]\n                                 [--custom]\n                                 [--deleted]\n                                 [--exists]\n                                 [--ids]\n                                 [--interval]\n                                 [--pool-name]\n                                 [--project]\n                                 [--resource-group]\n                                 [--subscription]\n                                 [--timeout]\n                                 [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--pool-name",
                "--project --project-name",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}