{
    "command_group": "az devcenter admin project-image",
    "description": "Manage project images.",
    "status": "GA",
    "commands": [
        {
            "name": "az devcenter admin project-image list",
            "summary": "List images for a project.",
            "syntax": "az devcenter admin project-image list --project\n                                      --resource-group\n                                      [--max-items]\n                                      [--next-token]",
            "required_parameters": [
                "--project --project-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--max-items",
                "--next-token"
            ],
            "examples": [
                "az devcenter admin project-image list --project-name \"DevProject\" --resource-group \"rg1\""
            ]
        },
        {
            "name": "az devcenter admin project-image show",
            "summary": "Get a project image.",
            "syntax": "az devcenter admin project-image show [--ids]\n                                      [--image-name]\n                                      [--project]\n                                      [--resource-group]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--image-name --name -n",
                "--project --project-name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az devcenter admin project-image show --project-name \"DevProject\" --name \"ContosoBaseImage\" --resource-group \"rg1\""
            ]
        }
    ]
}