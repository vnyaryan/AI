{
    "command_group": "az connectedvmware host",
    "description": "Host resource.",
    "status": "GA",
    "commands": [
        {
            "name": "az connectedvmware host create",
            "summary": "Create a host resource.",
            "syntax": "az connectedvmware host create --custom-location\n                               --inventory-item\n                               --location\n                               --name\n                               --resource-group\n                               --vcenter\n                               [--no-wait]\n                               [--tags]",
            "required_parameters": [
                "--custom-location -c",
                "--inventory-item -i",
                "--location -l",
                "--name -n",
                "--resource-group -g",
                "--vcenter -v"
            ],
            "optional_parameters": [
                "--no-wait",
                "--tags"
            ],
            "examples": [
                "az connectedvmware host create --custom-location \"custom location name\"                --location \"location name\" --inventory-item \"name or id the inventory item\"                --name \"host name\" --resource-group \"resource group name\"                --vcenter \"name or id of the vcenter\""
            ]
        },
        {
            "name": "az connectedvmware host delete",
            "summary": "Delete host resource.",
            "syntax": "az connectedvmware host delete [--force]\n                               [--ids]\n                               [--name]\n                               [--no-wait]\n                               [--resource-group]\n                               [--subscription]\n                               [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--force",
                "--ids",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az connectedvmware host delete --ids \"resource id\" --name \"host name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        },
        {
            "name": "az connectedvmware host list",
            "summary": "Retrieve a list of host of given resource group.",
            "syntax": "az connectedvmware host list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az connectedvmware host list --resource-group \"resource group name\""
            ]
        },
        {
            "name": "az connectedvmware host show",
            "summary": "Get details of a host by id, resource-group, host name, or subscription.",
            "syntax": "az connectedvmware host show [--ids]\n                             [--name]\n                             [--resource-group]\n                             [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az connectedvmware host show --ids \"resource id\" --name \"host name\"                --resource-group \"resource group name\" --subscription \"Name or ID of subscription\""
            ]
        }
    ]
}