{
    "command_group": "az bot slack",
    "description": "Manage the Slack Channel on a bot.",
    "status": "Preview",
    "commands": [
        {
            "name": "az bot slack create",
            "summary": "Create the Slack Channel on a bot.",
            "syntax": "az bot slack create --client-id\n                    --client-secret\n                    --name\n                    --resource-group\n                    --verification-token\n                    [--add-disabled {false, true}]\n                    [--landing-page-url]\n                    [--location]",
            "required_parameters": [
                "--client-id",
                "--client-secret",
                "--name -n",
                "--resource-group -g",
                "--verification-token"
            ],
            "optional_parameters": [
                "--add-disabled",
                "--landing-page-url",
                "--location -l"
            ],
            "examples": [
                "az bot slack create -n botName -g MyResourceGroup --client-id clientid \\\n--client-secret secret --verification-token token"
            ]
        },
        {
            "name": "az bot slack delete",
            "summary": "Delete the Slack Channel on a bot.",
            "syntax": "az bot slack delete --name\n                    --resource-group",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az bot slack show",
            "summary": "Get details of the Slack Channel on a bot.",
            "syntax": "az bot slack show --name\n                  --resource-group\n                  [--with-secrets {false, true}]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--with-secrets"
            ],
            "examples": []
        }
    ]
}