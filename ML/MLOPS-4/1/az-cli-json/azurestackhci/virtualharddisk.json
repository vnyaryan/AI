{
    "command_group": "az azurestackhci virtualharddisk",
    "description": "Manage virtualharddisk with azurestackhci.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az azurestackhci virtualharddisk create",
            "summary": "Create a virtual hard disk.",
            "syntax": "az azurestackhci virtualharddisk create --name\n                                        --resource-group\n                                        [--block-size-bytes]\n                                        [--disk-file-format]\n                                        [--disk-size-gb]\n                                        [--dynamic {false, true}]\n                                        [--extended-location]\n                                        [--location]\n                                        [--logical-sector-bytes]\n                                        [--physical-sector-bytes]\n                                        [--storagepath-id]\n                                        [--tags]",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--block-size-bytes",
                "--disk-file-format",
                "--disk-size-gb",
                "--dynamic",
                "--extended-location",
                "--location -l",
                "--logical-sector-bytes",
                "--physical-sector-bytes",
                "--storagepath-id",
                "--tags"
            ],
            "examples": [
                "az azurestackhci virtualharddisk create --resource-group \"test-rg\" --extended-location name=\"/subscriptions/a95612cb-f1fa-4daa-a4fd-272844fa512c/resourceGroups/dogfoodarc/providers/Microsoft.ExtendedLocation/customLocations/dogfood-location\" type=\"CustomLocation\" --location \"West US2\" --disk-size-bytes 32 --name \"test-vhd\" --storagepath-id \"/subscriptions/a95612cb-f1fa-4daa-a4fd-272844fa512c/resourceGroups/dogfoodarc/providers/Microsoft.AzureStackHCI/storagecontainers/test-storagepath\""
            ]
        },
        {
            "name": "az azurestackhci virtualharddisk delete",
            "summary": "Delete a virtual hard disk.",
            "syntax": "az azurestackhci virtualharddisk delete [--ids]\n                                        [--name]\n                                        [--resource-group]\n                                        [--subscription]\n                                        [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": [
                "az azurestackhci virtualharddisk delete --resource-group \"test-rg\" --name \"test-vhd\""
            ]
        },
        {
            "name": "az azurestackhci virtualharddisk list",
            "summary": "List all virtual hard disks.",
            "syntax": "az azurestackhci virtualharddisk list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az azurestackhci virtualharddisk list --resource-group \"test-rg\"",
                "az azurestackhci virtualharddisk list"
            ]
        },
        {
            "name": "az azurestackhci virtualharddisk show",
            "summary": "Get the details of a virtual hard disk.",
            "syntax": "az azurestackhci virtualharddisk show [--ids]\n                                      [--name]\n                                      [--resource-group]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az azurestackhci virtualharddisk show --resource-group \"test-rg\" --name \"test-vhd\""
            ]
        },
        {
            "name": "az azurestackhci virtualharddisk update",
            "summary": "Update a virtual hard disk.",
            "syntax": "az azurestackhci virtualharddisk update [--ids]\n                                        [--name]\n                                        [--resource-group]\n                                        [--subscription]\n                                        [--tags]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name",
                "--resource-group -g",
                "--subscription",
                "--tags"
            ],
            "examples": [
                "az azurestackhci virtualharddisk update --resource-group \"test-rg\" --tags additionalProperties=\"sample\" --name \"test-vhd\""
            ]
        }
    ]
}