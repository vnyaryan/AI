{
    "command_group": "az ams asset-filter",
    "description": "Manage asset filters for an Azure Media Services account.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams asset-filter create",
            "summary": "Create an asset filter.",
            "syntax": "az ams asset-filter create --account-name\n                           --asset-name\n                           --name\n                           --resource-group\n                           [--end-timestamp]\n                           [--first-quality]\n                           [--force-end-timestamp {false, true}]\n                           [--live-backoff-duration]\n                           [--presentation-window-duration]\n                           [--start-timestamp]\n                           [--timescale]\n                           [--tracks]",
            "required_parameters": [
                "--account-name -a",
                "--asset-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--end-timestamp",
                "--first-quality",
                "--force-end-timestamp",
                "--live-backoff-duration",
                "--presentation-window-duration",
                "--start-timestamp",
                "--timescale",
                "--tracks"
            ],
            "examples": [
                "az ams asset-filter create -a amsAccount -g resourceGroup -n filterName --force-end-timestamp=False --end-timestamp 200000 --start-timestamp 100000 --live-backoff-duration 60 --presentation-window-duration 600000 --timescale 1000 --first-quality 720 --asset-name assetName --tracks @C:\\tracks.json"
            ]
        },
        {
            "name": "az ams asset-filter delete",
            "summary": "Delete an asset filter.",
            "syntax": "az ams asset-filter delete [--account-name]\n                           [--asset-name]\n                           [--ids]\n                           [--name]\n                           [--resource-group]\n                           [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--asset-name",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams asset-filter list",
            "summary": "List all the asset filters of an Azure Media Services account.",
            "syntax": "az ams asset-filter list --account-name\n                         --asset-name\n                         --resource-group",
            "required_parameters": [
                "--account-name -a",
                "--asset-name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az ams asset-filter show",
            "summary": "Show the details of an asset filter.",
            "syntax": "az ams asset-filter show [--account-name]\n                         [--asset-name]\n                         [--ids]\n                         [--name]\n                         [--resource-group]\n                         [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--asset-name",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams asset-filter update",
            "summary": "Update the details of an asset filter.",
            "syntax": "az ams asset-filter update [--account-name]\n                           [--add]\n                           [--asset-name]\n                           [--end-timestamp]\n                           [--first-quality]\n                           [--force-end-timestamp {false, true}]\n                           [--force-string]\n                           [--ids]\n                           [--live-backoff-duration]\n                           [--name]\n                           [--presentation-window-duration]\n                           [--remove]\n                           [--resource-group]\n                           [--set]\n                           [--start-timestamp]\n                           [--subscription]\n                           [--timescale]\n                           [--tracks]",
            "required_parameters": [],
