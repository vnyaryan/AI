{
    "command_group": "az ams streaming-locator",
    "description": "Manage streaming locators for an Azure Media Services account.",
    "status": "GA",
    "commands": [
        {
            "name": "az ams streaming-locator create",
            "summary": "Create a streaming locator.",
            "syntax": "az ams streaming-locator create --account-name\n                                --asset-name\n                                --name\n                                --resource-group\n                                --streaming-policy-name\n                                [--alternative-media-id]\n                                [--content-key-policy-name]\n                                [--content-keys]\n                                [--end-time]\n                                [--filters]\n                                [--start-time]\n                                [--streaming-locator-id]",
            "required_parameters": [
                "--account-name -a",
                "--asset-name",
                "--name -n",
                "--resource-group -g",
                "--streaming-policy-name"
            ],
            "optional_parameters": [
                "--alternative-media-id",
                "--content-key-policy-name",
                "--content-keys",
                "--end-time",
                "--filters",
                "--start-time",
                "--streaming-locator-id"
            ],
            "examples": []
        },
        {
            "name": "az ams streaming-locator delete",
            "summary": "Delete a Streaming Locator.",
            "syntax": "az ams streaming-locator delete [--account-name]\n                                [--ids]\n                                [--name]\n                                [--resource-group]\n                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name -a",
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        },
        {
            "name": "az ams streaming-locator get-paths",
