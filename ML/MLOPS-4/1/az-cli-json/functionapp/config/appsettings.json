{
    "command_group": "az functionapp config appsettings",
    "description": "Configure function app settings.",
    "status": "GA",
    "commands": [
        {
            "name": "az functionapp config appsettings delete",
            "summary": "Delete a function app's settings.",
            "syntax": "az functionapp config appsettings delete --setting-names\n                                         [--ids]\n                                         [--name]\n                                         [--resource-group]\n                                         [--slot]\n                                         [--subscription]",
            "required_parameters": [
                "--setting-names"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--slot -s",
                "--subscription"
            ],
            "examples": [
                "az functionapp config appsettings delete --name MyFunctionApp --resource-group MyResourceGroup --setting-names {setting-names}"
            ]
        },
        {
            "name": "az functionapp config appsettings list",
            "summary": "Show settings for a function app.",
            "syntax": "az functionapp config appsettings list --name\n                                       --resource-group\n                                       [--slot]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--slot -s"
            ],
            "examples": [
                "az functionapp config appsettings list --name MyWebapp --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az functionapp config appsettings set",
            "summary": "Update a function app's settings.",
            "syntax": "az functionapp config appsettings set [--ids]\n                                      [--name]\n                                      [--resource-group]\n                                      [--settings]\n                                      [--slot]\n                                      [--slot-settings]\n                                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--settings",
                "--slot -s",
                "--slot-settings",
                "--subscription"
            ],
            "examples": [
                "az functionapp config appsettings set --name MyFunctionApp --resource-group MyResourceGroup --settings foo=bar AzureWebJobsStorage=$storageConnectionString",
                "az functionapp config appsettings set -g MyResourceGroup -n MyUniqueApp --settings mySetting=value @moreSettings.json"
            ]
        }
    ]
}