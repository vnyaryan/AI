{
    "command_group": "az fleet member",
    "description": "Commands to manage members.",
    "status": "GA",
    "commands": [
        {
            "name": "az fleet member create",
            "summary": "Creates or updates a member.",
            "syntax": "az fleet member create --fleet-name\n                       --member-cluster-id\n                       --name\n                       --resource-group\n                       [--no-wait]\n                       [--update-group]",
            "required_parameters": [
                "--fleet-name -f",
                "--member-cluster-id",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--update-group"
            ],
            "examples": [
                "az fleet member create -g MyFleetResourceGroup -f MyFleetName -n NameOfMember --update-group UpdateGroup1 --member-cluster-id \"/subscriptions/00000000-0000-0000-0000-000000000000/resourceGroups/MyFleetResourceGroup/providers/Microsoft.ContainerService/managedClusters/MyManagedCluster\""
            ]
        },
        {
            "name": "az fleet member delete",
            "summary": "Deletes a fleet member.",
            "syntax": "az fleet member delete --fleet-name\n                       --name\n                       --resource-group\n                       [--no-wait]\n                       [--yes]",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": [
                "az fleet member delete -g MyFleetResourceGroup -f MyFleetName -n NameOfMember"
            ]
        },
        {
            "name": "az fleet member list",
            "summary": "Lists a fleet's members.",
            "syntax": "az fleet member list --fleet-name\n                     --resource-group",
            "required_parameters": [
                "--fleet-name -f",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az fleet member list -g MyFleetResourceGroup -f MyFleetName"
            ]
        },
        {
            "name": "az fleet member reconcile",
            "summary": "Reconciles a member.",
            "syntax": "az fleet member reconcile --fleet-name\n                          --name\n                          --resource-group\n                          [--no-wait]",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": [
                "az fleet member reconcile -g MyFleetResourceGroup -f MyFleetName -n NameOfMember"
            ]
        },
        {
            "name": "az fleet member show",
            "summary": "Gets a fleet member.",
            "syntax": "az fleet member show --fleet-name\n                     --name\n                     --resource-group",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az fleet member show -g MyFleetResourceGroup -f MyFleetName -n NameOfMember"
            ]
        },
        {
            "name": "az fleet member update",
            "summary": "Update a member.",
            "syntax": "az fleet member update --fleet-name\n                       --name\n                       --resource-group\n                       [--update-group]",
            "required_parameters": [
                "--fleet-name -f",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--update-group"
            ],
            "examples": [
                "az fleet member update -g MyFleetResourceGroup -f MyFleetName -n NameOfMember --update-group UpdateGroup2"
            ]
        },
        {
            "name": "az fleet member wait",
            "summary": "Wait for a member resource to reach a desired state.",
            "syntax": "az fleet member wait --fleet-member-name\n                     --fleet-name\n                     --resource-group\n                     [--created]\n                     [--custom]\n                     [--deleted]\n                     [--exists]\n                     [--interval]\n                     [--timeout]\n                     [--updated]",
            "required_parameters": [
                "--fleet-member-name",
                "--fleet-name -f",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": []
        }
    ]
}