{
    "command_group": "az datashare provider-share-subscription",
    "description": "Manage provider share subscription with datashare.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az datashare provider-share-subscription adjust",
            "summary": "Adjust a share subscription's expiration date in a provider share.",
            "syntax": "az datashare provider-share-subscription adjust [--account-name]\n                                                [--expiration-date]\n                                                [--ids]\n                                                [--provider-share-subscription-id]\n                                                [--resource-group]\n                                                [--share-name]\n                                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--expiration-date",
                "--ids",
                "--provider-share-subscription-id",
                "--resource-group -g",
                "--share-name",
                "--subscription"
            ],
            "examples": [
                "az datashare provider-share-subscription adjust --account-name \"Account1\" --expiration-date \"2020-12-26T22:33:24.5785265Z\" --provider-share-subscription-id \"4256e2cf-0f82-4865-961b-12f83333f487\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\""
            ]
        },
        {
            "name": "az datashare provider-share-subscription list",
            "summary": "List share subscriptions in a provider share.",
            "syntax": "az datashare provider-share-subscription list --account-name\n                                              --resource-group\n                                              --share-name\n                                              [--skip-token]",
            "required_parameters": [
                "--account-name",
                "--resource-group -g",
                "--share-name"
            ],
            "optional_parameters": [
                "--skip-token"
            ],
            "examples": [
                "az datashare provider-share-subscription list --account-name \"Account1\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\""
            ]
        },
        {
            "name": "az datashare provider-share-subscription reinstate",
            "summary": "Reinstate share subscription in a provider share.",
            "syntax": "az datashare provider-share-subscription reinstate [--account-name]\n                                                   [--expiration-date]\n                                                   [--ids]\n                                                   [--provider-share-subscription-id]\n                                                   [--resource-group]\n                                                   [--share-name]\n                                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--expiration-date",
                "--ids",
                "--provider-share-subscription-id",
                "--resource-group -g",
                "--share-name",
                "--subscription"
            ],
            "examples": [
                "az datashare provider-share-subscription reinstate --account-name \"Account1\" --expiration-date \"2020-12-26T22:33:24.5785265Z\" --provider-share-subscription-id \"4256e2cf-0f82-4865-961b-12f83333f487\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\""
            ]
        },
        {
            "name": "az datashare provider-share-subscription revoke",
            "summary": "Revoke share subscription in a provider share.",
            "syntax": "az datashare provider-share-subscription revoke [--account-name]\n                                                [--ids]\n                                                [--no-wait]\n                                                [--provider-share-subscription-id]\n                                                [--resource-group]\n                                                [--share-name]\n                                                [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--ids",
                "--no-wait",
                "--provider-share-subscription-id",
                "--resource-group -g",
                "--share-name",
                "--subscription"
            ],
            "examples": [
                "az datashare provider-share-subscription revoke --account-name \"Account1\" --provider-share-subscription-id \"4256e2cf-0f82-4865-961b-12f83333f487\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\""
            ]
        },
        {
            "name": "az datashare provider-share-subscription show",
            "summary": "Get share subscription in a provider share.",
            "syntax": "az datashare provider-share-subscription show [--account-name]\n                                              [--ids]\n                                              [--provider-share-subscription-id]\n                                              [--resource-group]\n                                              [--share-name]\n                                              [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--ids",
                "--provider-share-subscription-id",
                "--resource-group -g",
                "--share-name",
                "--subscription"
            ],
            "examples": [
                "az datashare provider-share-subscription show --account-name \"Account1\" --provider-share-subscription-id \"4256e2cf-0f82-4865-961b-12f83333f487\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\""
            ]
        },
        {
            "name": "az datashare provider-share-subscription wait",
            "summary": "Place the CLI in a waiting state until a condition of the datashare provider-share-subscription is met.",
            "syntax": "az datashare provider-share-subscription wait [--account-name]\n                                              [--created]\n                                              [--custom]\n                                              [--deleted]\n                                              [--exists]\n                                              [--ids]\n                                              [--interval]\n                                              [--provider-share-subscription-id]\n                                              [--resource-group]\n                                              [--share-name]\n                                              [--subscription]\n                                              [--timeout]\n                                              [--updated]",
            "required_parameters": [],
            "optional_parameters": [
                "--account-name",
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--ids",
                "--interval",
                "--provider-share-subscription-id",
                "--resource-group -g",
                "--share-name",
                "--subscription",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az datashare provider-share-subscription wait --account-name \"Account1\" --provider-share-subscription-id \"4256e2cf-0f82-4865-961b-12f83333f487\" --resource-group \"SampleResourceGroup\" --share-name \"Share1\" --created"
            ]
        }
    ]
}