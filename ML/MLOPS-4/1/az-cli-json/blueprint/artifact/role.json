{
    "command_group": "az blueprint artifact role",
    "description": "Commands to manage blueprint role assignment artifact.",
    "status": "GA",
    "commands": [
        {
            "name": "az blueprint artifact role create",
            "summary": "Create blueprint role artifact.",
            "syntax": "az blueprint artifact role create --artifact-name\n                                  --blueprint-name\n                                  --principal-ids\n                                  --role-definition-id\n                                  [--depends-on]\n                                  [--description]\n                                  [--display-name]\n                                  [--management-group]\n                                  [--resource-group-art]\n                                  [--subscription]",
            "required_parameters": [
                "--artifact-name",
                "--blueprint-name",
                "--principal-ids",
                "--role-definition-id"
            ],
            "optional_parameters": [
                "--depends-on",
                "--description",
                "--display-name",
                "--management-group -m",
                "--resource-group-art",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint artifact role create \\\n--blueprint-name MyBlueprint --artifact-name MyRole --role-definition-id \\\n\"/providers/Microsoft.Authorization/roleDefinitions/00000000-0000-0000-0000-000000000000\" \\\n--principal-ids \"[parameters('[Usergrouporapplicationname]:MyRoleAssignmentName')]\""
            ]
        },
        {
            "name": "az blueprint artifact role update",
            "summary": "Update blueprint role artifact.",
            "syntax": "az blueprint artifact role update --artifact-name\n                                  --blueprint-name\n                                  [--depends-on]\n                                  [--description]\n                                  [--display-name]\n                                  [--management-group]\n                                  [--resource-group-art]\n                                  [--subscription]",
            "required_parameters": [
                "--artifact-name",
                "--blueprint-name"
            ],
            "optional_parameters": [
                "--depends-on",
                "--description",
                "--display-name",
                "--management-group -m",
                "--resource-group-art",
                "--subscription -s"
            ],
            "examples": [
                "az blueprint artifact role update \\\n--blueprint-name MyBlueprint --artifact-name MyRole --display-name \"My Big Role\""
            ]
        }
    ]
}