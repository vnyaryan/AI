{
    "command_group": "az vm host group",
    "description": "Manage Dedicated Host Groups.",
    "status": "GA",
    "commands": [
        {
            "name": "az vm host group create",
            "summary": "Create a dedicated host group.",
            "syntax": "az vm host group create --name\n                        --platform-fault-domain-count\n                        --resource-group\n                        [--automatic-placement {false, true}]\n                        [--location]\n                        [--tags]\n                        [--ultra-ssd-enabled {false, true}]\n                        [--zone]",
            "required_parameters": [
                "--name -n",
                "--platform-fault-domain-count -c",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--automatic-placement",
                "--location -l",
                "--tags",
                "--ultra-ssd-enabled",
                "--zone -z"
            ],
            "examples": [
                "az vm host group create --name MyDedicatedHostGroup --platform-fault-domain-count 2 --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az vm host group delete",
            "summary": "Delete a dedicated host group.",
            "syntax": "az vm host group delete [--host-group-name]\n                        [--ids]\n                        [--resource-group]\n                        [--subscription]\n                        [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--host-group-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az vm host group get-instance-view",
            "summary": "Get instance view of a dedicated host group.",
            "syntax": "az vm host group get-instance-view [--ids]\n                                   [--name]\n                                   [--resource-group]\n                                   [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm host group get-instance-view --name MyDedicatedHostGroup --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az vm host group list",
            "summary": "List dedicated host groups.",
            "syntax": "az vm host group list [--max-items]\n                      [--next-token]\n                      [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az vm host group show",
            "summary": "Get the details of a dedicated host group.",
            "syntax": "az vm host group show [--host-group-name]\n                      [--ids]\n                      [--resource-group]\n                      [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--host-group-name --name -n",
                "--ids",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az vm host group show --name MyDedicatedHostGroup --resource-group MyResourceGroup"
            ]
        },
        {
            "name": "az vm host group update",
            "summary": "Update a dedicated host group.",
            "syntax": "az vm host group update [--add]\n                        [--force-string]\n                        [--ids]\n                        [--name]\n                        [--remove]\n                        [--resource-group]\n                        [--set]\n                        [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--ids",
                "--name -n",
                "--remove",
                "--resource-group -g",
                "--set",
                "--subscription"
            ],
            "examples": []
        }
    ]
}