{
    "command_group": "az maintenance update",
    "description": "Manage update with maintenance.",
    "status": "GA",
    "commands": [
        {
            "name": "az maintenance update list",
            "summary": "List updates to resources.",
            "syntax": "az maintenance update list --provider-name\n                           --resource-group\n                           --resource-name\n                           --resource-type",
            "required_parameters": [
                "--provider-name",
                "--resource-group -g",
                "--resource-name",
                "--resource-type"
            ],
            "optional_parameters": [],
            "examples": [
                "az maintenance update list --provider-name \"Microsoft.Compute\" --resource-group \"examplerg\"  --resource-name \"smdtest1\" --resource-type \"virtualMachineScaleSets\""
            ]
        },
        {
            "name": "az maintenance update list-parent",
            "summary": "List updates to resources.",
            "syntax": "az maintenance update list-parent [--ids]\n                                  [--provider-name]\n                                  [--resource-group]\n                                  [--resource-name]\n                                  [--resource-parent-name]\n                                  [--resource-parent-type]\n                                  [--resource-type]\n                                  [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--provider-name",
                "--resource-group -g",
                "--resource-name",
                "--resource-parent-name",
                "--resource-parent-type",
                "--resource-type",
                "--subscription"
            ],
            "examples": [
                "az maintenance update list-parent --provider-name \"Microsoft.Compute\" --resource-group \"examplerg\" --resource-name \"1\" --resource-parent-name \"smdtest1\" --resource-parent-type \"virtualMachineScaleSets\" --resource-type \"virtualMachines\""
            ]
        }
    ]
}