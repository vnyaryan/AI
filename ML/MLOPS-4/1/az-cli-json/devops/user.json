{
    "command_group": "az devops user",
    "description": "Manage users.",
    "status": "GA",
    "commands": [
        {
            "name": "az devops user add",
            "summary": "Add user.",
            "syntax": "az devops user add --email-id\n                   --license-type {advanced, earlyAdopter, express, professional, stakeholder}\n                   [--detect {false, true}]\n                   [--org]\n                   [--send-email-invite {false, true}]",
            "required_parameters": [
                "--email-id",
                "--license-type"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--send-email-invite"
            ],
            "examples": []
        },
        {
            "name": "az devops user list",
            "summary": "List users in an organization [except for users which are added via AAD groups].",
            "syntax": "az devops user list [--detect {false, true}]\n                    [--org]\n                    [--skip]\n                    [--top]",
            "required_parameters": [],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--skip",
                "--top"
            ],
            "examples": []
        },
        {
            "name": "az devops user remove",
            "summary": "Remove user from an organization.",
            "syntax": "az devops user remove --user\n                      [--detect {false, true}]\n                      [--org]\n                      [--yes]",
            "required_parameters": [
                "--user"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az devops user show",
            "summary": "Show user details.",
            "syntax": "az devops user show --user\n                    [--detect {false, true}]\n                    [--org]",
            "required_parameters": [
                "--user"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization"
            ],
            "examples": []
        },
        {
            "name": "az devops user update",
            "summary": "Update license type for a user.",
            "syntax": "az devops user update --license-type {advanced, earlyAdopter, express, professional, stakeholder}\n                      --user\n                      [--detect {false, true}]\n                      [--org]",
            "required_parameters": [
                "--license-type",
                "--user"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization"
            ],
            "examples": []
        }
    ]
}