{
    "command_group": "az dt network private-link",
    "description": "Manage Digital Twins instance private-link operations.",
    "status": "GA",
    "commands": [
        {
            "name": "az dt network private-link list",
            "summary": "List private-links associated with the Digital Twins instance.",
            "syntax": "az dt network private-link list --dt-name\n                                [--resource-group]",
            "required_parameters": [
                "--dt-name --dtn -n"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az dt network private-link list -n {instance_name}"
            ]
        },
        {
            "name": "az dt network private-link show",
            "summary": "Show a private-link associated with the instance.",
            "syntax": "az dt network private-link show --dt-name\n                                --link-name\n                                [--resource-group]",
            "required_parameters": [
                "--dt-name --dtn -n",
                "--link-name --ln"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az dt network private-link show -n {instance_name} --link-name API"
            ]
        }
    ]
}