{
    "command_group": "az sql mi-arc",
    "description": "Manage Azure Arc-enabled SQL managed instances.",
    "status": "GA",
    "commands": [
        {
            "name": "az sql mi-arc create",
            "summary": "Create a SQL managed instance.",
            "syntax": "az sql mi-arc create --name\n                     [--ad-account-name]\n                     [--ad-connector-name]\n                     [--ad-encryption-types]\n                     [--admin-login-secret]\n                     [--agent-enabled]\n                     [--annotations]\n                     [--cert-private-key-file]\n                     [--cert-public-key-file]\n                     [--collation]\n                     [--cores-limit]\n                     [--cores-request]\n                     [--custom-location]\n                     [--dev]\n                     [--k8s-namespace]\n                     [--keytab-secret]\n                     [--labels]\n                     [--language]\n                     [--license-type]\n                     [--memory-limit]\n                     [--memory-request]\n                     [--no-external-endpoint]\n                     [--no-wait]\n                     [--orchestrator-replicas]\n                     [--path]\n                     [--primary-dns-name]\n                     [--primary-port-number]\n                     [--private-key-file]\n                     [--readable-secondaries]\n                     [--replicas {1, 2, 3}]\n                     [--resource-group]\n                     [--retention-days]\n                     [--secondary-dns-name]\n                     [--secondary-port-number]\n                     [--service-annotations]\n                     [--service-cert-secret]\n                     [--service-labels]\n                     [--service-type {LoadBalancer, NodePort}]\n                     [--storage-annotations]\n                     [--storage-class-backups]\n                     [--storage-class-data]\n                     [--storage-class-datalogs]\n                     [--storage-class-logs]\n                     [--storage-class-orchestrator-logs]\n                     [--storage-labels]\n                     [--sync-secondary-to-commit {-1, 0, 1, 2}]\n                     [--tde-mode]\n                     [--tde-protector-private-key-file]\n                     [--tde-protector-public-key-file]\n                     [--tde-protector-secret]\n                     [--tier]\n                     [--time-zone]\n                     [--trace-flags]\n                     [--use-k8s]\n                     [--volume-size-backups]\n                     [--volume-size-data]\n                     [--volume-size-datalogs]\n                     [--volume-size-logs]\n                     [--volume-size-orchestrator-logs]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ad-account-name",
                "--ad-connector-name",
                "--ad-encryption-types",
                "--admin-login-secret",
                "--agent-enabled",
                "--annotations",
                "--cert-private-key-file",
                "--cert-public-key-file",
                "--collation",
                "--cores-limit -c",
                "--cores-request",
                "--custom-location",
                "--dev",
                "--k8s-namespace -k",
                "--keytab-secret",
                "--labels",
                "--language",
                "--license-type -l",
                "--memory-limit -m",
                "--memory-request",
                "--no-external-endpoint",
                "--no-wait",
                "--orchestrator-replicas",
                "--path",
                "--primary-dns-name",
                "--primary-port-number",
                "--private-key-file",
                "--readable-secondaries",
                "--replicas",
                "--resource-group -g",
                "--retention-days",
                "--secondary-dns-name",
                "--secondary-port-number",
                "--service-annotations",
                "--service-cert-secret",
                "--service-labels",
                "--service-type",
                "--storage-annotations",
                "--storage-class-backups",
                "--storage-class-data -d",
                "--storage-class-datalogs",
                "--storage-class-logs",
                "--storage-class-orchestrator-logs",
                "--storage-labels",
                "--sync-secondary-to-commit",
                "--tde-mode",
                "--tde-protector-private-key-file",
                "--tde-protector-public-key-file",
                "--tde-protector-secret",
                "--tier -t",
                "--time-zone",
                "--trace-flags",
                "--use-k8s",
                "--volume-size-backups",
                "--volume-size-data",
                "--volume-size-datalogs",
                "--volume-size-logs",
                "--volume-size-orchestrator-logs"
            ],
            "examples": [
                "az sql mi-arc create -n sqlmi1 --k8s-namespace arc --use-k8s",
                "az sql mi-arc create -n sqlmi2 --replicas 3 --k8s-namespace arc --use-k8s",
                "az sql mi-arc create --name sqlmi3 --resource-group rg-name --custom-location arc-cl",
                "az sql mi-arc create --name contososqlmi --k8s-namespace arc --ad-connector-name arcadc --ad-account-name arcuser --keytab-secret arcuser-keytab-secret --primary-dns-name contososqlmi-primary.contoso.local --primary-port-number 8143 --use-k8s"
            ]
        },
        {
            "name": "az sql mi-arc delete",
            "summary": "Delete a SQL managed instance.",
            "syntax": "az sql mi-arc delete --name\n                     [--k8s-namespace]\n                     [--no-wait]\n                     [--resource-group]\n                     [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--no-wait",
                "--resource-group -g",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc delete --name sqlmi1 --k8s-namespace arc --use-k8s",
                "az sql mi-arc delete --name sqlmi1 --resource-group rg-name"
            ]
        },
        {
            "name": "az sql mi-arc get-mirroring-cert",
            "summary": "Retrieve certificate of availability group mirroring endpoint from sql mi and store in a file.",
            "syntax": "az sql mi-arc get-mirroring-cert --cert-file\n                                 --name\n                                 [--k8s-namespace]\n                                 [--use-k8s]",
            "required_parameters": [
                "--cert-file",
                "--name -n"
            ],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc get-mirroring-cert -n sqlmi1 --cert-file fileName1"
            ]
        },
        {
            "name": "az sql mi-arc list",
            "summary": "List SQL managed instances.",
            "syntax": "az sql mi-arc list [--custom-location]\n                   [--k8s-namespace]\n                   [--resource-group]\n                   [--use-k8s]",
            "required_parameters": [],
            "optional_parameters": [
                "--custom-location",
                "--k8s-namespace -k",
                "--resource-group -g",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc list --use-k8s"
            ]
        },
        {
            "name": "az sql mi-arc reprovision-replica",
            "summary": "Reprovision a replica in a SQL managed instance.",
            "syntax": "az sql mi-arc reprovision-replica --name\n                                  [--k8s-namespace]\n                                  [--no-wait]\n                                  [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--no-wait",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc reprovision-replica -n sqlmi1-1 --use-k8s --k8s-namespace namespace"
            ]
        },
        {
            "name": "az sql mi-arc show",
            "summary": "Show the details of a SQL managed instance.",
            "syntax": "az sql mi-arc show --name\n                   [--k8s-namespace]\n                   [--path]\n                   [--resource-group]\n                   [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--k8s-namespace -k",
                "--path -p",
                "--resource-group -g",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc show --name sqlmi1 --k8s-namespace arc --use-k8s",
                "az sql mi-arc show --name sqlmi1 --resource-group rg-name"
            ]
        },
        {
            "name": "az sql mi-arc update",
            "summary": "Update the configuration of a SQL managed instance.",
            "syntax": "az sql mi-arc update --name\n                     [--ad-encryption-types]\n                     [--agent-enabled]\n                     [--annotations]\n                     [--cert-private-key-file]\n                     [--cert-public-key-file]\n                     [--cores-limit]\n                     [--cores-request]\n                     [--k8s-namespace]\n                     [--keytab-secret]\n                     [--labels]\n                     [--license-type]\n                     [--memory-limit]\n                     [--memory-request]\n                     [--no-wait]\n                     [--orchestrator-replicas]\n                     [--path]\n                     [--preferred-primary-replica]\n                     [--readable-secondaries]\n                     [--replicas {1, 2, 3}]\n                     [--resource-group]\n                     [--retention-days]\n                     [--service-annotations]\n                     [--service-cert-secret]\n                     [--service-labels]\n                     [--sync-secondary-to-commit {-1, 0, 1, 2}]\n                     [--tde-mode]\n                     [--tde-protector-private-key-file]\n                     [--tde-protector-public-key-file]\n                     [--tde-protector-secret]\n                     [--tier]\n                     [--time-zone]\n                     [--trace-flags]\n                     [--use-k8s]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--ad-encryption-types",
                "--agent-enabled",
                "--annotations",
                "--cert-private-key-file",
                "--cert-public-key-file",
                "--cores-limit -c",
                "--cores-request",
                "--k8s-namespace -k",
                "--keytab-secret",
                "--labels",
                "--license-type -l",
                "--memory-limit -m",
                "--memory-request",
                "--no-wait",
                "--orchestrator-replicas",
                "--path",
                "--preferred-primary-replica",
                "--readable-secondaries",
                "--replicas",
                "--resource-group -g",
                "--retention-days",
                "--service-annotations",
                "--service-cert-secret",
                "--service-labels",
                "--sync-secondary-to-commit",
                "--tde-mode",
                "--tde-protector-private-key-file",
                "--tde-protector-public-key-file",
                "--tde-protector-secret",
                "--tier -t",
                "--time-zone",
                "--trace-flags",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc update --path ./spec.json -n sqlmi1 --use-k8s"
            ]
        },
        {
            "name": "az sql mi-arc upgrade",
            "summary": "Upgrade SQL managed instance.",
            "syntax": "az sql mi-arc upgrade [--desired-version]\n                      [--dry-run]\n                      [--field-filter]\n                      [--force]\n                      [--k8s-namespace]\n                      [--label-filter]\n                      [--name]\n                      [--no-wait]\n                      [--resource-group]\n                      [--target]\n                      [--use-k8s]",
            "required_parameters": [],
            "optional_parameters": [
                "--desired-version -v",
                "--dry-run -d",
                "--field-filter -f",
                "--force",
                "--k8s-namespace -k",
                "--label-filter -l",
                "--name -n",
                "--no-wait",
                "--resource-group -g",
                "--target",
                "--use-k8s"
            ],
            "examples": [
                "az sql mi-arc upgrade -n sqlmi1 -k arc --desired-version v1.1.0 --use-k8s"
            ]
        }
    ]
}