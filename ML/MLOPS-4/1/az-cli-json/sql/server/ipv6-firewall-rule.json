    "commands": [
        {
            "name": "az sql server ipv6-firewall-rule create",
            "summary": "Create an ipv6 firewall rule.",
            "syntax": "az sql server ipv6-firewall-rule create --name\n                                        --resource-group\n                                        --server\n                                        [--end-ipv6-address]\n                                        [--start-ipv6-address]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--server -s"
            ],
            "optional_parameters": [
                "--end-ipv6-address",
                "--start-ipv6-address"
            ],
            "examples": [
                "az sql server ipv6-firewall-rule create -g mygroup -s myserver -n myrule --start-ipv6-address 9a41:a145:2a80:6c8d:4628:a1b3:5812:3283 --end-ipv6-address 9a41:a145:2a80:6c8d:4628:a1b3:5812:3283"
