{
    "command_group": "az bicep",
    "description": "Bicep CLI command group.",
    "status": "GA",
    "commands": [
        {
            "name": "az bicep build",
            "summary": "Build a Bicep file.",
            "syntax": "az bicep build --file\n               [--no-restore]\n               [--outdir]\n               [--outfile]\n               [--stdout]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--no-restore",
                "--outdir",
                "--outfile",
                "--stdout"
            ],
            "examples": [
                "az bicep build --file {bicep_file}",
                "az bicep build --file {bicep_file} --stdout",
                "az bicep build --file {bicep_file} --outdir {out_dir}",
                "az bicep build --file {bicep_file} --outfile {out_file}",
                "az bicep build --file {bicep_file} --no-restore"
            ]
        },
        {
            "name": "az bicep build-params",
            "summary": "Build .bicepparam file.",
            "syntax": "az bicep build-params --file\n                      [--no-restore]\n                      [--outdir]\n                      [--outfile]\n                      [--stdout]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--no-restore",
                "--outdir",
                "--outfile",
                "--stdout"
            ],
            "examples": [
                "az bicep build-params --file {bicepparam_file}",
                "az bicep build-params --file {bicepparam_file} --stdout",
                "az bicep build-params --file {bicepparam_file} --outfile {out_file}"
            ]
        },
        {
            "name": "az bicep decompile",
            "summary": "Attempt to decompile an ARM template file to a Bicep file.",
            "syntax": "az bicep decompile --file\n                   [--force]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--force"
            ],
            "examples": [
                "az bicep decompile --file {json_template_file}",
                "az bicep decompile --file {json_template_file} --force"
            ]
        },
        {
            "name": "az bicep decompile-params",
            "summary": "Decompile a parameters .json file to .bicepparam.",
            "syntax": "az bicep decompile-params --file\n                          [--bicep-file]\n                          [--outdir]\n                          [--outfile]\n                          [--stdout]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--bicep-file",
                "--outdir",
                "--outfile",
                "--stdout"
            ],
            "examples": [
                "az bicep decompile-params --file {json_template_file}",
                "az bicep decompile-params --file {json_template_file} --bicep-file {bicep_file}",
                "az bicep decompile-params --file {json_template_file} --stdout",
                "az bicep decompile-params --file {json_template_file} --outdir {out_dir}",
                "az bicep decompile-params --file {json_template_file} --outfile {out_file}"
            ]
        },
        {
            "name": "az bicep format",
            "summary": "Format a Bicep file.",
            "syntax": "az bicep format --file\n                [--indent-kind {Space, Tab}]\n                [--indent-size]\n                [--insert-final-newline]\n                [--newline]\n                [--newline-kind {CR, CRLF, LF}]\n                [--outdir]\n                [--outfile]\n                [--stdout]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--indent-kind",
                "--indent-size",
                "--insert-final-newline",
                "--newline",
                "--newline-kind",
                "--outdir",
                "--outfile",
                "--stdout"
            ],
            "examples": [
                "az bicep format --file {bicep_file}",
                "az bicep format --file {bicep_file} --stdout",
                "az bicep format --file {bicep_file} --outdir {out_dir}",
                "az bicep format --file {bicep_file} --outfile {out_file}",
                "az bicep format --file {bicep_file} --insert-final-newline",
                "az bicep format --file {bicep_file} --indent-kind {indent_kind}",
                "az bicep format --file {bicep_file} --indent-size {indent_size}"
            ]
        },
        {
            "name": "az bicep generate-params",
            "summary": "Generate parameters file for a Bicep file.",
            "syntax": "az bicep generate-params --file\n                         [--include-params]\n                         [--no-restore]\n                         [--outdir]\n                         [--outfile]\n                         [--output-format]\n                         [--stdout]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--include-params",
                "--no-restore",
                "--outdir",
                "--outfile",
                "--output-format",
                "--stdout"
            ],
            "examples": [
                "az bicep generate-params --file {bicep_file}",
                "az bicep generate-params --file {bicep_file} --stdout",
                "az bicep generate-params --file {bicep_file} --outdir {out_dir}",
                "az bicep generate-params --file {bicep_file} --outfile {out_file}",
                "az bicep generate-params --file {bicep_file} --no-restore",
                "az bicep generate-params --file {bicep_file} --output-format {output_format} --include-params {include_params}"
            ]
        },
        {
            "name": "az bicep install",
            "summary": "Install Bicep CLI.",
            "syntax": "az bicep install [--target-platform {linux-arm64, linux-musl-x64, linux-x64, osx-arm64, osx-x64, win-arm64, win-x64}]\n                 [--version]",
            "required_parameters": [],
            "optional_parameters": [
                "--target-platform -t",
                "--version -v"
            ],
            "examples": [
                "az bicep install",
                "az bicep install --version v0.2.212",
                "az bicep install --target-platform linux-x64"
            ]
        },
        {
            "name": "az bicep lint",
            "summary": "Lint a Bicep file.",
            "syntax": "az bicep lint --file\n              [--diagnostics-format {default, sarif}]\n              [--no-restore]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--diagnostics-format",
                "--no-restore"
            ],
            "examples": [
                "az bicep lint --file {bicep_file}",
                "az bicep lint --file {bicep_file} --no-restore",
                "az bicep lint --file {bicep_file} --diagnostics-format {diagnostics_format}"
            ]
        },
        {
            "name": "az bicep list-versions",
            "summary": "List out all available versions of Bicep CLI.",
            "syntax": "az bicep list-versions",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az bicep publish",
            "summary": "Publish a bicep file to a remote module registry.",
            "syntax": "az bicep publish --file\n                 --target\n                 [--documentation-uri]\n                 [--documentationUri]\n                 [--force]\n                 [--with-source]",
            "required_parameters": [
                "--file -f",
                "--target -t"
            ],
            "optional_parameters": [
                "--documentation-uri -d",
                "--documentationUri",
                "--force",
                "--with-source"
            ],
            "examples": [
                "az bicep publish --file {bicep_file} --target \"br:{registry}/{module_path}:{tag}\"",
                "az bicep publish --file {bicep_file} --target \"br:{registry}/{module_path}:{tag} --force\"",
                "az bicep publish --file {bicep_file} --target \"br:{registry}/{module_path}:{tag}\" --documentation-uri {documentation_uri}",
                "az bicep publish --file {bicep_file} --target \"br:{registry}/{module_path}:{tag}\" --documentation-uri {documentation_uri} --with-source"
            ]
        },
        {
            "name": "az bicep restore",
            "summary": "Restore external modules for a bicep file.",
            "syntax": "az bicep restore --file\n                 [--force]",
            "required_parameters": [
                "--file -f"
            ],
            "optional_parameters": [
                "--force"
            ],
            "examples": [
                "az bicep restore --file {bicep_file}",
                "az bicep restore --file {bicep_file} --force"
            ]
        },
        {
            "name": "az bicep uninstall",
            "summary": "Uninstall Bicep CLI.",
            "syntax": "az bicep uninstall",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        },
        {
            "name": "az bicep upgrade",
            "summary": "Upgrade Bicep CLI to the latest version.",
            "syntax": "az bicep upgrade [--target-platform {linux-arm64, linux-musl-x64, linux-x64, osx-arm64, osx-x64, win-arm64, win-x64}]",
            "required_parameters": [],
            "optional_parameters": [
                "--target-platform -t"
            ],
            "examples": [
                "az bicep upgrade",
                "az bicep upgrade --target-platform linux-x64"
            ]
        },
        {
            "name": "az bicep version",
            "summary": "Show the installed version of Bicep CLI.",
            "syntax": "az bicep version",
            "required_parameters": [],
            "optional_parameters": [],
            "examples": []
        }
    ]
}