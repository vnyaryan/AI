{
    "command_group": "az signalr network-rule ip-rule",
    "description": "Manage SignalR Service IP rules.",
    "status": "GA",
    "commands": [
        {
            "name": "az signalr network-rule ip-rule add",
            "summary": "Add IP rule to SignalR Service.",
            "syntax": "az signalr network-rule ip-rule add --ip-rule\n                                    [--ids]\n                                    [--name]\n                                    [--resource-group]\n                                    [--subscription]",
            "required_parameters": [
                "--ip-rule"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az signalr network-rule ip-rule add -n MySignalR -g MyResourceGroup --ip-rule value=\"10.0.0.24\" action=\"Allow\" --ip-rule value=\"192.168.0.0/24\" action=\"Deny\""
            ]
        },
        {
            "name": "az signalr network-rule ip-rule remove",
            "summary": "Remove IP rule from SignalR Service.",
            "syntax": "az signalr network-rule ip-rule remove --ip-rule\n                                       [--ids]\n                                       [--name]\n                                       [--resource-group]\n                                       [--subscription]",
            "required_parameters": [
                "--ip-rule"
            ],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": [
                "az signalr network-rule ip-rule remove -n MySignalR -g MyResourceGroup --ip-rule value=\"10.0.0.24\" action=\"Allow\" --ip-rule value=\"192.168.0.0/24\" action=\"Deny\""
            ]
        }
    ]
}