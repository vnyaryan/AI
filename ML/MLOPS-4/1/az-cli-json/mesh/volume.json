{
    "command_group": "az mesh volume",
    "description": "Manage volumes.",
    "status": "Preview",
    "commands": [
        {
            "name": "az mesh volume create",
            "summary": "Create a volume.",
            "syntax": "az mesh volume create --location\n                      --name\n                      --resource-group\n                      [--template-file]\n                      [--template-uri]",
            "required_parameters": [
                "--location -l",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--template-file",
                "--template-uri"
            ],
            "examples": [
                "az mesh volume create --location westus --name myvolume --resource-group mygroup --template-uri https://mystorage.blob.core.windows.net/templates/volumeDescription.json",
                "az mesh volume create --location westus --name myvolume --resource-group mygroup --template-file ./volumeDescription.json"
            ]
        },
        {
            "name": "az mesh volume delete",
            "summary": "Delete a volume.",
            "syntax": "az mesh volume delete [--ids]\n                      [--name]\n                      [--resource-group]\n                      [--subscription]\n                      [--yes]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az mesh volume list",
            "summary": "List volumes.",
            "syntax": "az mesh volume list [--max-items]\n                    [--next-token]\n                    [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--max-items",
                "--next-token",
                "--resource-group -g"
            ],
            "examples": []
        },
        {
            "name": "az mesh volume show",
            "summary": "Get the details of a volume.",
            "syntax": "az mesh volume show [--ids]\n                    [--name]\n                    [--resource-group]\n                    [--subscription]",
            "required_parameters": [],
            "optional_parameters": [
                "--ids",
                "--name -n",
                "--resource-group -g",
                "--subscription"
            ],
            "examples": []
        }
    ]
}