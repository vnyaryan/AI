            "optional_parameters": [
                "--continuation-token",
                "--filters",
                "--order-by"
            ],
            "examples": [
                "az synapse activity-run query-by-pipeline-run --workspace-name testsynapseworkspace \\\n  --last-updated-after \"2020-09-03T00:36:44.3345758Z\" --last-updated-before \"2020-09-03T00:49:48.3686473Z\" \\\n  --name testpipeline --run-id \"53eeed66-ec46-11ea-8bd5-448500a5b1ac\""
