{
    "command_group": "az pipelines build",
    "description": "Manage builds.",
    "status": "GA",
    "commands": [
        {
            "name": "az pipelines build cancel",
            "summary": "Cancels if build is running.",
            "syntax": "az pipelines build cancel --build-id\n                          [--detect {false, true}]\n                          [--open]\n                          [--org]\n                          [--project]",
            "required_parameters": [
                "--build-id"
            ],
            "optional_parameters": [
                "--detect",
                "--open",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        },
        {
            "name": "az pipelines build list",
            "summary": "List build results.",
            "syntax": "az pipelines build list [--branch]\n                        [--definition-ids]\n                        [--detect {false, true}]\n                        [--org]\n                        [--project]\n                        [--reason {all, batchedCI, buildCompletion, checkInShelveset, individualCI, manual, pullRequest, schedule, triggered, userCreated, validateShelveset}]\n                        [--requested-for]\n                        [--result {canceled, failed, none, partiallySucceeded, succeeded}]\n                        [--status {all, cancelling, completed, inProgress, none, notStarted, postponed}]\n                        [--tags]\n                        [--top]",
            "required_parameters": [],
            "optional_parameters": [
                "--branch",
                "--definition-ids",
                "--detect",
                "--org --organization",
                "--project -p",
                "--reason",
                "--requested-for",
                "--result",
                "--status",
                "--tags",
                "--top"
            ],
            "examples": []
        },
        {
            "name": "az pipelines build queue",
            "summary": "Request (queue) a build.",
            "syntax": "az pipelines build queue [--branch]\n                         [--commit-id]\n                         [--definition-id]\n                         [--definition-name]\n                         [--detect {false, true}]\n                         [--open]\n                         [--org]\n                         [--project]\n                         [--queue-id]\n                         [--variables]",
            "required_parameters": [],
            "optional_parameters": [
                "--branch",
                "--commit-id",
                "--definition-id",
                "--definition-name",
                "--detect",
                "--open",
                "--org --organization",
                "--project -p",
                "--queue-id",
                "--variables"
            ],
            "examples": []
        },
        {
            "name": "az pipelines build show",
            "summary": "Get the details of a build.",
            "syntax": "az pipelines build show --id\n                        [--detect {false, true}]\n                        [--open]\n                        [--org]\n                        [--project]",
            "required_parameters": [
                "--id"
            ],
            "optional_parameters": [
                "--detect",
                "--open",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        }
    ]
