{
    "command_group": "az attestation signer",
    "description": "Manage signers.",
    "status": "Experimental",
    "commands": [
        {
            "name": "az attestation signer add",
            "summary": "Adds a new attestation policy certificate to the set of policy     management certificates.",
            "syntax": "az attestation signer add [--id]\n                          [--name]\n                          [--resource-group]\n                          [--signer]\n                          [--signer-file]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--resource-group -g",
                "--signer",
                "--signer-file -f"
            ],
            "examples": [
                "az attestation signer add -n \"myattestationprovider\" -g \"MyResourceGroup\" --signer \"eyAiYWxnIjoiUlMyNTYiLCAie...\""
            ]
        },
        {
            "name": "az attestation signer list",
            "summary": "Retrieves the set of certificates used to express policy for the     current tenant.",
            "syntax": "az attestation signer list [--id]\n                           [--name]\n                           [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--resource-group -g"
            ],
            "examples": [
                "az attestation signer list -n \"myattestationprovider\" -g \"MyResourceGroup\""
            ]
        },
        {
            "name": "az attestation signer remove",
            "summary": "Removes the specified policy management certificate.",
            "syntax": "az attestation signer remove [--id]\n                             [--name]\n                             [--resource-group]\n                             [--signer]\n                             [--signer-file]",
            "required_parameters": [],
            "optional_parameters": [
                "--id",
                "--name -n",
                "--resource-group -g",
                "--signer",
                "--signer-file -f"
            ],
            "examples": [
                "az attestation signer remove -n \"myattestationprovider\" -g \"MyResourceGroup\" --signer \"eyAiYWxnIjoiUlMyNTYiLCAie...\""
            ]
        }
    ]
}