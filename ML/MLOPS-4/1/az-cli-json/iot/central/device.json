{
    "command_group": "az iot central device",
    "description": "Manage and configure IoT Central devices.",
    "status": "GA",
    "commands": [
        {
            "name": "az iot central device compute-device-key",
            "summary": "Generate a derived device SAS key.",
            "syntax": "az iot central device compute-device-key --device-id\n                                         --pk",
            "required_parameters": [
                "--device-id -d",
                "--pk --primary-key"
            ],
            "optional_parameters": [],
            "examples": [
                "az iot central device compute-device-key --pk {primaryKey} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device create",
            "summary": "Create a device in IoT Central.",
            "syntax": "az iot central device create --app-id\n                             --device-id\n                             [--api-version {2022-06-30-preview, 2022-07-31}]\n                             [--central-api-uri]\n                             [--device-name]\n                             [--organizations]\n                             [--simulated {false, true}]\n                             [--template]\n                             [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--device-name",
                "--organizations --orgs",
                "--simulated",
                "--template",
                "--token"
            ],
            "examples": [
                "az iot central device create --app-id {appid} --device-id {deviceid}",
                "az iot central device create --app-id {appid} --device-id {deviceid} --template {devicetemplateid} --simulated"
            ]
        },
        {
            "name": "az iot central device delete",
            "summary": "Delete a device from IoT Central.",
            "syntax": "az iot central device delete --app-id\n                             --device-id\n                             [--api-version {2022-06-30-preview, 2022-07-31}]\n                             [--central-api-uri]\n                             [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device delete --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device list",
            "summary": "Get the list of devices for an IoT Central application.",
            "syntax": "az iot central device list --app-id\n                           [--api-version {2022-06-30-preview, 2022-07-31}]\n                           [--central-api-uri]\n                           [--edge-only]\n                           [--token]",
            "required_parameters": [
                "--app-id -n"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--edge-only -e",
                "--token"
            ],
            "examples": [
                "az iot central device list --app-id {appid}"
            ]
        },
        {
            "name": "az iot central device list-components",
            "summary": "List the components present in a device.",
            "syntax": "az iot central device list-components --app-id\n                                      --device-id\n                                      [--api-version {2022-06-30-preview, 2022-07-31}]\n                                      [--central-api-uri]\n                                      [--mn]\n                                      [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--mn --module-name",
                "--token"
            ],
            "examples": [
                "az iot central device list-components --app-id {appid} --device-id {deviceid}",
                "az iot central device list-components --app-id {appid} --module-name {modulename} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device list-modules",
            "summary": "List the modules present in a device.",
            "syntax": "az iot central device list-modules --app-id\n                                   --device-id\n                                   [--api-version {2022-06-30-preview, 2022-07-31}]\n                                   [--central-api-uri]\n                                   [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device list-modules --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device manual-failback",
            "summary": "Reverts the previously executed failover command by moving the device back to it's original IoT Hub.",
            "syntax": "az iot central device manual-failback --app-id\n                                      --device-id\n                                      [--api-version {2022-06-30-preview, 2022-07-31}]\n                                      [--central-api-uri]\n                                      [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device manual-failback --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device manual-failover",
            "summary": "Execute a manual failover of device across multiple IoT Hubs to validate device firmware's ability to reconnect using DPS to a different IoT Hub.",
            "syntax": "az iot central device manual-failover --app-id\n                                      --device-id\n                                      [--api-version {2022-06-30-preview, 2022-07-31}]\n                                      [--central-api-uri]\n                                      [--token]\n                                      [--ttl]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token",
                "--ttl --ttl-minutes"
            ],
            "examples": [
                "az iot central device manual-failover --app-id {appid} --device-id {deviceid} --ttl-minutes {ttl_minutes}"
            ]
        },
        {
            "name": "az iot central device registration-info",
            "summary": "Get registration info on device(s) from IoT Central.",
            "syntax": "az iot central device registration-info --app-id\n                                        --device-id\n                                        [--api-version {2022-06-30-preview, 2022-07-31}]\n                                        [--central-api-uri]\n                                        [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device registration-info --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device show",
            "summary": "Get a device from IoT Central.",
            "syntax": "az iot central device show --app-id\n                           --device-id\n                           [--api-version {2022-06-30-preview, 2022-07-31}]\n                           [--central-api-uri]\n                           [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device show --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device show-credentials",
            "summary": "Get device credentials from IoT Central.",
            "syntax": "az iot central device show-credentials --app-id\n                                       --device-id\n                                       [--api-version {2022-06-30-preview, 2022-07-31}]\n                                       [--central-api-uri]\n                                       [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--token"
            ],
            "examples": [
                "az iot central device show-credentials --app-id {appid} --device-id {deviceid}"
            ]
        },
        {
            "name": "az iot central device update",
            "summary": "Update a device in IoT Central.",
            "syntax": "az iot central device update --app-id\n                             --device-id\n                             [--api-version {2022-06-30-preview, 2022-07-31}]\n                             [--central-api-uri]\n                             [--device-name]\n                             [--enable {false, true}]\n                             [--organizations]\n                             [--simulated {false, true}]\n                             [--template]\n                             [--token]",
            "required_parameters": [
                "--app-id -n",
                "--device-id -d"
            ],
            "optional_parameters": [
                "--api-version --av",
                "--central-api-uri --central-dns-suffix",
                "--device-name",
                "--enable",
                "--organizations --orgs",
                "--simulated",
                "--template",
                "--token"
            ],
            "examples": [
                "az iot central device update --app-id {appid} --device-id {deviceid} --device-name {deviceName}",
                "az iot central device update --app-id {appid} --device-id {deviceid} --simulated false",
                "az iot central device update --app-id {appid} --device-id {deviceid} --organizations {organizations}",
                "az iot central device update --app-id {appid} --device-id {deviceid} --enable false"
            ]
        }
    ]
}