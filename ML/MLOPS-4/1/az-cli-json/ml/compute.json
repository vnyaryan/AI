{
    "command_group": "az ml compute",
    "description": "Manage Azure ML compute resources.",
    "status": "GA",
    "commands": [
        {
            "name": "az ml compute attach",
            "summary": "Attach an existing compute resource to a workspace.",
            "syntax": "az ml compute attach --resource-group\n                     --workspace-name\n                     [--admin-password]\n                     [--admin-username]\n                     [--file]\n                     [--identity-type]\n                     [--name]\n                     [--namespace]\n                     [--no-wait]\n                     [--resource-id]\n                     [--ssh-port]\n                     [--ssh-private-key-file]\n                     [--type]\n                     [--user-assigned-identities]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--admin-password",
                "--admin-username",
                "--file -f",
                "--identity-type",
                "--name -n",
                "--namespace",
                "--no-wait",
                "--resource-id",
                "--ssh-port",
                "--ssh-private-key-file",
                "--type -t",
                "--user-assigned-identities -i"
            ],
            "examples": []
        },
        {
            "name": "az ml compute connect-ssh",
            "summary": "Set up SSH connection to Compute Instance.",
            "syntax": "az ml compute connect-ssh --name\n                          --resource-group\n                          --workspace-name\n                          [--private-key-file-path]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--private-key-file-path -f"
            ],
            "examples": [
                "az ml compute connect-ssh --name ci1 --resource-group my-resource-group --workspace-name my-workspace --private-key-file-path sshkey.pem"
            ]
        },
        {
            "name": "az ml compute create",
            "summary": "Create a compute target.",
            "syntax": "az ml compute create --resource-group\n                     --workspace-name\n                     [--admin-password]\n                     [--admin-username]\n                     [--description]\n                     [--enable-node-public-ip]\n                     [--file]\n                     [--identity-type]\n                     [--idle-time-before-scale-down]\n                     [--location]\n                     [--max-instances]\n                     [--min-instances]\n                     [--name]\n                     [--no-wait]\n                     [--set]\n                     [--size]\n                     [--ssh-key-value]\n                     [--ssh-public-access-enabled]\n                     [--subnet]\n                     [--tags]\n                     [--tier]\n                     [--type]\n                     [--user-assigned-identities]\n                     [--user-object-id]\n                     [--user-tenant-id]\n                     [--vnet-name]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--admin-password",
                "--admin-username",
                "--description",
                "--enable-node-public-ip",
                "--file -f",
                "--identity-type",
                "--idle-time-before-scale-down -d",
                "--location -l",
                "--max-instances",
                "--min-instances",
                "--name -n",
                "--no-wait",
                "--set",
                "--size",
                "--ssh-key-value",
                "--ssh-public-access-enabled -p",
                "--subnet",
                "--tags",
                "--tier",
                "--type -t",
                "--user-assigned-identities -i",
                "--user-object-id",
                "--user-tenant-id",
                "--vnet-name"
            ],
            "examples": [
                "az ml compute create --file compute.yml --resource-group my-resource-group --workspace-name my-workspace",
                "az ml compute create --name nc6-cluster --size Standard_NC6 --min-instances 0 --max-instances 5 --type AmlCompute --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml compute delete",
            "summary": "Delete a compute target.",
            "syntax": "az ml compute delete --name\n                     --resource-group\n                     --workspace-name\n                     [--no-wait]\n                     [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az ml compute detach",
            "summary": "Detach a previously attached compute resource from a workspace.",
            "syntax": "az ml compute detach --name\n                     --resource-group\n                     --workspace-name\n                     [--no-wait]\n                     [--yes]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az ml compute enable-sso",
            "summary": "Enable/Disable Single sign-on on Compute Instance.",
            "syntax": "az ml compute enable-sso --name\n                         --resource-group\n                         --workspace-name\n                         [--disable]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--disable"
            ],
            "examples": [
                "az ml compute enable-sso --name ci1 --resource-group my-resource-group --workspace-name my-workspace",
                "az ml compute enable-sso --name ci1 --resource-group my-resource-group --workspace-name my-workspace --disable"
            ]
        },
        {
            "name": "az ml compute list",
            "summary": "List the compute targets in a workspace.",
            "syntax": "az ml compute list --resource-group\n                   --workspace-name\n                   [--max-results]\n                   [--type]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--max-results -r",
                "--type"
            ],
            "examples": [
                "az ml compute list --query \"[].{Name:name}\"  --output table --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml compute list-nodes",
            "summary": "List node details for a compute target. The only supported compute type for this command is AML compute.",
            "syntax": "az ml compute list-nodes --name\n                         --resource-group\n                         --workspace-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml compute list-nodes --name nc6-cluster --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml compute list-sizes",
            "summary": "List the VM sizes available by location.",
            "syntax": "az ml compute list-sizes --resource-group\n                         --workspace-name\n                         [--location]\n                         [--type]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--location -l",
                "--type -t"
            ],
            "examples": []
        },
        {
            "name": "az ml compute list-usage",
            "summary": "List the available usage resources for VMs.",
            "syntax": "az ml compute list-usage --resource-group\n                         --workspace-name\n                         [--location]",
            "required_parameters": [
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--location -l"
            ],
            "examples": []
        },
        {
            "name": "az ml compute restart",
            "summary": "Restart a ComputeInstance target.",
            "syntax": "az ml compute restart --name\n                      --resource-group\n                      --workspace-name\n                      [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml compute show",
            "summary": "Show details for a compute target.",
            "syntax": "az ml compute show --name\n                   --resource-group\n                   --workspace-name",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [],
            "examples": [
                "az ml compute show --name nc6-cluster --resource-group my-resource-group --workspace-name my-workspace"
            ]
        },
        {
            "name": "az ml compute start",
            "summary": "Start a ComputeInstance target.",
            "syntax": "az ml compute start --name\n                    --resource-group\n                    --workspace-name\n                    [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml compute stop",
            "summary": "Stop a ComputeInstance target.",
            "syntax": "az ml compute stop --name\n                   --resource-group\n                   --workspace-name\n                   [--no-wait]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az ml compute update",
            "summary": "Update a compute target.",
            "syntax": "az ml compute update --name\n                     --resource-group\n                     --workspace-name\n                     [--add]\n                     [--force-string]\n                     [--identity-type]\n                     [--idle-time-before-scale-down]\n                     [--max-instances]\n                     [--min-instances]\n                     [--no-wait]\n                     [--remove]\n                     [--set]\n                     [--tags]\n                     [--user-assigned-identities]",
            "required_parameters": [
                "--name -n",
                "--resource-group -g",
                "--workspace-name -w"
            ],
            "optional_parameters": [
                "--add",
                "--force-string",
                "--identity-type",
                "--idle-time-before-scale-down -d",
                "--max-instances",
                "--min-instances",
                "--no-wait",
                "--remove",
                "--set",
                "--tags",
                "--user-assigned-identities -i"
            ],
            "examples": [
                "az ml compute update --name nc6-cluster --min-instances 1 --resource-group my-resource-group --workspace-name my-workspace",
                "az ml compute update --name nc6-cluster --tags key1=value1 key2=value2 --resource-group my-resource-group --workspace-name my-workspace"
            ]
        }
    ]
}