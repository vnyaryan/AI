{
    "command_group": "az deployment-scripts",
    "description": "Manage deployment scripts at subscription or resource group scope.",
    "status": "GA",
    "commands": [
        {
            "name": "az deployment-scripts delete",
            "summary": "Delete a deployment script.",
            "syntax": "az deployment-scripts delete --name\n                             --resource-group\n                             [--yes]",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": [
                "az deployment-scripts delete --resource-group contoso-rg --name contosoBashScript"
            ]
        },
        {
            "name": "az deployment-scripts list",
            "summary": "List all deployment scripts.",
            "syntax": "az deployment-scripts list [--resource-group]",
            "required_parameters": [],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az deployment-scripts list",
                "az deployment-scripts list --resource-group contoso-rg"
            ]
        },
        {
            "name": "az deployment-scripts show",
            "summary": "Retrieve a deployment script.",
            "syntax": "az deployment-scripts show --name\n                           --resource-group",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az deployment-scripts show --resource-group contoso-rg --name contosoBashScript"
            ]
        },
        {
            "name": "az deployment-scripts show-log",
            "summary": "Show deployment script logs.",
            "syntax": "az deployment-scripts show-log --name\n                               --resource-group",
            "required_parameters": [
                "--name",
                "--resource-group -g"
            ],
            "optional_parameters": [],
            "examples": [
                "az deployment-scripts show-log --resource-group contoso-rg --name contosoBashScript"
            ]
        }
    ]
}