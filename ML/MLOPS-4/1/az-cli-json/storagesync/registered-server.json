{
    "command_group": "az storagesync registered-server",
    "description": "Manage registered server.",
    "status": "GA",
    "commands": [
        {
            "name": "az storagesync registered-server delete",
            "summary": "Unregister an on-premises server from it's storage sync service.",
            "syntax": "az storagesync registered-server delete --server-id\n                                        --storage-sync-service\n                                        [--no-wait]\n                                        [--resource-group]\n                                        [--yes]",
            "required_parameters": [
                "--server-id",
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--no-wait",
                "--resource-group -g",
                "--yes -y"
            ],
            "examples": [
                "az storagesync registered-server delete --resource-group \"SampleResourceGroup\" --storage-sync-service \\\n\"SampleStorageSyncService\" --server-id \"41166691-ab03-43e9-ab3e-0330eda162ac\""
            ]
        },
        {
            "name": "az storagesync registered-server list",
            "summary": "List all registered servers for a given storage sync service.",
            "syntax": "az storagesync registered-server list --storage-sync-service\n                                      [--resource-group]",
            "required_parameters": [
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storagesync registered-server list --resource-group \"SampleResourceGroup\" --storage-sync-service \\\n\"SampleStorageSyncService\""
            ]
        },
        {
            "name": "az storagesync registered-server show",
            "summary": "Show the properties for a given registered server.",
            "syntax": "az storagesync registered-server show --server-id\n                                      --storage-sync-service\n                                      [--resource-group]",
            "required_parameters": [
                "--server-id",
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storagesync registered-server show --resource-group \"SampleResourceGroup\" --storage-sync-service \\\n\"SampleStorageSyncService\" --server-id \"080d4133-bdb5-40a0-96a0-71a6057bfe9a\""
            ]
        },
        {
            "name": "az storagesync registered-server wait",
            "summary": "Place the CLI in a waiting state until a condition of a registered server is met.",
            "syntax": "az storagesync registered-server wait --resource-group\n                                      --server-id\n                                      --storage-sync-service-name\n                                      [--created]\n                                      [--custom]\n                                      [--deleted]\n                                      [--exists]\n                                      [--interval]\n                                      [--timeout]\n                                      [--updated]",
            "required_parameters": [
                "--resource-group -g",
                "--server-id",
                "--storage-sync-service-name"
            ],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--interval",
                "--timeout",
                "--updated"
            ],
            "examples": [
                "az storagesync registered-server wait --resource-group \"SampleResourceGroup\" \\\n--storage-sync-service \"SampleStorageSyncService\" \\\n--server-id \"080d4133-bdb5-40a0-96a0-71a6057bfe9a\" --deleted"
            ]
        }
    ]
}