{
    "command_group": "az storagesync sync-group",
    "description": "Manage sync group.",
    "status": "GA",
    "commands": [
        {
            "name": "az storagesync sync-group create",
            "summary": "Create a new sync group.",
            "syntax": "az storagesync sync-group create --name\n                                 --storage-sync-service\n                                 [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storagesync sync-group create --resource-group \"SampleResourceGroup\" \\\n--storage-sync-service \"SampleStorageSyncService\" --name \"SampleSyncGroup\""
            ]
        },
        {
            "name": "az storagesync sync-group delete",
            "summary": "Delete a given sync group.",
            "syntax": "az storagesync sync-group delete --name\n                                 --storage-sync-service\n                                 [--resource-group]\n                                 [--yes]",
            "required_parameters": [
                "--name -n",
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g",
                "--yes -y"
            ],
            "examples": [
                "az storagesync sync-group delete --resource-group \"SampleResourceGroup\" \\\n--storage-sync-service \"SampleStorageSyncService\" --name \"SampleSyncGroup\""
            ]
        },
        {
            "name": "az storagesync sync-group list",
            "summary": "List all sync groups in a storage sync service.",
            "syntax": "az storagesync sync-group list --storage-sync-service\n                               [--resource-group]",
            "required_parameters": [
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storagesync sync-group list --resource-group \"SampleResourceGroup\" \\\n--storage-sync-service \"SampleStorageSyncService\""
            ]
        },
        {
            "name": "az storagesync sync-group show",
            "summary": "Show the properties for a given sync group.",
            "syntax": "az storagesync sync-group show --name\n                               --storage-sync-service\n                               [--resource-group]",
            "required_parameters": [
                "--name -n",
                "--storage-sync-service"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az storagesync sync-group show --resource-group \"SampleResourceGroup\" \\\n--storage-sync-service \"SampleStorageSyncService\" --name \"SampleSyncGroup\""
            ]
        }
    ]
}