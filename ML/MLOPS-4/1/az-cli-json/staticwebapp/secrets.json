{
    "command_group": "az staticwebapp secrets",
    "description": "Manage deployment token for the static app.",
    "status": "GA",
    "commands": [
        {
            "name": "az staticwebapp secrets list",
            "summary": "List the deployment token for the static app.",
            "syntax": "az staticwebapp secrets list --name\n                             [--resource-group]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--resource-group -g"
            ],
            "examples": [
                "az staticwebapp secrets list --name MyStaticAppName"
            ]
        },
        {
            "name": "az staticwebapp secrets reset-api-key",
            "summary": "Reset the deployment token for the static app.",
            "syntax": "az staticwebapp secrets reset-api-key --name\n                                      [--no-wait]\n                                      [--resource-group]",
            "required_parameters": [
                "--name -n"
            ],
            "optional_parameters": [
                "--no-wait",
                "--resource-group -g"
            ],
            "examples": [
                "az staticwebapp secrets reset-api-key --name MyStaticAppName"
            ]
        }
    ]
}