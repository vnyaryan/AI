{
    "command_group": "az repos",
    "description": "Manage Azure Repos.",
    "status": "GA",
    "commands": [
        {
            "name": "az repos create",
            "summary": "Create a Git repository in a team project.",
            "syntax": "az repos create --name\n                [--detect {false, true}]\n                [--open]\n                [--org]\n                [--project]",
            "required_parameters": [
                "--name"
            ],
            "optional_parameters": [
                "--detect",
                "--open",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        },
        {
            "name": "az repos delete",
            "summary": "Delete a Git repository in a team project.",
            "syntax": "az repos delete --id\n                [--detect {false, true}]\n                [--org]\n                [--project]\n                [--yes]",
            "required_parameters": [
                "--id"
            ],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--project -p",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az repos list",
            "summary": "List Git repositories of a team project.",
            "syntax": "az repos list [--detect {false, true}]\n              [--org]\n              [--project]",
            "required_parameters": [],
            "optional_parameters": [
                "--detect",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        },
        {
            "name": "az repos show",
            "summary": "Get the details of a Git repository.",
            "syntax": "az repos show --repository\n              [--detect {false, true}]\n              [--open]\n              [--org]\n              [--project]",
            "required_parameters": [
                "--repository -r"
            ],
            "optional_parameters": [
                "--detect",
                "--open",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        },
        {
            "name": "az repos update",
            "summary": "Update the Git repository.",
            "syntax": "az repos update --repository\n                [--default-branch]\n                [--detect {false, true}]\n                [--name]\n                [--org]\n                [--project]",
            "required_parameters": [
                "--repository -r"
            ],
            "optional_parameters": [
                "--default-branch",
                "--detect",
                "--name",
                "--org --organization",
                "--project -p"
            ],
            "examples": []
        }
    ]
}