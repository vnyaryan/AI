{
    "command_group": "az lab environment",
    "description": "Manage environments for an azure devtest lab.",
    "status": "Preview",
    "commands": [
        {
            "name": "az lab environment create",
            "summary": "Create an existing environment. This operation can take a while to complete.",
            "syntax": "az lab environment create --arm-template\n                          --lab-name\n                          --name\n                          --resource-group\n                          [--artifact-source-name]\n                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                          [--parameters]\n                          [--tags]",
            "required_parameters": [
                "--arm-template",
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--artifact-source-name",
                "--no-wait",
                "--parameters",
                "--tags"
            ],
            "examples": []
        },
        {
            "name": "az lab environment delete",
            "summary": "Delete environment. This operation can take a while to complete.",
            "syntax": "az lab environment delete --lab-name\n                          --name\n                          --resource-group\n                          [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]\n                          [--yes]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--no-wait",
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az lab environment list",
            "summary": "List environments in a given user profile.",
            "syntax": "az lab environment list --lab-name\n                        --resource-group\n                        [--expand]\n                        [--filter]\n                        [--max-items]\n                        [--next-token]\n                        [--orderby]\n                        [--top]",
            "required_parameters": [
                "--lab-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand",
                "--filter",
                "--max-items",
                "--next-token",
                "--orderby",
                "--top"
            ],
            "examples": [
                "az lab environment list --lab-name Lab --resource-group rg"
            ]
        },
        {
            "name": "az lab environment show",
            "summary": "Get environment.",
            "syntax": "az lab environment show --lab-name\n                        --name\n                        --resource-group\n                        [--expand]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": []
        },
        {
            "name": "az lab environment wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az lab environment wait [--created]\n                        [--custom]\n                        [--deleted]\n                        [--exists]\n                        [--expand]\n                        [--ids]\n                        [--interval]\n                        [--lab-name]\n                        [--name]\n                        [--resource-group]\n                        [--subscription]\n                        [--timeout]\n                        [--updated]\n                        [--user-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--ids",
                "--interval",
                "--lab-name",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated",
                "--user-name"
            ],
            "examples": []
        }
    ]
}