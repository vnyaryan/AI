{
    "command_group": "az lab secret",
    "description": "Manage secrets of an azure devtest lab.",
    "status": "Preview",
    "commands": [
        {
            "name": "az lab secret delete",
            "summary": "Delete secret.",
            "syntax": "az lab secret delete --lab-name\n                     --name\n                     --resource-group\n                     [--yes]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--yes -y"
            ],
            "examples": []
        },
        {
            "name": "az lab secret list",
            "summary": "List secrets in a given user profile.",
            "syntax": "az lab secret list --lab-name\n                   --resource-group\n                   [--expand]\n                   [--filter]\n                   [--max-items]\n                   [--next-token]\n                   [--orderby]\n                   [--top]",
            "required_parameters": [
                "--lab-name",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand",
                "--filter",
                "--max-items",
                "--next-token",
                "--orderby",
                "--top"
            ],
            "examples": []
        },
        {
            "name": "az lab secret set",
            "summary": "Set a secret for a lab.",
            "syntax": "az lab secret set --lab-name\n                  --name\n                  --resource-group\n                  --value\n                  [--no-wait {0, 1, f, false, n, no, t, true, y, yes}]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g",
                "--value"
            ],
            "optional_parameters": [
                "--no-wait"
            ],
            "examples": []
        },
        {
            "name": "az lab secret show",
            "summary": "Get secret.",
            "syntax": "az lab secret show --lab-name\n                   --name\n                   --resource-group\n                   [--expand]",
            "required_parameters": [
                "--lab-name",
                "--name -n",
                "--resource-group -g"
            ],
            "optional_parameters": [
                "--expand"
            ],
            "examples": []
        },
        {
            "name": "az lab secret wait",
            "summary": "Place the CLI in a waiting state until a condition is met.",
            "syntax": "az lab secret wait [--created]\n                   [--custom]\n                   [--deleted]\n                   [--exists]\n                   [--expand]\n                   [--ids]\n                   [--interval]\n                   [--lab-name]\n                   [--name]\n                   [--resource-group]\n                   [--subscription]\n                   [--timeout]\n                   [--updated]\n                   [--user-name]",
            "required_parameters": [],
            "optional_parameters": [
                "--created",
                "--custom",
                "--deleted",
                "--exists",
                "--expand",
                "--ids",
                "--interval",
                "--lab-name",
                "--name -n",
                "--resource-group -g",
                "--subscription",
                "--timeout",
                "--updated",
                "--user-name"
            ],
            "examples": []
        }
    ]
}