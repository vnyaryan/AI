{
    "command_group": "az batch pool autoscale",
    "description": "Manage automatic scaling of Batch pools.",
    "status": "GA",
    "commands": [
        {
            "name": "az batch pool autoscale disable",
            "summary": "Disables automatic scaling for a Pool.",
            "syntax": "az batch pool autoscale disable --pool-id\n                                [--account-endpoint]\n                                [--account-key]\n                                [--account-name]",
            "required_parameters": [
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name"
            ],
            "examples": []
        },
        {
            "name": "az batch pool autoscale enable",
            "summary": "Enables automatic scaling for a Pool.",
            "syntax": "az batch pool autoscale enable --pool-id\n                               [--account-endpoint]\n                               [--account-key]\n                               [--account-name]\n                               [--auto-scale-evaluation-interval]\n                               [--auto-scale-formula]\n                               [--if-match]\n                               [--if-modified-since]\n                               [--if-none-match]\n                               [--if-unmodified-since]",
            "required_parameters": [
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--auto-scale-evaluation-interval",
                "--auto-scale-formula",
                "--if-match",
                "--if-modified-since",
                "--if-none-match",
                "--if-unmodified-since"
            ],
            "examples": []
        },
        {
            "name": "az batch pool autoscale evaluate",
            "summary": "Gets the result of evaluating an automatic scaling formula on the Pool.",
            "syntax": "az batch pool autoscale evaluate --pool-id\n                                 [--account-endpoint]\n                                 [--account-key]\n                                 [--account-name]\n                                 [--auto-scale-formula]\n                                 [--json-file]",
            "required_parameters": [
                "--pool-id"
            ],
            "optional_parameters": [
                "--account-endpoint",
                "--account-key",
                "--account-name",
                "--auto-scale-formula",
                "--json-file"
            ],
            "examples": []
        }
    ]
}